					<appinfo>
						<targetElement xmlns="http://www.opengis.net/gml/3.2">gsmlb:DeterminationMethodTerm</targetElement>
						<targetCodeList xmlns="http://www.opengis.net/gml/3.3/exr">DeterminationMethodTerm</targetCodeList>
					</appinfo>
				</annotation>
