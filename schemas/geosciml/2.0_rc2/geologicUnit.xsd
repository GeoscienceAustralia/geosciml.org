            <enumeration value="Deformation"/>
            <enumeration value="Excavation"/>
            <enumeration value="Geomorphologic"/>
            <enumeration value="LithotectonicUnit">
                <annotation>
                    <documentation>a unit defined by geologic history or deformation
Placeholder for units defined by a distinctive fabric</documentation>
                </annotation>
            </enumeration>
            <enumeration value="PolarityChronostratigraphicUnit">
                <annotation>
                    <documentation>a body of rock that contains its primary magnetic-polarity record</documentation>
                </annotation>
            </enumeration>
            <enumeration value="Pedostratigraphic"/>
            <enumeration value="Pedoderm"/>
            <enumeration value="MassMovement"/>
            <enumeration value="MagnetostratigraphicUnit">
