                    <documentation>a body of rock defined by specific remanent-magnetic properties </documentation>
                </annotation>
            </enumeration>
            <enumeration value="Lithostratigraphic"/>
            <enumeration value="Lithologic"/>
            <enumeration value="Lithogenetic"/>
            <enumeration value="GeophysicalUnit">
