<?xml version="1.0" encoding="UTF-8"?>
<mapping
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:gsmlc="http://xmlns.geosciml.org/Collection/3.0"
	xmlns="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:2.0"
	xsi:schemaLocation="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:2.0 http://projects.arcs.org.au/svn/fullmoon/trunk/resources/dependency-util/package-class-map.xsd"
	id="ClassMap_Collection_GeoSciML_v3.xml">
	<description>Provides a mapping of classes from the collection module of GeoSciML to elements from GeoSciML XML Schema Version 3.0.0</description>
	<imports>
		<schema prefix="gsmlc" namespace="http://xmlns.geosciml.org/Collection/3.0" schemaLocation="http://schemas.geosciml.org/collection/3.0/collection.xsd" />  <!-- Note prefix is "gsmlc", not "gsml"  -->
	</imports>

<!-- ==================================================================== -->
<!-- Collection leaf -->	
	
	<entry>
		<umlClass>GSML</umlClass>
		<umlStereotype>FeatureType</umlStereotype>
		<gmlObjectElement>gsmlc:GSML</gmlObjectElement>
		<gmlType>gsmlc:GSMLType</gmlType>
		<gmlPropertyType>gsmlc:GSMLPropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>GSMLitem</umlClass>
		<umlStereotype>Union</umlStereotype>
		<gmlObjectElement xsi:nil="true"/>
		<gmlType xsi:nil="true"/>
		<gmlPropertyType>gsmlc:GSMLitemPropertyType</gmlPropertyType>
	</entry>

</mapping>