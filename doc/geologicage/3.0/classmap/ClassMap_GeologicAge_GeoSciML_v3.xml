<?xml version="1.0" encoding="UTF-8"?>
<mapping xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:gsmlga="http://xmlns.geosciml.org/GeologicAge/3.0"
	xmlns="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:2.0"
	xsi:schemaLocation="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:2.0 http://projects.arcs.org.au/svn/fullmoon/trunk/resources/dependency-util/package-class-map.xsd"
	id="ClassMap_GeologicAge_GeoSciML_v3.xml">
	<description>Provides a mapping of classes from the GeologicAge module of GeoSciML to elements from GeoSciML XML Schema Version 3.0.0</description>
	<imports>
		<schema prefix="gsmlga" namespace="http://xmlns.geosciml.org/GeologicAge/3.0" schemaLocation="http://schemas.geosciml.org/geologicage/3.0/geologicAge.xsd"/>
	</imports>
<!-- ================================================================= -->
<!-- FeatureType -->
	<entry>
		<umlClass>GeologicEvent</umlClass>
		<umlStereotype>FeatureType</umlStereotype>
		<gmlObjectElement>gsmlga:GeologicEvent</gmlObjectElement>
		<gmlType>gsmlga:GeologicEventType</gmlType>
		<gmlPropertyType>gsmlga:GeologicEventPropertyType</gmlPropertyType>
	</entry>
<!-- Unstereotyped association class -->
	<entry>
		<umlClass>GeologicHistory</umlClass>
		<gmlObjectElement>gsmlga:GeologicHistory</gmlObjectElement>
		<gmlType>gsmlga:GeologicHistoryType</gmlType>
		<gmlPropertyType>gsmlga:GeologicHistoryPropertyType</gmlPropertyType>
	</entry>
<!-- DataTypes -->
	<entry>
		<umlClass>GeologicDateEstimate</umlClass>
		<umlStereotype>DataType</umlStereotype>
		<gmlObjectElement>gsmlga:GeologicDateEstimate</gmlObjectElement>
		<gmlType>gsmlga:GeologicDateEstimateType</gmlType>
		<gmlPropertyType>gsmlga:GeologicDateEstimatePropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>NumericAgeRange</umlClass>
		<umlStereotype>DataType</umlStereotype>
		<gmlObjectElement>gsmlga:NumericAgeRange</gmlObjectElement>
		<gmlType>gsmlga:NumericAgeRangeType</gmlType>
		<gmlPropertyType>gsmlga:NumericAgeRangePropertyType</gmlPropertyType>
	</entry>
</mapping>
