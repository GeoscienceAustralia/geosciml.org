	id="ClassMap_CGI_Utilities_GeoSciML_v3.xml">
	<description>Provides a mapping of classes from the utilities module of GeoSciML to elements from GeoSciML XML Schema Version 3.0.</description>
	<imports>
		<schema prefix="gsmlu" namespace="http://xmlns.geosciml.org/Utilities/3.0" schemaLocation="http://www.geosciml.org/cgiutilities/3.0/xsd/cgiUtilities.xsd" />
	</imports>
<!-- ===================================================================================== -->
	<entry>
		<umlClass>CGI_GeometricDescriptionValue</umlClass>
		<umlStereotype>DataType</umlStereotype>
		<gmlObjectElement>gsmlu:CGI_GeometricDescriptionValue</gmlObjectElement>
		<gmlType>gsmlu:CGI_GeometricDescriptionValueType</gmlType>
		<gmlPropertyType>gsmlu:CGI_GeometricDescriptionValuePropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>CGI_LinearOrientation</umlClass>
		<umlStereotype>DataType</umlStereotype>
