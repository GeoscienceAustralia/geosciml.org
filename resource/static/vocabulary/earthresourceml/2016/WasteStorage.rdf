   </skos:ConceptScheme>
   <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/waste-storage/surface">
      <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/2016.01/waste-storage"/>
      <skos:definition xml:lang="en">Mining waste storage at open-sky environment (ground level)</skos:definition>
      <skos:prefLabel xml:lang="en">surface</skos:prefLabel>
      <skos:notation rdf:datatype="LocalHierarchyKey">01</skos:notation>
      <skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/2016.01/waste-storage"/>
      <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/waste-storage/covered"/>
      <dcterms:source xml:lang="en">This vocabulary</dcterms:source>
   </skos:Concept>
   <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/waste-storage/covered">
      <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/2016.01/waste-storage"/>
