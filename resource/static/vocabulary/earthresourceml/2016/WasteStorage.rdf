      <skos:prefLabel xml:lang="en">covered</skos:prefLabel>
      <skos:notation rdf:datatype="LocalHierarchyKey">01.1</skos:notation>
      <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/waste-storage/surface"/>
