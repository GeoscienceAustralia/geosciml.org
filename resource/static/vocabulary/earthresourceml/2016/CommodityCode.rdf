   </skos:Concept>
   <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/commodity-code/holmium">
      <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/2016.01/commodity-code"/>
      <skos:definition xml:lang="en">Holmium is a chemical element with the symbol Ho and atomic number 67. Part of the lanthanide series, holmium is a rare earth element.</skos:definition>
      <skos:prefLabel xml:lang="en">holmium</skos:prefLabel>
      <skos:notation rdf:datatype="AbbreviatedLabel">Ho</skos:notation>
      <skos:notation rdf:datatype="LocalHierarchyKey">02.01.03.01.05</skos:notation>
