   <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/commodity-code/cerium">
      <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/2016.01/commodity-code"/>
      <skos:definition xml:lang="en">Cerium is a chemical element with symbol Ce and atomic number 58. It is a soft, silvery, ductile metal which easily oxidizes in air.</skos:definition>
      <skos:prefLabel xml:lang="en">cerium</skos:prefLabel>
      <skos:notation rdf:datatype="AbbreviatedLabel">Ce</skos:notation>
