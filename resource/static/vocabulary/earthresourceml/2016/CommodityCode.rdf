      <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/2016.01/commodity-code"/>
      <skos:definition xml:lang="en">Commodity is obtained by extracting from material mined from the Earth.</skos:definition>
      <skos:prefLabel xml:lang="en">produced commodity</skos:prefLabel>
      <skos:notation rdf:datatype="AbbreviatedLabel">ProdC</skos:notation>
      <skos:notation rdf:datatype="LocalHierarchyKey">02</skos:notation>
