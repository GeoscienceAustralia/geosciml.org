      <dcterms:source xml:lang="en">This vocabulary</dcterms:source>
   </skos:Concept>
   <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/commodity-code/thulium">
      <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/2016.01/commodity-code"/>
      <skos:definition xml:lang="en">Thulium is a chemical element that has the symbol Tm and atomic number 69. It is the thirteenth and antepenultimate element in the lanthanide series</skos:definition>
      <skos:prefLabel xml:lang="en">thulium</skos:prefLabel>
      <skos:notation rdf:datatype="AbbreviatedLabel">Tm</skos:notation>
      <skos:notation rdf:datatype="LocalHierarchyKey">02.01.03.01.08</skos:notation>
