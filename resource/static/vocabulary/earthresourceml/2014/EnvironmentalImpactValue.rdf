        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/environmental-impact/mine-drainage"/>
        <skos:notation rdf:datatype="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact">neutral-mine-drainage</skos:notation>
