        <skos:prefLabel xml:lang="en">surface aqueous liquid discharge</skos:prefLabel>
        <skos:definition>Impact of aqueous liquid emission into the subsurface.</skos:definition>
        <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact"/>
      </skos:Concept>
    </skos:member>
    <skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/environmental-impact/liquid-emission"/>
    <skos:member>
      <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/environmental-impact/dust">
        <dc:source xml:lang="en">ISO 4225 (ISO, 1994), quoted in http://www.who.int/occupational_health/publications/en/oehairbornedust3.pdf; http://ecorestoration.montana.edu/mineland/guide/problem/impacts/fugitive.htm</dc:source>
        <skos:notation rdf:datatype="gtwg:LocalHierarchyKey">01.1.2..</skos:notation>
        <skos:broader>
          <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/environmental-impact/particulate-emission">
            <dc:source xml:lang="en">This vocabulary</dc:source>
            <skos:notation rdf:datatype="gtwg:LocalHierarchyKey">01.1..</skos:notation>
            <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/environmental-impact/emission"/>
            <skos:notation rdf:datatype="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact">particulate-emission</skos:notation>
            <skos:prefLabel xml:lang="en">particulate emission</skos:prefLabel>
            <skos:definition>Emission of suspended particulate material into the air or water</skos:definition>
            <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact"/>
          </skos:Concept>
        </skos:broader>
        <skos:notation rdf:datatype="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact">dust</skos:notation>
        <skos:prefLabel xml:lang="en">dust</skos:prefLabel>
        <skos:definition>Emission of dust into the air. Dust is defines as small solid particles with a diameter less than 75 µm. The particles are small enough to remain suspended in the air for some time, but settle out under their own weight in still air.</skos:definition>
        <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact"/>
      </skos:Concept>
    </skos:member>
    <skos:member>
      <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/environmental-impact/heat">
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:notation rdf:datatype="gtwg:LocalHierarchyKey">04..</skos:notation>
        <skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact"/>
        <skos:notation rdf:datatype="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact">heat</skos:notation>
        <skos:prefLabel xml:lang="en">heat</skos:prefLabel>
        <skos:definition>Impact includes noticable thermal release from mining activity (e.g. warm water discharge, exothermic mineral reactions).</skos:definition>
        <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact"/>
      </skos:Concept>
    </skos:member>
    <skos:member>
      <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/environmental-impact/landslide">
        <dc:source xml:lang="en">ISPRA - mining structural problems database and classification of underground mining sites by means of a risk index.</dc:source>
        <skos:notation rdf:datatype="gtwg:LocalHierarchyKey">02.5..</skos:notation>
        <skos:broader>
          <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/environmental-impact/physical-impact">
            <dc:source xml:lang="en">This vocabulary</dc:source>
            <skos:notation rdf:datatype="gtwg:LocalHierarchyKey">02..</skos:notation>
            <skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact"/>
            <skos:notation rdf:datatype="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact">physical-impact</skos:notation>
            <skos:prefLabel xml:lang="en">physical impact</skos:prefLabel>
            <skos:definition>Impact due to direct physical effects of natural processes resulting from mining activity.</skos:definition>
            <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact"/>
          </skos:Concept>
        </skos:broader>
        <skos:notation rdf:datatype="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact">landslide</skos:notation>
        <skos:altLabel xml:lang="en">slope failure</skos:altLabel>
        <skos:prefLabel xml:lang="en">landslide</skos:prefLabel>
        <skos:definition>Downslope displacement of material due to excavation related to resource extraction. Includes slope failure on waste or tailings heaps.</skos:definition>
        <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact"/>
      </skos:Concept>
    </skos:member>
    <skos:member>
      <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/environmental-impact/dam-failure">
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:notation rdf:datatype="gtwg:LocalHierarchyKey">02.4..</skos:notation>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/environmental-impact/physical-impact"/>
        <skos:notation rdf:datatype="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact">dam-failure</skos:notation>
        <skos:prefLabel xml:lang="en">dam failure</skos:prefLabel>
        <skos:definition>Breach or destruction of a large water or tailing containment structure and release of contained water that could result in downstream damage or pollution.</skos:definition>
        <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact"/>
      </skos:Concept>
    </skos:member>
    <skos:member>
      <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/environmental-impact/gaseous-emission">
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:notation rdf:datatype="gtwg:LocalHierarchyKey">01.3..</skos:notation>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/environmental-impact/emission"/>
        <skos:notation rdf:datatype="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact">gaseous-emission</skos:notation>
        <skos:prefLabel xml:lang="en">gaseous emission</skos:prefLabel>
        <skos:definition>Emission of gaseous substance, including steam or release of gas resulting from mining activity.</skos:definition>
        <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact"/>
      </skos:Concept>
    </skos:member>
    <skos:member>
      <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/environmental-impact/erosion">
        <dc:source xml:lang="en">IGH International Glossary of Hydrology. WMO - UNESCO, 1992. 413 p.</dc:source>
        <skos:notation rdf:datatype="gtwg:LocalHierarchyKey">02.1..</skos:notation>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/environmental-impact/physical-impact"/>
        <skos:notation rdf:datatype="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact">erosion</skos:notation>
        <skos:prefLabel xml:lang="en">erosion</skos:prefLabel>
        <skos:definition>Impact due to loss of material shaped by mining activities through water or wind action.</skos:definition>
        <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact"/>
      </skos:Concept>
    </skos:member>
    <skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/environmental-impact/emission"/>
    <skos:member>
      <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/environmental-impact/basic-mine-drainage">
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:notation rdf:datatype="gtwg:LocalHierarchyKey">01.2.1.2.1.2</skos:notation>
        <skos:broader>
          <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/environmental-impact/mine-drainage">
            <dc:source xml:lang="en">This vocabulary</dc:source>
            <skos:notation rdf:datatype="gtwg:LocalHierarchyKey">01.2.1.2.1</skos:notation>
            <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/environmental-impact/surface-aqueous-liquid-discharge"/>
            <skos:notation rdf:datatype="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact">mine-drainage</skos:notation>
            <skos:prefLabel xml:lang="en">mine drainage</skos:prefLabel>
            <skos:definition>Surface discharge of aqueous liquid that has interacted with mineralized rock disturbed by Earth Resource extraction activities</skos:definition>
            <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact"/>
          </skos:Concept>
        </skos:broader>
        <skos:notation rdf:datatype="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact">basic-mine-drainage</skos:notation>
        <skos:prefLabel xml:lang="en">basic mine drainage</skos:prefLabel>
        <skos:definition>Mine drainage that has a pH value of greater than 7.0.</skos:definition>
        <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact"/>
      </skos:Concept>
    </skos:member>
    <skos:member>
      <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/environmental-impact/collapse">
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:notation rdf:datatype="gtwg:LocalHierarchyKey">02.3.1..</skos:notation>
        <skos:broader>
          <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/environmental-impact/subsidence">
            <dc:source xml:lang="en">This vocabulary</dc:source>
            <skos:notation rdf:datatype="gtwg:LocalHierarchyKey">02.3..</skos:notation>
            <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/environmental-impact/physical-impact"/>
            <skos:notation rdf:datatype="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact">subsidence</skos:notation>
            <skos:prefLabel xml:lang="en">subsidence</skos:prefLabel>
            <skos:definition>Settlement of land or soil through compaction of loose or excavated material</skos:definition>
            <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact"/>
          </skos:Concept>
        </skos:broader>
        <skos:notation rdf:datatype="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact">collapse</skos:notation>
        <skos:prefLabel xml:lang="en">collapse</skos:prefLabel>
        <skos:definition>Collapse of surface material into underground excavations, resulting in major surface subsidence and disruption.</skos:definition>
        <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact"/>
      </skos:Concept>
    </skos:member>
