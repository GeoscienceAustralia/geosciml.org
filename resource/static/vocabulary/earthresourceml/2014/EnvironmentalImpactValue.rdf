    <skos:member>
      <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/environmental-impact/runoff-water">
        <skos:notation rdf:datatype="gtwg:LocalHierarchyKey">01.2.1.2.2</skos:notation>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/environmental-impact/surface-aqueous-liquid-discharge"/>
        <skos:notation rdf:datatype="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact">runoff-water</skos:notation>
        <skos:altLabel xml:lang="en">surface water effluents</skos:altLabel>
        <skos:prefLabel xml:lang="en">runoff water</skos:prefLabel>
        <skos:definition>Surface discharge of aqueous liquid sourced by rainfall that has not interacted with mineralized rock.</skos:definition>
        <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact"/>
      </skos:Concept>
    </skos:member>
