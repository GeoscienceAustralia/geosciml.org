  <skos:ConceptScheme rdf:about="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact">
    <skos:definition xml:lang="en">This resource defines the concept space for Environmental Impact Category vocabulary concepts by extension; it includes all concepts in this conceptScheme, as well as concepts in previous versions of the scheme.</skos:definition>
    <skos:prefLabel xml:lang="en">Environmental Impact Category vocabulary</skos:prefLabel>
    <dc:creator xml:lang="en">CGI Geoscience Terminology Working Group</dc:creator>
  </skos:ConceptScheme>
  <skos:Collection rdf:about="http://resource.geosciml.org/classifier/cgi/environmental-impact/">
    <skos:member>
      <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/environmental-impact/non-aqueous-liquid-emission">
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:notation rdf:datatype="gtwg:LocalHierarchyKey">01.2.2..</skos:notation>
        <skos:broader>
          <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/environmental-impact/liquid-emission">
            <dc:source xml:lang="en">This vocabulary</dc:source>
            <skos:notation rdf:datatype="gtwg:LocalHierarchyKey">01.2..</skos:notation>
            <skos:broader>
              <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/environmental-impact/emission">
                <dc:source xml:lang="en">This vocabulary. adapted from (ESH Environment, Safety and Health Thesaurus/Dictionary. U.S. Department of Energy. DOE/EH-0186, 1991. 510 p.): Any spilling, leaking, pumping, pouring, emitting, emptying, discharging, injecting, escaping, leaching, dumping, or otherwise disposing of substances into the environment.</dc:source>
                <skos:notation rdf:datatype="gtwg:LocalHierarchyKey">01..</skos:notation>
                <skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact"/>
                <skos:notation rdf:datatype="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact">emission</skos:notation>
                <skos:prefLabel xml:lang="en">emission</skos:prefLabel>
                <skos:definition>Any release or disposal of a substance into the environment</skos:definition>
                <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact"/>
              </skos:Concept>
            </skos:broader>
            <skos:notation rdf:datatype="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact">liquid-emission</skos:notation>
            <skos:prefLabel xml:lang="en">liquid emission</skos:prefLabel>
            <skos:definition>Emission of liquids from resource extraction site; principal concern is likely to be toxic chemical constituents in the liquids, but these are too varied to enumerate in this vocabulary</skos:definition>
            <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact"/>
          </skos:Concept>
        </skos:broader>
        <skos:notation rdf:datatype="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact">non-aqueous-liquid-emission</skos:notation>
        <skos:prefLabel xml:lang="en">non-aqueous liquid emission</skos:prefLabel>
        <skos:definition>Emission of liquid in which water is not the most abundant constituent.</skos:definition>
        <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact"/>
      </skos:Concept>
    </skos:member>
    <skos:member>
      <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/environmental-impact/radiation">
        <dc:source xml:lang="en">ISPRA - mining structural problems database and classification of underground mining sites by means of a risk index.</dc:source>
        <skos:notation rdf:datatype="gtwg:LocalHierarchyKey">01.4.1</skos:notation>
        <skos:broader>
          <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/environmental-impact/health-impact">
            <dc:source xml:lang="en">ISPRA - mining structural problems database and classification of underground mining sites by means of a risk index.</dc:source>
            <skos:notation rdf:datatype="gtwg:LocalHierarchyKey">03…</skos:notation>
            <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/environmental-impact/emission"/>
            <skos:notation rdf:datatype="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact">health-impact</skos:notation>
            <skos:prefLabel xml:lang="en">health impact</skos:prefLabel>
            <skos:definition>Resource extraction activity directly or indirectly impacts the health of living organisms (human, other animals, plants...)</skos:definition>
            <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact"/>
          </skos:Concept>
        </skos:broader>
        <skos:broader>
          <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/environmental-impact/radioactive-emission">
            <dc:source xml:lang="en">This vocabulary</dc:source>
            <skos:notation rdf:datatype="gtwg:LocalHierarchyKey">01.4..</skos:notation>
            <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/environmental-impact/health-impact"/>
            <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/environmental-impact/emission"/>
            <skos:notation rdf:datatype="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact">radioactive-emission</skos:notation>
            <skos:prefLabel xml:lang="en">radioactive emission</skos:prefLabel>
            <skos:definition>Emission of radioactive material or energy radiation due to radioactive decay of uncoverning of minerals.</skos:definition>
            <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact"/>
          </skos:Concept>
        </skos:broader>
        <skos:notation rdf:datatype="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact">radiation</skos:notation>
        <skos:prefLabel xml:lang="en">radiation</skos:prefLabel>
        <skos:definition>Emission on air of radionuclides substance gangerous for many health situation</skos:definition>
        <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact"/>
      </skos:Concept>
    </skos:member>
    <skos:member>
      <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/environmental-impact/surface-aqueous-liquid-discharge">
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:notation rdf:datatype="gtwg:LocalHierarchyKey">01.2.1.2</skos:notation>
        <skos:broader>
          <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/environmental-impact/aqueous-liquid-emission">
            <dc:source xml:lang="en">This vocabulary</dc:source>
            <skos:notation rdf:datatype="gtwg:LocalHierarchyKey">01.2.1</skos:notation>
