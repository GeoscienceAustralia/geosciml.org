            <skos:notation rdf:datatype="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact">aqueous-liquid-emission</skos:notation>
            <skos:prefLabel xml:lang="en">aqueous liquid emission</skos:prefLabel>
            <skos:definition>Emission of liquids in which water is the most abundant constituent.</skos:definition>
            <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201401/environmental-impact"/>
          </skos:Concept>
