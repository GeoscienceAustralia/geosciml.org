    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
	xmlns:gtwg="http://resource.geosciml.org/authority/cgi/gtwg"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://resource.geosciml.org/classifierscheme/cgi/201403/raw-material-role">
  <owl:Ontology rdf:about="http://resource.geosciml.org/static/vocabulary/earthresourceml/2014/RawMaterialRole.rdf">
    <owl:imports rdf:resource="http://purl.org/dc/elements/1.1/"/>
