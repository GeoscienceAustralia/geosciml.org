      <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/mine-status/abandoned"/>
      <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/mine-status/care-and-maintenance"/>
      <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/mine-status/retention"/>
      <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/mine-status/historic"/>
      <dcterms:source xml:lang="en">this vocabulary</dcterms:source>
   </skos:Concept>
