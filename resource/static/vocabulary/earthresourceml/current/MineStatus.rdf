   </skos:Concept>
   <skos:ConceptScheme rdf:about="http://resource.geosciml.org/classifierscheme/cgi/2016.01/mine-status">
      <skos:hasTopConcept rdf:resource="http://resource.geosciml.org/classifier/cgi/mine-status/not-operating"/>
   </skos:ConceptScheme>
   <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/mine-status/not-operating">
      <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/2016.01/mine-status"/>
      <skos:definition xml:lang="en">A mine has been operating and then closed or suspended</skos:definition>
      <skos:prefLabel xml:lang="en">not operating</skos:prefLabel>
      <skos:notation rdf:datatype="LocalHierarchyKey">02..</skos:notation>
      <skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/2016.01/mine-status"/>
