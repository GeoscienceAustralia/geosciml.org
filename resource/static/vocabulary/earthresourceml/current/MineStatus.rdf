   <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/mine-status/operating">
      <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/2016.01/mine-status"/>
      <skos:definition xml:lang="en">A mine is operating</skos:definition>
      <skos:prefLabel xml:lang="en">operating</skos:prefLabel>
      <skos:notation rdf:datatype="LocalHierarchyKey">01..</skos:notation>
      <skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/2016.01/mine-status"/>
