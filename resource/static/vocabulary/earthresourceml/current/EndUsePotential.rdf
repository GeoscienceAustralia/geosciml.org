   </skos:Concept>
   <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/end-use-potential/nuclear-energy-source">
      <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/2016.01/end-use-potential"/>
      <skos:definition xml:lang="en">Material has potential for energy production through use in nuclear reactor</skos:definition>
      <skos:prefLabel xml:lang="en">nuclear energy source</skos:prefLabel>
      <skos:notation rdf:datatype="LocalHierarchyKey">04.2..</skos:notation>
      <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/end-use-potential/energy-minerals"/>
      <dcterms:source xml:lang="en">this vocabulary</dcterms:source>
   </skos:Concept>
