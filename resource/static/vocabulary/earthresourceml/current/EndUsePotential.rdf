      <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/2016.01/end-use-potential"/>
      <skos:definition xml:lang="en">Precious or semi precious stones or metals for use in jewellery or decorative work</skos:definition>
      <skos:prefLabel xml:lang="en">gemstone</skos:prefLabel>
      <skos:notation rdf:datatype="LocalHierarchyKey">05..</skos:notation>
      <skos:altLabel xml:lang="en">precious stones</skos:altLabel>
      <skos:altLabel xml:lang="en">semi-precious stones</skos:altLabel>
      <skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/2016.01/end-use-potential"/>
      <dcterms:source xml:lang="en">BGS Mines and Quarries Database</dcterms:source>
