	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/geneticcategory/biological_sedimentary_genesis"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/geneticcategory/metamorphic_genesis"/>
