	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/geneticcategory/subaqueous_extrusive_genesis"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/geneticcategory/impact_genesis"/>
