momclass:
      rdf:type skos:Collection ;
      rdfs:label "mapped feature observation method concepts"@en ;
      skos:definition "This resource defines the concept space for mapped feature observation method concepts by extension; it includes all concepts in this conceptScheme, as well as concepts in previous versions of the scheme."@en ;
      skos:member momclass:observed_elevation_data , momclass:observed_outcrop_gps_location , momclass:inferred_projection_covering_mapped_unit , momclass:observed_distant_outcrop , momclass:inferred , momclass:observed_outcrop__topographic_map_location , momclass:inferred_radiometric_survey , momclass:inferred_aeromagnetic_survey , momclass:inferred_projection_unmapped_surficial_material , momclass:inferred_indirect_method , momclass:published_map , momclass:inferred_ground_magnetic_survey , momclass:surveyed , momclass:inferred_projection__observed_locations , momclass:observed_outcrop , momclass:inferred_drill_cuttings , momclass:inferred_projection_vegetation , momclass:direct_observation , momclass:compilation , momclass:inferred_borehole_geophysical_log , momclass:observed_aerial_imagery , momclass:inertial_survey , momclass:inferred_remote_sensing_imagery , momclass:inferred_gravity_data , momclass:observed_borehole_core ;
      skos:prefLabel "CGI Mapped Feature Observation Method"@en .

