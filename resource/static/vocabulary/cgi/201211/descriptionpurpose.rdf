	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/descriptionpurpose/typical_norm"/>
	<skos:prefLabel xml:lang="en">CGI Description Purpose</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/descriptionpurpose/0001">
