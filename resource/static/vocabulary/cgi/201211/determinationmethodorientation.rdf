	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Collection"/>
	<rdfs:label xml:lang="en">orientation determination method concepts</rdfs:label>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/standard_on_site_measure"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/measure_on_outcrop"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/three_point_determination"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/estimate_from_air_photo"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/visual_surface_estimation_on_outcrop"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/photogeologic_determination"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/calculated_average_orientation"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/estimate_from_distance"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/method_unknown"/>
