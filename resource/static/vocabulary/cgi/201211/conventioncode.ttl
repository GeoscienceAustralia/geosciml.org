@prefix ConCode:  <http://resource.geosciml.org/classifier/cgi/conventioncode/> .
@prefix cgi:     <http://resource.geosciml.org/classifierscheme/cgi/201211/> .
@prefix dc:      <http://purl.org/dc/elements/1.1/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix owl2xml:  <http://www.w3.org/2006/12/owl2-xml#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix void:    <http://rdfs.org/ns/void#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

ConCode:
      rdf:type skos:Collection ;
      rdfs:label "strike and dip measurement reporting convention"@en ;
      skos:definition "This resource defines the concept space for Strike and Dip quanity convention codes by extension; it includes all concepts in this conceptScheme, as well as concepts in previous versions of the scheme."@en ;
      skos:member ConCode:dip_dip_direction , ConCode:strike_dip_rhr ;
      skos:prefLabel "CGI Strike and Dip Measurement Reporting Convention"@en .

<http://resource.geosciml.org/classifier/cgi/conventioncode/0001>
      rdf:type owl:NamedIndividual .

<http://resource.geosciml.org/classifier/cgi/conventioncode/0002>
      rdf:type owl:NamedIndividual .

ConCode:dip_dip_direction
      rdf:type skos:Concept ;
      dc:source "GeoSciML v.2" ;
      skos:definition "The orientation measurement consists of a dip and a dip direction. Dip is the angle that the structural surface (eg bedding, fault plane) makes with the horizontal measured perpendicular to the strike of the structure and in the vertical plane Dip direction is the azimuth perpindicular to the strike of the structure" ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/conventioncode/0001> ;
      skos:inScheme cgi:conventioncode ;
      skos:prefLabel "dip dip direction" .

ConCode:strike_dip_rhr
      rdf:type skos:Concept ;
      dc:source "GeoSciML v2" ;
      skos:definition "The strike and dip of planar data is listed according to the ‘right-hand rule’ or, as one looks along the strike direction, the surface dips to the right. Dip is the angle that the structural surface (eg bedding, fault plane) makes with the horizontal measured perpindicular to the strike of the structure and in the vertical plane" ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/conventioncode/0002> ;
      skos:historyNote "Updated to 201211 version by SMR; add collection entity, check all pref labels are lower case, remove owl:NamedIndividual and Owl:Thing rdf:types."@en ;
      skos:inScheme cgi:conventioncode ;
      skos:prefLabel "strike dip right hand rule"@en .

cgi:conventioncode
      rdf:type owl:Ontology , skos:ConceptScheme ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)" ;
      dc:identifier "http://resource.geosciml.org/classifierscheme/cgi/201211/conventioncode" ;
      dc:language "en" ;
      dc:publisher "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)" ;
      dc:title "CGI Structure Measurement Convention Term Vocabulary"@en ;
      owl:imports <http://rdfs.org/ns/void> , <http://www.w3.org/2004/02/skos/core> ;
      owl:versionInfo "201211" ;
      skos:definition "This file contains the 201211 version of CGI Convention Code list; use to specify the convention used for reporting the orientation of a planar feature. Used in CGI_PlanarOrientation, is enumeration in GeoSciML XML schema. This list is for information purposes."@en ;
      skos:historyNote "2010-11-27 SMR update URN to http URI create v201202"@en , "2012-02-27 SMR add skos:exactMatch triples to map URIs for concepts in this vocabulary to number-token URIs in 201012 version of same concepts."@en , "2010-02-08. SMR Clean up namespace declarations so concept URI's follow CGI URN scheme; concepts have URN, are not document fragments."@en , "2012-11-24 SMR Update to 201211 version; add collection entity, check all pref labels are lower case, remove owl:NamedIndividual and Owl:Thing rdf:types."@en , "2012-02-07 SMR update URI to replace numeric final token with English-language string as in original URN scheme."@en , "2009-07-14. Generate SKOS encoding of vocabulary from Excel spreadsheet."@en ;
      skos:prefLabel "CGI Structure Measurement Convention Term Vocabulary"@en .

<http://resource.geosciml.org/vocabulary/cgi/201211/conventioncode>
      rdf:type void:Dataset ;
      rdfs:label "Convention code vocabulary"^^xsd:string .
