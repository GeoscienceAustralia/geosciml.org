	<owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">201211</owl:versionInfo>
	<skos:hasTopConcept rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/fault"/>
