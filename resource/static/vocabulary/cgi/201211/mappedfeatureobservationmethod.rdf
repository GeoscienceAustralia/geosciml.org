	<rdf:type rdf:resource="http://rdfs.org/ns/void#Dataset"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mapped feature observation method vocabulary</rdfs:label>
</rdf:Description>

