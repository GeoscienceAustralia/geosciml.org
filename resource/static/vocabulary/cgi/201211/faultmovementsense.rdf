	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/no_movement_sense"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/detachment"/>
	<skos:prefLabel xml:lang="en">CGI Fault Movement Sense</skos:prefLabel>
</rdf:Description>

