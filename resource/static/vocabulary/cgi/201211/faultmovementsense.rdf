	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Collection"/>
	<rdfs:label xml:lang="en">fault movement sense concepts</rdfs:label>
	<skos:definition xml:lang="en">This resource defines the concept space for fault movement sense concepts by extension; it includes all concepts in this conceptScheme, as well as concepts in previous versions of the scheme.</skos:definition>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/thrust"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/generic_decollement"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/normal_dextral"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/dextral"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/reverse_sinistral"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/thrust_decollement"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/normal_sinistral"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/normal"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/sinistral"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/reverse_dextral"/>
