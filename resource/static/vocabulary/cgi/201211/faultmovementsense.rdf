<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementsense/detachment">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source>GGIC vocabularies working group (Australia)</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/normal"/>
	<skos:definition>A regional-scale low-angle normal fault.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/0001"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense"/>
	<skos:prefLabel xml:lang="en">detachment</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementsense/dextral">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source>GGIC vocabularies working group (Australia)</dc:source>
	<skos:definition>Right-lateral separation sense; in plan view, the side opposite the observer appears displaced to the right.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/0002"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense"/>
	<skos:prefLabel xml:lang="en">dextral</skos:prefLabel>
	<skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/reverse_dextral"/>
	<skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/normal_dextral"/>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementsense/generic_decollement">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source>Neuendorf et al. 2005</dc:source>
	<skos:definition>A large-displacement (kilometers or tens of kilometers) shallowly dipping to subhorizontal fault or shear zone.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/0003"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense"/>
	<skos:prefLabel xml:lang="en">generic decollement</skos:prefLabel>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementsense/no_movement_sense">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source>Froitzheim, N., Pleuger, J. &amp; Nagel, T.J., 2006. Extraction faults. Journal of Structural Geology v28, pp. 1388-1395.</dc:source>
	<skos:definition>The fault-parallel displacement is effectively zero, as in an extraction fault.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/0004"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense"/>
	<skos:prefLabel xml:lang="en">no movement sense</skos:prefLabel>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementsense/normal">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source>GGIC vocabularies working group (Australia)</dc:source>
	<skos:definition>The hanging wall appears to have moved down relative to the footwall; dip of fault usually 45-90 degrees.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/0005"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense"/>
	<skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/detachment"/>
	<skos:prefLabel xml:lang="en">normal</skos:prefLabel>
	<skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/normal_dextral"/>
	<skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/normal_sinistral"/>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementsense/normal_dextral">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source>GGIC vocabularies working group (Australia)</dc:source>
	<skos:definition>The movement sense includes both normal and dextral components.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/0006"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense"/>
	<skos:prefLabel xml:lang="en">normal dextral</skos:prefLabel>
	<skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/normal"/>
	<skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/dextral"/>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementsense/normal_sinistral">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source>GGIC vocabularies working group (Australia)</dc:source>
	<skos:definition>The movement sense includes both normal and sinistral components.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/0007"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense"/>
	<skos:prefLabel xml:lang="en">normal sinistral</skos:prefLabel>
	<skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/sinistral"/>
	<skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/normal"/>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementsense/reverse">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source>GGIC vocabularies working group (Australia)</dc:source>
	<skos:definition>The hanging wall appears to have moved down relative to the footwall; dip of fault usually greater than 45 degrees.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/0008"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense"/>
	<skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/thrust"/>
	<skos:prefLabel xml:lang="en">reverse</skos:prefLabel>
	<skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/reverse_dextral"/>
	<skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/reverse_sinistral"/>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementsense/reverse_dextral">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source>GGIC vocabularies working group (Australia)</dc:source>
	<skos:definition>The movement sense includes both reverse and dextral components.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/0009"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense"/>
	<skos:prefLabel xml:lang="en">reverse dextral</skos:prefLabel>
	<skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/reverse"/>
	<skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/dextral"/>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementsense/reverse_sinistral">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source>GGIC vocabularies working group (Australia)</dc:source>
	<skos:definition>The movement sense includes both reverse and sinistral components.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/0010"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense"/>
	<skos:prefLabel xml:lang="en">reverse sinistral</skos:prefLabel>
	<skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/sinistral"/>
	<skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/reverse"/>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementsense/sinistral">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source>GGIC vocabularies working group (Australia)</dc:source>
	<skos:definition>Left-lateral separation sense; in plan view, the side opposite the observer appears displaced to the left.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/0011"/>
	<skos:historyNote xml:lang="en">2010-02-08 SMR add related links between dextral, sinistral, normal, reverse and oblique slip types</skos:historyNote>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense"/>
	<skos:prefLabel xml:lang="en">sinistral</skos:prefLabel>
	<skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/reverse_sinistral"/>
	<skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/normal_sinistral"/>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementsense/thrust">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source>GGIC vocabularies working group (Australia)</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/reverse"/>
	<skos:definition>Reverse fault with dip typically less than 45 degrees; horizontal compression, rather than vertical displacement is characteristic.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/0012"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense"/>
	<skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/thrust_decollement"/>
	<skos:prefLabel xml:lang="en">thrust</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementsense/thrust_decollement">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source>GeoScience Victoria and others</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/thrust"/>
	<skos:definition>A regional-scale low-angle thrust fault.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementsense/0013"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense"/>
	<skos:prefLabel xml:lang="en">thrust decollement</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
	<dc:creator>Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
	<dc:format>application/rdf+xml</dc:format>
	<dc:identifier>http://resource.geosciml.org/classifierscheme/cgi/201211/faultmovementsense</dc:identifier>
	<dc:language>en</dc:language>
	<dc:publisher>Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:publisher>
	<dc:title xml:lang="en">CGI Fault Movement Sense Term Vocabulary</dc:title>
	<owl:imports rdf:resource="http://rdfs.org/ns/void"/>
	<owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
	<owl:versionInfo>201211</owl:versionInfo>
	<skos:changeNote xml:lang="en">2009-12-07 SMR Update metadata properties for version, creator, title, and format.  Change skos:HistoryNote to dc:source for information on origin of terms and definitions.</skos:changeNote>
	<skos:definition xml:lang="en">This file contains the CGI Fault Movement Sense vocabulary. Terms to specify the relative direction of movement of bodies of rock across a fault (GeoSciML ShearDisplacementStructure) surface. Examples include 'sinistral', 'dextral', 'normal', 'reverse'.</skos:definition>
	<skos:historyNote xml:lang="en">2013-02-04 SMR Check update to 201211 version; add collection entity, check all pref labels are lower case, remove owl:NamedIndividual and Owl:Thing rdf:types. Add missing for missing definitions.</skos:historyNote>
	<skos:historyNote xml:lang="en">2010-11-27 SMR replace URN identifiers with http URI according to CGI URI scheme</skos:historyNote>
	<skos:historyNote xml:lang="en">2010-02-13 Add narrower relationship links by adding skos:narrower link in Protege, defining skos:narrower as inverse of skos:broader, calculating with Pellet reasoner to add inferred axioms, and using 'file/Export inferred axioms as ontology' in Protege.</skos:historyNote>
	<skos:historyNote xml:lang="en">2012-02-27 SMR add skos:exactMatch triples to map URIs for concepts in this vocabulary to number-token URIs in 201012 version of same concepts.</skos:historyNote>
	<skos:historyNote xml:lang="en">2012-02-07 SMR update URI to replace numeric final token with English-language string as in original URN scheme.</skos:historyNote>
	<skos:historyNote xml:lang="en">2009-07-14. Generate SKOS encoding of vocabulary from Excel spreadsheet.</skos:historyNote>
	<skos:historyNote xml:lang="en">2010-02-08. SMR Clean up namespace declarations so concept URI's follow CGI URN scheme; concepts have URN, are not document fragments.</skos:historyNote>
	<skos:prefLabel xml:lang="en">CGI Fault Movement Sense Term Vocabulary</skos:prefLabel>
</rdf:Description>

