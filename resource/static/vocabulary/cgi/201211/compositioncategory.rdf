<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/pelitic">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">NADM SLTTm 2004</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/quartz_feldspar_pelitic"/>
	<skos:definition xml:lang="en">Material for which the sum of modal quartz+feldspar+ mica + aluminous mineral is greater than or equal to 70 percent, and aluminous mineral + mica content is greater than or equal to 40 percent.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0050"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:prefLabel xml:lang="en">pelitic</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/peralkaline">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">Shand 1947</dc:source>
	<skos:definition xml:lang="en">Aluminum oxide (Al2O3) is less than sum of K and Na oxide.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0051"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:prefLabel xml:lang="en">peralkaline</skos:prefLabel>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/peraluminous">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">Shand 1947</dc:source>
	<skos:definition xml:lang="en">Aluminum oxide (Al2O3) greater than sum of Na, K and Ca oxide.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0052"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:prefLabel xml:lang="en">peraluminous</skos:prefLabel>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/petroliferous">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">This vocabulary</dc:source>
	<skos:definition xml:lang="en">Crude oil or natural gas is present, generally in trace amounts, commonly detected by smell</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0053"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:prefLabel xml:lang="en">petroliferous</skos:prefLabel>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/phosphate_chemistry">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">Struik etal 2002; This vocabulary</dc:source>
	<skos:definition xml:lang="en">Phosphate (PO4) is the most abundant (mole percent) anion.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0054"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:prefLabel xml:lang="en">phosphate chemistry</skos:prefLabel>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/phosphatic">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">This vocabulary</dc:source>
	<skos:definition xml:lang="en">Greater than accessory amount (greater than 5 percent by volume) of phosphate minerals are present</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0055"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:prefLabel xml:lang="en">phosphatic</skos:prefLabel>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/polymictic">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">Jackson, 1997, p. 501</dc:source>
	<skos:definition xml:lang="en">Grains or clasts are composed of many mineral or rock types</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0056"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:prefLabel xml:lang="en">polymictic</skos:prefLabel>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/quartz_feldspar_pelitic">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">NADM SLTTm 2004</dc:source>
	<skos:definition xml:lang="en">Material for which the sum of modal quartz+feldspar+ mica + aluminous mineral is greater than or equal to 70 percent.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0057"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/pelitic"/>
	<skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/semi_pelitic"/>
	<skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/quartzo_feldspathic"/>
	<skos:prefLabel xml:lang="en">quartz feldspar pelitic</skos:prefLabel>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/quartzo_feldspathic">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">NADM SLTTm 2004</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/quartz_feldspar_pelitic"/>
	<skos:definition xml:lang="en">Material for which the sum of modal quartz+feldspar+ mica + aluminous mineral is greater than or equal to 70 percent, and quartz + feldspar (sensu Robertson, 1999) greater than 60 percent.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0058"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/quartzose"/>
	<skos:prefLabel xml:lang="en">quartzo feldspathic</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/quartzose">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">NADM SLTTm 2004</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/quartzo_feldspathic"/>
	<skos:definition xml:lang="en">Material consists of greater than or equal to 75 percent quartz.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0059"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:prefLabel xml:lang="en">quartzose</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/quartzose_detrital_mode">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">Folk1968; This vocabulary</dc:source>
	<skos:definition xml:lang="en">Detrital mineral grains in a granular rock consist of greater than 95 percent quartz. This petrographic criteria follows the most restrictive definition of the composition field at the quartz vertex of the sandstone QFL triangle (Folk, 1968). This use of term quartzose is more restrictive that that used in the mineralogical composition term vocabulary, reflecting the interpretive significance of pure quartz sandstones in sedimentary petrology.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0060"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:prefLabel xml:lang="en">quartzose detrital mode</skos:prefLabel>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/semi_pelitic">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">NADM SLTTm 2004</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/quartz_feldspar_pelitic"/>
	<skos:definition xml:lang="en">Material for which the sum of modal quartz+feldspar+ mica + aluminous mineral is greater than or equal to 70 percent, and quartz+ feldspar less than 60 percent</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0061"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:prefLabel xml:lang="en">semi pelitic</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/silicate_chemistry">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">Neuendorf et al 2005; Struik 2002; This vocabulary</dc:source>
	<skos:definition xml:lang="en">composed of minerals whose crystal structure contains SiO4 tetrahedra, either isolated or joined through one or more oxygen atoms to form groups, chains, sheets or three-dimensional structures with metallic cations. composition ranges from 20-100 percent SiO2. Typically some Al substitutues for silica. A survey of chemical analyses in the Georock database (query FOR ALL GEOLOGICAL SETTINGS, MATERIAL: WHOLE ROCK (AND VOLCANIC GLASS), PUB. YEARS: 2005-1970), 10/10/2005 state, http://georoc.mpch-mainz.gwdg.de/georoc/Entry.html) discovered that the lowest silica (Si02) content for igneous rock is about 20 percent. This is consistent with the lowest silica content found for a silicate mineral is about 21 percent for a chlorite variety found by scanning the mineral analyses in Deer, Howie, and Zussman (1966).</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0062"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/very_low_silica_chemistry"/>
	<skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/high_silica_chemistry"/>
	<skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/low_silica_chemistry"/>
	<skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/intermediate_silica_chemistry"/>
	<skos:prefLabel xml:lang="en">silicate chemistry</skos:prefLabel>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/silicic">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">This vocabulary</dc:source>
	<skos:definition xml:lang="en">Greater than accessory amount (greater than 5 percent by volume) of silica, amorphous, as quartz, or any of the silica polymorphs are present</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0063"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:prefLabel xml:lang="en">silicic</skos:prefLabel>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/sub_feldspathic_detrital_mode">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">Folk1968</dc:source>
	<skos:definition xml:lang="en">Sandstone petrographic composition term indicating that detrital mode for clastic rock (Q-F-L) includes less than 50 percent lithic fragments, between 75 and 95 percent quartz (thus less than 25 percent feldspar). This corresponds to the subarkose field of Folk (1968); the 75 percent quartz boundary is consistent with the quartzsose composition as defined in the mineralogic compostion terms vocabulary, but if more than a few percent 'matrix' or cement is present, this is not necessarily equivalent to the mineralogical composition term.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0064"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:prefLabel xml:lang="en">sub feldspathic detrital mode</skos:prefLabel>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/sub_lithic_detrital_mode">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">Folk1968</dc:source>
	<skos:definition xml:lang="en">Sandstone petrographic composition term indicating that detrital mode for clastic rock (Q-F-L) includes more than 50 percent lithic fragments, between 75 and 95 percent quartz (thus less than 25 percent feldspar). This corresponds to the sublitharenite field of Folk (1968); the 75 percent quartz boundary is consistent with quartzsose composition as defined in the mineralogic compostion terms vocabulary.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0065"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:prefLabel xml:lang="en">sub lithic detrital mode</skos:prefLabel>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/subaluminous">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">Shand 1947</dc:source>
	<skos:definition xml:lang="en">Aluminum oxide within 10 percent of sum of Na and K oxide.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0066"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:prefLabel xml:lang="en">subaluminous</skos:prefLabel>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/sulfate">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">This vocabulary</dc:source>
	<skos:definition xml:lang="en">Material predominantly composed of sulfate minerals</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0067"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:prefLabel xml:lang="en">sulfate</skos:prefLabel>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/sulfate_chemistry">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">Struik etal 2002; This vocabulary</dc:source>
	<skos:definition xml:lang="en">Sulfate is the most abundant anion</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0068"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:prefLabel xml:lang="en">sulfate chemistry</skos:prefLabel>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/sulfide">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">This vocabulary</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/sulfide_bearing"/>
	<skos:definition xml:lang="en">Material predominantly composed of sulfide minerals</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0069"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:prefLabel xml:lang="en">sulfide</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/sulfide_bearing">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">This vocabulary</dc:source>
	<skos:definition xml:lang="en">Greater than accessory amount (greater than 5 percent by volume) of sulfide mineral are present</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0070"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/sulfide"/>
