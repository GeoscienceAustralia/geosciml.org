</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/sulfosalt_chemistry">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">Neuendorf et al 2005; Struik 2002; This vocabulary</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/sulfide_chemistry"/>
	<skos:definition xml:lang="en">Sulfide in which a semimetal (As, Sb, Bi) is present in a cation role</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0072"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:prefLabel xml:lang="en">sulfosalt chemistry</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/ultramafic">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">NADM SLTTm 2004</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/ferromagnesian"/>
	<skos:definition xml:lang="en">Material consists of greater than 90 percent ferromagnesian silicate minerals..</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0073"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:prefLabel xml:lang="en">ultramafic</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/very_low_silica_chemistry">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">Struik2002; SLTTs2004; This vocabulary</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/silicate_chemistry"/>
	<skos:definition xml:lang="en">Containing between about 20 and 44 percent SiO2. A survey of chemical analyses in the Georock database (query FOR ALL GEOLOGICAL SETTINGS, MATERIAL: WHOLE ROCK (AND VOLCANIC GLASS), PUB. YEARS: 2005-1970), 10/10/2005 state, http://georoc.mpch-mainz.gwdg. syn ultrabasic.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0074"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:prefLabel xml:lang="en">very low silica chemistry</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/volcanic_lithic">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">This vocabulary</dc:source>
	<skos:definition xml:lang="en">Sedimentary material contains significant broken volcanic rock fragments or vitric fragments, irrespective of their pyroclastic or epiclastic origin.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0075"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:prefLabel xml:lang="en">volcanic lithic</skos:prefLabel>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
</rdf:Description>

