	<skos:definition xml:lang="en">Greater than 75 percent of the material consists of a single mineral species.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0044"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:prefLabel xml:lang="en">monomineralic</skos:prefLabel>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/monomictic">
