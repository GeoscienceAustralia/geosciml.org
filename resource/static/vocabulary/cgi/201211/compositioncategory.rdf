	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/sulfide_chemistry">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">Neuendorf et al 2005; Struik 2002; This vocabulary</dc:source>
	<skos:definition xml:lang="en">Sulfur is the most abundant anion; cations are metals or semimetals.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0071"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/sulfosalt_chemistry"/>
