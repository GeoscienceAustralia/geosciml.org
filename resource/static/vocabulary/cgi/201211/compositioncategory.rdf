	<dc:source xml:lang="en">This vocabulary</dc:source>
	<skos:definition xml:lang="en">Greater than accessory amount (greater than 5 percent by volume) of mica group mineral are present</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/compositioncategory/0043"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
	<skos:prefLabel xml:lang="en">micaceous</skos:prefLabel>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/compositioncategory/mono_mineralic">
