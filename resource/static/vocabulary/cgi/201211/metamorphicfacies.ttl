metafac:
      rdf:type skos:Collection ;
      rdfs:label "metamorphic facies concepts"@en ;
      skos:definition "This resource defines the concept space for metamorphic facies concepts by extension; it includes all concepts in this conceptScheme, as well as concepts in previous versions of the scheme."@en ;
      skos:member metafac:no_metamorphic_minerals , metafac:epidote_amphibolite_metamorphic_facies , metafac:glaucophane_schist_facies , metafac:zeolite_metamorphic_facies , metafac:sanidinite_metamorphic_facies , metafac:amphibolite_metamorphic_facies , metafac:greenschist_metamorphic_facies , metafac:granulite_facies , metafac:metamorphic_facies_unknown , metafac:pyroxene_hornfels_metamorphic_facies , metafac:metamorphic_facies_not_specified , metafac:eclogite_metamorphic_facies , metafac:subgreenschist_metamorphic_facies ;
      skos:prefLabel "CGI Metamorphic Facies"@en .

