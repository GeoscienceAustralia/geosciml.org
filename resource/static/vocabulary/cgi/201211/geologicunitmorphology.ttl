geounitmorph:arch_morphology
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "a raised elongate body, morphologically equivalent to upside down channel"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0001> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "arch morphology"@en ;
      skos:related geounitmorph:longitudinal_section_of_cylinder .

geounitmorph:arcuate_sheet
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:sheet ;
      skos:definition "sheet in which one of L or W is a curved arc" ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0002> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "arcuate sheet"@en .

geounitmorph:basin_shape
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "Geometric analog is convex down discoid, term generally connotes a kilometer-scale feature. Map view may be circular, elliptical, or irregular; characteristic feature is thickening from the edges towards the center.  Typically used to describe a body of sedimentary rock deposited in a low area in the Earth's crust, in which case there is an implication that the top is (or was) a surface of deposition."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0003> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "basin shape"@en ;
      skos:related geounitmorph:discus , geounitmorph:hemispheroid .

geounitmorph:blanket_shape
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "a thin, widespread body with width to thickness ratio greater than 1000 to 1 that covers an underlying substrate. A layer of known great lateral extent. Morphology equivalent to bed or layer, but implies that body forms the top of some unit, instead of occurring within a body."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0004> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "blanket shape"@en ;
      skos:related geounitmorph:sheet .

geounitmorph:blob
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:three_dimensional_body ;
      skos:definition "Body with smooth surface, broadly equant shape, irregular protuberances, no symmetry axes, and a boundary that can not be separated into sides." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0005> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "blob"@en .

geounitmorph:block
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:polygonal_cylinder ;
      skos:definition "Equant polygonal cylinder, L=W=H, definable edges separate bounding surfaces. Grades to tabular prism as one horizontal dimension becomes distinctly longer than the other, and tablet or polygonal cylinder as one vertical axis becomes distinctly shorter or longer than the horizontal axes." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0006> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "block"@en .

geounitmorph:boudin_shape
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "Layer or bed of rock deformed into sausage-shaped segments (boudins), either separated or joined by pinched connections. Morphology generally defined in a profile view, third dimension is not denoted; use of term also implies that the body is not isolated, but is related to an original body elongate on some dimension that has been stretched and separated into separate bodies"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0007> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "boudin shape"@en ;
      skos:related geounitmorph:bread_loaf_ellipsoid .

geounitmorph:bread_loaf_ellipsoid
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:ellipsoid ;
      skos:definition "Ellipsoid with L 10 times W or H" ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0008> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "bread loaf ellipsoid"@en .

geounitmorph:channel_shape
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "a strongly elongate lens or ribbon with a convex downward lower surface, near planar upper surface.  Morphology is equivalent to upside down arch."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0009> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "channel shape"@en ;
      skos:related geounitmorph:longitudinal_section_of_cylinder , geounitmorph:ribbon .

geounitmorph:circular_cylinder
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:cylinder ;
      skos:definition "Elliptical cylinder that has circular symmetry about long axis; distinct side surfaces are not defined. Cross section is circular" ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0010> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "circular cylinder"@en .

geounitmorph:circular_profile
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:equant_profile ;
      skos:definition "equant profile with arcuate boundary" ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0011> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "circular profile"@en .

geounitmorph:column_shape
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "Cylindrical body, equant cross section, long in direction normal to equant cross section. Morphology equivalent to pipe, without denotation of transport function."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0012> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "column shape"@en ;
      skos:related geounitmorph:circular_cylinder , geounitmorph:polygonal_cylinder .

geounitmorph:cone
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:discus ;
      skos:definition "discus with one planar side, and circular symmetry on axis; geologic cones typically have height <(<) diameter" ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0013> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "cone shape"@en .

geounitmorph:cone_segment
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:three_dimensional_body ;
      skos:definition "Body that is section of a cone formed by splitting the cone along a surface parallel to the cone axis, that interesect approximately along the axis." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0014> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "cone segment"@en .

geounitmorph:conical_sheet
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:sheet ;
      skos:definition "sheet that conforms to surface of a cone." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0015> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "conical sheet"@en .

geounitmorph:cupola
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "a large upward projection of the roof of an igneous intrusion into the country rock above. A possibly irregular dome like or columnar body rooted in a larger body."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0016> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "cupola"@en ;
      skos:related geounitmorph:hemispheroid .

geounitmorph:cylinder
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:three_dimensional_body ;
      skos:definition "Body defined by translation of a generating plane figure with approximately equant cross section (circle, ellipsoid, polygon) along a straight axial line. H>=W=L. Axis is direction normal to generating plane figure, which forms parallel top and bottom bounding surface." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0017> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:narrower geounitmorph:disc , geounitmorph:circular_cylinder , geounitmorph:polygonal_cylinder , geounitmorph:prism , geounitmorph:rod , geounitmorph:elliptical_cylinder ;
      skos:prefLabel "cylinder"@en .

geounitmorph:dart_ellipsoid
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:ellipsoid ;
      skos:definition "Ellipsoid with L more than 50 times W or H" ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0018> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "dart ellipsoid"@en .

geounitmorph:diapir
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "Broadly inverted tear drop morphology. Body of magmatic or mobile sedimentary material intruded into overlying rocks"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0019> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "diapir"@en ;
      skos:related geounitmorph:circular_cylinder .

geounitmorph:dike
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:altLabel "dyke"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "A generally sheet-like intrusive rock body that cuts across bedding or foliation in the host rock, or intrudes massive host rock."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0020> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "dike"@en ;
      skos:related geounitmorph:sheet .

geounitmorph:disc
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:cylinder ;
      skos:definition "Cylinder with height less than diameter. L=W>H; edges separates side from top and bottom. Grades to oblate ellipsoid as edges lose definition." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0021> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "disc"@en .

geounitmorph:discoid_anulus
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:three_dimensional_body ;
      skos:definition "Cone with central part removed along cylindrical volume approximately along the cone axis" ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0022> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "discoid anulus"@en .

geounitmorph:discus
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:three_dimensional_body ;
      skos:definition "Body with two sides that converge to define an edge that is circular in profile; edge separates definable top and bottom. L=W>H. Grades to oblate ellipsoid as edge separating sides loses definition, cone as one side becomes distinctly planar." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0023> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:narrower geounitmorph:cone , geounitmorph:elongate_discus ;
      skos:prefLabel "discus"@en .

geounitmorph:dome
      rdf:type skos:Concept ;
      dc:source "this vocabulary"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "Steep-sided disk-like body, for example and accumulation of high viscosity lava above and around a volcanic vent. A structure in which the top of the body dips gently away in all directions e.g. salt dome, extrusion dome"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0024> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "dome"@en ;
      skos:related geounitmorph:discus , geounitmorph:hemispheroid .

geounitmorph:dome_sheet
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:sheet ;
      skos:definition "sheet in which L and W are curved arcs." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0025> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "dome sheet"@en .

geounitmorph:ductolith
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "Concordant intrusion, teardrop-shaped in cross section."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0026> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "ductolith"@en ;
      skos:related geounitmorph:diapir , geounitmorph:tear_drop .

geounitmorph:ellipsoid
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:three_dimensional_body ;
      skos:definition "Body that contains three orthogonal axes defining three surfaces of reflection symmetry, profiles along the surfaces are elliptical. Boundary can not be separated into sides (no edges). Becomes cylinder or disc when bounding surface can be considered parallel to one of the axis. L>=W>=H" ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0027> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:narrower geounitmorph:spheroid , geounitmorph:bread_loaf_ellipsoid , geounitmorph:dart_ellipsoid , geounitmorph:sheet_ellipsoid , geounitmorph:pillow_ellipsoid , geounitmorph:pancake_ellipsoid , geounitmorph:football_ellipsoid ;
      skos:prefLabel "ellipsoid"@en .

geounitmorph:elliptical_cylinder
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:cylinder ;
      skos:definition "Cylinder that has elliptical cross section; distinct side surfaces are not defined." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0028> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "elliptical cylinder"@en .

geounitmorph:elliptical_profile
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:planar_profile ;
      skos:definition "Profile approximates an ellipse; ratio of long axis to short axis is greater than 1.4" ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0029> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "elliptical profile"@en .

geounitmorph:elongate_discus
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:discus ;
      skos:definition "discus for which the profile defined by the bounding edge is longer in one direction than other. L>W>>H" ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0030> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "elongate discus"@en .

geounitmorph:equant_polygonal_profile
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:equant_profile ;
      skos:definition "equant profile with boundary that can be divided into distinct straight sides separated by higher curvature corners." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0031> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "equant polygonal profile"@en .

geounitmorph:equant_profile
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:planar_profile ;
      skos:definition "profile for which the ratio of length of longest chord to shortest chord is less than 1.4." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0032> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:narrower geounitmorph:circular_profile , geounitmorph:equant_polygonal_profile ;
      skos:prefLabel "equant profile"@en .

geounitmorph:ethmolith
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "discordant, funnel-shaped intrusion, tapers downward"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0033> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "ethmolith"@en ;
      skos:related geounitmorph:cone .

geounitmorph:fan
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "A cone segment, generally applied to surface morphology of sedimentary deposit with a gentle surface slope, radiating from a localized sediment source."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0034> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "fan"@en ;
      skos:related geounitmorph:cone_segment .

geounitmorph:football_ellipsoid
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:ellipsoid ;
      skos:definition "Ellipsoid with L 2 to 5 times W or H" ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0035> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "football ellipsoid"@en .

geounitmorph:geologic_body
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:three_dimensional_body ;
      skos:definition "A 3 dimensional body defined based on geometric shape, with some geologic connotation or denotation."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0036> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:narrower geounitmorph:dike , geounitmorph:ductolith , geounitmorph:diapir , geounitmorph:layer_shape , geounitmorph:cupola , geounitmorph:arch_morphology , geounitmorph:graben_shape , geounitmorph:laccolith , geounitmorph:blanket_shape , geounitmorph:basin_shape , geounitmorph:phacolith , geounitmorph:mound , geounitmorph:pod , geounitmorph:lens , geounitmorph:dome , geounitmorph:boudin_shape , geounitmorph:sphenolith , geounitmorph:channel_shape , geounitmorph:fan , geounitmorph:ring_dyke , geounitmorph:sill_form , geounitmorph:column_shape , geounitmorph:trough , geounitmorph:ethmolith , geounitmorph:lopolith ;
      skos:prefLabel "geologic body"@en .

geounitmorph:graben_shape
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "an elongate body bounded on both sides by normal faults that dip toward each other"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0037> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "graben shape"@en ;
      skos:related geounitmorph:tabular_prism .

geounitmorph:hemispheroid
      rdf:type skos:Concept ;
      rdfs:comment "This vocabulary"@en ;
      skos:broader geounitmorph:three_dimensional_body ;
      skos:definition "Section of a spheroid formed by removing everything on the smaller volume side of a surface that intersects the spheroid. Grades to cone as suface generator (line rotated about axis to generate volume above lower planar boundary) becomes straight."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0038> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "hemispheroid"@en .

geounitmorph:irregular_profile
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:planar_profile ;
      skos:definition "profile does not conform to any of the regular geometries associated with other categories in the vocabulary." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0039> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "irregular profile"@en .

geounitmorph:laccolith
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "A tabular or lenticular igneous body whose long dimensions parallel layering in the host rock, and which as a convex-up roof and a known or assumed flat floor"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0040> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "laccolith"@en ;
      skos:related geounitmorph:discus , geounitmorph:sheet_ellipsoid .

geounitmorph:layer_shape
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "Body geometry characterized by generally thin character relative to the lateral extent of the body (on order of 1:100). Thin bodies of lesser extent are lenses. Connotes a non-intrusive body; equivalent intrusive body would be dike (or sill). Denotes that body is in a sequence of similar sheet-like bodies between other rock bodies"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0041> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "layer shape"@en ;
      skos:related geounitmorph:blanket_shape , geounitmorph:sill_form .

geounitmorph:lens
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "A body bounded by converging surfaces (at least one of which is curved), thick in the middle and thinning out toward the edges, resembling a convex lens."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0042> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "lens"@en ;
      skos:related geounitmorph:discus .

geounitmorph:lenticular_profile
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:planar_profile ;
      skos:definition "profile with ratio of long axis length to short axis length greater than 5 with sides that taper to a point, shaped like the profile of an optical lense." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0043> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "lenticular profile"@en .

geounitmorph:longitudinal_section_of_cylinder
      rdf:type skos:Concept ;
      rdfs:comment "long axis may be curved as in meandering channel"@en ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:three_dimensional_body ;
      skos:definition "Body that is section of a circular or elliptical cylinder formed by a splitting the cylinder along a surface approximately parallel to its long axis. L>>W>H." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0044> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "longitudinal section of cylinder"@en .

geounitmorph:lopolith
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "a large, concordant, typically layered igneous intrusion whose floor is convex-down and whose roof may be convex-down or flat."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0045> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "lopolith"@en ;
      skos:related geounitmorph:discus , geounitmorph:sheet_ellipsoid .

geounitmorph:mound
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "Lense like body of rock, lateral extent less than 100m, generally flat at bottom, with stratigraphic pinch out of beds (onlap) along upper sides."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0046> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "mound"@en ;
      skos:related geounitmorph:discus .

geounitmorph:pancake_ellipsoid
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:ellipsoid ;
      skos:definition "Ellipsoid with H on order of .01 of L or W" ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0047> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "pancake ellipsoid"@en .

geounitmorph:phacolith
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "concordant, lenticular intrusion, emplaced along a fold axis"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0048> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "phacolith"@en ;
      skos:related geounitmorph:longitudinal_section_of_cylinder .

geounitmorph:pillow_ellipsoid
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:ellipsoid ;
      skos:definition "Ellipsoid with H on order of 0.1 of L or W" ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0049> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "pillow ellipsoid"@en .

geounitmorph:pipe
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:column_shape ;
      skos:definition "Rock body that has a length to width ratio greater than 10 to 1, with two sub equal width dimensions."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0050> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "pipe"@en .

geounitmorph:planar_profile
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:definition "shape is defined by intersection of body with some surface, generally either a cross section or map view"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0051> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:narrower geounitmorph:polygonal_profile , geounitmorph:elliptical_profile , geounitmorph:lenticular_profile , geounitmorph:equant_profile , geounitmorph:irregular_profile ;
      skos:prefLabel "planar profile"@en ;
      skos:topConceptOf cgi:geologicunitmorphology .

geounitmorph:planar_sheet
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:sheet ;
      skos:definition "sheet in which L and W axes are straight lines" ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0052> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "planar sheet"@en .

geounitmorph:planar_vein
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:vein ;
      skos:definition "vein with length and width significantly greater than its thickness, giving the vein a tabular form"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0053> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "planar vein"@en ;
      skos:related geounitmorph:dike , geounitmorph:layer_shape .

geounitmorph:pod
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "a small intrusion of elongate or lenticular form"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0054> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "pod"@en ;
      skos:related geounitmorph:blob .

geounitmorph:polygonal_cylinder
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:cylinder ;
      skos:definition "Cylinder in which the edges bounding side surfaces are parallel or subparallel to cylinder axis. Cross section is polygon. Length of axis may be called height or thickness, and is typically in the vertical direction (distinguishing polygonal cylinder from tabular prism and wedge)." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0055> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:narrower geounitmorph:block , geounitmorph:tablet , geounitmorph:wedge ;
      skos:prefLabel "polygonal cylinder"@en .

geounitmorph:polygonal_profile
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:planar_profile ;
      skos:definition "Profile for which the ratio of length of longest chord to shortest chord is greater than 1.4.profile and a boundary that can be divided into distinct straight sides separated by higher curvature corners." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0056> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:narrower geounitmorph:square_profile ;
      skos:prefLabel "polygonal profile"@en .

geounitmorph:prism
      rdf:type skos:Concept ;
      dc:source "This vocabulary; Krynine 1948"@en ;
      skos:broader geounitmorph:cylinder ;
      skos:definition "Body of material that is long in one dimension, and thins rapidly in a perpendicular direction. A long narrow wedge-shaped body that has a width to thickness ratio  greater than 5 to 1 but less than 50 to 1 (Krynine, 1948). Similar to wedge, but profile may be variable"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0057> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "prism"@en ;
      skos:related geounitmorph:wedge .

geounitmorph:ribbon
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:three_dimensional_body ;
      skos:definition "Body that is very thin relative to width and and length. Long axis may be curved. L>(>>)W>>>H. H is typically in vertical direction." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0058> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "ribbon"@en .

geounitmorph:ring_dyke
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:altLabel "ring dyke"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "an arcuate or subcircular dyke, generally associated with a volcanic centre."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0059> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "ring dike"@en ;
      skos:related geounitmorph:arcuate_sheet .

geounitmorph:rock_body_geometry_irregular
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:three_dimensional_body ;
      skos:definition "Rock body geometry is irregular and can not be characterized using terminology." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0060> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "rock body geometry irregular"@en .

geounitmorph:rock_body_geometry_not_specified
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:three_dimensional_body ;
      skos:definition "Rock body geometry is not specified. Use in normative descriptions where any morphology is allowed." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0061> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "rock body geometry not specified"@en .

geounitmorph:rod
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:cylinder ;
      skos:definition "Cylinder with very long axis, generally circular or elliptical cross section (generating figure). H>>>W=L. Long axis may be in any orientation, and may be curved." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0062> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "rod"@en .

geounitmorph:sheet
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:three_dimensional_body ;
      skos:definition "Body that is very thin relative to lateral extent, so thin that is considered to consist of two bounding surfaces joined at an edge. Aspect ratio is L=W>>>H. The geometry of the edge joining the surfaces is not specified in the definition. If the geometry of the edge comes into play in the definition, becomes ribbon, disc or tablet(?). Includes dike, vein, sill and bed-shaped geologic units."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0063> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:narrower geounitmorph:dome_sheet , geounitmorph:arcuate_sheet , geounitmorph:sigmoid_sheet , geounitmorph:planar_sheet , geounitmorph:conical_sheet ;
      skos:prefLabel "sheet"@en ;
      skos:related geounitmorph:vein , geounitmorph:layer_shape , geounitmorph:dike , geounitmorph:sill_form .

geounitmorph:sheet_ellipsoid
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:ellipsoid ;
      skos:definition "Ellipsoid with one axis much smaller than others H on order of .001 of L or W" ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0064> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "sheet ellipsoid"@en .

geounitmorph:sigmoid_sheet
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:sheet ;
      skos:definition "sheet with 's' curvature of L or W direction" ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0065> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "sigmoid sheet"@en .

geounitmorph:sigmoidal_vein
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:vein ;
      skos:definition "vein curved so as to resemble the letter S in section"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0066> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "sigmoidal vein"@en ;
      skos:related geounitmorph:sigmoid_sheet .

geounitmorph:sill_form
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "a tabular intrusive igneous body with long dimensions parallel to bedding or foliation of  sedimentary or metamorphic host rock."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0067> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "sill form"@en ;
      skos:related geounitmorph:sheet , geounitmorph:layer_shape , geounitmorph:sheet_ellipsoid .

geounitmorph:sphenolith
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "wedge-like intrusion, partly concordant, partly discordant."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0068> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "sphenolith"@en ;
      skos:related geounitmorph:prism , geounitmorph:wedge .

geounitmorph:spheroid
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:ellipsoid ;
      skos:definition "Ellipsoid with L, W, H all approximately equal" ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0069> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "spheroid"@en .

geounitmorph:square_profile
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:polygonal_profile ;
      skos:definition "polygonal profile that approximates a square" ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0070> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "square profile"@en .

geounitmorph:tablet
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:polygonal_cylinder ;
      skos:definition "Polygonal cylinder with height less than length or width, disc with edges separating side into multiple segments." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0071> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "tablet"@en .

geounitmorph:tabular_prism
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:three_dimensional_body ;
      skos:definition "Body defined by pairs of parallel, approximately planar surfaces separated by definable edges. Grades to ellipsoid as edges lose definition, to block as axial ratios approach 1, to polygonal cylinder as L approaches W. L>>W>H. H is in vertical direction, L is horizontal." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0072> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "tabular prism"@en .

geounitmorph:tear_drop
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:three_dimensional_body ;
      skos:definition "Body bounded by hemispheroid on one side, cone on other, with circular symmetry around cone axis" ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0073> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "tear drop"@en .

geounitmorph:three_dimensional_body
      rdf:type skos:Concept ;
      rdfs:comment "This vocabulary is constructed as a collection of purely geometric concepts to specify the geometry of bodies. These geometric forms are abstracted in terms of edges that separate boundary surfaces. Smooth forms have no edges."@en ;
      dc:source "This vocabulary"@en ;
      skos:definition "any three dimensional body" ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0074> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:narrower geounitmorph:geologic_body , geounitmorph:discus , geounitmorph:discoid_anulus , geounitmorph:tear_drop , geounitmorph:tabular_prism , geounitmorph:longitudinal_section_of_cylinder , geounitmorph:blob , geounitmorph:ellipsoid , geounitmorph:sheet , geounitmorph:cone_segment , geounitmorph:cylinder , geounitmorph:ribbon , geounitmorph:rock_body_geometry_irregular , geounitmorph:hemispheroid , geounitmorph:rock_body_geometry_not_specified ;
      skos:prefLabel "three dimensional body"@en ;
      skos:topConceptOf cgi:geologicunitmorphology .

geounitmorph:trough
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "shape of a body of sedimentary rock deposited in an elongate depression filled with sediment. large size distinguishes from channel"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0075> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "trough shape"@en ;
      skos:related geounitmorph:longitudinal_section_of_cylinder .

geounitmorph:vein
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:geologic_body ;
      skos:definition "a tabular or sheetlike epigenetic mineral filling of a fault or fracture"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0076> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:narrower geounitmorph:planar_vein , geounitmorph:sigmoidal_vein ;
      skos:prefLabel "vein shape" ;
      skos:related geounitmorph:sheet .

geounitmorph:wedge
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader geounitmorph:polygonal_cylinder ;
      skos:definition "Body defined by translation of trangle with W >> H along a horizontal axis. A polygonal cylinder with wedge-like triangular generating surface and horizontal axis. Body changes thickness (H) in W direction, approaching 0 thickness on one side. L>>W>H." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/0077> ;
      skos:inScheme cgi:geologicunitmorphology ;
      skos:prefLabel "wedge"@en ;
      skos:related geounitmorph:prism .

