valqual:always
      rdf:type skos:Concept ;
      dc:source "GeoSciML UML model; definition this vocabulary" ;
      skos:definition "all instances of the observed entity have this property value."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/valuequalifier/0001> ;
      skos:inScheme cgi:valuequalifier ;
      skos:prefLabel "always"@en .

valqual:approximate
      rdf:type skos:Concept ;
      dc:source "GeoSciML UML model; definition this vocabulary" ;
      skos:definition "specified value is approximate"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/valuequalifier/0002> ;
      skos:inScheme cgi:valuequalifier ;
      skos:prefLabel "approximate"@en .

valqual:common
      rdf:type skos:Concept ;
      dc:source "GeoSciML UML model; definition this vocabulary" ;
      skos:definition "reported property value is commonly observered"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/valuequalifier/0004> ;
      skos:inScheme cgi:valuequalifier ;
      skos:prefLabel "common"@en .

valqual:equalTo
      rdf:type skos:Concept ;
      dc:source "GeoSciML UML model; definition this vocabulary" ;
      skos:definition "reported property value is the observered value"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/valuequalifier/0005> ;
      skos:inScheme cgi:valuequalifier ;
      skos:prefLabel "equalTo"@en .

valqual:greaterThan
      rdf:type skos:Concept ;
      dc:source "GeoSciML UML model; definition this vocabulary" ;
      skos:definition "reported property value is lower bound"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/valuequalifier/0007> ;
      skos:inScheme cgi:valuequalifier ;
      skos:prefLabel "greater than"@en .

valqual:lessThan
      rdf:type skos:Concept ;
      dc:source "GeoSciML UML model; definition this vocabulary" ;
      skos:definition "reported property value is upper bound"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/valuequalifier/0009> ;
      skos:inScheme cgi:valuequalifier ;
      skos:prefLabel "less than"@en .

valqual:never
      rdf:type skos:Concept ;
      dc:source "GeoSciML UML model; definition this vocabulary" ;
      skos:definition "reported property value is never observed"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/valuequalifier/0012> ;
      skos:inScheme cgi:valuequalifier ;
      skos:prefLabel "never"@en .

valqual:rare
      rdf:type skos:Concept ;
      dc:source "GeoSciML UML model; definition this vocabulary" ;
      skos:definition "reported property value is rarely observed."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/valuequalifier/0018> ;
