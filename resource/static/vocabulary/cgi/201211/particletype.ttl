      rdf:type skos:Collection ;
      rdfs:label "particle type concepts"@en ;
      skos:definition "This resource defines the concept space for particle type concepts by extension; it includes all concepts in this conceptScheme, as well as concepts in previous versions of the scheme."@en ;
      skos:member parttyp:solid_constituent_particle , parttyp:variole , parttyp:epiclast , parttyp:coprolite , parttyp:accidental_pyroclastic_fragment , parttyp:crystal_fragment , parttyp:glomerocryst , parttyp:cognate_fragments , parttyp:lithophysae , parttyp:igneous_inclusion , parttyp:cavity , parttyp:juvenile_pyroclastic_fragment , parttyp:bioclast , parttyp:material_fossil , parttyp:spherule , parttyp:mineral_clast , parttyp:fecal_pellet , parttyp:ooid , parttyp:crystalline_grain , parttyp:hydroclast , parttyp:peloid , parttyp:biogenic_particle , parttyp:paleoblast , parttyp:lithic_clast , parttyp:igneous_vein , parttyp:pisoid , parttyp:coated_particle , parttyp:xenocryst , parttyp:intrusive_sheet , parttyp:axiolite , parttyp:aggregate_non-clastic_particle , parttyp:concretionary_particle , parttyp:fluidal_pyroclast , parttyp:reworked_pyroclastic_fragment , parttyp:miarolitic_cavity , parttyp:ventrifact , parttyp:nodule , parttyp:amygdule , parttyp:syngenetic_particle , parttyp:fiamme , parttyp:oncoid , parttyp:crystallite , parttyp:shard , parttyp:granular_particle , parttyp:vesicle , parttyp:fenestra , parttyp:fluid_inclusion , parttyp:lithoclast , parttyp:pyroclast , parttyp:vug , parttyp:microlite , parttyp:intraclast , parttyp:syngenetic_nodule , parttyp:holoblast , parttyp:bleb , parttyp:schlieren , parttyp:pore_space , parttyp:pseudomorph , parttyp:orb , parttyp:hydrothermal_vein , parttyp:paramorph , parttyp:autoclast ;
      skos:prefLabel "CGI Particle Type"@en .

