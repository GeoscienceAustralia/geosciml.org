parttyp:accidental_pyroclastic_fragment
      rdf:type skos:Concept ;
      dc:source "Gillespie and Styles 1999" ;
      skos:broader parttyp:pyroclast ;
      skos:definition "Fragment of rock generated by disruption as a direct result of volcanic action but not formed by previous activity of the magmatic system; generally derived from subvolcanic basement."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0001> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "accidental pyroclastic fragment"@en .

parttyp:aggregate_non-clastic_particle
      rdf:type skos:Concept ;
      dc:source "this vocabulary"@en ;
      skos:broader parttyp:solid_constituent_particle ;
      skos:definition "Generic term for a non-clastic particle that is itself composed of an aggregation of particles."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0002> ;
      skos:inScheme cgi:particletype ;
      skos:narrower parttyp:igneous_inclusion , parttyp:spherule , parttyp:axiolite , parttyp:nodule , parttyp:schlieren , parttyp:concretionary_particle , parttyp:intrusive_sheet ;
      skos:prefLabel "aggregate non clastic particle"@en .

parttyp:amygdule
      rdf:type skos:Concept ;
      dc:source "based on Neuendorf et al 2005" ;
      skos:broader parttyp:cavity ;
      skos:definition "Cavity filled with secondary minerals; denotes that cavities are filled vesicles, thus restricted to volcanic rock."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0003> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "amygdule"@en .

parttyp:autoclast
      rdf:type skos:Concept ;
      dc:source "Gillespie and Styles,1999" ;
      skos:broader parttyp:granular_particle ;
      skos:definition "Fragments of extrusive igneous rock formed by mechanical friction of moving lava flow, breakage of chilled lava flow rinds, or gravity crumbling of active spines and domes. Gillespie and Styles (1999) include as type of juvenile pyroclastic fragment, inconsistent with definition of pyroclastic by IUGS and BGS."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0004> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "autoclast"@en .

parttyp:axiolite
      rdf:type skos:Concept ;
      dc:source "Williams,Turner,Gilbert 1954" ;
      skos:broader parttyp:aggregate_non-clastic_particle ;
      skos:definition "Elongate to irregular lense with radial aggregates of acicular and fibrous mineral coalescing along a central axis."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0005> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "axiolite"@en .

parttyp:bioclast
      rdf:type skos:Concept ;
      dc:source "NADM SLTTs 2004"@en ;
      skos:broader parttyp:biogenic_particle ;
      skos:definition "Granular sedimentary particle that is a \"fragmentary piece of a shell, bone, or other hard skeletal structure of an animal, plant, or protozoan. May be fossilized or non-fossilized\". Use in situations where fossil organism can not be identified, thus 'material fossil' is inappropriate."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0006> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "bioclast"@en .

parttyp:biogenic_particle
      rdf:type skos:Concept ;
      dc:source "NADM SLTTs 2004"@en ;
      skos:broader parttyp:granular_particle ;
      skos:definition "A type of granular particle formed by the physiological activities of organisms (plants, animals, protozoa) that produce body parts which subsequently are incorporated into a sediment aggregate. Contrast with nonbiogenic particle. Includes biogenic objects that are whole or fragmentary (bioclasts); can be fossilized or non-fossilized."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0007> ;
      skos:inScheme cgi:particletype ;
      skos:narrower parttyp:fecal_pellet , parttyp:coprolite , parttyp:bioclast , parttyp:material_fossil ;
      skos:prefLabel "biogenic particle"@en .

parttyp:bleb
      rdf:type skos:Concept ;
      dc:source "this vocabulary"@en ;
      skos:broader parttyp:solid_constituent_particle ;
      skos:definition "Generic term for a small rounded particle of uncertain origin"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0008> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "bleb"@en .

parttyp:cavity
      rdf:type skos:Concept ;
      dc:source "this vocabulary"@en ;
      skos:definition "Constituent is empty space between particles in a compound material; generally filled with some sort of fluid. Cavity particle types can be used to describe porosity in a granular material."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0009> ;
      skos:inScheme cgi:particletype ;
      skos:narrower parttyp:fluid_inclusion , parttyp:vesicle , parttyp:pore_space , parttyp:lithophysae , parttyp:fenestra , parttyp:amygdule , parttyp:vug ;
      skos:prefLabel "cavity"@en ;
      skos:topConceptOf cgi:particletype .

parttyp:coated_particle
      rdf:type skos:Concept ;
      dc:source "NADM SLTTs 2004"^^xsd:string ;
      skos:broader parttyp:granular_particle ;
      skos:definition "A general term for a grain that has coats or layers of foreign material, usually fine mud-sized and occasionally recrystallized, that form concentric or overlapping shells around a core of rock, shell, peloidal, or intraclastic material. (eg: oolith, pisolith)"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0010> ;
      skos:inScheme cgi:particletype ;
      skos:narrower parttyp:pisoid , parttyp:oncoid , parttyp:ooid ;
      skos:prefLabel "coated particle"@en .

parttyp:cognate_fragments
      rdf:type skos:Concept ;
      dc:source "Base on Gillespie and Styles, 1999" ;
      skos:broader parttyp:pyroclast ;
      skos:definition "Pyroclast consisting of rock which formed during earlier (related) volcanic activity that have been ejected with other pyroclastic debris during a later eruption."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0011> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "cognate fragments"@en .

parttyp:concretionary_particle
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al, 2005" ;
      skos:broader parttyp:aggregate_non-clastic_particle ;
      skos:definition "A hard, compact mass or aggregate of mineral matter, normally subsperical but commonly oblate, disc-shaped or irregular. Formed by precipitation of mineral from solution in the pores of a granular rock, localized around a nucleus or center, to define a discrete, sharply separated object. Size ranges from cm to meters. Particle geometry description for concretion describes the concretion size and shape, not the size and shape of particles forming the concretion."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0012> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "concretionary particle"@en .

parttyp:coprolite
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader parttyp:biogenic_particle ;
      skos:definition "The fossilised excrement of vertebrate animals"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0013> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "coprolite"@en .

parttyp:crystal_fragment
      rdf:type skos:Concept ;
      dc:source "this vocabulary"@en ;
      skos:broader parttyp:pyroclast ;
      skos:definition "A fragment of a crystal interpreted to have been broken by pyroclastic processes."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0014> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "crystal fragment"@en .

parttyp:crystalline_grain
      rdf:type skos:Concept ;
      dc:source "Struik, 2002; NADM SLTTs 2004" ;
      skos:broader parttyp:solid_constituent_particle ;
      skos:definition "Constituent that is the product of crystallization during formation of a compound Earth material (see NADMC1 2004). Serves to distinguish crystalline from granular rocks (Struik, 2002). Includes constituents crystallized in evaporite environments, during diagenesis or hydrothermal alteration, or in other low-temperature environments."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0015> ;
      skos:inScheme cgi:particletype ;
      skos:narrower parttyp:paleoblast , parttyp:pseudomorph , parttyp:microlite , parttyp:holoblast , parttyp:crystallite , parttyp:xenocryst ;
      skos:prefLabel "crystalline grain"@en .

parttyp:crystallite
      rdf:type skos:Concept ;
      dc:source "Williams,Turner,Gilbert, 1954" ;
      skos:broader parttyp:crystalline_grain ;
      skos:definition "Constitutent occurs as minute, spherical, rod, or hair-like forms. Typically appear isotropic in thin section."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0016> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "crystallite"@en .

parttyp:epiclast
      rdf:type skos:Concept ;
      dc:source "Schmid 1981"@en ;
      skos:broader parttyp:granular_particle ;
      skos:definition "An individual constituent, grain or fragment of a sediment or rock, produced by erosion of a larger rock mass. Particle whose origin as a fragment is a result of surface (sedimentary, weathering...) processes. 'Crystals, crystal fragments, glass and rock fragments that have been liberated from any type of pre-existing consolidated rock (volcanic or non-volcanic) by weathering or erosion and transported from the site of origin by gravity, air, water, or ice' [Schmid, 1981]. Distinguished from intraclast in that epiclast is derived from a pre-existing rock from outside the basin of deposition (NADM SLTTs 2004) before introduction into a compoundMaterial (sense of NADMC1, 2004)."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0017> ;
      skos:inScheme cgi:particletype ;
      skos:narrower parttyp:mineral_clast , parttyp:lithic_clast ;
      skos:prefLabel "epiclast"@en .

parttyp:fecal_pellet
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader parttyp:peloid , parttyp:biogenic_particle ;
      skos:definition "an organic excrement, mainly of invertebrates, occuring especially in modern marine sediments but also fossilised in some sedimentary rocks, usually with simple ovoid form, less commonly rod-shaped."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0018> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "fecal pellet"@en .

parttyp:fenestra
      rdf:type skos:Concept ;
      dc:source "based on Neuendorf et al 2005" ;
      skos:broader parttyp:cavity ;
      skos:definition "Primary or penecontemporaneous gap or cavity in the framework of a sedimentary rock, larger than grain-supported intersticies. May be open space or have partial to complete fill with secondary cement or introduced sediment."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0019> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "fenestra"@en .

parttyp:fiamme
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al 2005; http://en.wikipedia.org/wiki/Fiamme" ;
      skos:broader parttyp:igneous_inclusion ;
      skos:definition "Lens-shaped bodies, usually mm to cm thick, and centimeters to 1-2 decimeters long, typically seen on surfaces of some pyroclastic rocks. The name fiamme comes from the Italian word for flames, describing their shape. The term is descriptive and non-genetic.Generally interpreted to form by the collapse of pumice fragments during welding in a hot pyroclastic deposit."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0020> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "fiamme"@en .

parttyp:fluid_inclusion
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader parttyp:cavity ;
      skos:definition "A cavity within a crystal containing liquid and/or gas, formed by the entrapment in crystal irregularities of fluid, commonly that from which the mineral crystallised."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0021> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "fluid inclusion"@en .

parttyp:fluidal_pyroclast
      rdf:type skos:Concept ;
      dc:source "Base on Gillespie and Styles, 1999" ;
      skos:broader parttyp:juvenile_pyroclastic_fragment ;
      skos:definition "Pyroclast that has a a rounded, fluidal shape indicating that it was in a wholly or partly molten state during formation and subsequent transport. Concept corresponds to bomb, but with no size denotation."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0022> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "fluidal pyroclast"@en .

parttyp:glomerocryst
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader parttyp:igneous_inclusion ;
      skos:definition "An aggregate of crystals of the same mineral"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0023> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "glomerocryst"@en .

parttyp:granular_particle
      rdf:type skos:Concept ;
      dc:source "NADM SLTTs 2004"@en ;
      skos:broader parttyp:solid_constituent_particle ;
      skos:definition "Solid constituent in a compound earth material (sense of NADMC1) that is a pre-existing object before incorporation into a particulate aggregate. \"...a component of solid material that has the form of grains, clasts, fragments, or whole objects of any size, shape, composition, texture, and structure.\" (NADMSC SLTTs, 2004)"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0024> ;
      skos:inScheme cgi:particletype ;
      skos:narrower parttyp:ventrifact , parttyp:peloid , parttyp:autoclast , parttyp:coated_particle , parttyp:pyroclast , parttyp:biogenic_particle , parttyp:intraclast , parttyp:reworked_pyroclastic_fragment , parttyp:epiclast , parttyp:syngenetic_particle ;
      skos:prefLabel "granular particle"@en .

parttyp:holoblast
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader parttyp:crystalline_grain ;
      skos:definition "A crystalline grain in a metamorphic rock that is newly and completely formed during metamorphism"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0025> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "holoblast"@en .

parttyp:hydroclast
      rdf:type skos:Concept ;
      dc:source "Gillespie and Styles 1999" ;
      skos:broader parttyp:juvenile_pyroclastic_fragment ;
      skos:definition "Juvenile pyroclastic fragment formed by magma-water interaction during subaqueous or subglacial extrusion; typically consist of chilled glass."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0026> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "hydroclast"@en .

parttyp:hydrothermal_vein
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al 2005; this vocabulary" ;
      skos:broader parttyp:intrusive_sheet ;
      skos:definition "a tabular or sheet-like part of a compound material formed by hydrothermal (or other metasomatic) mineral filling a fracture; may be associated with replacement of the host rock adjacent to the body"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0027> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "hydrothermal vein"@en .

parttyp:igneous_inclusion
      rdf:type skos:Concept ;
      dc:source "this vocabulary"@en ;
      skos:broader parttyp:aggregate_non-clastic_particle ;
      skos:definition "Aggregate non-clastic particle that are of igneous origin. Particle types for compound material are restricted to inclusions that are typically of a size consistent with hand-sample description to characterize a kind of rock or a rock sample; larger inclusion types are included in the GeologicUnitPart role vocabulary."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0028> ;
      skos:inScheme cgi:particletype ;
      skos:narrower parttyp:orb , parttyp:glomerocryst , parttyp:fiamme ;
      skos:prefLabel "igneous inclusion"@en .

parttyp:igneous_vein
      rdf:type skos:Concept ;
      dc:source "this vocabulary"@en ;
      skos:broader parttyp:intrusive_sheet ;
      skos:definition "a tabular or sheet like part of a compound material formed by the intrusion of magma"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0029> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "igneous vein"@en .

parttyp:intraclast
      rdf:type skos:Concept ;
      dc:source "NADM SLTTs 2004; Neuendorf et al., 2005; Folk, 1959" ;
      skos:broader parttyp:granular_particle ;
      skos:definition "A broad, general term introduced by Folk (1959, p. 4) for a component of limestone, representing a torn-up and re-worked fragment of a penecontemporaneous sediment (usually weakly consolidated) that has been eroded within the basin of deposition...and re-deposited there...The fragment may range in size from fine sand to gravel..."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0030> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "intraclast"@en .

parttyp:intrusive_sheet
      rdf:type skos:Concept ;
      dc:source "this vocabulary"@en ;
      skos:broader parttyp:aggregate_non-clastic_particle ;
      skos:definition "a tabular or sheet-like part of a compound material, genetic origin not specified"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0031> ;
      skos:inScheme cgi:particletype ;
      skos:narrower parttyp:igneous_vein , parttyp:hydrothermal_vein ;
      skos:prefLabel "intrusive sheet"@en .

parttyp:juvenile_pyroclastic_fragment
      rdf:type skos:Concept ;
      dc:source "Gillespie and Styles 1999" ;
      skos:broader parttyp:pyroclast ;
      skos:definition "Fragment formed directly from cooling magma during transport prior to primary deposition."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0032> ;
      skos:inScheme cgi:particletype ;
      skos:narrower parttyp:fluidal_pyroclast , parttyp:hydroclast , parttyp:shard ;
      skos:prefLabel "juvenile pyroclastic fragment"@en .

parttyp:lithic_clast
      rdf:type skos:Concept ;
      dc:source "this vocabulary"@en ;
      skos:broader parttyp:epiclast ;
      skos:definition "A clast derived by erosion from older, pre-existing rock materials of any kind, usually beyond the basin of accumulation but occasionally from within the basin of deposition (as from a submarine or subaqueous exposure adjacent to a depositional site). A clast that is identified as a piece of rock."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0033> ;
      skos:inScheme cgi:particletype ;
      skos:narrower parttyp:lithoclast ;
      skos:prefLabel "lithic clast"@en .

parttyp:lithoclast
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al., 2005, p. 375" ;
      skos:broader parttyp:lithic_clast ;
      skos:definition "A mechanically formed and deposited fragment of a carbonate rock...derived from an older, lithified limestone or dolomite within, adjacent to, or outside the depositional site."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0034> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "lithoclast"@en .

parttyp:lithophysae
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al 2005" ;
      skos:broader parttyp:cavity ;
      skos:definition "Bubble-like cavity with concentric shells of finely crystalline minerals; open space remains in core of structure. Radiating fibrous structure may be present in secondary mineral fill. Typically in silicic volcanic rock."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0035> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "lithophysae"@en .

parttyp:material_fossil
      rdf:type skos:Concept ;
      dc:source "GeoSciML v2 RC1 model element scope note" ;
      skos:broader parttyp:biogenic_particle ;
      skos:definition "The preserved remains or replaced remains (casts) of plants and animals. A fossil type may have one or more described associated organisms. If particle type is materialFossil, an additional type property element may provide a reference to a Paleontologic description of the fossil."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0036> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "material fossil"@en .

parttyp:miarolitic_cavity
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al 2005" ;
      skos:broader parttyp:vug ;
      skos:definition "An irregular cavity in a phaneritic igneous rock into which small crystals of the rock-forming minerals protrude."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0037> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "miarolitic cavity"@en .

parttyp:microlite
      rdf:type skos:Concept ;
      dc:source "Williams,Turner,Gilbert, 1954" ;
      skos:broader parttyp:crystalline_grain ;
      skos:definition "Constituent occurs as minute incipient crystals that display some birefringence in thin section."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0038> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "microlite"@en .

parttyp:mineral_clast
      rdf:type skos:Concept ;
      dc:source "NADM SLTTs 2004"@en ;
      skos:broader parttyp:epiclast ;
      skos:definition "An epiclast that is composed of a single mineral."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0039> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "mineral clast"@en .

parttyp:nodule
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader parttyp:aggregate_non-clastic_particle ;
      skos:definition "An irregularly rounded mass of a mineral or mineral aggregate normally having a warty or knobby surface and no internal sructure, usually with a contrasting composition from the enclosing sediment or rock matrix in which it is embedded, and that can be separated as a discrete mass from the host material. Igneous nodules should be classified as 'Igneous inclusions'."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0040> ;
      skos:inScheme cgi:particletype ;
      skos:narrower parttyp:syngenetic_nodule ;
      skos:prefLabel "nodule"@en .

parttyp:oncoid
      rdf:type skos:Concept ;
      dc:source "Jackson 1997; Hallsworth and Knox 1999" ;
      skos:broader parttyp:coated_particle ;
      skos:definition "A '...coated grain with a cortex of irregular, partially overlapping laminae. They are typically irregular in shape and may exhibit biogenic structures. Some forms lack a distinct nucleus. Oncoids are generally larger than 2 mm.' (Hallsworth and Knox, 1999, p. 27). Synonymous with oncolite: A small, variously shaped, concentrically laminated, calcareous sedimentary structure, resembling an oolith, and formed by the accretion of successive layer[s]... It...generally does not exceed 10 cm in dimension. (Jackson, 1997, p. 446)."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0041> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "oncoid"@en .

parttyp:ooid
      rdf:type skos:Concept ;
      dc:source "Jackson1997; Hallsworth and Knox 1999" ;
      skos:broader parttyp:coated_particle ;
      skos:definition "A general, nongenetic term for a particle that resembles an oolith in outer appearance and size- (Jackson, 1997, p. 447). Hallsworth and Knox (1999, p. 27) describe ooids as coated grains that typically are spherical or ellipsoidal in shape, with the degree of roundness increasing outward. Concentric to semi-concentric coats are smoothly and evenly laminated. A nucleus usually is present, and may have a composition different from the coatings. Biogenic structures are not obvious. NADMSC SLTTs (2004) considers synomous with oolith."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0042> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "ooid"@en .

parttyp:orb
      rdf:type skos:Concept ;
      dc:source "this vocabulary"@en ;
      skos:broader parttyp:igneous_inclusion ;
      skos:definition "igneous constituent typically mafic, equant rounded spheroid with concentric mineralogic banding"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0043> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "orb"@en .

parttyp:paleoblast
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al. 2005" ;
      skos:broader parttyp:crystalline_grain ;
      skos:definition "A crystal or remnant of a crystal that is older than other mineral grains in the rock, especially in metamorphic rocks"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0044> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "paleoblast"@en .

parttyp:paramorph
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader parttyp:pseudomorph ;
      skos:definition "a pseudomorph with the same composition as the original crystal (eg: calcite after aragonite)"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0045> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "paramorph"@en .

parttyp:peloid
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader parttyp:granular_particle ;
      skos:definition "a usually rounded aggregate of clay-sized calcareous (micritic) material; origin and size is not specified"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0046> ;
      skos:inScheme cgi:particletype ;
      skos:narrower parttyp:fecal_pellet ;
      skos:prefLabel "peloid"@en .

parttyp:pisoid
      rdf:type skos:Concept ;
      dc:source "NADM SLTTs 2004; Jackson 1997" ;
      skos:broader parttyp:coated_particle ;
      skos:definition "\"A round or ellipsoidal accretionary body resembling a pea in size and shape.... A pisoid...is larger and less regular in form than an ooid, although it may have the same concentric and/or radial internal structure\" (Jackson, 1997, p. 489)."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0047> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "pisoid"@en .

parttyp:pore_space
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al 2005, this vocabulary" ;
      skos:broader parttyp:cavity ;
      skos:definition "Open space between particles in a granular aggregate"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0048> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "pore space"@en .

parttyp:pseudomorph
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al. 2005; Spry 1969; Fettes and Desmons, 2007" ;
      skos:broader parttyp:crystalline_grain ;
      skos:definition "Constituent particle that has the outward form of a mineral species or particle type, but the original minerals composing the particle have been altered or replaced by different minerals; a secondary mineral whose outward crystal form has been inherited from the orignal mineral it has replaced."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0049> ;
      skos:inScheme cgi:particletype ;
      skos:narrower parttyp:paramorph ;
      skos:prefLabel "pseudomorph"@en .

parttyp:pyroclast
      rdf:type skos:Concept ;
      dc:source "NADM SLTTs 2004; Jackson, 1997; Gillespie and Styles 1999" ;
      skos:broader parttyp:granular_particle ;
      skos:definition "\"An individual particle ejected during a volcanic eruption.\" (Jackson, 1997, p. 521). Clast whose origin is a direct result of volcanic process (excludes fragments in lava autobreccia) and has not been reworked by sedimentary processes (Gillespie and Styles 1999)."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0050> ;
      skos:inScheme cgi:particletype ;
      skos:narrower parttyp:cognate_fragments , parttyp:accidental_pyroclastic_fragment , parttyp:juvenile_pyroclastic_fragment , parttyp:crystal_fragment ;
      skos:prefLabel "pyroclast"@en .

parttyp:reworked_pyroclastic_fragment
      rdf:type skos:Concept ;
      dc:source "Gillespie and Styles 1999" ;
      skos:broader parttyp:granular_particle ;
      skos:definition "Fragment formed as a direct result of volcanic activity and reworked by sedimentary processes."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0051> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "reworked pyroclastic fragment"@en ;
      skos:related parttyp:pyroclast .

parttyp:schlieren
      rdf:type skos:Concept ;
      dc:source "GGIPAC"@en ;
      skos:broader parttyp:aggregate_non-clastic_particle ;
      skos:definition "a tabular body, generally a few cm to a few metres long, within a plutonic rock, having different mineral proportions and colour to the surrounding rock"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0052> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "schlieren"@en .

parttyp:shard
      rdf:type skos:Concept ;
      dc:source "GGIPAC"@en ;
      skos:broader parttyp:juvenile_pyroclastic_fragment ;
      skos:definition "a vitric fragment in pyroclastic rocks, often with a characteristically curved surface of fracture. Shards generally consist of bubble-wall fragments produced by disintegration of pumice during or after an eruption"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0053> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "shard"@en .

parttyp:solid_constituent_particle
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al 2005" ;
      skos:definition "Particle is defined as a \"General term, used without restriction as to shape composition or internal structure, for a separable or distinct unit in a [compound material].\" (Neuendorf et al, 2005). Unclassified solid constituent in a compound material. Note that the term particle has no size denotation as used here." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0054> ;
      skos:inScheme cgi:particletype ;
      skos:narrower parttyp:crystalline_grain , parttyp:granular_particle , parttyp:bleb , parttyp:aggregate_non-clastic_particle ;
      skos:prefLabel "solid constituent particle" ;
      skos:topConceptOf cgi:particletype .

parttyp:spherule
      rdf:type skos:Concept ;
      dc:source "Williams,Turner,Gilbert 1954" ;
      skos:broader parttyp:aggregate_non-clastic_particle ;
      skos:definition "a rounded or spherical mass of acicular or fibrous mineral, generally in glassy siliceous lava."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0055> ;
      skos:inScheme cgi:particletype ;
      skos:narrower parttyp:variole ;
      skos:prefLabel "spherule"@en .

parttyp:syngenetic_nodule
      rdf:type skos:Concept ;
      dc:source "NADM SLTTs 2004; Jackson 1997" ;
      skos:broader parttyp:nodule ;
      skos:definition "Particle formed by chemical precipitation at sediment-water interface, lacking layered structure that characterizes coated grains. Includes glauconite grains, manganese nodules, phosphate grains."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0056> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "syngenetic nodule"@en .

parttyp:syngenetic_particle
      rdf:type skos:Concept ;
      dc:source "NADM SLTTs 2004"@en ;
      skos:broader parttyp:granular_particle ;
      skos:definition "A type of nonbiogenic particle formed by in situ or intrabasinal physical-chemical-mechanical processes without the direct biochemical activity of organisms, penecontemporaneously with sediment accumulation or during diagenetic modification."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0057> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "syngenetic particle"@en .

parttyp:variole
      rdf:type skos:Concept ;
      dc:source "Williams,Turner,Gilbert 1954" ;
      skos:broader parttyp:spherule ;
      skos:definition "Spherulitic cluster of crystals in mafic rock, usually consists of divergent plagioclase fibers, with or without interstitial glass, or intergrown with granules of pyroxene, olivine or iron ore. (equivalent to Spherule, but in mafic rock)"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0058> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "variole"@en .

parttyp:ventrifact
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader parttyp:granular_particle ;
      skos:definition "A granular particle that has been shaped, worn, faceted, cut or polished by the action of windblown sand." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0059> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "ventrifact" .

parttyp:vesicle
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al 2005" ;
      skos:broader parttyp:cavity ;
      skos:definition "Cavity in volcanic rock formed by trapped gas. Use amygdule if filled with secondary mineral." ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0060> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "vesicle" .

parttyp:vug
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al 2005" ;
      skos:broader parttyp:cavity ;
      skos:definition "Irregular cavity in rock; generic term with no connotation of origin of cavity. May be lined with crystals of different mineral compostion to the host rock"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0061> ;
      skos:inScheme cgi:particletype ;
      skos:narrower parttyp:miarolitic_cavity ;
      skos:prefLabel "vug"@en .

parttyp:xenocryst
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader parttyp:crystalline_grain ;
      skos:definition "A crystal that resembles a phenocryst in an igneous rock, but that is foreign to the rock in which it occurs"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0062> ;
      skos:inScheme cgi:particletype ;
      skos:prefLabel "xenocryst"@en .

cgi:particletype
      rdf:type owl:Ontology , skos:ConceptScheme ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)" ;
      dc:format "application/rdf+xml" ;
      dc:identifier "http://resource.geosciml.org/classifierscheme/cgi/201211/particletype" ;
      dc:language "en" ;
      dc:publisher "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)" ;
      dc:title "CGI Particle Type Categories"@en ;
      owl:imports <http://rdfs.org/ns/void> , <http://www.w3.org/2004/02/skos/core> ;
      owl:versionInfo "201211" ;
      skos:changeNote "2009-12-07 SMR Update metadata properties for version, creator, title, and format.  Change skos:HistoryNote to dc:source for information on origin of terms and definitions."@en ;
      skos:definition "This file contains the CGI Particle type vocabulary. Used to specify particleType in particleTypeDescription associated with a compoundMaterial. Terms to specify the nature of individual particles of each constituent in an aggregation, based mostly on their genesis. Because the constituents are identifiable parts, the part units must have some definition. Examples include clasts, crystals, pore space, material fossil. Constituent type is determined based on the nature of the particles, and ideally is independent of the relationship between particles in a compound material aggregation."@en ;
      skos:historyNote "2012-11-24 SMR Update to 201211 version; add collection entity, check all pref labels are lower case, remove owl:NamedIndividual and Owl:Thing rdf:types."@en , "2012-02-07 SMR update URI to replace numeric final token with English-language string as in original URN scheme."@en , "2010-11-28 SMR Replace URN identifiers with http URI identifiers according to CGI URI scheme (see https://www.seegrid.csiro.au/wiki/bin/view/CGIModel/PersistentIdentifiersInGeoSciMLServices)"@en , "2009-07-14. Generate SKOS encoding of vocabulary from Excel spreadsheet."@en , "2010-02-08. SMR Clean up namespace declarations so concept URI's follow CGI URN scheme; concepts have URN, are not document fragments."@en , "2010-02-13 Add narrower relationship links by adding skos:narrower link in Protege, defining skos:narrower as inverse of skos:broader, calculating with Pellet reasoner to add inferred axioms, and using 'file/Export inferred axioms as ontology' in Protege."@en , "2012-02-27 SMR add skos:exactMatch triples relating URI in this vocabulary with URI for equivalent concepts in 201012 version that use numeric tokens."@en ;
      skos:prefLabel "CGI Particle Type Categories"@en .

<http://resource.geosciml.org/vocabulary/cgi/201211/particletype>
      rdf:type void:Dataset ;
      rdfs:label "Particle type vocabulary"^^xsd:string .

skos:ConceptScheme
      rdf:type owl:Class .
