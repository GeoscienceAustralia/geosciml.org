eventenv:abandoned_channel
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader eventenv:river_plain_system_setting ;
      skos:definition "A drainage channel along which runoff no longer occurs, as on an alluvial fan"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0001> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "abandoned river channel setting"@en .

eventenv:above_carbonate_compensation_depth_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary"@en ;
      skos:broader eventenv:marine_setting ;
      skos:definition "Marine environment in which carbonate sediment does not dissolve before reaching the sea floor and can accumulate."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0002> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "above carbonate compensation depth setting"@en .

eventenv:abyssal
