      dc:source "Neuendorf et al., 2005" ;
      skos:broader eventenv:marine_setting ;
      skos:definition "The ocean environment at water depths between 3,500 and 6,000 metres"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0003> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "abyssal setting"@en .

