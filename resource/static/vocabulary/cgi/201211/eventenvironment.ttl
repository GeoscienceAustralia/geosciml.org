      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0062> ;
      skos:inScheme cgi:eventenvironment ;
      skos:narrower eventenv:subglacial_setting , eventenv:englacial_setting , eventenv:proglacial_setting , eventenv:supraglacial_setting , eventenv:glacier_lateral_setting , eventenv:glacial_outwash_plain_setting ;
      skos:prefLabel "glacier related setting"@en .

eventenv:glacier_terminus_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "NADM SLTTs, 2004" ;
      skos:broader eventenv:proglacial_setting ;
      skos:definition "Region of sediment deposition due to melting of glacier ice. ablation and flow till setting."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0063> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "glacier terminus setting"@en .

eventenv:hadal
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader eventenv:marine_setting ;
      skos:definition "The deepest oceanic environment, i.e., over 6,000 m in depth. Always in deep sea trench."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0064> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "hadal setting"@en ;
      skos:related eventenv:deep_sea_trench .

eventenv:high_pressure_low_temperature_earth_interior_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary"@en ;
      skos:broader eventenv:earth_interior_setting ;
      skos:definition "High pressure environment characterized by geothermal gradient significantly lower than standard continental geotherm; enviornment in which blueschist facies metamorphic rocks form. Typically associated with subduction zones."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0065> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "high pressure low temperature earth interior setting" .

eventenv:hillslope_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "North American Geologic-map Data Model Science Language Technical Team, 2004, Sedimentary materials:  science language for their classification, description, and interpretation in digital geologic-map databases, Version 1.0 (12/18/2004): U.S. Geological Survey Open-File Report 2004-1451 appendix C, 595 p., accessed at http://pubs.usgs.gov/of/2004/1451/sltt/appendixC/; Hawley, J.W., and Parsons, R.B. 1980. Glossary of selected geomorphic and geologic terms. Mimeo. USDA Soil Conservation Service, West National Technical Center, Portland, OR. 30 p." ;
      skos:broader eventenv:earth_surface_setting ;
      skos:definition "Earth surface setting characterized by surface slope angles high enough that gravity alone becomes a significant factor in geomorphic development, as well as base-of-slope areas influenced by hillslope processes. Hillslope activities include creep, sliding, slumping, falling, and other downslope movements caused by slope collapse induced by gravitational influence on earth materials. May be subaerial or subaqueous."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0066> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "hillslope setting"@en .

eventenv:hinterland_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary"@en ;
      skos:broader eventenv:tectonic_setting , eventenv:continental_crust ;
      skos:definition "Tectonic setting in the internal part of an orogenic belt, characterized by plastic deformation of rocks accompanied by significant metamorphism, typically involving crystalline basement rocks. Typically denotes the most structurally thickened part of an orogenic belt, between a magmatic arc or collision zone and a more 'external' foreland setting."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0067> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "hinterland tectonic setting"@en .

eventenv:hot_spot_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary"@en ;
      skos:broader eventenv:tectonic_setting ;
      skos:definition "Setting in a zone of high heat flow from the mantle. Typically identified in intraplate settings, but hot spot may also interact with active plate margins (Iceland...). Includes surface manifestations like volcanic center, but also includes crust and mantle manifestations as well."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0068> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "hot spot setting"@en ;
      skos:related eventenv:intraplate_tectonic_setting .

eventenv:humid_temperate_climatic_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Cleland, D.T.; Avers, P.E.; McNab, W.H.; Jensen, M.E.; Bailey, R.G., King, T.; Russell, W.E. 1997. National Hierarchical Framework of Ecological Units, in Boyce, M. S.; Haney, A., ed., Ecosystem Management Applications for Sustainable Forest and Wildlife  Resources: Yale University Press, New Haven, CT. pp. 181-200."@en ;
      skos:broader eventenv:earth_surface_setting ;
      skos:definition "Setting with seasonal climate having hot to cold or humid to arid seasons."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0069> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "humid temperate climatic setting"@en .

eventenv:humid_tropical_climatic_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Cleland, D.T.; Avers, P.E.; McNab, W.H.; Jensen, M.E.; Bailey, R.G., King, T.; Russell, W.E. 1997. National Hierarchical Framework of Ecological Units, in Boyce, M. S.; Haney, A., ed., Ecosystem Management Applications for Sustainable Forest and Wildlife  Resources: Yale University Press, New Haven, CT. pp. 181-200."@en ;
      skos:broader eventenv:earth_surface_setting ;
      skos:definition "Setting with hot, humid climate influenced by equatorial air masses, no winter season."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0070> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "humid tropical climatic setting"@en .

eventenv:hypabyssal_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary"@en ;
      skos:broader eventenv:earth_interior_setting ;
      skos:definition "Igneous environment close to the Earth's surface, characterized by more rapid cooling than plutonic setting to produce generally fine-grained intrusive igneous rock that is commonly associated with co-magmatic volcanic rocks."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0071> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "hypabyssal setting"@en .

eventenv:inactive_spreading_center
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "This vocabulary"@en ;
      skos:broader eventenv:plate_spreading_center , eventenv:intraplate_tectonic_setting ;
