      dc:source "this vocabulary"@en ;
      skos:broader eventenv:neritic ;
      skos:definition "The ocean environment at depths between 30 and 100 metres"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0096> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "middle neritic setting"@en .

eventenv:mud_flat_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "North American Geologic-map Data Model Science Language Technical Team, 2004, Sedimentary materials:  science language for their classification, description, and interpretation in digital geologic-map databases, Version 1.0 (12/18/2004): U.S. Geological Survey Open-File Report 2004-1451 appendix C, 595 p., accessed at http://pubs.usgs.gov/of/2004/1451/sltt/appendixC/; Jackson, 1997" ;
      skos:broader eventenv:low_energy_shoreline_setting ;
      skos:definition "A relatively level area of fine grained material (e.g. silt) along a shore (as in a sheltered estuary or chenier-plain) or around an island, alternately covered and uncovered by the tide or covered by shallow water, and barren of vegetation. Includes most tidal flats, but lacks denotation of tidal influence.."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0097> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "mud flat setting"@en ;
      skos:related eventenv:tidal_flat_setting .

eventenv:neritic
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader eventenv:marine_setting ;
      skos:definition "The ocean environment at depths between low-tide level and 200 metres, or between low-tide level and approximately the edge of the continental shelf"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0098> ;
      skos:inScheme cgi:eventenvironment ;
      skos:narrower eventenv:inner_neritic , eventenv:outer_neritic , eventenv:middle_neritic ;
      skos:prefLabel "neritic setting"@en ;
      skos:related eventenv:continental_shelf_setting .

eventenv:ocean_highland_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary"@en ;
      skos:broader eventenv:marine_setting ;
      skos:definition "Broad category for subaqueous marine settings characterized by significant relief above adjacent sea floor."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0099> ;
      skos:inScheme cgi:eventenvironment ;
      skos:narrower eventenv:mid_ocean_ridge_setting , eventenv:seamount_setting , eventenv:oceanic_plateau_setting ;
      skos:prefLabel "ocean highland setting"@en ;
      skos:related eventenv:oceanic_crust .

eventenv:oceanic_crust
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader eventenv:crust ;
      skos:definition "That type of the Earth's crust which underlies the ocean basins. The oceanic crust is 5-10 km thick; it has a density of 2.9 g/cm3, and compressional seismic-wave velocities travelling through it at 4-7.2 km/sec. Setting in crust produced by submarine volcanism at a mid ocean ridge."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0100> ;
      skos:inScheme cgi:eventenvironment ;
      skos:narrower eventenv:lower_oceanic_crustal_setting , eventenv:upper_oceanic_crustal_setting ;
      skos:prefLabel "oceanic crustal setting"@en .

eventenv:oceanic_plateau_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Reading 1978"@en ;
      skos:broader eventenv:ocean_highland_setting ;
      skos:definition "Region of elevated ocean crust that commonly rises to within 2-3 km of the surface above an abyssal sea floor that lies several km deeper. Climate and water depths are such that a marine carbonate platform does not develop."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0101> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "oceanic plateau setting"@en ;
      skos:related eventenv:intraplate_tectonic_setting .

eventenv:outer_neritic
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary"@en ;
      skos:broader eventenv:neritic ;
      skos:definition "The ocean environment at depths between 100 and 200 metres or between low-tide level and approximately the edge of the continental shelf"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0102> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "outer neritic setting"@en .

eventenv:passive_continental_margin_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "This vocabulary"@en ;
      skos:broader eventenv:tectonic_setting ;
      skos:definition "Boundary of continental crust into oceanic crust of an oceanic basin that is not a subduction zone or transform fault system. Generally is rifted margin formed when ocean basin was initially formed."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0103> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "passive continental margin setting"@en .

eventenv:pediment_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "North American Geologic-map Data Model Science Language Technical Team, 2004, Sedimentary materials:  science language for their classification, description, and interpretation in digital geologic-map databases, Version 1.0 (12/18/2004): U.S. Geological Survey Open-File Report 2004-1451 appendix C, 595 p., accessed at http://pubs.usgs.gov/of/2004/1451/sltt/appendixC/; NRCS, 2001" ;
      skos:broader eventenv:piedmont_slope_system_setting ;
      skos:definition "A gently sloping erosional surface developed at the foot of a receding hill or mountain slope. The surface may be essentially bare, exposing earth material that extends beneath adjacent uplands; or it may be thinly mantled with alluvium and colluvium, ultimately in transit from upland front to basin or valley lowland. In hill-foot slope terrain the mantle is designated \"pedisediment.\" The term has been used in several geomorphic contexts: Pediments may be classed with respect to (a) landscape positions, for example, intermontane-basin piedmont or valley-border footslope surfaces (respectively, apron and terrace pediments (Cooke and Warren, 1973)); (b) type of material eroded, bedrock or regolith; or (c) combinations of the above. compare - Piedmont slope.."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0104> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "pediment setting"@en .

eventenv:piedmont_slope_system_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Hawley and Parsons, 1980+F98" ;
      skos:broader eventenv:subaerial_setting ;
      skos:definition "Location on gentle slope at the foot of a mountain; generally used in terms of intermontane-basin terrain. Main components include: (a) An erosional surface on bedrock adjacent to the receding mountain front (pediment, rock pediment); (b) A constructional surface comprising individual alluvial fans and interfan valleys, also near the mountain front; and (c) A distal complex of coalescent fans (bajada), and alluvial slopes without fan form. Piedmont slopes grade to basin-floor depressions with alluvial and temporary lake plains or to surfaces associated with through drainage."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0105> ;
      skos:inScheme cgi:eventenvironment ;
      skos:narrower eventenv:alluvial_fan , eventenv:pediment_setting , eventenv:alluvial_plain ;
      skos:prefLabel "piedmont slope system setting"@en .

eventenv:plate_margin_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "This vocabulary"@en ;
      skos:broader eventenv:tectonic_setting ;
      skos:definition "Tectonic setting at the boundary between two tectonic plates."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0106> ;
      skos:inScheme cgi:eventenvironment ;
      skos:narrower eventenv:subduction_zone_setting , eventenv:active_continental_margin_setting , eventenv:transform_plate_boundary_setting , eventenv:forearc_setting , eventenv:volcanic_arc_setting , eventenv:active_spreading_center ;
      skos:prefLabel "plate margin setting"@en .

eventenv:plate_spreading_center
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary"@en ;
      skos:broader eventenv:tectonic_setting ;
      skos:definition "Tectonic setting where new oceanic crust is being or has been formed at a divergent plate boundary. Includes active and inactive spreading centers."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0107> ;
      skos:inScheme cgi:eventenvironment ;
      skos:narrower eventenv:inactive_spreading_center , eventenv:active_spreading_center ;
      skos:prefLabel "plate spreading center setting"@en .

eventenv:playa_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Based on Hawley and Parsons, 1980" ;
      skos:broader eventenv:arid_or_semi_arid_environment_setting , eventenv:terrestrial_setting , eventenv:subaerial_setting ;
      skos:definition "The usually dry and nearly level plain that occupies the lowest parts of closed depressions, such as those occurring on intermontane basin floors. Temporary flooding occurs primarily in response to precipitation-runoff events."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0108> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "playa setting"@en .

eventenv:polar_climatic_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Cleland, D.T.; Avers, P.E.; McNab, W.H.; Jensen, M.E.; Bailey, R.G., King, T.; Russell, W.E. 1997. National Hierarchical Framework of Ecological Units, in Boyce, M. S.; Haney, A., ed., Ecosystem Management Applications for Sustainable Forest and Wildlife  Resources: Yale University Press, New Haven, CT. pp. 181-200."@en ;
      skos:broader eventenv:earth_surface_setting ;
      skos:definition "Setting with climate dominated by temperatures below the freezing temperature of water. Includes polar deserts because precipitation is generally scant at high latitude. Climate controlled by arctic air masses, cold dry environment with short summer."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0109> ;
      skos:inScheme cgi:eventenvironment ;
      skos:narrower eventenv:glacier_related_setting ;
      skos:prefLabel "polar climatic"@en .

eventenv:prodelta
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader eventenv:subaqueous_setting , eventenv:deltaic_system_setting ;
      skos:definition "The part of a delta that is below the effective depth of wave erosion, lying beyond the delta front, and sloping gently down to the floor of the basin into which the delta is advancing and where clastic river sediment ceases to be a significant part of the basin-floor deposits; it is entirely below the water level"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0110> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "prodelta setting"@en .

eventenv:proglacial_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader eventenv:glacier_related_setting ;
      skos:definition "Immediately in front of or just beyond the outer limits of a glacier or ice sheet, generally at or near its lower end; said of lakes, streams, deposits, and other features produced by or derived from the glacier ice"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0111> ;
      skos:inScheme cgi:eventenvironment ;
      skos:narrower eventenv:glacier_terminus_setting ;
      skos:prefLabel "proglacial setting"@en .

eventenv:reef_flat
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader eventenv:biological_reef_setting ;
      skos:definition "A stony platform of"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0112> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "reef flat setting"@en .

eventenv:regional_metamorphic_origin
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "NADM metamorphic rock vocabulary SLTTm1.0, 2004" ;
      skos:broader eventenv:earth_interior_setting ;
      skos:definition "Metamorphism not obviously localized along contacts of igneous bodies; includes burial metamorphism and ocean ridge metamorphism"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0113> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "regional metamorphic setting"@en .

eventenv:river_channel
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader eventenv:river_plain_system_setting ;
      skos:definition "The bed where a natural body of surface water flows or may flow; a natural passageway or depression of perceptible extent containing continuously or periodically flowing water, or forming a connecting link between two bodies of water; a watercourse"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0114> ;
      skos:inScheme cgi:eventenvironment ;
      skos:narrower eventenv:meandering_channel , eventenv:braided_channel ;
      skos:prefLabel "river channel setting"@en ;
      skos:related eventenv:subaqueous_setting .

eventenv:river_plain_system_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary"@en ;
      skos:broader eventenv:terrestrial_setting ;
      skos:definition "Geologic setting dominated by a river system; river plains may occur in any climatic setting. Includes active channels, abandoned channels, levees, oxbow lakes, flood plain. May be part of an alluvial plain that includes terraces composed of abandoned river plain deposits."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0115> ;
      skos:inScheme cgi:eventenvironment ;
      skos:narrower eventenv:floodplain , eventenv:cutoff_meander , eventenv:abandoned_channel , eventenv:river_channel ;
      skos:prefLabel "river plain system setting"@en ;
      skos:related eventenv:subaerial_setting .

eventenv:rocky_coast_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "North American Geologic-map Data Model Science Language Technical Team, 2004, Sedimentary materials:  science language for their classification, description, and interpretation in digital geologic-map databases, Version 1.0 (12/18/2004): U.S. Geological Survey Open-File Report 2004-1451 appendix C, 595 p., accessed at http://pubs.usgs.gov/of/2004/1451/sltt/appendixC/" ;
      skos:broader eventenv:shoreline_settings ;
      skos:definition "Shoreline with significant relief and abundant rock outcrop."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0116> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "rocky coast setting"@en .

eventenv:sand_plain
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader eventenv:subaerial_setting , eventenv:arid_or_semi_arid_environment_setting , eventenv:aeolian_process_setting , eventenv:terrestrial_setting ;
      skos:definition "A sand-covered plain dominated by aeolian processes."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0117> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "sand plain setting"@en .

eventenv:seamount_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Reading 1978" ;
      skos:broader eventenv:ocean_highland_setting , eventenv:intraplate_tectonic_setting ;
      skos:definition "Setting that consists of a conical mountain on the ocean floor (guyot). Typically characterized by active volcanism, pelagic sedimentation. If the mountain is high enough to reach the photic zone, carbonate production may result in reef building to produce a carbonate platform or atoll setting."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0118> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "seamount setting"@en .

eventenv:shoreline_settings
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "based on Neuendorf et al, 2005" ;
      skos:broader eventenv:earth_surface_setting ;
      skos:definition "Geologic settings characterized by location adjacent to the ocean or a lake. A zone of indefinite width (may be many kilometers), bordering a body of water that extends from the water line inland to the first major change in landform features. Includes settings that may be subaerial, intermittently subaqueous, or shallow subaqueous, but are intrinsically associated with the interface between land areas and water bodies."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0119> ;
      skos:inScheme cgi:eventenvironment ;
      skos:narrower eventenv:barrier_island_coastline_setting , eventenv:deltaic_system_setting , eventenv:tidal_setting , eventenv:beach , eventenv:estuary_setting , eventenv:strandplain_setting , eventenv:supratidal , eventenv:rocky_coast_setting , eventenv:lagoonal_setting , eventenv:carbonate_dominated_shoreline_setting , eventenv:coastal_plain_setting , eventenv:low_energy_shoreline_setting ;
      skos:prefLabel "shoreline settings"@en .

eventenv:slope_rise_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "based on NADM SLTTs, 2004" ;
      skos:broader eventenv:marine_setting ;
      skos:definition "The part of a subaqueous basin that is between a bordering shelf setting, which separate the basin from an adjacent landmass, and a very low-relief basin plain setting."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0120> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "slope rise setting"@en ;
      skos:related eventenv:transitional_crust .

eventenv:slow_spreading_center_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Macdonald 1982" ;
      skos:broader eventenv:active_spreading_center ;
      skos:definition "Spreading center at which the opening rate is less than 50 mm per year."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0121> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "slow spreading center setting"@en .

eventenv:strandplain_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "based on Jackson 1997, p. 626" ;
      skos:broader eventenv:subaerial_setting , eventenv:shoreline_settings ;
      skos:definition "A prograded shore built seaward by waves and currents, and continuous for some distance along the coast. It is characterized by subparallel beach ridges and swales, in places with associated dunes."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0122> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "strandplain setting"@en ;
      skos:related eventenv:terrestrial_setting .

eventenv:subaerial_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader eventenv:earth_surface_setting ;
      skos:definition "Setting at the interface between the solid earth and the atmosphere, includes some shallow subaqueous settings in river channels and playas. Characterized by conditions and processes, such as erosion, that exist or operate in the open air on or immediately adjacent to the land surface"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0123> ;
      skos:inScheme cgi:eventenvironment ;
      skos:narrower eventenv:floodplain , eventenv:supratidal , eventenv:coastal_plain_setting , eventenv:aeolian_process_setting , eventenv:strandplain_setting , eventenv:gibber_plain , eventenv:playa_setting , eventenv:piedmont_slope_system_setting , eventenv:sand_plain ;
      skos:prefLabel "subaerial setting"@en ;
      skos:related eventenv:terrestrial_setting .

eventenv:subaqueous_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "based on North American Geologic-map Data Model Science Language Technical Team, 2004, Sedimentary materials:  science language for their classification, description, and interpretation in digital geologic-map databases, Version 1.0 (12/18/2004): U.S. Geological Survey Open-File Report 2004-1451 appendix C, 595 p., accessed at http://pubs.usgs.gov/of/2004/1451/sltt/appendixC/;" ;
      skos:broader eventenv:earth_surface_setting ;
      skos:definition "Setting situated in or under permanent, standing water. Used for marine and lacustrine settings, but not for fluvial settings."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0124> ;
      skos:inScheme cgi:eventenvironment ;
      skos:narrower eventenv:barrier_lagoon , eventenv:interdistributary_bay , eventenv:intertidal , eventenv:prodelta , eventenv:tidal_channel , eventenv:lagoonal_setting , eventenv:marine_setting ;
      skos:prefLabel "subaqueous setting"@en .

eventenv:subduction_zone_setting
      rdf:type skos:Concept ;
      rdfs:comment "This vocabulary"@en ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      skos:broader eventenv:plate_margin_setting ;
      skos:definition "Tectonic setting at which a tectonic plate, usually oceanic, is moving down into the mantle beneath another overriding plate."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0125> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "subduction zone setting" .

eventenv:subglacial_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader eventenv:glacier_related_setting ;
      skos:definition "Formed or accumulated in or by the bottom parts of a glacier or ice sheet; said of meltwater streams, till, moraine, etc."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0126> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "subglacial setting"@en .

eventenv:submarine_fan_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary"@en ;
      skos:broader eventenv:marine_setting ;
      skos:definition "Large fan-shaped cones of sediment on the ocean floor, generally associated with submarine canyons that provide sediment supply to build the fan.."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0127> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "submarine fan setting"@en .

eventenv:supraglacial_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "North American Geologic-map Data Model Science Language Technical Team, 2004, Sedimentary materials:  science language for their classification, description, and interpretation in digital geologic-map databases, Version 1.0 (12/18/2004): U.S. Geological Survey Open-File Report 2004-1451 appendix C, 595 p., accessed at http://pubs.usgs.gov/of/2004/1451/sltt/appendixC/" ;
      skos:broader eventenv:glacier_related_setting ;
      skos:definition "\"Carried upon, deposited from, or pertaining to the top surface of a glacier or ice sheet; said of meltwater streams, till, drift, etc. \" (Jackson, 1997, p. 639). Dreimanis (1988, p. 39) recommendation that \"supraglacial\" supersede \"superglacial\" is followed."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0128> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "supraglacial setting"@en .

eventenv:supratidal
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader eventenv:subaerial_setting , eventenv:shoreline_settings ;
      skos:definition "Pertaining to the shore area marginal to the littoral zone, just above high-tide level"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0129> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "supratidal setting"@en .

eventenv:swamp_or_marsh_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Neuendorf et al 2005; Soil Science Society of America, 1997." ;
      skos:broader eventenv:wetland_setting ;
      skos:definition "A water-saturated, periodically wet or continually flooded area with the surface not deeply submerged, essentially without the formation of peat. Marshes are characterized by sedges, cattails, rushes, or other aquatic and grasslike vegetation. Swamps are characterized by tree and brush vegetation."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0130> ;
      skos:inScheme cgi:eventenvironment ;
      skos:narrower eventenv:tidal_marsh ;
      skos:prefLabel "swamp or marsh setting"@en ;
      skos:related eventenv:subaqueous_setting .

eventenv:tectonic_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "This vocabulary"@en ;
      skos:definition "Setting defined by relationships to tectonic plates on or in the Earth."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0131> ;
      skos:inScheme cgi:eventenvironment ;
      skos:narrower eventenv:extended_terrane , eventenv:mantle , eventenv:collisional_setting , eventenv:plate_margin_setting , eventenv:foreland_setting , eventenv:intraplate_tectonic_setting , eventenv:crust , eventenv:hinterland_setting , eventenv:hot_spot_setting , eventenv:passive_continental_margin_setting , eventenv:back_arc_setting , eventenv:plate_spreading_center ;
      skos:prefLabel "tectonically defined setting"@en ;
      skos:topConceptOf cgi:eventenvironment .

eventenv:terrestrial_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary"@en ;
      skos:broader eventenv:earth_surface_setting ;
      skos:definition "Setting characterized by absence of direct marine influence. Most of the subaerial settings are also terrestrial, but lacustrine settings, while terrestrial, are not subaerial, so the subaerial settings are not included as subcategories."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0132> ;
      skos:inScheme cgi:eventenvironment ;
      skos:narrower eventenv:playa_setting , eventenv:upper_delta_plain , eventenv:gibber_plain , eventenv:bog , eventenv:river_plain_system_setting , eventenv:lacustrine_setting , eventenv:glacial_outwash_plain_setting , eventenv:sand_plain ;
      skos:prefLabel "terrestrial setting"@en ;
      skos:related eventenv:continental_crust .

eventenv:tidal_channel
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader eventenv:tidal_flat_setting , eventenv:subaqueous_setting ;
      skos:definition "A major channel followed by the tidal currents, extending from offshore into a tidal marsh or a tidal flat."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0133> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "tidal channel setting"@en ;
      skos:related eventenv:marine_setting .

eventenv:tidal_flat_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader eventenv:tidal_setting ;
      skos:definition "An extensive, nearly horizontal, barren tract of land that is alternately covered and uncovered by the tide, and consisting of unconsolidated sediment (mostly mud and sand). It may form the top surface of a deltaic deposit."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0134> ;
      skos:inScheme cgi:eventenvironment ;
      skos:narrower eventenv:tidal_marsh , eventenv:tidal_channel ;
      skos:prefLabel "tidal flat setting"@en ;
      skos:related eventenv:low_energy_shoreline_setting .

eventenv:tidal_marsh
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader eventenv:tidal_flat_setting , eventenv:swamp_or_marsh_setting ;
      skos:definition "A marsh bordering a coast (as in a shallow lagoon or sheltered bay), formed of mud and of the resistant mat of roots of salt-tolerant plants, and regularly inundated during high tides; a marshy tidal flat."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0135> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "tidal marsh setting"@en .

eventenv:tidal_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary"@en ;
      skos:broader eventenv:shoreline_settings ;
      skos:definition "Setting subject to tidal processes"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0136> ;
      skos:inScheme cgi:eventenvironment ;
      skos:narrower eventenv:intertidal , eventenv:lower_delta_plain , eventenv:tidal_flat_setting ;
      skos:prefLabel "tidal setting"@en .

eventenv:transform_plate_boundary_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "This vocabulary"@en ;
      skos:broader eventenv:plate_margin_setting ;
      skos:definition "Plate boundary at which the adjacent plates are moving laterally relative to each other."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0137> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "transform plate boundary setting"@en .

eventenv:transitional_crust
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader eventenv:crust ;
      skos:definition "Crust formed in the transition zone between continental and oceanic crust, during the history of continental rifting that culminates in the formation of a new ocean."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0138> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "transitional crustal setting"@en .

eventenv:ultra_high_pressure_crustal_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary"@en ;
      skos:broader eventenv:earth_interior_setting ;
      skos:definition "Setting characterized by pressures characteristic of upper mantle, but indicated by mineral assemblage in crustal composition rocks."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0139> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "ultra high pressure crustal setting"@en .

eventenv:upper_bathyal
      rdf:type skos:Concept ;
      dc:source "Berggren, W.A. & Miller, K.G., 1989. Cenozoic bathyal and abyssal calcareous benthic foraminiferal zonation. Micropalaeontology 35, 308-320" ;
      skos:broader eventenv:bathyal ;
      skos:definition "The ocean environment at water depths between 200 and 600 metres"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0140> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "upper bathyal setting"@en .

eventenv:upper_continental_crustal_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary"@en ;
      skos:broader eventenv:continental_crust ;
      skos:definition "Continental crustal setting dominated by non metamorphosed to low greenschist facies metamorphic rocks, and brittle deformation."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0141> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "upper continental crustal setting"@en .

eventenv:upper_delta_plain
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Reading, H.G. & Collinson, J.D., 1996. Clastic coasts. 154-231 in Reading, H.G. (ed.), Sedimentary Environments: Processses, Facies and Stratigraphy, third edition. Blackwell Science, Oxford" ;
      skos:broader eventenv:terrestrial_setting , eventenv:delta_plain ;
      skos:definition "The part of a delta plain essentially unaffected by basinal processes. They do not differ substantially from alluvial environments except that areas of swamp, marsh and lakes are usually more widespread and channels may bifurcate downstream"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0142> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "upper delta plain setting"@en .

eventenv:upper_mantle
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Neuendorf et al., 2005" ;
      skos:broader eventenv:mantle ;
      skos:definition "That part of the mantle which lies above a depth of about 660 km and has a density of 3.4 g/cm3 to 4.0 g/cm3 with increasing depth. Similarly, P-wave velocity increases from about 8 to 11 km/sec with depth and S wave velocity increases from about 4.5 to 6 km/sec with depth. It is presumed to be peridotitic in composition. It includes the subcrustal lithosphere the asthenosphere and the transition zone;"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0143> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "upper mantle setting"@en .

eventenv:upper_oceanic_crustal_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary"@en ;
      skos:broader eventenv:oceanic_crust ;
      skos:definition "Oceanic crustal setting dominated by extrusive rocks, abyssal oceanic sediment, with increasing mafic intrusive rock in lower part."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0144> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "upper oceanic crustal setting"@en .

eventenv:volcanic_arc_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "1-GE WP3, Neuendorf et al.  (2005); p. 710"@en ;
      skos:broader eventenv:plate_margin_setting ;
      skos:definition "A generally curvillinear belt of volcanoes above a subduction zone."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0145> ;
      skos:inScheme cgi:eventenvironment ;
      skos:prefLabel "volcanic arc setting"@en .

eventenv:wetland_setting
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary"@en ;
      skos:broader eventenv:earth_surface_setting ;
      skos:definition "Setting characterized by gentle surface slope, and at least intermittent presence of standing water, which may be fresh, brackish, or saline. Wetland may be terrestrial setting or shoreline setting."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/eventenvironment/0146> ;
      skos:inScheme cgi:eventenvironment ;
      skos:narrower eventenv:swamp_or_marsh_setting , eventenv:bog ;
      skos:prefLabel "wetland setting"@en ;
      skos:related eventenv:terrestrial_setting , eventenv:shoreline_settings , eventenv:subaqueous_setting .

cgi:eventenvironment
      rdf:type owl:Ontology , skos:ConceptScheme ;
      rdfs:comment "2010-04-14 SMR Version 201001.1. Edit preferred labels (English) for consistent usage of 'setting' for all concept labels. "@en , "Throughout this vocabulary, the term 'setting' is used in the English-language perferred labels to indicate that the terms denote environments defined based on geoscientific criteria"@en ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:format "application/rdf+xml" ;
      dc:identifier "http://resource.geosciml.org/classifierscheme/cgi/201211/eventenvironment" ;
      dc:language "en" ;
      dc:publisher "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:title "CGI Event Environment Categories"@en ;
      owl:imports <http://www.w3.org/2004/02/skos/core> , <http://rdfs.org/ns/void> ;
      owl:versionInfo "201211" ;
      skos:definition "This file contains the CGI Event Environment vocabulary. Vocabulary for populating event environment properties in GeoSciML documents. The physical setting within which a GeologicEvent takes place. GeologicEnvironment is construed broadly to include physical settings on the Earth surface specified by climate, tectonics, physiography or geography, and settings in the Earth’s interior specified by pressure, temperature, chemical environment, or tectonics. Environments are places."@en ;
      skos:hasTopConcept eventenv:earth_surface_setting , eventenv:tectonic_setting , eventenv:extra_terrestrial_origin , eventenv:earth_interior_setting ;
      skos:historyNote "2010-02-11 Add narrower relationship links by adding skos:narrower link in Protege, defining skos:narrower as inverse of skos:broader, calculating with Pellet reasoner to add inferred axioms, and using 'file/Export inferred axioms as ontology' in Protege."@en , "2012-11-24 SMR Update to 201211 version; add collection entity, check all pref labels are lower case, remove owl:NamedIndividual and Owl:Thing rdf:types."@en , "2012-02-27 SMR add skos:exactMatch triples to map URIs for concepts in this vocabulary to number-token URIs in 201012 version of same concepts."@en , "2009-12-07 SMR Update metadata properties for version, creator, title, and format.  Change skos:HistoryNote to dc:source for information on origin of terms and definitions."@en , "2012-02-07 SMR update URI to replace numeric final token with English-language string as in original URN scheme."@en , "2010 11 27 SMR change identifiers from URN to http URI following CGI URI scheme"@en ;
      skos:prefLabel "CGI Event Environment Categories"@en .

