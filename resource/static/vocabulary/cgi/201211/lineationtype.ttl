lintyp2:augen_lineation
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary" ;
      skos:broader lintyp2:tectonic_lineation , lintyp2:particle_shape_lineation ;
      skos:definition "A shape lineation defined by elongate augen."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lineationtype/0001> ;
      skos:inScheme cgi:lineationtype ;
      skos:prefLabel "augen lineation"@en .

lintyp2:bedding_cleavage_intersection_lineation
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary" ;
      skos:broader lintyp2:tectonic_lineation , lintyp2:intersection_lineation ;
      skos:definition "Lineation defined by the intersection of bedding surfaces and cleavage surfaces."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lineationtype/0002> ;
      skos:inScheme cgi:lineationtype ;
      skos:prefLabel "bedding cleavage intersection lineation"@en .

lintyp2:biotite_mineral_lineation
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary" ;
      skos:broader lintyp2:mineral_lineation ;
      skos:definition "Mineral lineation defined by aligned elongate biotite aggregates in a foliation surface. Biotite does not have an elongate (prismatic, acicular) mineral habit, so production of lineation must reflect elongate aggregates of biotite crystals."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lineationtype/0003> ;
      skos:inScheme cgi:lineationtype ;
      skos:prefLabel "biotite mineral lineation"@en .

lintyp2:cleavage_intersection_lineation
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary" ;
      skos:broader lintyp2:tectonic_lineation , lintyp2:intersection_lineation ;
      skos:definition "Lineation defined by the intersection of two cleavages."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lineationtype/0004> ;
      skos:inScheme cgi:lineationtype ;
      skos:prefLabel "cleavage intersection lineation"@en .

lintyp2:crenulation_lineation
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary" ;
      skos:broader lintyp2:tectonic_lineation , lintyp2:lineation ;
      skos:definition "Lineation defined by small-scale wrinkles, typically of phylosillicate minerals, on another surface, typically a cleavage or schistosity. Use instead of intersection lineation if the wrinkles are the measured feature and the intersecting planar feature is not apparent."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lineationtype/0005> ;
      skos:inScheme cgi:lineationtype ;
      skos:prefLabel "crenulation lineation"@en .

lintyp2:flow_lineation
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Geoscience Victoria, Australia" ;
      skos:broader lintyp2:lineation ;
      skos:definition "A linear alignment of fabric elements, especially elongate or rod-shaped minerals, interpreted to have been aligned by flow of magma, lava, pyroclastic material, or diapiric flow of sedimentary rock."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lineationtype/0006> ;
      skos:inScheme cgi:lineationtype ;
      skos:prefLabel "flow lineation"@en .

lintyp2:hornblende_mineral_lineation
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary" ;
      skos:broader lintyp2:mineral_lineation ;
      skos:definition "Mineral lineation defined by aligned hornblende crystals."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lineationtype/0007> ;
      skos:inScheme cgi:lineationtype ;
      skos:prefLabel "hornblende mineral lineation"@en .

lintyp2:igneous_lineation
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary" ;
      skos:broader lintyp2:lineation ;
      skos:definition "Lineation defined by alignment of elongate fabric elements formed in lava or magma before complete crystallization. Some typical fabric elements are elongate phenocrysts and vesicles."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lineationtype/0008> ;
      skos:inScheme cgi:lineationtype ;
      skos:narrower lintyp2:igneous_mineral_lineation , lintyp2:igneous_vesicle_lineation , lintyp2:igneous_particle_shape_lineation ;
      skos:prefLabel "igneous lineation"@en .

lintyp2:igneous_mineral_lineation
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary" ;
      skos:broader lintyp2:mineral_lineation , lintyp2:igneous_lineation ;
      skos:definition "Lineation in igneous rock defined by aligned crystals where there is evidence that the crystal alignment is related to process occurring before the rock was solidified."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lineationtype/0009> ;
      skos:inScheme cgi:lineationtype ;
      skos:prefLabel "igneous mineral lineation"@en .

lintyp2:igneous_particle_shape_lineation
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary" ;
      skos:broader lintyp2:particle_shape_lineation , lintyp2:igneous_lineation ;
      skos:definition "Lineation in an igneous rock, usually welded tuff, defined by elongate objects in the rock (usually pumice pads, also elongate crystals). Descriptive--elogation may be due to deformation or to rotation/alignment of originally elongate (prolate ellipsoid) objects in the rock."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lineationtype/0010> ;
      skos:inScheme cgi:lineationtype ;
      skos:prefLabel "igneous particle shape lineation"@en .

lintyp2:igneous_vesicle_lineation
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary" ;
      skos:broader lintyp2:igneous_lineation ;
      skos:definition "lineation defined in a volcanic rock by aligned elongation direction of vesicles."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lineationtype/0011> ;
      skos:inScheme cgi:lineationtype ;
      skos:prefLabel "igneous vesicle lineation"@en .

lintyp2:intersection_lineation
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Neuendorf et al 2005; this vocabulary" ;
      skos:broader lintyp2:lineation ;
      skos:definition "A lineation produced by the intersection of two planar fabrics."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lineationtype/0012> ;
      skos:inScheme cgi:lineationtype ;
      skos:narrower lintyp2:cleavage_intersection_lineation , lintyp2:bedding_cleavage_intersection_lineation ;
      skos:prefLabel "intersection lineation"@en .

lintyp2:lineation
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Neuendorf et al 2005; this vocabulary" ;
      skos:definition "Nongenetic term for a penetrative linear structure."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lineationtype/0013> ;
      skos:inScheme cgi:lineationtype ;
      skos:narrower lintyp2:particle_shape_lineation , lintyp2:rodding_lineation , lintyp2:slickenline , lintyp2:tectonic_lineation , lintyp2:igneous_lineation , lintyp2:flow_lineation , lintyp2:crenulation_lineation , lintyp2:pencil_lineation , lintyp2:intersection_lineation ;
      skos:prefLabel "lineation"@en ;
      skos:topConceptOf cgi:lineationtype .

lintyp2:mineral_lineation
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Neuendorf et al 2005; this vocabulary" ;
      skos:broader lintyp2:particle_shape_lineation ;
      skos:definition "Lineation defined by alignment of elongate crystals that have a prismatic crystal habit or of monomineralic grain aggregates."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lineationtype/0014> ;
      skos:inScheme cgi:lineationtype ;
      skos:narrower lintyp2:biotite_mineral_lineation , lintyp2:sillimanite_mineral_lineation , lintyp2:hornblende_mineral_lineation , lintyp2:igneous_mineral_lineation ;
      skos:prefLabel "mineral lineation"@en .

lintyp2:particle_shape_lineation
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary" ;
      skos:broader lintyp2:lineation ;
      skos:definition "Lineation defined by alignment of long axes of prolate-shaped particles such as clasts, fossils, crystals, or phenocrysts. Particles may be polycrystalline aggregates formed by deformation of pre-existing particles. General category does not specify genesis of particles or their alignment."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lineationtype/0015> ;
      skos:inScheme cgi:lineationtype ;
      skos:narrower lintyp2:augen_lineation , lintyp2:mineral_lineation , lintyp2:quartz_fibre_lineation , lintyp2:stretching_lineation , lintyp2:igneous_particle_shape_lineation ;
      skos:prefLabel "particle shape lineation"@en .

lintyp2:pencil_lineation
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "based on Neuendorf et al 2005" ;
      skos:broader lintyp2:lineation ;
      skos:definition "Lineation defined by tendency of rock to break into long, slender pieces resembling pencils. Typically produced by intesection of bedding and cleavage or intersection of two cleavages. Fabric is generally only observed in weakly metamorphosed rocks."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lineationtype/0016> ;
      skos:inScheme cgi:lineationtype ;
      skos:prefLabel "pencil lineation"@en .

lintyp2:quartz_fibre_lineation
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary" ;
      skos:broader lintyp2:particle_shape_lineation ;
      skos:definition "Aligned quartz fibres that define a measurable lineation"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lineationtype/0017> ;
      skos:inScheme cgi:lineationtype ;
      skos:prefLabel "quartz fibre lineation"@en .

lintyp2:rodding_lineation
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "Jackson 1997" ;
      skos:broader lintyp2:tectonic_lineation , lintyp2:lineation ;
      skos:definition "Linear structure in metamorphic rocks in which the stronger parts have been shaped into parallel rods (eg. vein quartz)"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lineationtype/0018> ;
      skos:inScheme cgi:lineationtype ;
      skos:prefLabel "rodding lineation"@en .

lintyp2:sillimanite_mineral_lineation
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary" ;
      skos:broader lintyp2:mineral_lineation ;
      skos:definition "Mineral lineation defined by aligned sillimanite crystals."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lineationtype/0019> ;
      skos:inScheme cgi:lineationtype ;
      skos:prefLabel "sillimanite mineral lineation"@en .

lintyp2:slickenline
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "based on Neuendorf et al 2005" ;
      skos:broader lintyp2:tectonic_lineation , lintyp2:lineation ;
      skos:definition "Lineation on a brittle slip surface defined by grooves, ridges, or striations"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lineationtype/0020> ;
      skos:inScheme cgi:lineationtype ;
      skos:prefLabel "slickenline"@en .

lintyp2:stretching_lineation
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary" ;
      skos:broader lintyp2:tectonic_lineation , lintyp2:particle_shape_lineation ;
      skos:definition "Lineation defined by elongate (prolate) fabric elements in which the alignment of the fabric elements, and the formation of prolate fabric elements is interpreted to be the product of crystal plastic deformation processes. Associated with mylontic foliation."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lineationtype/0021> ;
      skos:inScheme cgi:lineationtype ;
      skos:prefLabel "stretching lineation"@en .

lintyp2:tectonic_lineation
      rdf:type skos:Concept ;
      dc:creator "Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)"@en ;
      dc:source "this vocabulary" ;
      skos:broader lintyp2:lineation ;
      skos:definition "Penetrative linear structure in a rock body defined by fabric elements that are the product of tectonic processes. Linear is used in the sense that the orientation of the structure can be represented as a line in some sufficiently small neighborhood of any point in the material containig the fabric. Penetrative denotes that the fabric elements defining the structure are repeated at distances so small, compared with the scale of the wholeâ€¦that they can be considered to pervade it uniformly and be present at every point (paraphrase of Turner and Weiss, 1963, p. 21)."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lineationtype/0022> ;
      skos:inScheme cgi:lineationtype ;
      skos:narrower lintyp2:crenulation_lineation , lintyp2:augen_lineation , lintyp2:cleavage_intersection_lineation , lintyp2:slickenline , lintyp2:stretching_lineation , lintyp2:rodding_lineation , lintyp2:bedding_cleavage_intersection_lineation ;
      skos:prefLabel "tectonic lineation"@en .

