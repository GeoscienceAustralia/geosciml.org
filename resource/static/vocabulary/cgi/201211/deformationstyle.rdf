<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/deformationstyle/">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Collection"/>
	<rdfs:label xml:lang="en">fault deformation style concepts</rdfs:label>
	<skos:definition xml:lang="en">This resource defines the concept space for fault deformation styles by extension; it includes all concepts in this conceptScheme, as well as concepts in previous versions of the scheme.</skos:definition>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/deformationstyle/mixed_brittle_ductile"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/deformationstyle/brittle"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/deformationstyle/ductile"/>
	<skos:prefLabel xml:lang="en">CGI Fault Deformation Style</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/deformationstyle/brittle">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">this vocabulary</dc:source>
	<skos:definition xml:lang="en">Deformation in fault zone has been accommodated primarily through fracturing and loss of continuity between adjacent rock bodies.</skos:definition>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/deformationstyle"/>
	<skos:prefLabel xml:lang="en">brittle deformation</skos:prefLabel>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/deformationstyle"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/deformationstyle/ductile">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">this vocabulary</dc:source>
	<skos:definition xml:lang="en">Displacement across shear displacement structure largely accommodated by plastic deformation of the rock body without loss of macroscopic continuity.</skos:definition>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/deformationstyle"/>
	<skos:prefLabel xml:lang="en">ductile deformation style</skos:prefLabel>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/deformationstyle"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/deformationstyle/mixed_brittle_ductile">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">this vocabulary</dc:source>
	<skos:definition xml:lang="en">deformation in fault zone has been accommodated by both fracturing and plastic shape change in rock bodies.</skos:definition>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/deformationstyle"/>
	<skos:prefLabel xml:lang="en">mixed brittle and ductile</skos:prefLabel>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/deformationstyle"/>
</rdf:Description>

