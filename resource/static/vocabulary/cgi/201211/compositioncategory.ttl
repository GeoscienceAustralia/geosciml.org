@prefix cgi:     <http://resource.geosciml.org/classifierscheme/cgi/201211/> .
@prefix compCat:  <http://resource.geosciml.org/classifier/cgi/compositioncategory/> .
@prefix compCat2:  <http://resource.geosciml.org/classifierscheme/cgi/201211/compositioncategory/> .
@prefix dc:      <http://purl.org/dc/elements/1.1/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix owl2xml:  <http://www.w3.org/2006/12/owl2-xml#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix void:    <http://rdfs.org/ns/void#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

compCat:
      rdf:type skos:Collection ;
      rdfs:label "composition category concepts"@en ;
      skos:member compCat:quartzo_feldspathic , compCat:barium_bearing_carbonate_chemistry , compCat:impure_calcsilicate_or_carbonate , compCat:bioclastic , compCat:sulfide_bearing , compCat:mono_mineralic , compCat:carbonate , compCat:iron_bearing_carbonate_chemistry , compCat:calcareous , compCat:sulfate_chemistry , compCat:very_low_silica_chemistry , compCat:iron_rich_carbonate_chemistry , compCat:quartzose_detrital_mode , compCat:carbonaceous_chemistry , compCat:feldspathic_detrital_mode , compCat:arkosic_detrital_mode , compCat:feldspathic , compCat:volcanic_lithic , compCat:carbonate_bearing , compCat:lithic_detrital_mode , compCat:monomictic , compCat:argillic , compCat:calcsilicate , compCat:sub_feldspathic_detrital_mode , compCat:quartz_feldspar_pelitic , compCat:dolomite_monomineralic_carbonate , compCat:halide_chemistry , compCat:calcium_rich_carbonate_chemistry , compCat:chloride_chemistry , compCat:phosphate_chemistry , compCat:nitrate_chemistry , compCat:monomineralic_carbonate , compCat:peraluminous , compCat:silicate_chemistry , compCat:micaceous , compCat:carbonate_chemistry , compCat:metallic_oxide_bearing , compCat:sulfide , compCat:petroliferous , compCat:ferromagnesian , compCat:lithic_rich_detrital_mode , compCat:polymictic , compCat:low_silica_chemistry , compCat:peralkaline , compCat:asphaltic , compCat:quartzose , compCat:metaluminous , compCat:lithic_arkosic_detrital_mode , compCat:other_composition , compCat:lithic , compCat:calcsilicate_bearing , compCat:calcareous_monomeralic_carbonate , compCat:fluoride_chemistry , compCat:ultramafic , compCat:phosphatic , compCat:sulfide_chemistry , compCat:ferruginous , compCat:intraclastic , compCat:subaluminous , compCat:oxide , compCat:intermediate_silica_chemistry , compCat:carbonaceous , compCat:chloritic , compCat:high_silica_chemistry , compCat:dolomitic , compCat:semi_pelitic , compCat:pelitic , compCat:mafic , compCat:magnesium_rich_carbonate_chemistry , compCat:composition_not_specified , compCat:sulfate , compCat:feldspathic_lithic_detrital_mode , compCat:sub_lithic_detrital_mode , compCat:silicic , compCat:sulfosalt_chemistry ;
      skos:prefLabel "CGI Composition Category"@en .

compCat:argillic
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:definition "Greater than accessory amount (greater than 5 percent by volume) of clay is present"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0001> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "argillic"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:arkosic_detrital_mode
      rdf:type skos:Concept ;
      dc:source "Folk1968"@en ;
      skos:broader compCat:feldspathic_detrital_mode ;
      skos:definition "Sandstone petrographic composition term indicating that detrital mode for clastic rock (Q-F-L) includes less than 75 percent quartz and the feldspar to lithic ratio is greater than 3 to 1. This corresponds to the arkose field of Folk (1968). For clastic rocks with little or no matrix and cement, rocks in this detrital mode composition category will also have quartz-feldspathic mineralogical composition."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0002> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "arkosic detrital mode"@en .

compCat:asphaltic
      rdf:type skos:Concept ;
      dc:source "Jackson 1997"@en ;
      skos:broader compCat:carbonaceous ;
      skos:definition "Material contains a significant amount of asphalt, which is a 'dark brown to black viscous liquid or low-melting solid bitumen that consists almost entirely of carbon and hydrogen...[and] formed in oil-bearing rocks by the evaporation of volatiles...' (Jackson, 1997, p. 39)."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0003> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "asphaltic"@en .

compCat:barium_bearing_carbonate_chemistry
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader compCat:carbonate_chemistry ;
      skos:definition "CO3 is the predominant anion, and greater than 5 mole percent of cations are Ba."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0004> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "barium bearing carbonate chemistry"@en .

compCat:bioclastic
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:definition "Carbonate sedimentary material contains significant broken skeletal material"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0005> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "bioclastic"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:calcareous
      rdf:type skos:Concept ;
      dc:source "NADM SLTTm 2004"@en ;
      skos:broader compCat:carbonate_bearing ;
      skos:definition "Calcium carbonate minerals are present, carbonate minerals form less than 50 percent of rock, material contains sufficient CaCO3 to react forming bubbles when hydrochloric acid is applied."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0006> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "calcareous"@en .

compCat:calcareous_monomeralic_carbonate
      rdf:type skos:Concept ;
      dc:source "NADM SLTTm 2004"@en ;
      skos:broader compCat:monomineralic_carbonate ;
      skos:definition "Carbonate minerals form greater than 75 percent of rock, and calcium carbonate minerals forms greater than 75 percent of carbonate minerals."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0007> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "calcareous monomeralic carbonate"@en .

compCat:calcium_rich_carbonate_chemistry
      rdf:type skos:Concept ;
      dc:source "Struik etal 2002; This vocabulary"@en ;
      skos:broader compCat:carbonate_chemistry ;
      skos:definition "CO3 is the predominante anion, and the Ca is the most abundant (mole percent) cation."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0008> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "calcium rich carbonate chemistry"@en .

compCat:calcsilicate
      rdf:type skos:Concept ;
      dc:source "NADM SLTTm 2004"@en ;
      skos:broader compCat:impure_calcsilicate_or_carbonate ;
      skos:definition "Material consists of greater than or equal to 50 percent calcsilicate or carbonate minerals and carbonate minerals less than or equal to calcsilicate minerals in mineral mode."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0009> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "calcsilicate"@en .

compCat:calcsilicate_bearing
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:definition "Calcsilicate minerals of any sort are present, but less than 50 percent of rock"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0010> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "calcsilicate bearing"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:carbonaceous
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:definition "Material containing significant carbon or hydrocarbon mineral phases, including graphite, coal, oil, gas, lignite, peat."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0011> ;
      skos:inScheme cgi:compositioncategory ;
      skos:narrower compCat:asphaltic ;
      skos:prefLabel "carbonaceous"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:carbonaceous_chemistry
      rdf:type skos:Concept ;
      dc:source "Struik2002; SLTTs2004; This vocabulary"@en ;
      skos:definition "Containing greater than 5 percent reduced carbon in compounds that are typically the diagenetic products of dead organisms (e.g. coal, bitumen and petroleum). 5 percent value is chosen for consistency with SLTTs usage of term carbonaceous."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0012> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "carbonaceous chemistry"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:carbonate
      rdf:type skos:Concept ;
      dc:source "NADM SLTTm 2004"@en ;
      skos:broader compCat:impure_calcsilicate_or_carbonate ;
      skos:definition "Material consists of greater than 50 percent calcsilicate or carbonate minerals and carbonate minerals greater than calcsilicate minerals in mineral mode. Metacarbonate of NADMSC SLTTm (2004) terminology, adopted here for non-genetic use."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0013> ;
      skos:inScheme cgi:compositioncategory ;
      skos:narrower compCat:monomineralic_carbonate ;
      skos:prefLabel "carbonate"@en .

compCat:carbonate_bearing
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:definition "Carbonate minerals of any sort are present, but less than 50 percent of rock."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0014> ;
      skos:inScheme cgi:compositioncategory ;
      skos:narrower compCat:dolomitic , compCat:calcareous ;
      skos:prefLabel "carbonate bearing"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:carbonate_chemistry
      rdf:type skos:Concept ;
      dc:source "Struik etal 2002; This vocabulary"@en ;
      skos:definition "The carbonate (CO3) anion is the predominant anion (by mole percent) in chemical analysis. Sub-categories may be defined by dominant cation. Material in this chemical class overlaps with the monomineralic carbonate class, and probably overlaps with the Carbonate class in the mineralogical composition classification."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0015> ;
      skos:inScheme cgi:compositioncategory ;
      skos:narrower compCat:barium_bearing_carbonate_chemistry , compCat:calcium_rich_carbonate_chemistry , compCat:magnesium_rich_carbonate_chemistry , compCat:iron_bearing_carbonate_chemistry ;
      skos:prefLabel "carbonate chemistry"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:chloride_chemistry
      rdf:type skos:Concept ;
      dc:source "Struik etal 2002; This vocabulary"@en ;
      skos:broader compCat:halide_chemistry ;
      skos:definition "Chloride is the most abundant (mole percent) anion"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0016> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "chloride chemistry"@en .

compCat:chloritic
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:definition "Greater than accessory amount (greater than 5 percent by volume) of chlorite group minerals are present"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0017> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "chloritic"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:composition_not_specified
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:definition "composition not specified. Use in normative descriptions where any composition property is allowed."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0018> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "composition not specified"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:dolomite_monomineralic_carbonate
      rdf:type skos:Concept ;
      dc:source "NADM SLTTm 2004"@en ;
      skos:broader compCat:monomineralic_carbonate ;
      skos:definition "Carbonate minerals form greater than 75 percent of rock, and dolomite forms greater than 75 percent of carbonate minerals."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0019> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "dolomite monomineralic carbonate"@en .

compCat:dolomitic
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader compCat:carbonate_bearing ;
      skos:definition "Dolomite is present, carbonate minerals form less than 50 percent of rock."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0020> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "dolomitic"@en .

compCat:feldspathic
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:definition "Sedimentary material contains significant feldspar mineral content"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0021> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "feldspathic"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:feldspathic_detrital_mode
      rdf:type skos:Concept ;
      dc:source "Dott1964; Folk1968"@en ;
      skos:definition "General sandstone petrography term for a sandstone in which the ratio of feldspar to lithic fragments is greater than 1:1 and less than 75 percent quartz is present in the detrital mode. Subsumes lithic arkosic and arkosic sandstone petrographic composition classes. Usage of modifier feldspathic is consistent with petrographic subdivisions of Dott (1964) and Williams, Turner and Gilbert (1982)"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0022> ;
      skos:inScheme cgi:compositioncategory ;
      skos:narrower compCat:arkosic_detrital_mode , compCat:lithic_arkosic_detrital_mode ;
      skos:prefLabel "feldspathic detrital mode"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:feldspathic_lithic_detrital_mode
      rdf:type skos:Concept ;
      dc:source "Folk1968"@en ;
      skos:definition "Sandstone petrographic composition term indicating that detrital mode for clastic rock (Q-F-L) includes less than 75 percent quartz and the feldspar to lithic ratio is between 1 to 1 and 1 to 3. This corresponds to the feldspathic litharenite field of Folk (1968). Rocks in this class are likely to be quartzo-feldspathic in terms of mineralogical composition terms in this vocablary, but because the mineralogical composition of the lithic fraction is not constrainged, this correlation is not necessary."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0023> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "feldspathic lithic detrital mode"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:ferromagnesian
      rdf:type skos:Concept ;
      dc:source "NADM SLTTm 2004"@en ;
      skos:definition "Material consists of greater than 40 percent dark ferromagnesian silicate minerals. Standard term defined by Bates and Jackson (1987) to mean 'containing iron and magnesium'. Subsumes mafic and ultramafic."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0024> ;
      skos:inScheme cgi:compositioncategory ;
      skos:narrower compCat:mafic , compCat:ultramafic ;
      skos:prefLabel "ferromagnesian"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:ferruginous
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:definition "Greater than accessory amount (greater than 5 percent by volume) of non-silicate iron minerals (hematite, magnetite, siderite...) are present. Rocks containing abundant iron-rich silicate minerals would use the 'Ferromagnesian' terms."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0025> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "ferruginous"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:fluoride_chemistry
      rdf:type skos:Concept ;
      dc:source "Struik etal 2002; This vocabulary"@en ;
      skos:broader compCat:halide_chemistry ;
      skos:definition "Fluorine is the most abundant (mole percent) anion"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0026> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "fluoride chemistry"@en .

compCat:halide_chemistry
      rdf:type skos:Concept ;
      dc:source "Struik etal 2002; This vocabulary"@en ;
      skos:definition "One or more of halide group elements (F, Cl, Br, I) are most abundant (mole percent) anion"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0027> ;
      skos:inScheme cgi:compositioncategory ;
      skos:narrower compCat:fluoride_chemistry , compCat:nitrate_chemistry , compCat:chloride_chemistry ;
      skos:prefLabel "halide chemistry"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:high_silica_chemistry
      rdf:type skos:Concept ;
      dc:source "Struik2002; SLTTs2004; This vocabulary"@en ;
      skos:broader compCat:silicate_chemistry ;
      skos:definition "Containing 65 percent or more SiO2 by weight. Syn acid."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0028> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "high silica chemistry"@en .

compCat:impure_calcsilicate_or_carbonate
      rdf:type skos:Concept ;
      dc:source "NADM SLTTm 2004"@en ;
      skos:definition "Material consists of greater than 50 percent calcsilicate or carbonate minerals and relative proportion of calcsilicate and carbonate minerals is unknown or not specified."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0029> ;
      skos:inScheme cgi:compositioncategory ;
      skos:narrower compCat:carbonate , compCat:calcsilicate ;
      skos:prefLabel "impure calcsilicate or carbonate"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:intermediate_silica_chemistry
      rdf:type skos:Concept ;
      dc:source "Struik2002; SLTTs2004; This vocabulary"@en ;
      skos:broader compCat:silicate_chemistry ;
      skos:definition "Containing SiO2 in the range 52-65 percent, sometimes described as \"intermediate\" rocks."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0030> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "Intermediate silica chemistry"@en .

compCat:intraclastic
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:definition "Carbonate sedimentary material contains signficant intraclast content"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0031> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "intraclastic"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:iron_bearing_carbonate_chemistry
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader compCat:carbonate_chemistry ;
      skos:definition "CO3 is the predominant anion, and iron is greater than 5 mole percent of the cations (includes Fe, Mg, Mn, Ba, Sr, Ca...)."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0032> ;
      skos:inScheme cgi:compositioncategory ;
      skos:narrower compCat:iron_rich_carbonate_chemistry ;
      skos:prefLabel "iron bearing carbonate chemistry"@en .

compCat:iron_rich_carbonate_chemistry
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader compCat:iron_bearing_carbonate_chemistry ;
      skos:definition "CO3 is the predominant anion, and iron is the most abundant (mole percent) cation"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0033> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "iron rich carbonate chemistry"@en .

compCat:lithic
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:definition "Sedimentary material contains significant detrital rock fragments derived by erosion from older, pre-existing rock materials"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0034> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "lithic"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:lithic_arkosic_detrital_mode
      rdf:type skos:Concept ;
      dc:source "Folk1968"@en ;
      skos:broader compCat:feldspathic_detrital_mode ;
      skos:definition "Sandstone petrographic composition term indicating that detrital mode for clastic rock (Q-F-L) includes less than 75 percent quartz and the ratio of lithic fragments to feldspar is between 1:1 and 1:3. This corresponds to the lithic arkose field of Folk (1968); the 75 percent quartz boundary is chosen to be consistent with quartzsose composition as defined in the mineralogic compostion terms vocabulary. Because of the abundance of lithic fragments for which the mineralogic composition is not specified, and the presence of undiscernible matrix and cement that are not incuded in the detrital mode, corresondence with the mineralogical composition terms is very approximate."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0035> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "lithic arkosic detrital mode"@en .

compCat:lithic_detrital_mode
      rdf:type skos:Concept ;
      dc:source "Folk1968"@en ;
      skos:definition "Sandstone petrographic composition term indicating that detrital mode for clastic rock (Q-F-L) includes less than 75 percent quartz and the lithic to feldspar ratio is greater than 3 to 1. This corresponds to the litharenite field of Folk (1968). Because the mineralogy of the lithic fragments is not constrained, the mineralogical composition is largely independent of the detrital mode composition for this class."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0036> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "lithic detrital mode"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:lithic_rich_detrital_mode
      rdf:type skos:Concept ;
      dc:source "Dott1964; Folk1968"@en ;
      skos:definition "Sandstone petrographic composition term indicating that the ratio of lithic fragments to feldspar is greater than 1:1, and less than 75 percent quartz is present in the detrital mode. Because the mineralogy of the lithic fragments is not constrained, the mineralogical composition is largely independent of the detrital mode composition for this class."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0037> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "lithic rich detrital mode"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:low_silica_chemistry
      rdf:type skos:Concept ;
      dc:source "Struik2002; SLTTs2004; This vocabulary"@en ;
      skos:broader compCat:silicate_chemistry ;
      skos:definition "Containing SiO2 in the range 44-52 percent. Syn basic."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0038> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "low silica chemistry"@en .

compCat:mafic
      rdf:type skos:Concept ;
      dc:source "NADM SLTTm 2004"@en ;
      skos:broader compCat:ferromagnesian ;
      skos:definition "Material consists of greater than or equal to 40 percent and less than 90 percent ferromagnesian silicate minerals."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0039> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "mafic"@en .

compCat:magnesium_rich_carbonate_chemistry
      rdf:type skos:Concept ;
      dc:source "Struik etal 2002; This vocabulary"@en ;
      skos:broader compCat:carbonate_chemistry ;
      skos:definition "CO3 is the predominant anion, and Mg is the most abundant (mole percent) cation."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0040> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "magnesium-rich carbonate chemistry"@en .

compCat:metallic_oxide_bearing
      rdf:type skos:Concept ;
      dc:source "Struik2002; SLTTs2004; This vocabulary"@en ;
      skos:definition "Greater than accessory amounts (greater than 5 percent by volume) of base metal oxide minerals such as magnetite (Fe3O4) or pyrolusite (MnO2) are present."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0041> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "metallic oxide bearing"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:metaluminous
      rdf:type skos:Concept ;
      dc:source "Shand 1947"@en ;
      skos:definition "Chemical composition term used for igneous rocks, denoting that content of aluminum oxide is greater than sodium oxide plus potassium oxide, but aluminum oxide less than sum of Na, K and Ca oxide."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0042> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "metaluminous"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:micaceous
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:definition "Greater than accessory amount (greater than 5 percent by volume) of mica group mineral are present"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0043> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "micaceous"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:mono_mineralic
      rdf:type skos:Concept ;
      dc:source "NADM SLTTm 2004"@en ;
      skos:definition "Greater than 75 percent of the material consists of a single mineral species."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0044> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "monomineralic"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:monomictic
      rdf:type skos:Concept ;
      dc:source "Jackson 1997"@en ;
      skos:definition "Grains or clasts are composed of a single rock or mineral type (Jackson, 1997)"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0045> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "monomictic"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:monomineralic_carbonate
      rdf:type skos:Concept ;
      dc:source "NADM SLTTm 2004"@en ;
      skos:broader compCat:carbonate ;
      skos:definition "Rock in which carbonate minerals form greater than 75 percent of rock."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0046> ;
      skos:inScheme cgi:compositioncategory ;
      skos:narrower compCat:calcareous_monomeralic_carbonate , compCat:dolomite_monomineralic_carbonate ;
      skos:prefLabel "monomineralic carbonate"@en .

compCat:nitrate_chemistry
      rdf:type skos:Concept ;
      dc:source "Struik etal 2002; This vocabulary"@en ;
      skos:broader compCat:halide_chemistry ;
      skos:definition "Nitrate (NO3) is most abundant (mole percent) anion."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0047> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "nitrate chemistry"@en .

compCat:other_composition
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:definition "composition is known but doesnâ€™t' fit in any other category"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0048> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "other composition"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:oxide
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:definition "Material predominantly composed of oxide minerals"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0049> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "oxide"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:pelitic
      rdf:type skos:Concept ;
      dc:source "NADM SLTTm 2004"@en ;
      skos:broader compCat:quartz_feldspar_pelitic ;
      skos:definition "Material for which the sum of modal quartz+feldspar+ mica + aluminous mineral is greater than or equal to 70 percent, and aluminous mineral + mica content is greater than or equal to 40 percent."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0050> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "pelitic"@en .

compCat:peralkaline
      rdf:type skos:Concept ;
      dc:source "Shand 1947"@en ;
      skos:definition "Aluminum oxide (Al2O3) is less than sum of K and Na oxide."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0051> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "peralkaline"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:peraluminous
      rdf:type skos:Concept ;
      dc:source "Shand 1947"@en ;
      skos:definition "Aluminum oxide (Al2O3) greater than sum of Na, K and Ca oxide."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0052> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "peraluminous"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:petroliferous
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:definition "Crude oil or natural gas is present, generally in trace amounts, commonly detected by smell"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0053> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "petroliferous"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:phosphate_chemistry
      rdf:type skos:Concept ;
      dc:source "Struik etal 2002; This vocabulary"@en ;
      skos:definition "Phosphate (PO4) is the most abundant (mole percent) anion."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0054> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "phosphate chemistry"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:phosphatic
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:definition "Greater than accessory amount (greater than 5 percent by volume) of phosphate minerals are present"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0055> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "phosphatic"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:polymictic
      rdf:type skos:Concept ;
      dc:source "Jackson, 1997, p. 501"@en ;
      skos:definition "Grains or clasts are composed of many mineral or rock types"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0056> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "polymictic"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:quartz_feldspar_pelitic
      rdf:type skos:Concept ;
      dc:source "NADM SLTTm 2004"@en ;
      skos:definition "Material for which the sum of modal quartz+feldspar+ mica + aluminous mineral is greater than or equal to 70 percent."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0057> ;
      skos:inScheme cgi:compositioncategory ;
      skos:narrower compCat:pelitic , compCat:semi_pelitic , compCat:quartzo_feldspathic ;
      skos:prefLabel "quartz feldspar pelitic"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:quartzo_feldspathic
      rdf:type skos:Concept ;
      dc:source "NADM SLTTm 2004"@en ;
      skos:broader compCat:quartz_feldspar_pelitic ;
      skos:definition "Material for which the sum of modal quartz+feldspar+ mica + aluminous mineral is greater than or equal to 70 percent, and quartz + feldspar (sensu Robertson, 1999) greater than 60 percent."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0058> ;
      skos:inScheme cgi:compositioncategory ;
      skos:narrower compCat:quartzose ;
      skos:prefLabel "quartzo feldspathic"@en .

compCat:quartzose
      rdf:type skos:Concept ;
      dc:source "NADM SLTTm 2004"@en ;
      skos:broader compCat:quartzo_feldspathic ;
      skos:definition "Material consists of greater than or equal to 75 percent quartz."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0059> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "quartzose"@en .

compCat:quartzose_detrital_mode
      rdf:type skos:Concept ;
      dc:source "Folk1968; This vocabulary"@en ;
      skos:definition "Detrital mineral grains in a granular rock consist of greater than 95 percent quartz. This petrographic criteria follows the most restrictive definition of the composition field at the quartz vertex of the sandstone QFL triangle (Folk, 1968). This use of term quartzose is more restrictive that that used in the mineralogical composition term vocabulary, reflecting the interpretive significance of pure quartz sandstones in sedimentary petrology."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0060> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "quartzose detrital mode"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:semi_pelitic
      rdf:type skos:Concept ;
      dc:source "NADM SLTTm 2004"@en ;
      skos:broader compCat:quartz_feldspar_pelitic ;
      skos:definition "Material for which the sum of modal quartz+feldspar+ mica + aluminous mineral is greater than or equal to 70 percent, and quartz+ feldspar less than 60 percent"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0061> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "semi pelitic"@en .

compCat:silicate_chemistry
      rdf:type owl:Thing , skos:Concept ;
      dc:source "Neuendorf et al 2005; Struik 2002; This vocabulary"@en ;
      skos:definition "composed of minerals whose crystal structure contains SiO4 tetrahedra, either isolated or joined through one or more oxygen atoms to form groups, chains, sheets or three-dimensional structures with metallic cations. composition ranges from 20-100 percent SiO2. Typically some Al substitutues for silica. A survey of chemical analyses in the Georock database (query FOR ALL GEOLOGICAL SETTINGS, MATERIAL: WHOLE ROCK (AND VOLCANIC GLASS), PUB. YEARS: 2005-1970), 10/10/2005 state, http://georoc.mpch-mainz.gwdg.de/georoc/Entry.html) discovered that the lowest silica (Si02) content for igneous rock is about 20 percent. This is consistent with the lowest silica content found for a silicate mineral is about 21 percent for a chlorite variety found by scanning the mineral analyses in Deer, Howie, and Zussman (1966)."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0062> ;
      skos:inScheme cgi:compositioncategory ;
      skos:narrower compCat:very_low_silica_chemistry , compCat:high_silica_chemistry , compCat:low_silica_chemistry , compCat:intermediate_silica_chemistry ;
      skos:prefLabel "silicate chemistry"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:silicic
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:definition "Greater than accessory amount (greater than 5 percent by volume) of silica, amorphous, as quartz, or any of the silica polymorphs are present"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0063> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "silicic"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:sub_feldspathic_detrital_mode
      rdf:type skos:Concept ;
      dc:source "Folk1968"@en ;
      skos:definition "Sandstone petrographic composition term indicating that detrital mode for clastic rock (Q-F-L) includes less than 50 percent lithic fragments, between 75 and 95 percent quartz (thus less than 25 percent feldspar). This corresponds to the subarkose field of Folk (1968); the 75 percent quartz boundary is consistent with the quartzsose composition as defined in the mineralogic compostion terms vocabulary, but if more than a few percent 'matrix' or cement is present, this is not necessarily equivalent to the mineralogical composition term."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0064> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "sub feldspathic detrital mode"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:sub_lithic_detrital_mode
      rdf:type skos:Concept ;
      dc:source "Folk1968"@en ;
      skos:definition "Sandstone petrographic composition term indicating that detrital mode for clastic rock (Q-F-L) includes more than 50 percent lithic fragments, between 75 and 95 percent quartz (thus less than 25 percent feldspar). This corresponds to the sublitharenite field of Folk (1968); the 75 percent quartz boundary is consistent with quartzsose composition as defined in the mineralogic compostion terms vocabulary."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0065> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "sub lithic detrital mode"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:subaluminous
      rdf:type skos:Concept ;
      dc:source "Shand 1947"@en ;
      skos:definition "Aluminum oxide within 10 percent of sum of Na and K oxide."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0066> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "subaluminous"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:sulfate
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:definition "Material predominantly composed of sulfate minerals"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0067> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "sulfate"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:sulfate_chemistry
      rdf:type skos:Concept ;
      dc:source "Struik etal 2002; This vocabulary"@en ;
      skos:definition "Sulfate is the most abundant anion"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0068> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "sulfate chemistry"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:sulfide
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:broader compCat:sulfide_bearing ;
      skos:definition "Material predominantly composed of sulfide minerals"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0069> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "sulfide"@en .

compCat:sulfide_bearing
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:definition "Greater than accessory amount (greater than 5 percent by volume) of sulfide mineral are present"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0070> ;
      skos:inScheme cgi:compositioncategory ;
      skos:narrower compCat:sulfide ;
      skos:prefLabel "sulfide bearing"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:sulfide_chemistry
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al 2005; Struik 2002; This vocabulary"@en ;
      skos:definition "Sulfur is the most abundant anion; cations are metals or semimetals."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0071> ;
      skos:inScheme cgi:compositioncategory ;
      skos:narrower compCat:sulfosalt_chemistry ;
      skos:prefLabel "sulfide chemistry"@en ;
      skos:topConceptOf cgi:compositioncategory .

compCat:sulfosalt_chemistry
      rdf:type skos:Concept ;
      dc:source "Neuendorf et al 2005; Struik 2002; This vocabulary"@en ;
      skos:broader compCat:sulfide_chemistry ;
      skos:definition "Sulfide in which a semimetal (As, Sb, Bi) is present in a cation role"@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0072> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "sulfosalt chemistry"@en .

compCat:ultramafic
      rdf:type skos:Concept ;
      dc:source "NADM SLTTm 2004"@en ;
      skos:broader compCat:ferromagnesian ;
      skos:definition "Material consists of greater than 90 percent ferromagnesian silicate minerals.."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0073> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "ultramafic"@en .

compCat:very_low_silica_chemistry
      rdf:type skos:Concept ;
      dc:source "Struik2002; SLTTs2004; This vocabulary"@en ;
      skos:broader compCat:silicate_chemistry ;
      skos:definition "Containing between about 20 and 44 percent SiO2. A survey of chemical analyses in the Georock database (query FOR ALL GEOLOGICAL SETTINGS, MATERIAL: WHOLE ROCK (AND VOLCANIC GLASS), PUB. YEARS: 2005-1970), 10/10/2005 state, http://georoc.mpch-mainz.gwdg. syn ultrabasic."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0074> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "very low silica chemistry"@en .

compCat:volcanic_lithic
      rdf:type skos:Concept ;
      dc:source "This vocabulary"@en ;
      skos:definition "Sedimentary material contains significant broken volcanic rock fragments or vitric fragments, irrespective of their pyroclastic or epiclastic origin."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0075> ;
      skos:inScheme cgi:compositioncategory ;
      skos:prefLabel "volcanic lithic"@en ;
      skos:topConceptOf cgi:compositioncategory .

