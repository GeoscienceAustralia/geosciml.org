<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/consolidationdegree/consolidated">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">This vocabulary</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/consolidation_not_specified"/>
	<skos:definition>Particulate constituents of a compound material adhere to each other strongly enough that the aggregate can be considered a solid material in its own right.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/001"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/consolidationdegree"/>
	<skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/indurated"/>
	<skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/well_consolidated"/>
	<skos:prefLabel>consolidated</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/consolidationdegree/consolidation_not_specified">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">This vocabulary</dc:source>
	<skos:definition>In normative descriptions, indicates that consolidation state is not a determining factor in identification, it may have any value.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/002"/>
	<skos:historyNote xml:lang="en">2012-02-07 SMR update URI to replace numeric final token with English-language string as in original URN scheme.</skos:historyNote>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/consolidationdegree"/>
	<skos:prefLabel xml:lang="en">consolidation not specified</skos:prefLabel>
	<skos:topConceptOf rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/consolidationdegree"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/consolidationdegree/consolidation_variable">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">This vocabulary</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/consolidation_not_specified"/>
	<skos:definition>Consolidation ranges from unconsolidated to indurated on scale of description</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/003"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/consolidationdegree"/>
	<skos:prefLabel>consolidation variable</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/consolidationdegree/incipient_consolidation">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source>NADM SLTTs 2004 after Bowles 1986</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/unconsolidated"/>
	<skos:definition>Shoveled with difficulty; relative density 0.4 - 0.7.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/011"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/consolidationdegree"/>
	<skos:prefLabel>incipient consolidation</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/consolidationdegree/indurated">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source>NADM SLTTs 2004 after Bowles 1987</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/consolidated"/>
	<skos:definition>Requires blasting or heavy equipment to loosen; Relative density 0.9-1.0. Rings to blow of hammer.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/007"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/consolidationdegree"/>
	<skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/slightly_indurated"/>
	<skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/variable_induration"/>
	<skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/moderately_indurated"/>
	<skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/well_indurated"/>
	<skos:prefLabel>indurated</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/consolidationdegree/moderately_indurated">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">This vocabulary</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/indurated"/>
	<skos:definition>Multiple blows with standard rock hammer (less than 1 kg) are required to break rock.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/009"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/consolidationdegree"/>
	<skos:prefLabel>moderately indurated</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/consolidationdegree/slightly_indurated">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">This vocabulary</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/indurated"/>
	<skos:definition>Rock can be broken with single blow from standard rock hammer (less than 1 kg mass).</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/008"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/consolidationdegree"/>
	<skos:prefLabel>slightly indurated</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/consolidationdegree/unconsolidated">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">This vocabulary</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/consolidation_not_specified"/>
	<skos:definition>Particulate constituents of a compound material do not adhere to each other strongly enough that the aggregate can be considered a solid in its own right.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/004"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/consolidationdegree"/>
	<skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/incipient_consolidation"/>
	<skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/unconsolidated_very_loose"/>
	<skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/unconsolidated_loose"/>
	<skos:prefLabel>unconsolidated</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/consolidationdegree/unconsolidated_loose">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source>NADM SLTTs 2004 after Bowles 1985</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/unconsolidated"/>
	<skos:definition>Easily shoveled, can be indented with fingers; Relative density 0.2-0.4.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/013"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/consolidationdegree"/>
	<skos:prefLabel>unconsolidated, loose</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/consolidationdegree/unconsolidated_very_loose">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source>NADM SLTTs 2004 after Bowles 1984</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/unconsolidated"/>
	<skos:definition>Easily indented with fingers; Relative density 0.0-0.2.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/005"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/consolidationdegree"/>
	<skos:prefLabel>unconsolidated, very loose</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/consolidationdegree/variable_induration">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">This vocabulary</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/indurated"/>
	<skos:definition>Material is lithified, but induration varies at scale of description.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/006"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/consolidationdegree"/>
	<skos:prefLabel>variable induration</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/consolidationdegree/well_consolidated">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source>NADM SLTTs 2004 after Bowles 1986</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/consolidated"/>
	<skos:definition>Requires pick to loosen for shoveling; relative density 0.7-0.9.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/012"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/consolidationdegree"/>
	<skos:prefLabel>well consolidated</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/consolidationdegree/well_indurated">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">This vocabulary</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/indurated"/>
	<skos:definition>Particles in the rock are strongly bound together such that rock surface can only be broken with great difficulty using standard rock hammer (less than 1 kg mass).</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/010"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/consolidationdegree"/>
	<skos:prefLabel>well indurated</skos:prefLabel>
</rdf:Description>

