<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/accidental_pyroclastic_fragment">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source>Gillespie and Styles 1999</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/pyroclast"/>
	<skos:definition xml:lang="en">Fragment of rock generated by disruption as a direct result of volcanic action but not formed by previous activity of the magmatic system; generally derived from subvolcanic basement.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0001"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/particletype"/>
	<skos:prefLabel xml:lang="en">accidental pyroclastic fragment</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/aggregate_non-clastic_particle">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">this vocabulary</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/solid_constituent_particle"/>
	<skos:definition xml:lang="en">Generic term for a non-clastic particle that is itself composed of an aggregation of particles.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0002"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/particletype"/>
