<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/proportionterm/rare">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">this vocabulary; follows Neuendorf et al 2005 usage scoped to coal description</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/minor"/>
	<skos:broaderTransitive rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/minor"/>
	<skos:broaderTransitive rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/subordinate"/>
	<skos:broaderTransitive rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/present"/>
	<skos:definition xml:lang="en">component constitutes less than 5 percent of the volume of the described entity.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/0007"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/proportionterm"/>
	<skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/trace"/>
	<skos:narrowerTransitive rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/trace"/>
	<skos:prefLabel xml:lang="en">rare</skos:prefLabel>
	<skos:semanticRelation rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/minor"/>
	<skos:semanticRelation rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/trace"/>
	<skos:semanticRelation rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/subordinate"/>
	<skos:semanticRelation rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/present"/>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/proportionterm/subordinate">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">this vocabulary</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/present"/>
