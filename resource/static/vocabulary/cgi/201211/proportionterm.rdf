	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/major"/>
	<skos:broaderTransitive rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/present"/>
	<skos:definition xml:lang="en">component constitutes greater than 75 percent of the volume of the described entity.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/0002"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/proportionterm"/>
	<skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/all"/>
	<skos:prefLabel xml:lang="en">major</skos:prefLabel>
</rdf:Description>

