<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/proportionterm/all">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">this vocabulary</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/dominant"/>
	<skos:broaderTransitive rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/present"/>
	<skos:broaderTransitive rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/major"/>
	<skos:definition xml:lang="en">component constitutes effectively 100 percent of the volume of the described entity.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/0001"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/proportionterm"/>
