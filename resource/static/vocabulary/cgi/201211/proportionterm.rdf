	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/present"/>
	<skos:definition xml:lang="en">Constituent forms more of the described entity than any other constituent.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/0005"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/proportionterm"/>
	<skos:prefLabel xml:lang="en">most abundant</skos:prefLabel>
</rdf:Description>

