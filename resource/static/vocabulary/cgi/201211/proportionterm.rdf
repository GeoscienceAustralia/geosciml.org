</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/proportionterm/trace">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">this vocabulary; Neuendorf et al 2005</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/rare"/>
	<skos:broaderTransitive rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/subordinate"/>
	<skos:broaderTransitive rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/minor"/>
	<skos:broaderTransitive rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/present"/>
	<skos:definition xml:lang="en">component constitutes less than approximately 1 percent of the volume of the described entity. Presence is detectable but too small for accurate determination.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/0009"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/proportionterm"/>
	<skos:prefLabel xml:lang="en">trace</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://resource.geosciml.org/classifier/cgi/proportionterm/variable">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dc:source xml:lang="en">this vocabulary</dc:source>
	<skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/present"/>
	<skos:definition xml:lang="en">component varies in proportion throughout the described entity.</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/proportionterm/0010"/>
	<skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/201211/proportionterm"/>
