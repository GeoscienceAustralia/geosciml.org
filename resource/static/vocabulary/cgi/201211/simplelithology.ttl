      skos:definition "Dioritic rock that contains between 0 and 10 percent feldspathoid minerals in the QAPF fraction. QAPF field 10'."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lithology/0090> ;
      skos:inScheme cgi:simplelithology ;
