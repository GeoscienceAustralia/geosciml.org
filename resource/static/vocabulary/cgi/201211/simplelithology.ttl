      skos:definition "Foid dioritoid in which the plagioclase to total feldspar ratio is between 0.1 and 0.9.  Includes rocks defined modally in QAPF field 13."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lithology/0102> ;
      skos:inScheme cgi:simplelithology ;
      skos:prefLabel "foid monzodiorite"@en .

slith:foid_monzogabbro
      rdf:type skos:Concept ;
      rdfs:seeAlso "essexite"@en ;
      dc:source "LeMaitre et al. 2002" ;
      skos:broader slith:foid_gabbroid ;
      skos:definition "Foid gabbroid that has a plagioclase to total feldspar ratio between 0.5 and 0.9. Includes rocks defined modally in QAPF field 13."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lithology/0103> ;
      skos:inScheme cgi:simplelithology ;
      skos:prefLabel "foid monzogabbro"@en .

slith:foid_monzosyenite
      rdf:type skos:Concept ;
      dc:source "LeMaitre et al. 2002" ;
      skos:broader slith:foid_syenitoid ;
      skos:definition "Foid syenitoid rock that has a plagioclase to total feldspar ratio of between 0.1 and 0.5.  Includes rocks defined modally in QAPF Field 12."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lithology/0104> ;
      skos:inScheme cgi:simplelithology ;
      skos:prefLabel "foid monzosyenite"@en .

slith:foid_syenite
      rdf:type skos:Concept ;
      dc:source "LeMaitre et al. 2002" ;
      skos:broader slith:foid_syenitoid ;
      skos:definition "Foid syenitoid that has a plagioclase to total feldspar ratio of less than 0.1. Includes rocks defined modally in QAPF field 11."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lithology/0105> ;
      skos:inScheme cgi:simplelithology ;
      skos:prefLabel "foid syenite"@en .

slith:foid_syenitoid
      rdf:type skos:Concept ;
      dc:source "LeMaitre et al. 2002" ;
      skos:broader slith:phaneritic_igneous_rock ;
      skos:definition "Phaneritic crystalline igneous rock with M less than 90, contains between 10 and 60 percent feldspathoid mineral in the QAPF fraction, and has a plagioclase to total feldspar ratio less than 0.5. Includes QAPF fields 11 and 12."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lithology/0106> ;
      skos:inScheme cgi:simplelithology ;
      skos:narrower slith:foid_syenite , slith:foid_monzosyenite ;
      skos:prefLabel "foid syenitoid"@en .

slith:foidite
      rdf:type skos:Concept ;
      dc:source "LeMaitre et al. 2002" ;
      skos:broader slith:foiditoid ;
      skos:definition "Foiditoid that contains greater than 90 percent feldspathoid minerals in the QAPF fraction."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lithology/0107> ;
      skos:inScheme cgi:simplelithology ;
      skos:prefLabel "foidite"@en .

slith:foiditoid
      rdf:type skos:Concept ;
      dc:source "LeMaitre et al. 2002" ;
      skos:altLabel "foiditic rock"@en , "foidite (sensu lato)"@en ;
      skos:broader slith:fine_grained_igneous_rock ;
      skos:definition "Fine grained crystalline rock containing less than 90 percent mafic minerals and more than 60 percent feldspathoid minerals in the QAPF fraction. Includes rocks defined modally in QAPF field 15 or chemically in TAS field F."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lithology/0108> ;
      skos:inScheme cgi:simplelithology ;
      skos:narrower slith:tephritic_foidite , slith:basanitic_foidite , slith:foidite , slith:phonolitic_foidite ;
      skos:prefLabel "foiditoid"@en .

