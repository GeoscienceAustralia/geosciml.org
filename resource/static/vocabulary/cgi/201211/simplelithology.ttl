      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lithology/0211> ;
      skos:inScheme cgi:simplelithology ;
      skos:prefLabel "quartz anorthosite"@en .

slith:quartz_diorite
      rdf:type skos:Concept ;
