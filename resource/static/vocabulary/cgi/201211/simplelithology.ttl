      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lithology/0208> ;
      skos:inScheme cgi:simplelithology ;
      skos:prefLabel "pyroxenite"@en .

slith:quartz_alkali_feldspar_syenite
