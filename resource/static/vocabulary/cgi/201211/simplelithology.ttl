slith:foid_bearing_alkali_feldspar_trachyte
      rdf:type skos:Concept ;
      dc:source "LeMaitre et al. 2002" ;
      skos:broader slith:alkali_feldspar_trachytic_rock ;
      skos:definition "Alkali feldspar trachytic rock that contains no quartz and between 0 and 10 percent feldspathoid mineral in the QAPF fraction. QAPF field 6'."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lithology/0088> ;
      skos:inScheme cgi:simplelithology ;
      skos:prefLabel "foid bearing alkali feldspar trachyte"@en .

slith:foid_bearing_anorthosite
      rdf:type skos:Concept ;
      dc:source "LeMaitre et al. 2002" ;
