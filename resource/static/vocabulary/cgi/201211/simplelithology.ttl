      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lithology/0089> ;
      skos:inScheme cgi:simplelithology ;
      skos:prefLabel "foid bearing anorthosite"@en .

slith:foid_bearing_diorite
      rdf:type skos:Concept ;
      dc:source "LeMaitre et al. 2002" ;
