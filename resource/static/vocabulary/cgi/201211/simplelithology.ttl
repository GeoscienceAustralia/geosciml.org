      skos:broader slith:igneous_material ;
      skos:definition "igneous_material of unspecified consolidation state in which greater than 75 percent of the rock consists of fragments produced as a result of igneous rock-forming process."@en ;
      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lithology/0111> ;
      skos:inScheme cgi:simplelithology ;
      skos:narrower slith:pyroclastic_material , slith:fragmental_igneous_rock ;
      skos:prefLabel "fragmental igneous material"@en .

slith:fragmental_igneous_rock
      rdf:type skos:Concept ;
      dc:source "This vocabulary" ;
      skos:broader slith:igneous_rock , slith:fragmental_igneous_material ;
