      skos:prefLabel "pyroclastic rock"@en .

slith:pyroxenite
      rdf:type skos:Concept ;
      rdfs:seeAlso "orthopyroxenite"@en , "clinopyroxenite"@en , "websterite"@en ;
      dc:source "LeMaitre et al. 2002" ;
      skos:broader slith:ultramafic_igneous_rock , slith:phaneritic_igneous_rock ;
