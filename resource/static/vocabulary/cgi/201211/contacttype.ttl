      rdf:type skos:Collection ;
      rdfs:label "contact type concepts"@en ;
      skos:definition "This resource defines the concept space for Contact Types by extension; it includes all concepts in this conceptScheme, as well as concepts in previous versions of the scheme."@en ;
      skos:member contyp2:mineralisation_assemblage_contact , contyp2:lithogenetic_contact , contyp2:buttress_unconformity , contyp2:radiometric_contact , contyp2:glacial_stationary_line , contyp2:paraconformable_contact , contyp2:magnetic_polarity_contact , contyp2:magnetic_contact , contyp2:nonconformable_contact , contyp2:sedimentary_facies_contact , contyp2:metamorphic_contact , contyp2:impact_structure_boundary , contyp2:unconformable_contact , contyp2:contact , contyp2:igneous_phase_contact , contyp2:magnetic_susceptiblity_contact , contyp2:chronostratigraphic_zone_contact , contyp2:metasomatic_facies_contact , contyp2:disconformable_contact , contyp2:deformation_zone_contact , contyp2:alteration_facies_contact , contyp2:density_contact , contyp2:conductivity_contact , contyp2:sedimentary_intrusive_contact , contyp2:geologic_province_contact , contyp2:faulted_contact , contyp2:igneous_intrusive_contact , contyp2:volcanic_subsidence_zone_boundary , contyp2:seismic_contact , contyp2:depositional_contact , contyp2:weathering_contact , contyp2:magnetization_contact , contyp2:metamorphic_facies_contact , contyp2:geophysical_contact , contyp2:conformable_contact , contyp2:angular_unconformable_contact ;
      skos:prefLabel "CGI Contact Type"@en .

