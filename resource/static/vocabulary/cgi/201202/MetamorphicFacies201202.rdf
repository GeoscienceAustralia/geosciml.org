    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/amphibolite_metamorphic_facies -->

    <skos:Concept rdf:about="&metamorphicfacies;amphibolite_metamorphic_facies">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Smulikowski et al. 2003; Fettes and Desmons 2007</dc:source>
        <skos:prefLabel xml:lang="en">Amphibolite metamorphic facies</skos:prefLabel>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Metamorphic facies characterized in rocks of basaltic composition by hornblende-plagioclase (plagioclase more calcic than An17).</skos:definition>
        <skos:exactMatch rdf:resource="&metamorphicfacies;0001"/>
        <skos:broader rdf:resource="&metamorphicfacies;metamorphic_facies_not_specified"/>
        <skos:inScheme rdf:resource="&cgi;metamorphicfacies"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/eclogite_metamorphic_facies -->

    <skos:Concept rdf:about="&metamorphicfacies;eclogite_metamorphic_facies">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Eclogite metamorphic facies</skos:prefLabel>
        <dc:source xml:lang="en">Fettes and Desmons 2007</dc:source>
        <skos:definition xml:lang="en">omphacite-garnet-quartz (no plagioclase, olivine stable with garnet); assemblage for rocks of basaltic composition</skos:definition>
        <skos:exactMatch rdf:resource="&metamorphicfacies;0002"/>
        <skos:broader rdf:resource="&metamorphicfacies;metamorphic_facies_not_specified"/>
        <skos:inScheme rdf:resource="&cgi;metamorphicfacies"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/epidote_amphibolite_metamorphic_facies -->

    <skos:Concept rdf:about="&metamorphicfacies;epidote_amphibolite_metamorphic_facies">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Epidote amphibolite metamorphic facies</skos:prefLabel>
        <dc:source xml:lang="en">Fettes and Desmons 2007</dc:source>
        <skos:definition xml:lang="en">hornblende-albite-epidote(-chlorite); assemblage for rocks of basaltic composition</skos:definition>
        <skos:exactMatch rdf:resource="&metamorphicfacies;0003"/>
        <skos:broader rdf:resource="&metamorphicfacies;metamorphic_facies_not_specified"/>
        <skos:inScheme rdf:resource="&cgi;metamorphicfacies"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/glaucophane_schist_facies -->

    <skos:Concept rdf:about="&metamorphicfacies;glaucophane_schist_facies">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Glaucophane-schist facies</skos:prefLabel>
        <skos:definition xml:lang="en">Metamorphic facies characterized in rocks of basaltic composition by glaucophane-epidote (-garnet), glaucophane-lawsonite, or glaucophane-lawsonite-jadeite.</skos:definition>
        <dc:source xml:lang="en">Smulikowski et al., 2003; Fettes and Desmons 2007</dc:source>
        <skos:exactMatch rdf:resource="&metamorphicfacies;0004"/>
        <skos:broader rdf:resource="&metamorphicfacies;metamorphic_facies_not_specified"/>
        <skos:inScheme rdf:resource="&cgi;metamorphicfacies"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/granulite_facies -->

    <skos:Concept rdf:about="&metamorphicfacies;granulite_facies">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Granulite facies</skos:prefLabel>
        <skos:definition xml:lang="en">Metamorphic facies characterized in rocks of basaltic composition by the mineral assemblage clinopyroxene-orthopyroxene-plagioclase (olivine not stable with plagioclase or with garnet), or especially high-temperature varieties or polymorphs of minerals (e</skos:definition>
        <dc:source xml:lang="en">Smulikowski et al., 2003; Fettes and Desmons 2007</dc:source>
        <skos:exactMatch rdf:resource="&metamorphicfacies;0005"/>
        <skos:broader rdf:resource="&metamorphicfacies;metamorphic_facies_not_specified"/>
        <skos:inScheme rdf:resource="&cgi;metamorphicfacies"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/greenschist_metamorphic_facies -->

    <skos:Concept rdf:about="&metamorphicfacies;greenschist_metamorphic_facies">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Greenschist metamorphic facies</skos:prefLabel>
        <skos:definition xml:lang="en">Metamorphic facies characterized in rocks of basaltic composition by actinolite-albite-epidote-chlorite (an epidote group mineral is the diagnostic Ca-Al silicate rather than prehnite or pumpellyite).</skos:definition>
        <dc:source xml:lang="en">Smulikowski et al., 2003; Fettes and Desmons 2007</dc:source>
        <skos:exactMatch rdf:resource="&metamorphicfacies;0006"/>
        <skos:broader rdf:resource="&metamorphicfacies;metamorphic_facies_not_specified"/>
        <skos:inScheme rdf:resource="&cgi;metamorphicfacies"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/metamorphic_facies_not_specified -->

    <skos:Concept rdf:about="&metamorphicfacies;metamorphic_facies_not_specified">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Metamorphic facies not specified</skos:prefLabel>
        <skos:definition xml:lang="en">Metamorphosed, but facies not specified in more detail. For use in normative description to indicate that any metamorphic facies value is valid.</skos:definition>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:exactMatch rdf:resource="&metamorphicfacies;0007"/>
        <skos:narrower rdf:resource="&metamorphicfacies;amphibolite_metamorphic_facies"/>
        <skos:narrower rdf:resource="&metamorphicfacies;eclogite_metamorphic_facies"/>
        <skos:narrower rdf:resource="&metamorphicfacies;epidote_amphibolite_metamorphic_facies"/>
        <skos:narrower rdf:resource="&metamorphicfacies;glaucophane_schist_facies"/>
        <skos:narrower rdf:resource="&metamorphicfacies;granulite_facies"/>
        <skos:narrower rdf:resource="&metamorphicfacies;greenschist_metamorphic_facies"/>
        <skos:narrower rdf:resource="&metamorphicfacies;pyroxene_hornfels_metamorphic_facies"/>
        <skos:narrower rdf:resource="&metamorphicfacies;sanidinite_metamorphic_facies"/>
        <skos:narrower rdf:resource="&metamorphicfacies;subgreenschist_metamorphic_facies"/>
        <skos:narrower rdf:resource="&metamorphicfacies;zeolite_metamorphic_facies"/>
        <skos:topConceptOf rdf:resource="&cgi;metamorphicfacies"/>
        <skos:inScheme rdf:resource="&cgi;metamorphicfacies"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/metamorphic_facies_unknown -->

    <skos:Concept rdf:about="&metamorphicfacies;metamorphic_facies_unknown">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">For use in instance descriptions to indicate that no data are available to constrain metamorphic facies.  May be metamorphosed or non-metamorphosed.</skos:definition>
        <skos:prefLabel xml:lang="en">Metamorphic facies unknown</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:exactMatch rdf:resource="&metamorphicfacies;0008"/>
        <skos:topConceptOf rdf:resource="&cgi;metamorphicfacies"/>
        <skos:inScheme rdf:resource="&cgi;metamorphicfacies"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/no_metamorphic_minerals -->

    <skos:Concept rdf:about="&metamorphicfacies;no_metamorphic_minerals">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Corresponds to non-metamorphosed condition; since metamorphic facies classification is based on mineral assemblages, absence of metamorphic minerals means that the classification does not apply.</skos:definition>
        <skos:prefLabel xml:lang="en">No metamorphic minerals</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:exactMatch rdf:resource="&metamorphicfacies;0009"/>
        <skos:inScheme rdf:resource="&cgi;metamorphicfacies"/>
        <skos:topConceptOf rdf:resource="&cgi;metamorphicfacies"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/pyroxene_hornfels_metamorphic_facies -->

    <skos:Concept rdf:about="&metamorphicfacies;pyroxene_hornfels_metamorphic_facies">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Metamorphic facies characterized in rocks of basaltic composition by clinopyroxene-orthopyroxene-plagioclase (olivine stable with plagioclase).</skos:definition>
        <skos:prefLabel xml:lang="en">Pyroxene hornfels metamorphic facies</skos:prefLabel>
        <dc:source xml:lang="en">Smulikowski et al. 2003;Fettes and Desmons 2007</dc:source>
        <skos:exactMatch rdf:resource="&metamorphicfacies;0010"/>
        <skos:broader rdf:resource="&metamorphicfacies;metamorphic_facies_not_specified"/>
        <skos:inScheme rdf:resource="&cgi;metamorphicfacies"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/sanidinite_metamorphic_facies -->

    <skos:Concept rdf:about="&metamorphicfacies;sanidinite_metamorphic_facies">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source xml:lang="en">Fettes and Desmons 2007</dc:source>
        <skos:prefLabel xml:lang="en">Sanidinite metamorphic facies</skos:prefLabel>
        <skos:definition xml:lang="en">distinguished from the pyroxene hornfels facies by the occurrence of especially high-temperature varieties and polymorphs of minerals (eg. pigeonite, K-rich labradorite); assemblage for rocks of basaltic composition</skos:definition>
        <skos:exactMatch rdf:resource="&metamorphicfacies;0011"/>
        <skos:broader rdf:resource="&metamorphicfacies;metamorphic_facies_not_specified"/>
        <skos:inScheme rdf:resource="&cgi;metamorphicfacies"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/subgreenschist_metamorphic_facies -->

    <skos:Concept rdf:about="&metamorphicfacies;subgreenschist_metamorphic_facies">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Metamorphic facies characterized in rocks of basaltic composition by prehnite-pumpellyite, pumpellyite-actinolite, prehnite-actinolite (prehnite and pumpellyite are the diagnostic Ca-Al silicates rather than minerals of the epidote or zeolite groups).</skos:definition>
        <dc:source xml:lang="en">Smulikowski et al. 2003; Fettes and Desmons 2007</dc:source>
        <skos:prefLabel xml:lang="en">Subgreenschist metamorphic facies</skos:prefLabel>
        <skos:exactMatch rdf:resource="&metamorphicfacies;0012"/>
        <skos:broader rdf:resource="&metamorphicfacies;metamorphic_facies_not_specified"/>
        <skos:inScheme rdf:resource="&cgi;metamorphicfacies"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/zeolite_metamorphic_facies -->

    <skos:Concept rdf:about="&metamorphicfacies;zeolite_metamorphic_facies">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Metamorphic facies characterized in rocks of basaltic composition by zeolite minerals such as laumontite and heulandites (in place of other Ca-Al silicates such as prehnite, pumpellyite and epidote).</skos:definition>
        <dc:source xml:lang="en">Smulikowski et al. 2003; Fettes and Desmons 2007</dc:source>
        <skos:prefLabel xml:lang="en">Zeolite metamorphic facies</skos:prefLabel>
        <skos:exactMatch rdf:resource="&metamorphicfacies;0013"/>
        <skos:broader rdf:resource="&metamorphicfacies;metamorphic_facies_not_specified"/>
        <skos:inScheme rdf:resource="&cgi;metamorphicfacies"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifierscheme/cgi/201202/metamorphicfacies -->

    <skos:ConceptScheme rdf:about="&cgi;metamorphicfacies">
