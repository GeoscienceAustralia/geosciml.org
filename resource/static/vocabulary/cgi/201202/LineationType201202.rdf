    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/augen_lineation -->

    <skos:Concept rdf:about="&lineationtype2;augen_lineation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en">A shape lineation defined by elongate augen.</skos:definition>
        <skos:prefLabel xml:lang="en">Augen lineation</skos:prefLabel>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source>this vocabulary</dc:source>
        <skos:exactMatch rdf:resource="&lineationtype2;0001"/>
        <skos:broader rdf:resource="&lineationtype2;particle_shape_lineation"/>
        <skos:broader rdf:resource="&lineationtype2;tectonic_lineation"/>
        <skos:inScheme rdf:resource="&cgi;lineationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/bedding_cleavage_intersection_lineation -->

    <skos:Concept rdf:about="&lineationtype2;bedding_cleavage_intersection_lineation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>this vocabulary</dc:source>
        <skos:prefLabel xml:lang="en">Bedding cleavage intersection lineation</skos:prefLabel>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Lineation defined by the intersection of bedding surfaces and cleavage surfaces.</skos:definition>
        <skos:exactMatch rdf:resource="&lineationtype2;0002"/>
        <skos:broader rdf:resource="&lineationtype2;intersection_lineation"/>
        <skos:broader rdf:resource="&lineationtype2;tectonic_lineation"/>
        <skos:inScheme rdf:resource="&cgi;lineationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/biotite_mineral_lineation -->

    <skos:Concept rdf:about="&lineationtype2;biotite_mineral_lineation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en">Biotite mineral lineation</skos:prefLabel>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Mineral lineation defined by aligned elongate biotite aggregates in a foliation surface. Biotite does not have an elongate (prismatic, acicular) mineral habit, so production of lineation must reflect elongate aggregates of biotite crystals.</skos:definition>
        <dc:source>this vocabulary</dc:source>
        <skos:exactMatch rdf:resource="&lineationtype2;0003"/>
        <skos:broader rdf:resource="&lineationtype2;mineral_lineation"/>
        <skos:inScheme rdf:resource="&cgi;lineationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/cleavage_intersection_lineation -->

    <skos:Concept rdf:about="&lineationtype2;cleavage_intersection_lineation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>this vocabulary</dc:source>
        <skos:prefLabel xml:lang="en">Cleavage intersection lineation</skos:prefLabel>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Lineation defined by the intersection of two cleavages.</skos:definition>
        <skos:exactMatch rdf:resource="&lineationtype2;0004"/>
        <skos:broader rdf:resource="&lineationtype2;intersection_lineation"/>
        <skos:broader rdf:resource="&lineationtype2;tectonic_lineation"/>
        <skos:inScheme rdf:resource="&cgi;lineationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/crenulation_lineation -->

    <skos:Concept rdf:about="&lineationtype2;crenulation_lineation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Crenulation lineation</skos:prefLabel>
        <skos:definition xml:lang="en">Lineation defined by small-scale wrinkles, typically of phylosillicate minerals, on another surface, typically a cleavage or schistosity. Use instead of intersection lineation if the wrinkles are the measured feature and the intersecting planar feature is not apparent.</skos:definition>
        <dc:source>this vocabulary</dc:source>
        <skos:exactMatch rdf:resource="&lineationtype2;0005"/>
        <skos:broader rdf:resource="&lineationtype2;lineation"/>
        <skos:broader rdf:resource="&lineationtype2;tectonic_lineation"/>
        <skos:inScheme rdf:resource="&cgi;lineationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/flow_lineation -->

    <skos:Concept rdf:about="&lineationtype2;flow_lineation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en">A linear alignment of fabric elements, especially elongate or rod-shaped minerals, interpreted to have been aligned by flow of magma, lava, pyroclastic material, or diapiric flow of sedimentary rock.</skos:definition>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Flow lineation</skos:prefLabel>
        <dc:source>Geoscience Victoria, Australia</dc:source>
        <skos:exactMatch rdf:resource="&lineationtype2;0006"/>
        <skos:broader rdf:resource="&lineationtype2;lineation"/>
        <skos:inScheme rdf:resource="&cgi;lineationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/hornblende_mineral_lineation -->

    <skos:Concept rdf:about="&lineationtype2;hornblende_mineral_lineation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Hornblende mineral lineation</skos:prefLabel>
        <dc:source>this vocabulary</dc:source>
        <skos:definition xml:lang="en">Mineral lineation defined by aligned hornblende crystals.</skos:definition>
        <skos:exactMatch rdf:resource="&lineationtype2;0007"/>
        <skos:broader rdf:resource="&lineationtype2;mineral_lineation"/>
        <skos:inScheme rdf:resource="&cgi;lineationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/igneous_lineation -->

    <skos:Concept rdf:about="&lineationtype2;igneous_lineation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en">Igneous lineation</skos:prefLabel>
        <dc:source>this vocabulary</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Lineation defined by alignment of elongate fabric elements formed in lava or magma before complete crystallization. Some typical fabric elements are elongate phenocrysts and vesicles.</skos:definition>
        <skos:exactMatch rdf:resource="&lineationtype2;0008"/>
        <skos:narrower rdf:resource="&lineationtype2;igneous_mineral_lineation"/>
        <skos:narrower rdf:resource="&lineationtype2;igneous_particle_shape_lineation"/>
        <skos:narrower rdf:resource="&lineationtype2;igneous_vesicle_lineation"/>
        <skos:broader rdf:resource="&lineationtype2;lineation"/>
        <skos:inScheme rdf:resource="&cgi;lineationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/igneous_mineral_lineation -->

    <skos:Concept rdf:about="&lineationtype2;igneous_mineral_lineation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en">Igneous mineral lineation</skos:prefLabel>
        <skos:definition xml:lang="en">Lineation in igneous rock defined by aligned crystals where there is evidence that the crystal alignment is related to process occurring before the rock was solidified.</skos:definition>
        <dc:source>this vocabulary</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:exactMatch rdf:resource="&lineationtype2;0009"/>
        <skos:broader rdf:resource="&lineationtype2;igneous_lineation"/>
        <skos:broader rdf:resource="&lineationtype2;mineral_lineation"/>
        <skos:inScheme rdf:resource="&cgi;lineationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/igneous_particle_shape_lineation -->

    <skos:Concept rdf:about="&lineationtype2;igneous_particle_shape_lineation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source>this vocabulary</dc:source>
        <skos:prefLabel xml:lang="en">Igneous particle shape lineation</skos:prefLabel>
        <skos:definition xml:lang="en">Lineation in an igneous rock, usually welded tuff, defined by elongate objects in the rock (usually pumice pads, also elongate crystals). Descriptive--elogation may be due to deformation or to rotation/alignment of originally elongate (prolate ellipsoid) objects in the rock.</skos:definition>
        <skos:exactMatch rdf:resource="&lineationtype2;0010"/>
        <skos:broader rdf:resource="&lineationtype2;igneous_lineation"/>
        <skos:broader rdf:resource="&lineationtype2;particle_shape_lineation"/>
        <skos:inScheme rdf:resource="&cgi;lineationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/igneous_vesicle_lineation -->

    <skos:Concept rdf:about="&lineationtype2;igneous_vesicle_lineation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>this vocabulary</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Igneous vesicle lineation</skos:prefLabel>
        <skos:definition xml:lang="en">lineation defined in a volcanic rock by aligned elongation direction of vesicles.</skos:definition>
        <skos:exactMatch rdf:resource="&lineationtype2;0011"/>
        <skos:broader rdf:resource="&lineationtype2;igneous_lineation"/>
        <skos:inScheme rdf:resource="&cgi;lineationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/intersection_lineation -->

    <skos:Concept rdf:about="&lineationtype2;intersection_lineation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Neuendorf et al 2005; this vocabulary</dc:source>
        <skos:definition xml:lang="en">A lineation produced by the intersection of two planar fabrics.</skos:definition>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Intersection lineation</skos:prefLabel>
        <skos:exactMatch rdf:resource="&lineationtype2;0012"/>
        <skos:narrower rdf:resource="&lineationtype2;bedding_cleavage_intersection_lineation"/>
        <skos:narrower rdf:resource="&lineationtype2;cleavage_intersection_lineation"/>
        <skos:broader rdf:resource="&lineationtype2;lineation"/>
        <skos:inScheme rdf:resource="&cgi;lineationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/lineation -->

    <skos:Concept rdf:about="&lineationtype2;lineation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Neuendorf et al 2005; this vocabulary</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Lineation</skos:prefLabel>
        <skos:definition xml:lang="en">Nongenetic term for a penetrative linear structure.</skos:definition>
        <skos:exactMatch rdf:resource="&lineationtype2;0013"/>
        <skos:narrower rdf:resource="&lineationtype2;crenulation_lineation"/>
        <skos:narrower rdf:resource="&lineationtype2;flow_lineation"/>
        <skos:narrower rdf:resource="&lineationtype2;igneous_lineation"/>
        <skos:narrower rdf:resource="&lineationtype2;intersection_lineation"/>
        <skos:narrower rdf:resource="&lineationtype2;particle_shape_lineation"/>
        <skos:narrower rdf:resource="&lineationtype2;pencil_lineation"/>
        <skos:narrower rdf:resource="&lineationtype2;rodding_lineation"/>
        <skos:narrower rdf:resource="&lineationtype2;slickenline"/>
        <skos:narrower rdf:resource="&lineationtype2;tectonic_lineation"/>
        <skos:topConceptOf rdf:resource="&cgi;lineationtype"/>
