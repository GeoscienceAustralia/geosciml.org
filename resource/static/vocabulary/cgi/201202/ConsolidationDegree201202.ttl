###  http://www.w3.org/2002/07/owl#topDataProperty

owl:topDataProperty rdf:type owl:ObjectProperty .



###  http://www.w3.org/2002/07/owl#topObjectProperty

owl:topObjectProperty rdf:type owl:ObjectProperty .



###  http://www.w3.org/2004/02/skos/core#broadMatch

skos:broadMatch rdf:type owl:ObjectProperty .



###  http://www.w3.org/2004/02/skos/core#broader

skos:broader rdf:type owl:ObjectProperty ;
             
             rdfs:label "has broader"@en ;
             
             rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core"^^xsd:anyURI ;
             
             rdfs:comment """Broader concepts are typically rendered as parents in a concept
      hierarchy (tree)."""@en ,
                          """By convention, skos:broader is only used to assert an immediate
      (i.e. direct) hierarchical link between two conceptual resources."""@en ;
             
             owl:inverseOf skos:narrower .



###  http://www.w3.org/2004/02/skos/core#changeNote

skos:changeNote rdf:type owl:ObjectProperty ;
                
                rdfs:subPropertyOf owl:topDataProperty .



###  http://www.w3.org/2004/02/skos/core#closeMatch

skos:closeMatch rdf:type owl:ObjectProperty ,
                         owl:SymmetricProperty ;
                
                rdfs:label "close match"@en ;
                
                rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core"^^xsd:anyURI ;
                
                owl:inverseOf skos:closeMatch ;
                
                rdfs:subPropertyOf skos:mappingRelation .



###  http://www.w3.org/2004/02/skos/core#definition

skos:definition rdf:type owl:ObjectProperty ;
                
                rdfs:label "definition"@en ;
                
                rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core"^^xsd:anyURI ;
                
                rdfs:subPropertyOf owl:topDataProperty .



###  http://www.w3.org/2004/02/skos/core#exactMatch

skos:exactMatch rdf:type owl:ObjectProperty ,
                         owl:SymmetricProperty ,
                         owl:TransitiveProperty ;
                
                rdfs:label "exact match"@en ;
                
                rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core"^^xsd:anyURI ;
                
                rdfs:comment """skos:exactMatch is disjoint with each of the properties
      skos:broadMatch and skos:relatedMatch."""@en ;
                
                rdfs:subPropertyOf skos:closeMatch ;
                
                owl:inverseOf skos:exactMatch .



###  http://www.w3.org/2004/02/skos/core#hasTopConcept

skos:hasTopConcept rdf:type owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ,
                            owl:ObjectProperty ;
                   
                   rdfs:label "has top concept"@en ;
                   
                   rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core"^^xsd:anyURI ;
                   
                   rdfs:range skos:Concept ;
                   
                   rdfs:domain skos:ConceptScheme .



###  http://www.w3.org/2004/02/skos/core#historyNote

skos:historyNote rdf:type owl:ObjectProperty ;
                 
                 rdfs:subPropertyOf owl:topDataProperty .



###  http://www.w3.org/2004/02/skos/core#inScheme

skos:inScheme rdf:type owl:ObjectProperty ;
              
              rdfs:label "in scheme"@en ;
              
              rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core"^^xsd:anyURI ;
              
              rdfs:comment "A concept may be a member of more than one concept scheme."@en ;
              
              rdfs:subPropertyOf owl:topObjectProperty ;
              
              rdfs:range skos:ConceptScheme .



###  http://www.w3.org/2004/02/skos/core#mappingRelation

skos:mappingRelation rdf:type owl:ObjectProperty ;
                     
                     rdfs:label "mapping relation"@en ;
                     
                     rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core"^^xsd:anyURI ;
                     
                     rdfs:comment """These concept mapping relations mirror semantic relations, and the
      data model defined below is similar (with the exception of skos:exactMatch) to the data model
      defined for semantic relations. A distinct vocabulary is provided for concept mapping
      relations, to provide a convenient way to differentiate links within a concept scheme from
      links between concept schemes. However, this pattern of usage is not a formal requirement of
      the SKOS data model, and relies on informal definitions of best practice."""@en ;
                     
                     rdfs:subPropertyOf skos:semanticRelation .



###  http://www.w3.org/2004/02/skos/core#member

skos:member rdf:type owl:ObjectProperty ;
            
            rdfs:label "member"@en ;
            
            rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core"^^xsd:anyURI ;
            
            rdfs:subPropertyOf owl:topObjectProperty ;
            
            rdfs:domain skos:Collection ;
            
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( skos:Collection
                                       skos:Concept
                                     )
                       ] .



###  http://www.w3.org/2004/02/skos/core#memberList

skos:memberList rdf:type owl:FunctionalProperty ,
                         owl:ObjectProperty ;
                
                rdfs:label "member list"@en ;
                
                rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core"^^xsd:anyURI ;
                
                rdfs:comment """For any resource, every item in the list given as the value of the
      skos:memberList property is also a value of the skos:member property."""@en ;
                
                rdfs:subPropertyOf owl:topObjectProperty ;
                
                rdfs:domain skos:OrderedCollection .



###  http://www.w3.org/2004/02/skos/core#narrowMatch

skos:narrowMatch rdf:type owl:ObjectProperty ;
                 
                 rdfs:label "narrower match"@en ;
                 
                 rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core"^^xsd:anyURI ;
                 
                 owl:inverseOf skos:broadMatch ;
                 
                 rdfs:subPropertyOf skos:mappingRelation ,
                                    skos:narrower .



###  http://www.w3.org/2004/02/skos/core#narrower

skos:narrower rdf:type owl:ObjectProperty ;
              
              rdfs:label "has narrower"@en ;
              
              rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core"^^xsd:anyURI ;
              
              rdfs:comment """By convention, skos:broader is only used to assert an immediate
      (i.e. direct) hierarchical link between two conceptual resources."""@en ,
                           """Narrower concepts are typically rendered as children in a concept
      hierarchy (tree)."""@en .



###  http://www.w3.org/2004/02/skos/core#notation

skos:notation rdf:type owl:ObjectProperty ;
              
              rdfs:label "notation"@en ;
              
              rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core"^^xsd:anyURI ;
              
              rdfs:comment """By convention, skos:notation is used with a typed literal in the
      object position of the triple."""@en ;
              
              rdfs:subPropertyOf owl:topDataProperty .



###  http://www.w3.org/2004/02/skos/core#prefLabel

skos:prefLabel rdf:type owl:ObjectProperty ;
               
               rdfs:subPropertyOf owl:topDataProperty .



###  http://www.w3.org/2004/02/skos/core#related

skos:related rdf:type owl:ObjectProperty ,
                      owl:SymmetricProperty ;
             
             rdfs:label "related to"@en ;
             
             rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core"^^xsd:anyURI ;
             
             rdfs:comment "skos:related is disjoint with skos:broaderTransitive"@en ;
             
             owl:inverseOf skos:related ;
             
             rdfs:subPropertyOf skos:semanticRelation .



###  http://www.w3.org/2004/02/skos/core#relatedMatch

skos:relatedMatch rdf:type owl:ObjectProperty ,
                           owl:SymmetricProperty ;
                  
                  rdfs:label "related match"@en ;
                  
                  rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core"^^xsd:anyURI ;
                  
                  rdfs:subPropertyOf skos:mappingRelation ,
                                     skos:related ;
                  
                  owl:inverseOf skos:relatedMatch .



###  http://www.w3.org/2004/02/skos/core#semanticRelation

skos:semanticRelation rdf:type owl:ObjectProperty ;
                      
                      rdfs:label "semantic relation"@en ;
                      
                      rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core"^^xsd:anyURI ;
                      
                      rdfs:comment """This property should not be used directly, but as a super-property
      for all properties denoting a relationship of meaning between concepts."""@en ;
                      
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      
                      rdfs:domain skos:Concept ;
                      
                      rdfs:range skos:Concept .



###  http://www.w3.org/2004/02/skos/core#topConceptOf

skos:topConceptOf rdf:type owl:AsymmetricProperty ,
                           owl:IrreflexiveProperty ,
                           owl:ObjectProperty ;
                  
                  rdfs:label "top concept in scheme"@en ;
                  
                  rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core"^^xsd:anyURI ;
                  
                  rdfs:subPropertyOf skos:inScheme .





