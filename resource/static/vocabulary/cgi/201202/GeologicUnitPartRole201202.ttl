                                                                                           <http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/tectonic_block> ,
                                                                                           <http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/xenolith> ;
                                                                             
                                                                             skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/geologicunitpartrole> .



###  http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/irregular_lithosome

<http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/irregular_lithosome> rdf:type owl:NamedIndividual ,
                                                                                                owl:Thing ,
                                                                                                skos:Concept ;
                                                                                       
                                                                                       skos:prefLabel "Irregular lithosome"@en ;
                                                                                       
                                                                                       dc:source "This vocabulary"@en ;
                                                                                       
                                                                                       skos:definition "lithosome in a mixed/heterogeneous lithodemic unit that occurs in irregular bodies within unit" ;
                                                                                       
                                                                                       skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/0009> ;
                                                                                       
                                                                                       skos:broader <http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/lithosome> ;
                                                                                       
                                                                                       skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/geologicunitpartrole> .



###  http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/layer_lithosome

<http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/layer_lithosome> rdf:type owl:NamedIndividual ,
                                                                                            owl:Thing ,
                                                                                            skos:Concept ;
                                                                                   
                                                                                   skos:definition "lithosome in igneous or metamorphic geologic unit that occurs as layers alternating with other constituents." ;
                                                                                   
                                                                                   skos:prefLabel "Layer lithosome"@en ;
                                                                                   
                                                                                   dc:source "This vocabulary"@en ;
                                                                                   
                                                                                   skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/0010> ;
                                                                                   
                                                                                   skos:broader <http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/lithosome> ;
                                                                                   
                                                                                   skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/geologicunitpartrole> .



###  http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/lenticular_lithosome

<http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/lenticular_lithosome> rdf:type owl:NamedIndividual ,
                                                                                                 owl:Thing ,
                                                                                                 skos:Concept ;
                                                                                        
                                                                                        skos:definition "lithosome occurs as discrete lense-shaped bodies, not connected with other bodies." ;
                                                                                        
                                                                                        skos:prefLabel "Lenticular lithosome"@en ;
                                                                                        
                                                                                        dc:source "This vocabulary"@en ;
                                                                                        
                                                                                        skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/0011> ;
                                                                                        
                                                                                        skos:broader <http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/lithosome> ;
                                                                                        
                                                                                        skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/geologicunitpartrole> .



###  http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/lithosome

<http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/lithosome> rdf:type owl:NamedIndividual ,
                                                                                      owl:Thing ,
                                                                                      skos:Concept ;
                                                                             
                                                                             skos:definition "A kind of rock body that has multiple occurrences in a single geologic unit. A mass of rock of uniform character, characterized by geometry, composition, and internal structure. Generally denotes rock mass that is the product of a particular rock forming process or related sequence of processes in the containing unit. Example--bouma sequence, point bar sequence. A particular lithosome may be characterized by the presence of blocks, but blocks are not treated as kinds of lithosome because the internal character of the blocks is determined by a separate genetic sequence from the described unit. This vocabulary generalizes the concept defined in Neuendorf et al 2005 to include bodies of igneous or metamorphic rock as well as sedimentary rock. NADM SLTTs (2004) used the term 'lithotope' with similar meaning for sedimentary rocks." ;
                                                                             
                                                                             skos:prefLabel "Lithosome"@en ;
                                                                             
                                                                             dc:source "This vocabulary"@en ;
                                                                             
                                                                             skos:exactMatch <http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/0012> ;
                                                                             
                                                                             skos:narrower <http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/bed_lithosome> ,
                                                                                           <http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/cyclic_bedding_package> ,
                                                                                           <http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/geologic_unit_matrix> ,
                                                                                           <http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/irregular_lithosome> ,
                                                                                           <http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/layer_lithosome> ,
                                                                                           <http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/lenticular_lithosome> ;
                                                                             
                                                                             skos:broader <http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/part_of> ;
                                                                             
                                                                             skos:narrower <http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/vein_or_dike_lithosome> ;
                                                                             
                                                                             skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/geologicunitpartrole> .



###  http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/marker_bed

