                                            skos:prefLabel "Pebble gravel size sediment"@en ;
                                            
                                            skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lithology/0186>  ;
                                            
                                            skos:broader SimpleLithology:gravel_size_sediment .



###  http://resource.geosciml.org/classifier/cgi/lithology/pegmatite

SimpleLithology:pegmatite rdf:type owl:NamedIndividual ,
                                   owl:Thing ,
                                   skos:Concept ;
                          
                          skos:definition "Exceptionally coarse grained crystalline rock with interlocking crystals; most grains are 1cm or more diameter; composition is generally that of granite, but the term may refer to the coarse grained facies of any type of igneous rock;usually found as irregular dikes, lenses, or veins associated with plutons or batholiths." ;
                          
                          dc:source "Neuendorf et al. 2005" ;
                          
                          skos:prefLabel "Pegmatite"@en ;
                          
