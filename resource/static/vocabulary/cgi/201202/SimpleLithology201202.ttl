###  http://resource.geosciml.org/classifier/cgi/lithology/gabbro

SimpleLithology:gabbro rdf:type owl:NamedIndividual ,
                                owl:Thing ,
                                skos:Concept ;
                       
                       skos:prefLabel "Gabbro"@en ;
                       
                       skos:definition "Gabbroic rock that contains between 0 and 5 percent quartz and no feldspathoid mineral in the QAPF fraction.  Includes rocks defined modally in QAPF Field 10 as gabbro." ;
                       
                       dc:source "LeMaitre et al. 2002" ;
                       
                       rdfs:comment "Note that this category includes gabbro (sensu stricto) of LeMaitre et al. 2002, but is broader, including the other rock types defined by orthopyroxene-clinopyroxene-olivine-hornblende mineral ratios."@en ;
                       
                       skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lithology/0114>  ;
                       
                       skos:broader SimpleLithology:gabbroic_rock ;
                       
                       skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/simplelithology> .



###  http://resource.geosciml.org/classifier/cgi/lithology/gabbroic_rock

SimpleLithology:gabbroic_rock rdf:type owl:NamedIndividual ,
                                       owl:Thing ,
                                       skos:Concept ;
                              
                              dc:source "LeMaitre et al. 2002" ;
                              
                              rdfs:seeAlso "Gabbro (sensu stricto)"@en ;
                              
                              skos:prefLabel "Gabbroic rock"@en ;
                              
                              skos:definition "Gabbroid that has a plagioclase to total feldspar ratio greater than 0.9 in the QAPF fraction.  Includes QAPF fields 10*, 10, and 10'. This category includes the various categories defined in LeMaitre et al. (2002) based on the mafic mineralogy, but apparently not subdivided based on the quartz/feldspathoid content."@en ;
                              
                              rdfs:seeAlso "Gabbronorite"@en ,
                                           "Norite"@en ,
                                           "Troctolite"@en ;
                              
                              skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lithology/0115>  ;
                              
                              skos:broader SimpleLithology:basic_igneous_rock ;
                              
                              skos:narrower SimpleLithology:foid_bearing_gabbro ,
                                            SimpleLithology:gabbro ;
                              
                              skos:broader SimpleLithology:gabbroid ;
                              
                              skos:narrower SimpleLithology:quartz_gabbro ;
                              
                              skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/simplelithology> .



###  http://resource.geosciml.org/classifier/cgi/lithology/gabbroid

SimpleLithology:gabbroid rdf:type owl:NamedIndividual ,
                                  owl:Thing ,
                                  skos:Concept ;
                         
                         dc:source "LeMaitre et al. 2002" ;
                         
                         skos:prefLabel "Gabbroid"@en ;
                         
                         skos:definition "Phaneritic crystalline igneous rock that contains less than 90 percent mafic minerals, and up to 20 percent quartz or up to 10 percent feldspathoid in the QAPF fraction. The ratio of plagioclase to total feldspar is greater than 0.65, and anorthite content of the plagioclase is greater than 50 percent. Includes rocks defined modally in QAPF fields 9 and 10 and their subdivisions."@en ;
                         
                         skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lithology/0116>  ;
                         
