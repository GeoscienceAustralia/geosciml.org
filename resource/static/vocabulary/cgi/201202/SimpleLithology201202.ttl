                          skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/simplelithology> .



###  http://resource.geosciml.org/classifier/cgi/lithology/peridotite

SimpleLithology:peridotite rdf:type owl:NamedIndividual ,
                                    owl:Thing ,
                                    skos:Concept ;
                           
                           dc:source "LeMaitre et al. 2002" ;
                           
                           skos:prefLabel "Peridotite"@en ;
                           
                           skos:definition "Ultramafic rock consisting of more than 40 percent (by volume) olivine with pyroxene and/or amphibole and little or no feldspar. Commonly altered to serpentinite. Includes rocks defined modally in the ultramafic rock classification as dunite, harzburgite, lherzolite, wehrlite, olivinite, pyroxene peridotite, pyroxene hornblende peridotite or hornblende peridotite." ;
                           
                           rdfs:seeAlso "dunite"@en ,
                                        "harzburgite"@en ,
                                        "lherzolite"@en ,
                                        "olivinite"@en ,
                                        "wherlite"@en ;
                           
                           skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lithology/0188>  ;
                           
                           skos:broader SimpleLithology:phaneritic_igneous_rock ,
                                        SimpleLithology:ultramafic_igneous_rock ;
                           
                           skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/simplelithology> .



