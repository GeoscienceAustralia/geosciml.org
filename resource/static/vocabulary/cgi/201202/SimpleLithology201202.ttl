###  http://resource.geosciml.org/classifier/cgi/lithology/ooze

SimpleLithology:ooze rdf:type owl:NamedIndividual ,
                              owl:Thing ,
                              skos:Concept ;
                     
                     skos:definition "Biogenic sediment consisting of less than 1 percent gravel-size (greater than or equal to 2 mm) particles, with a sand to mud ratio less than 1 to 9, and less than 50 percent carbonate minerals."@en ;
                     
                     rdfs:comment "Neuendorf et al. 2005 put cutoff at 30 percent skeletal remains; this is raised to 50 percent in This vocabulary for consistency with definition of other Biogenic sediment category"@en ;
                     
                     skos:prefLabel "Ooze"@en ;
                     
                     dc:source "based on Bates and Jackson 1987 & Hallsworth & Knox 1999"@en ;
                     
                     skos:altLabel "biogenic mud"@en ;
                     
                     skos:historyNote "update definition, add mud as related term  in this version"@en ;
                     
                     skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lithology/0177>  ;
                     
                     skos:broader SimpleLithology:biogenic_sediment ;
                     
                     skos:narrower SimpleLithology:carbonate_ooze ;
                     
                     skos:broader SimpleLithology:mud_size_sediment ;
                     
                     skos:narrower SimpleLithology:siliceous_ooze ;
                     
                     skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/simplelithology> .



###  http://resource.geosciml.org/classifier/cgi/lithology/organic_bearing_mudstone

SimpleLithology:organic_bearing_mudstone rdf:type owl:NamedIndividual ,
                                                  owl:Thing ,
                                                  skos:Concept ;
                                         
                                         skos:definition "Mudstone that contains a significant amount of organic carbon, typically kerogen. Commonly finely laminated, brown or black in color."@en ;
                                         
                                         dc:source "Neuendorf et al. 2005; http://en.wikipedia.org/wiki/Oil_shale"@en ;
                                         
                                         skos:altLabel "Oil shale"@en ;
                                         
                                         skos:prefLabel "Organic bearing mudstone"@en ;
                                         
                                         rdfs:seeAlso "tar sand"@en ;
                                         
                                         skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lithology/0178>  ;
                                         
                                         skos:broader SimpleLithology:generic_mudstone ;
                                         
                                         skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/simplelithology> .



###  http://resource.geosciml.org/classifier/cgi/lithology/organic_rich_sediment

SimpleLithology:organic_rich_sediment rdf:type owl:NamedIndividual ,
                                               owl:Thing ,
                                               skos:Concept ;
                                      
                                      skos:definition "Sediment with color, composition, texture and apparent density indicating greater than 50 percent organic content by weight on a moisture-free basis." ;
                                      
                                      dc:source "SLTTs 2004" ;
                                      
                                      skos:prefLabel "Organic rich sediment"@en ;
                                      
                                      rdfs:comment "The broader relation from organic rich sediment to biogenic sediment is based on the inference that organic rich material is always biogenic in origin. Biogenic is a broader category because not all biogenic materials are organic rich, for example shells or phosphatic bone."@en ;
                                      
                                      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/lithology/0179>  ;
                                      
                                      skos:broader SimpleLithology:biogenic_sediment ,
                                                   SimpleLithology:organic_rich_sedimentary_material ;
                                      
                                      skos:narrower SimpleLithology:peat ,
                                                    SimpleLithology:sapropel ;
                                      
                                      skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/simplelithology> .



