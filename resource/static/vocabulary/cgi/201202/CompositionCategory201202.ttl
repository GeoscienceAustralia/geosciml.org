###  http://resource.geosciml.org/classifier/cgi/compositioncategory/arkosic_detrital_mode

CompositionCategory:arkosic_detrital_mode rdf:type owl:NamedIndividual ,
       owl:Thing ,
       skos:Concept ;
                    
                    skos:prefLabel "Arkosic detrital mode"@en ;
                    
                    dc:source "Folk1968"@en ;
                    
                    skos:definition "Sandstone petrographic composition term indicating that detrital mode for clastic rock (Q-F-L) includes less than 75 percent quartz and the feldspar to lithic ratio is greater than 3 to 1. This corresponds to the arkose field of Folk (1968). For clastic rocks with little or no matrix and cement, rocks in this detrital mode composition category will also have quartz-feldspathic mineralogical composition."@en ;
                    
                    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0002> ;
                    
                    skos:broader CompositionCategory:feldspathic_detrital_mode ;
                    
                    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/compositioncategory> .



###  http://resource.geosciml.org/classifier/cgi/compositioncategory/asphaltic

CompositionCategory:asphaltic rdf:type owl:NamedIndividual ,
                 owl:Thing ,
                 skos:Concept ;
        
        skos:prefLabel "Asphaltic"@en ;
        
        dc:source "Jackson 1997"@en ;
        
        skos:definition "Material contains a significant amount of asphalt, which is a 'dark brown to black viscous liquid or low-melting solid bitumen that consists almost entirely of carbon and hydrogen...[and] formed in oil-bearing rocks by the evaporation of volatiles...' (Jackson, 1997, p. 39)."@en ;
        
        skos:exactMatch <http://resource.geosciml.org/classifier/cgi/compositioncategory/0003> ;
        
        skos:broader CompositionCategory:carbonaceous ;
        
        skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/compositioncategory> .



