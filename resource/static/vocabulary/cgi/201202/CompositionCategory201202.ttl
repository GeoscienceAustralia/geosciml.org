skos:prefLabel rdf:type owl:DatatypeProperty .





#################################################################
#
#    Classes
#
#################################################################


###  http://www.w3.org/2002/07/owl#Thing

owl:Thing rdf:type owl:Class .



###  http://www.w3.org/2004/02/skos/core#Collection

skos:Collection rdf:type owl:Class ;
                
                rdfs:label "Collection"@en ;
                
                rdfs:subClassOf owl:Thing ;
                
                owl:disjointWith skos:Concept ,
           skos:ConceptScheme ;
                
                rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core"^^xsd:anyURI ;
                
                rdfs:comment """Labelled collections can be used with collectable semantic relation
      properties e.g. skos:narrower, where you would like a set of concepts to be displayed under a
      'node label' in the hierarchy."""@en .



###  http://www.w3.org/2004/02/skos/core#Concept

skos:Concept rdf:type owl:Class ;
             
             rdfs:label "Concept"@en ;
             
             rdfs:subClassOf owl:Thing ;
             
             owl:disjointWith skos:ConceptScheme ,
        skos:OrderedCollection ;
             
             rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core"^^xsd:anyURI .



###  http://www.w3.org/2004/02/skos/core#ConceptScheme

skos:ConceptScheme rdf:type owl:Class ;
                   
                   rdfs:label "Concept Scheme"@en ;
                   
                   rdfs:subClassOf owl:Thing ;
                   
                   owl:disjointWith skos:OrderedCollection ;
                   
                   rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core"^^xsd:anyURI ;
                   
                   rdfs:comment """A concept scheme may be defined to include concepts from different
      sources."""@en ,
          """Thesauri, classification schemes, subject heading lists, taxonomies,
      'folksonomies', and other types of controlled vocabulary are all examples of concept schemes.
      Concept schemes are also embedded in glossaries and terminologies."""@en .



###  http://www.w3.org/2004/02/skos/core#OrderedCollection

skos:OrderedCollection rdf:type owl:Class ;
 
 rdfs:label "Ordered Collection"@en ;
 
 rdfs:subClassOf skos:Collection ;
 
 rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core"^^xsd:anyURI ;
 
 rdfs:comment "Ordered collections can be used where you would like a set of concepts to be displayed in a specific order, and optionally under a 'node label'."@en .





