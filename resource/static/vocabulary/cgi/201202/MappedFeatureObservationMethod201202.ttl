###  http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/compilation

momclassifier:compilation rdf:type owl:NamedIndividual ,
                                   owl:Thing ,
                                   skos:Concept ;
                          
                          skos:prefLabel "Compilation"@en ;
                          
                          skos:definition "Mapped feature geometry derived from one or more published or unpublished data sources, involving some interpretation or generalization by the compiler; not an exact digital reproduction of the sources."@en ;
                          
                          dc:source "this vocabulary"@en ;
                          
                          skos:exactMatch <http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0001>  ;
                          
                          skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/mappedfeatureobservationmethod> ;
                          
                          skos:topConceptOf <http://resource.geosciml.org/classifierscheme/cgi/201202/mappedfeatureobservationmethod> .



###  http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/direct_observation

momclassifier:direct_observation rdf:type owl:NamedIndividual ,
                                          owl:Thing ,
                                          skos:Concept ;
                                 
                                 skos:prefLabel "Direct observation"@en ;
                                 
                                 dc:source "Federal Geographic Data Committee, 2006" ;
                                 
                                 skos:definition "Indicates that mapped feature was delineated and a specifier assigned by direct visual observation."@en ;
                                 
                                 skos:exactMatch <http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0002>  ;
                                 
                                 skos:narrower momclassifier:observed_aerial_imagery ,
                                               momclassifier:observed_borehole_core ,
                                               momclassifier:observed_outcrop ;
                                 
                                 skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/mappedfeatureobservationmethod> ;
                                 
                                 skos:topConceptOf <http://resource.geosciml.org/classifierscheme/cgi/201202/mappedfeatureobservationmethod> .



###  http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/inertial_survey

momclassifier:inertial_survey rdf:type owl:NamedIndividual ,
                                       owl:Thing ,
                                       skos:Concept ;
                              
                              skos:prefLabel "Inertial survey"@en ;
                              
                              skos:definition "mapped feature is located by inertial survey methods" ;
                              
                              dc:source "this vocabulary"@en ;
                              
                              skos:exactMatch <http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0003>  ;
                              
                              skos:broader momclassifier:surveyed ;
                              
                              skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/mappedfeatureobservationmethod> .



###  http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/inferred

momclassifier:inferred rdf:type owl:NamedIndividual ,
                                owl:Thing ,
                                skos:Concept ;
                       
                       dc:source "Federal Geographic Data Committee, 2006" ;
                       
                       skos:definition "Indicates that mapped feature is not directly observable, location and specification based on inference from available evidence." ;
                       
                       skos:prefLabel "Inferred"@en ;
                       
                       skos:exactMatch <http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0004>  ;
                       
                       skos:narrower momclassifier:inferred_drill_cuttings ,
                                     momclassifier:inferred_indirect_method ,
                                     momclassifier:inferred_projection__observed_locations ;
                       
                       skos:topConceptOf <http://resource.geosciml.org/classifierscheme/cgi/201202/mappedfeatureobservationmethod> ;
                       
                       skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/mappedfeatureobservationmethod> .



###  http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/inferred_aeromagnetic_survey

momclassifier:inferred_aeromagnetic_survey rdf:type owl:NamedIndividual ,
                                                    owl:Thing ,
                                                    skos:Concept ;
                                           
                                           skos:prefLabel "Inferred-aeromagnetic survey"@en ;
                                           
                                           dc:source "based on Matti et al 1997; definition this vocabulary" ;
                                           
                                           skos:definition "Location and specification inferred based on magnetic field data acquired by airborne sensor." ;
                                           
                                           skos:exactMatch <http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0005>  ;
                                           
                                           skos:broader momclassifier:inferred_indirect_method ;
                                           
                                           skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/mappedfeatureobservationmethod> .



###  http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/inferred_borehole_geophysical_log

momclassifier:inferred_borehole_geophysical_log rdf:type owl:NamedIndividual ,
                                                         owl:Thing ,
                                                         skos:Concept ;
                                                
                                                skos:prefLabel "Inferred-borehole geophysical log"@en ;
                                                
                                                skos:definition "Location and specification inferred based on measurements made by electronic bore hole logging device(s) that measure geophysical properties." ;
                                                
                                                dc:source "this vocabulary"@en ;
                                                
                                                skos:exactMatch <http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0006>  ;
                                                
                                                skos:broader momclassifier:inferred_indirect_method ;
                                                
                                                skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/mappedfeatureobservationmethod> .



###  http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/inferred_drill_cuttings

momclassifier:inferred_drill_cuttings rdf:type owl:NamedIndividual ,
                                               owl:Thing ,
                                               skos:Concept ;
                                      
                                      skos:definition "Geologic feature in bore hole inferred based on interpretation of drill cuttings." ;
                                      
                                      skos:prefLabel "Inferred-drill cuttings"@en ;
                                      
                                      dc:source "this vocabulary"@en ;
                                      
                                      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0007>  ;
                                      
                                      skos:broader momclassifier:inferred ;
                                      
                                      skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/mappedfeatureobservationmethod> .



###  http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/inferred_gravity_data

momclassifier:inferred_gravity_data rdf:type owl:NamedIndividual ,
                                             owl:Thing ,
                                             skos:Concept ;
                                    
                                    skos:definition "Location and specification inferred based on measured variations in the acceleration of gravity related to mass of underlying Earth material." ;
                                    
                                    skos:prefLabel "Inferred-gravity data"@en ;
                                    
                                    dc:source "based on Matti et al 1997; definition this vocabulary" ;
                                    
                                    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0008>  ;
                                    
                                    skos:broader momclassifier:inferred_indirect_method ;
                                    
                                    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/mappedfeatureobservationmethod> .



###  http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/inferred_ground_magnetic_survey

momclassifier:inferred_ground_magnetic_survey rdf:type owl:NamedIndividual ,
                                                       owl:Thing ,
                                                       skos:Concept ;
                                              
                                              skos:prefLabel "Inferred-ground magnetic survey"@en ;
                                              
                                              dc:source "based on Matti et al 1997; definition this vocabulary" ;
                                              
                                              skos:definition "Location and specification inferred based on magnetic field data acquired by sensor on the earth surface, either at manually occupied stations, or by a vehicle-based continuously collected sensor data." ;
                                              
                                              skos:exactMatch <http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0009>  ;
                                              
                                              skos:broader momclassifier:inferred_indirect_method ;
                                              
                                              skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/mappedfeatureobservationmethod> .



###  http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/inferred_indirect_method

momclassifier:inferred_indirect_method rdf:type owl:NamedIndividual ,
                                                owl:Thing ,
                                                skos:Concept ;
                                       
                                       skos:definition "Location and specification inferred based on properties related to lithology, e.g. geophysical or geochemical signature, vegetation, surface texture, reflectance." ;
                                       
                                       dc:source "based on Matti et al 1997; definition this vocabulary" ;
                                       
                                       skos:prefLabel "Inferred using indirect method"@en ;
                                       
                                       skos:exactMatch <http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0010>  ;
                                       
                                       skos:broader momclassifier:inferred ;
                                       
                                       skos:narrower momclassifier:inferred_aeromagnetic_survey ,
                                                     momclassifier:inferred_borehole_geophysical_log ,
                                                     momclassifier:inferred_gravity_data ,
                                                     momclassifier:inferred_ground_magnetic_survey ,
                                                     momclassifier:inferred_radiometric_survey ,
                                                     momclassifier:inferred_remote_sensing_imagery ;
                                       
                                       skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/mappedfeatureobservationmethod> .



###  http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/inferred_projection__observed_locations

momclassifier:inferred_projection__observed_locations rdf:type owl:NamedIndividual ,
                                                               owl:Thing ,
                                                               skos:Concept ;
                                                      
                                                      skos:prefLabel "Inferred projection between observed locations"@en ;
                                                      
                                                      skos:definition "Location and specification determined based on inferred continuity with observed locations, and assumption of continuity of the feature." ;
                                                      
                                                      dc:source "this vocabulary"@en ;
                                                      
                                                      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0011>  ;
                                                      
                                                      skos:broader momclassifier:inferred ;
                                                      
                                                      skos:narrower momclassifier:inferred_projection_covering_mapped_unit ,
                                                                    momclassifier:inferred_projection_unmapped_surficial_material ,
                                                                    momclassifier:inferred_projection_vegetation ;
                                                      
                                                      skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/mappedfeatureobservationmethod> .



###  http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/inferred_projection_covering_mapped_unit

momclassifier:inferred_projection_covering_mapped_unit rdf:type owl:NamedIndividual ,
                                                                owl:Thing ,
                                                                skos:Concept ;
                                                       
                                                       dc:source "Federal Geographic Data Committee, 2006" ;
                                                       
                                                       skos:prefLabel "Inferred projection beneath covering mapped unit"@en ;
                                                       
                                                       skos:definition "Phenomenon inferred to exist based on continuity with observed locations, and assumption of continuity of the feature. Mappable feature is covered by mapped younger geologic unit. Concealed feature."@en ;
                                                       
                                                       skos:exactMatch <http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0012>  ;
                                                       
                                                       skos:broader momclassifier:inferred_projection__observed_locations ;
                                                       
                                                       skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/mappedfeatureobservationmethod> .



###  http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/inferred_projection_unmapped_surficial_material

momclassifier:inferred_projection_unmapped_surficial_material rdf:type owl:NamedIndividual ,
                                                                       owl:Thing ,
                                                                       skos:Concept ;
                                                              
                                                              skos:definition "Location and specification determined based on inferred continuity with observed locations, and assumption of continuity of the feature. Not observable because of covering mantle of soil, talus or colluvium, not mapped as separate geologic units." ;
                                                              
                                                              skos:prefLabel "Inferred projection beneath unmapped surficial material"@en ;
                                                              
                                                              dc:source "this vocabulary"@en ;
                                                              
                                                              skos:exactMatch <http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0013>  ;
                                                              
                                                              skos:broader momclassifier:inferred_projection__observed_locations ;
                                                              
                                                              skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/mappedfeatureobservationmethod> .



###  http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/inferred_projection_vegetation

momclassifier:inferred_projection_vegetation rdf:type owl:NamedIndividual ,
                                                      owl:Thing ,
                                                      skos:Concept ;
                                             
                                             skos:prefLabel "Inferred projection-obscured by vegetation"@en ;
                                             
                                             skos:definition "Location and specification determined based on inferred continuity with observed locations, and assumption of continuity of the feature. Not observable because of obscuring vegetation, but inferred to crop out at surface." ;
                                             
                                             dc:source "this vocabulary"@en ;
                                             
                                             skos:exactMatch <http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0014>  ;
                                             
                                             skos:broader momclassifier:inferred_projection__observed_locations ;
                                             
                                             skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/mappedfeatureobservationmethod> .



###  http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/inferred_radiometric_survey

momclassifier:inferred_radiometric_survey rdf:type owl:NamedIndividual ,
                                                   owl:Thing ,
                                                   skos:Concept ;
                                          
                                          skos:definition "Location and specification inferred based on flux or intensity of radiation related to the decay of radioactive elements in observed Earth material. Use for both airborne and ground-surface based sensor." ;
                                          
                                          skos:prefLabel "Inferred-radiometric survey"@en ;
                                          
                                          dc:source "based on Matti et al 1997; definition this vocabulary" ;
                                          
                                          skos:exactMatch <http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0015>  ;
                                          
                                          skos:broader momclassifier:inferred_indirect_method ;
                                          
