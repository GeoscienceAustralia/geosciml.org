###  http://resource.geosciml.org/classifier/cgi/stratigraphicrank/complex

<http://resource.geosciml.org/classifier/cgi/stratigraphicrank/complex> rdf:type owl:NamedIndividual ,
                                                                                 owl:Thing ,
                                                                                 skos:Concept ;
                                                                        
                                                                        dc:source "Salvador, A., ed. 1994. International Stratigraphic Guide: International Union of Geological Sciences, Trondheim, NO, 214 p." ;
                                                                        
                                                                        skos:definition "A lithostratigraphic unit composed of diverse types of any class or classes of rock (sedimentary, igneous, metamorphic), and characterized by irregularly mixed lithology or by hightly complicated structure relations to the extent that the original sequence of the component rocks may be obscured and the individual rocks or rock sequence cannot be readily mapped."@en ;
                                                                        
                                                                        skos:prefLabel "Complex"@en ;
                                                                        
                                                                        skos:exactMatch <http://resource.geosciml.org/classifier/cgi/stratigraphicrank/0015> ;
                                                                        
                                                                        skos:related <http://resource.geosciml.org/classifier/cgi/stratigraphicrank/lithodeme> ;
                                                                        
                                                                        skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/stratigraphicrank> ;
                                                                        
                                                                        skos:topConceptOf <http://resource.geosciml.org/classifierscheme/cgi/201202/stratigraphicrank> .



###  http://resource.geosciml.org/classifier/cgi/stratigraphicrank/formation

<http://resource.geosciml.org/classifier/cgi/stratigraphicrank/formation> rdf:type owl:NamedIndividual ,
                                                                                   owl:Thing ,
                                                                                   skos:Concept ;
                                                                          
                                                                          skos:definition "A body of rock identified by lithic characteristics and stratigraphic position; it is usually but not necessarily tabular and is mappable at the Earth's surface or traceable in the subsurface." ;
                                                                          
                                                                          skos:prefLabel "Formation"@en ;
                                                                          
                                                                          dc:source "North American Commission on Stratigraphic Nomenclature, 2005; Salvador, 1994" ;
                                                                          
                                                                          skos:exactMatch <http://resource.geosciml.org/classifier/cgi/stratigraphicrank/0003> ;
                                                                          
                                                                          skos:narrowerTransitive <http://resource.geosciml.org/classifier/cgi/stratigraphicrank/bed> ;
                                                                          
                                                                          skos:broader <http://resource.geosciml.org/classifier/cgi/stratigraphicrank/group> ;
                                                                          
