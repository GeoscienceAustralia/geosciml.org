###  http://resource.geosciml.org/classifier/cgi/stratigraphicrank/group

<http://resource.geosciml.org/classifier/cgi/stratigraphicrank/group> rdf:type owl:NamedIndividual ,
                                                                               owl:Thing ,
                                                                               skos:Concept ;
                                                                      
                                                                      dc:source "North American Commission on Stratigraphic Nomenclature, 2005;  Salvador, 1994" ;
                                                                      
                                                                      skos:definition "A group is the lithostratigraphic unit next higher in rank to either subgroup or formation. A groups may consist of two or more subgroups or two or more formations or a combination of both." ;
                                                                      
                                                                      skos:prefLabel "Group"@en ;
                                                                      
                                                                      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/stratigraphicrank/0004> ;
                                                                      
                                                                      skos:narrower <http://resource.geosciml.org/classifier/cgi/stratigraphicrank/formation> ,
                                                                                    <http://resource.geosciml.org/classifier/cgi/stratigraphicrank/subgroup> ;
                                                                      
                                                                      skos:related <http://resource.geosciml.org/classifier/cgi/stratigraphicrank/suite> ;
                                                                      
                                                                      skos:broader <http://resource.geosciml.org/classifier/cgi/stratigraphicrank/supergroup> ;
                                                                      
                                                                      skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/stratigraphicrank> .



###  http://resource.geosciml.org/classifier/cgi/stratigraphicrank/lithodeme

