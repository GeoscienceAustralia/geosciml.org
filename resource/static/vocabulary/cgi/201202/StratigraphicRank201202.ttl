###  http://resource.geosciml.org/classifier/cgi/stratigraphicrank/bed

<http://resource.geosciml.org/classifier/cgi/stratigraphicrank/bed> rdf:type owl:NamedIndividual ,
                                                                             owl:Thing ,
                                                                             skos:Concept ;
                                                                    
                                                                    dc:source "North American Commission on Stratigraphic Nomenclature, 2005; Salvador, 1994" ;
                                                                    
                                                                    skos:definition "The smallest formal lithostratigraphic unit, usually a distinctive lithic entity which can be distinguished from adjacent rocks by one or more physical characteristics." ;
                                                                    
                                                                    skos:prefLabel "Bed"@en ;
                                                                    
                                                                    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/stratigraphicrank/0001> ;
                                                                    
                                                                    skos:broaderTransitive <http://resource.geosciml.org/classifier/cgi/stratigraphicrank/formation> ;
                                                                    
                                                                    skos:broader <http://resource.geosciml.org/classifier/cgi/stratigraphicrank/member> ;
                                                                    
                                                                    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/stratigraphicrank> .



