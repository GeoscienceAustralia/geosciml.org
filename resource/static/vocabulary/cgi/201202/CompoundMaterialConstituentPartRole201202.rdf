    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;vein">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition>Thin sheet of material intruded into the rock. May be hydrothermal, magmatic, or sedimentary.</skos:definition>
