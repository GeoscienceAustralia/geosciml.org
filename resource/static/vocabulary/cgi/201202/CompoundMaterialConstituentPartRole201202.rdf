        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;irregular_part"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;layer"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;overgrowth"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;small_concretion"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;vein"/>
        <skos:topConceptOf rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


