    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/armoured_relict_crystal -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;armoured_relict_crystal">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition>a crystal of an earlier rock that is prevented from further reaction in a later rock by a rim of reaction products</skos:definition>
        <dc:source>base on Neuendorf et al 2005</dc:source>
        <skos:prefLabel xml:lang="en">Armoured relict crystal</skos:prefLabel>
        <skos:exactMatch rdf:resource="&CompoundMaterialConstituentPartRole;0001"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;particulate_constituent"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/armoured_relict_inclusion -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;armoured_relict_inclusion">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition>an inclusion of an earlier rock that is prevented from further reaction in a later rock by a rim of reaction products</skos:definition>
        <skos:prefLabel xml:lang="en">Armoured relict inclusion</skos:prefLabel>
        <dc:source>base on Neuendorf et al 2005</dc:source>
        <skos:exactMatch rdf:resource="&CompoundMaterialConstituentPartRole;0002"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;inclusion"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/chadacryst -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;chadacryst">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Neuendorf et al., 2005</dc:source>
        <skos:prefLabel xml:lang="en">Chadacryst</skos:prefLabel>
        <skos:definition>the enclosed crystal in a poikolitic texture</skos:definition>
        <skos:exactMatch rdf:resource="&CompoundMaterialConstituentPartRole;0003"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;particulate_constituent"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/clast_supporting_orthomatrix -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;clast_supporting_orthomatrix">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en">Clast supporting orthomatrix</skos:prefLabel>
        <skos:definition>Orthomatrix in matrix supported sedimentary rock.</skos:definition>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:exactMatch rdf:resource="&CompoundMaterialConstituentPartRole;0004"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;orthomatrix"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/clot -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;clot">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition>A cluster of ferromagnesian minerals in an igneous rock, from several centimeters to decimeters in diameter, that may be a segregation or an altered xenolith.</skos:definition>
        <skos:prefLabel xml:lang="en">Clot</skos:prefLabel>
        <dc:source>Neuendorf et al, 2005</dc:source>
        <skos:exactMatch rdf:resource="&CompoundMaterialConstituentPartRole;0005"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;structural_part"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/constituent_role_not_specified -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;constituent_role_not_specified">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en">Constituent role not specified</skos:prefLabel>
        <skos:definition>For descriptions in which role of constituent in compound material is not specified.</skos:definition>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:exactMatch rdf:resource="&CompoundMaterialConstituentPartRole;0006"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
        <skos:topConceptOf rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/corona -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;corona">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition>Constituent occurs as a concentric envelope enclosing another constituent. Corona is a non-genetic term.</skos:definition>
        <dc:source>Williams, Turner and Gilbert, 1954; Spry, 1969; Fettes and Desmons, 2007</dc:source>
        <skos:prefLabel xml:lang="en">Corona</skos:prefLabel>
        <skos:exactMatch rdf:resource="&CompoundMaterialConstituentPartRole;0007"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;corrosion_rim"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;kelyphitic_rim"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;structural_part"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/corrosion_rim -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;corrosion_rim">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition>A corona formed by a secondary mineral around an orginal igneous crystal, formed by modification of the crystal by the corrosive action of its parent magma</skos:definition>
        <skos:prefLabel xml:lang="en">Corrosion rim</skos:prefLabel>
        <dc:source>Neuendorf et al., 2005</dc:source>
        <skos:exactMatch rdf:resource="&CompoundMaterialConstituentPartRole;0008"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;corona"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/crystalline_framework_constituent -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;crystalline_framework_constituent">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition>Constituent forms an interconnected network of discernible crystals.</skos:definition>
        <skos:prefLabel xml:lang="en">Crystalline framework constituent</skos:prefLabel>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:exactMatch rdf:resource="&CompoundMaterialConstituentPartRole;0009"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;particulate_constituent"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/dropstone -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;dropstone">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>NADM SLTTs 2004; Jackson, 1997, p 193</dc:source>
        <skos:definition>&quot;An oversized stone in laminated sediment that depresses the underlying laminae and may be covered by -draped laminae. Most dropstones originate through ice-rafting; other sources are floating tree roots and kelp holdfasts....&quot;</skos:definition>
        <skos:prefLabel xml:lang="en">Dropstone</skos:prefLabel>
        <skos:exactMatch rdf:resource="&CompoundMaterialConstituentPartRole;0010"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;structural_part"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/epimatrix -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;epimatrix">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>NADM SLTTs 2004; Dickinson, 1970</dc:source>
        <skos:definition>A type of matrix consisting of inhomogeneous interstitial materials grown in originally open interstices during diagenesis, but lacking the homogeneity and clear textural evidence of pore-filling needed to classify as phyllosilicate cement.</skos:definition>
        <skos:prefLabel xml:lang="en">Epimatrix</skos:prefLabel>
        <skos:exactMatch rdf:resource="&CompoundMaterialConstituentPartRole;0011"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;matrix"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/floating_clast -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;floating_clast">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Pettijohn et al., 1972, p. 90; SMR</dc:source>
        <skos:definition>Constituent is a clast that has no visible contacts with other clasts. Interpreted to be largely or completly immersed in matrix or cement.</skos:definition>
        <skos:prefLabel xml:lang="en">Floating clast</skos:prefLabel>
        <skos:exactMatch rdf:resource="&CompoundMaterialConstituentPartRole;0012"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;structural_part"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/granular_framework_constituent -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;granular_framework_constituent">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Jackson, 1997, p. 250</dc:source>
        <skos:definition>&quot;Constituent forms a rigid arrangement of particles that support one another at their points of contact...constituting a mechanically firm structure capable of supporting open pore spaces, although interstices may be occupied by cement or matrix&quot;</skos:definition>
        <skos:prefLabel xml:lang="en">Granular framework constituent</skos:prefLabel>
        <skos:exactMatch rdf:resource="&CompoundMaterialConstituentPartRole;0013"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;particulate_constituent"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/groundmass_constituent -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;groundmass_constituent">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition>Constituent forms an interconnected network of material enclosing other constituents that are disguished by larger grain size.</skos:definition>
        <skos:prefLabel xml:lang="en">Groundmass constituent</skos:prefLabel>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:exactMatch rdf:resource="&CompoundMaterialConstituentPartRole;0014"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;interstitial_constituent"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/inclusion -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;inclusion">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Neuendorf et al, 2005</dc:source>
        <skos:prefLabel xml:lang="en">Inclusion</skos:prefLabel>
        <skos:definition>a fragment of older material within an igneous rock to which it may or may not be genetically related</skos:definition>
        <skos:exactMatch rdf:resource="&CompoundMaterialConstituentPartRole;0015"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;armoured_relict_inclusion"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;structural_part"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/interstitial_constituent -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;interstitial_constituent">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition>Constituent is distributed through the material between other constituent particles</skos:definition>
        <skos:prefLabel xml:lang="en">Interstitial constituent</skos:prefLabel>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:exactMatch rdf:resource="&CompoundMaterialConstituentPartRole;0016"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;groundmass_constituent"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;matrix"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;sedimentary_rock_cement"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
        <skos:topConceptOf rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/irregular_part -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;irregular_part">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition>A constituent with irregular distribution and geometry, as in pseudobreccia or patch migmatite.</skos:definition>
        <skos:prefLabel xml:lang="en">Irregular part</skos:prefLabel>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:exactMatch rdf:resource="&CompoundMaterialConstituentPartRole;0017"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;structural_part"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/kelyphitic_rim -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;kelyphitic_rim">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition>Corona that consists of concentric bands with radial fibrous texture.</skos:definition>
        <dc:source>Williams, Turner and Gilbert, 1954; Fettes and Desmons, 2007</dc:source>
        <skos:prefLabel xml:lang="en">Kelyphitic rim</skos:prefLabel>
        <skos:exactMatch rdf:resource="&CompoundMaterialConstituentPartRole;0018"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;corona"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/layer -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;layer">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition>A thin sheet compositionally distinct from the surrounding material, related to primary genesis of rock, e.g. sedimentary layers, metamorphic segregation</skos:definition>
        <skos:prefLabel xml:lang="en">Layer</skos:prefLabel>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:exactMatch rdf:resource="&CompoundMaterialConstituentPartRole;0019"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;structural_part"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/matrix -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;matrix">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>NADM SLTTs 2004; Jackson, 1997</dc:source>
        <skos:definition>Constituent forms finer-grained material interstitial to a framework constituent. &quot;The finer-grained material enclosing, or filling the interstices between, the larger grains or particles of a sediment or sedimentary rock....The term refers to the relative size and disposition of the particles, and no particular particle size is implied&quot; (Jackson, 1997, p. 393). May be classifiable into orthomatrix, protomatrix, epimatrix, pseudomatrix, and unclassified matrix.</skos:definition>
        <skos:prefLabel xml:lang="en">Matrix</skos:prefLabel>
        <skos:exactMatch rdf:resource="&CompoundMaterialConstituentPartRole;0020"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;epimatrix"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;interstitial_constituent"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;orthomatrix"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;protomatrix"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;pseudomatrix"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/megacryst -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;megacryst">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition>any crystal in an igneous or metamorphic rock that is sgnificantly larger than the surounding groundmass. May be a phenocryst, xenocryst, porphyroblast or porphyroclast</skos:definition>
        <dc:source>Neuendorf et al 2005</dc:source>
        <skos:prefLabel xml:lang="en">Megacryst</skos:prefLabel>
        <skos:exactMatch rdf:resource="&CompoundMaterialConstituentPartRole;0021"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;particulate_constituent"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;phenocryst"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;porphyroblast"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;porphyroclast"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/oikocryst -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;oikocryst">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition>Constituent occurs as crystals that poikilitically enclose crystals of other phases in an igneous rock.</skos:definition>
        <dc:source>Neuendorf et al., 2005</dc:source>
        <skos:prefLabel xml:lang="en">Oikocryst</skos:prefLabel>
        <skos:exactMatch rdf:resource="&CompoundMaterialConstituentPartRole;0022"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;particulate_constituent"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/orthomatrix -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;orthomatrix">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>NADM SLTTs 2004; Dickinson, 1970</dc:source>
        <skos:definition>A type of matrix consisting of recrystallized detrital lutum (clay fraction, less than 2 micron) or protomatrix.</skos:definition>
        <skos:prefLabel xml:lang="en">Orthomatrix</skos:prefLabel>
        <skos:exactMatch rdf:resource="&CompoundMaterialConstituentPartRole;0023"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;clast_supporting_orthomatrix"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;matrix"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/overgrowth -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;overgrowth">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>NADM SLTTs 2004; Neuendorf et al. 2005</dc:source>
        <skos:definition>Constituent crystallized in crystallographic continuity with some other mineral constituent, typically quartz or calcite. In a clastic rock, commonly forms cement as well, but this should be represented using two role attribute links, &#39;overgrowth&#39; and cement, because overgrowth does not necessarily imply cement.</skos:definition>
        <skos:prefLabel xml:lang="en">Overgrowth</skos:prefLabel>
        <skos:exactMatch rdf:resource="&CompoundMaterialConstituentPartRole;0024"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;structural_part"/>
        <skos:inScheme rdf:resource="&cgi;compoundmaterialconstituentpartrole"/>
    </skos:Concept>
    


