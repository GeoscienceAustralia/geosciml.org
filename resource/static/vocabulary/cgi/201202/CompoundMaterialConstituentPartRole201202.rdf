    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/phenoclast -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;phenoclast">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Neuendorf et al., 2005</dc:source>
        <skos:prefLabel xml:lang="en">Phenoclast</skos:prefLabel>
        <skos:definition>a relatively large and conspicuous fragment in a sediment or sedimentary rock</skos:definition>
