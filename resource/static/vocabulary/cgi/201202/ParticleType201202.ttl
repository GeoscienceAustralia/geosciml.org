                                                                             skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0028> ;
                                                                             
                                                                             skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/aggregate_non-clastic_particle> ;
                                                                             
                                                                             skos:narrower <http://resource.geosciml.org/classifier/cgi/particletype/fiamme> ,
                                                                                           <http://resource.geosciml.org/classifier/cgi/particletype/glomerocryst> ,
                                                                                           <http://resource.geosciml.org/classifier/cgi/particletype/orb> ;
                                                                             
                                                                             skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/igneous_vein

<http://resource.geosciml.org/classifier/cgi/particletype/igneous_vein> rdf:type owl:NamedIndividual ,
                                                                                 owl:Thing ,
                                                                                 skos:Concept ;
                                                                        
                                                                        skos:prefLabel "Igneous vein"@en ;
                                                                        
                                                                        skos:definition "a tabular or sheet like part of a compound material formed by the intrusion of magma" ;
                                                                        
                                                                        dc:source "this vocabulary"@en ;
                                                                        
                                                                        skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0029> ;
                                                                        
                                                                        skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/intrusive_sheet> ;
                                                                        
                                                                        skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/intraclast

<http://resource.geosciml.org/classifier/cgi/particletype/intraclast> rdf:type owl:NamedIndividual ,
                                                                               owl:Thing ,
                                                                               skos:Concept ;
                                                                      
                                                                      dc:source "NADM SLTTs 2004; Neuendorf et al., 2005; Folk, 1959" ;
                                                                      
                                                                      skos:definition "A broad, general term introduced by Folk (1959, p. 4) for a component of limestone, representing a torn-up and re-worked fragment of a penecontemporaneous sediment (usually weakly consolidated) that has been eroded within the basin of deposition...and re-deposited there...The fragment may range in size from fine sand to gravel..." ;
                                                                      
                                                                      skos:prefLabel "Intraclast"@en ;
                                                                      
                                                                      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0030> ;
                                                                      
                                                                      skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/granular_particle> ;
                                                                      
                                                                      skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/intrusive_sheet

<http://resource.geosciml.org/classifier/cgi/particletype/intrusive_sheet> rdf:type owl:NamedIndividual ,
                                                                                    owl:Thing ,
                                                                                    skos:Concept ;
                                                                           
                                                                           skos:prefLabel "Intrusive sheet"@en ;
                                                                           
                                                                           skos:definition "a tabular or sheet-like part of a compound material, genetic origin not specified" ;
                                                                           
