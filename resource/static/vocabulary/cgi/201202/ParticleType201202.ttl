                                                                                         <http://resource.geosciml.org/classifier/cgi/particletype/ooid> ,
                                                                                         <http://resource.geosciml.org/classifier/cgi/particletype/pisoid> ;
                                                                           
                                                                           skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/cognate_fragments

<http://resource.geosciml.org/classifier/cgi/particletype/cognate_fragments> rdf:type owl:NamedIndividual ,
                                                                                      owl:Thing ,
                                                                                      skos:Concept ;
                                                                             
                                                                             dc:source "Base on Gillespie and Styles, 1999" ;
                                                                             
                                                                             skos:prefLabel "Cognate fragments"@en ;
                                                                             
                                                                             skos:definition "Pyroclast consisting of rock which formed during earlier (related) volcanic activity that have been ejected with other pyroclastic debris during a later eruption." ;
                                                                             
                                                                             skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0011> ;
                                                                             
                                                                             skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/pyroclast> ;
                                                                             
                                                                             skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/concretionary_particle

<http://resource.geosciml.org/classifier/cgi/particletype/concretionary_particle> rdf:type owl:NamedIndividual ,
                                                                                           owl:Thing ,
                                                                                           skos:Concept ;
                                                                                  
                                                                                  dc:source "Neuendorf et al, 2005" ;
                                                                                  
                                                                                  skos:definition "A hard, compact mass or aggregate of mineral matter, normally subsperical but commonly oblate, disc-shaped or irregular. Formed by precipitation of mineral from solution in the pores of a granular rock, localized around a nucleus or center, to define a discrete, sharply separated object. Size ranges from cm to meters. Particle geometry description for concretion describes the concretion size and shape, not the size and shape of particles forming the concretion." ;
                                                                                  
                                                                                  skos:prefLabel "Concretionary particle"@en ;
                                                                                  
                                                                                  skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0012> ;
                                                                                  
                                                                                  skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/aggregate_non-clastic_particle> ;
                                                                                  
                                                                                  skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/coprolite

<http://resource.geosciml.org/classifier/cgi/particletype/coprolite> rdf:type owl:NamedIndividual ,
                                                                              owl:Thing ,
                                                                              skos:Concept ;
                                                                     
                                                                     skos:definition "The fossilised excrement of vertebrate animals" ;
                                                                     
                                                                     skos:prefLabel "Coprolite"@en ;
                                                                     
                                                                     dc:source "Neuendorf et al., 2005" ;
                                                                     
                                                                     skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0013> ;
                                                                     
                                                                     skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/biogenic_particle> ;
                                                                     
                                                                     skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/crystal_fragment

<http://resource.geosciml.org/classifier/cgi/particletype/crystal_fragment> rdf:type owl:NamedIndividual ,
                                                                                     owl:Thing ,
                                                                                     skos:Concept ;
                                                                            
                                                                            skos:definition "A fragment of a crystal interpreted to have been broken by pyroclastic processes." ;
                                                                            
                                                                            skos:prefLabel "Crystal fragment"@en ;
                                                                            
                                                                            dc:source "this vocabulary"@en ;
                                                                            
                                                                            skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0014> ;
                                                                            
                                                                            skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/pyroclast> ;
                                                                            
                                                                            skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/crystalline_grain

<http://resource.geosciml.org/classifier/cgi/particletype/crystalline_grain> rdf:type owl:NamedIndividual ,
                                                                                      owl:Thing ,
                                                                                      skos:Concept ;
                                                                             
                                                                             skos:definition "Constituent that is the product of crystallization during formation of a compound Earth material (see NADMC1 2004). Serves to distinguish crystalline from granular rocks (Struik, 2002). Includes constituents crystallized in evaporite environments, during diagenesis or hydrothermal alteration, or in other low-temperature environments." ;
                                                                             
                                                                             skos:prefLabel "Crystalline grain"@en ;
                                                                             
                                                                             dc:source "Struik, 2002; NADM SLTTs 2004" ;
                                                                             
                                                                             skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0015> ;
                                                                             
                                                                             skos:narrower <http://resource.geosciml.org/classifier/cgi/particletype/crystallite> ,
                                                                                           <http://resource.geosciml.org/classifier/cgi/particletype/holoblast> ,
                                                                                           <http://resource.geosciml.org/classifier/cgi/particletype/microlite> ,
                                                                                           <http://resource.geosciml.org/classifier/cgi/particletype/paleoblast> ,
                                                                                           <http://resource.geosciml.org/classifier/cgi/particletype/pseudomorph> ;
                                                                             
                                                                             skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/solid_constituent_particle> ;
                                                                             
                                                                             skos:narrower <http://resource.geosciml.org/classifier/cgi/particletype/xenocryst> ;
                                                                             
                                                                             skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/crystallite

<http://resource.geosciml.org/classifier/cgi/particletype/crystallite> rdf:type owl:NamedIndividual ,
                                                                                owl:Thing ,
                                                                                skos:Concept ;
                                                                       
                                                                       dc:source "Williams,Turner,Gilbert, 1954" ;
                                                                       
                                                                       skos:definition "Constitutent occurs as minute, spherical, rod, or hair-like forms. Typically appear isotropic in thin section." ;
                                                                       
                                                                       skos:prefLabel "Crystallite"@en ;
                                                                       
                                                                       skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0016> ;
                                                                       
                                                                       skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/crystalline_grain> ;
                                                                       
                                                                       skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/epiclast

<http://resource.geosciml.org/classifier/cgi/particletype/epiclast> rdf:type owl:NamedIndividual ,
                                                                             owl:Thing ,
                                                                             skos:Concept ;
                                                                    
                                                                    skos:definition "An individual constituent, grain or fragment of a sediment or rock, produced by erosion of a larger rock mass. Particle whose origin as a fragment is a result of surface (sedimentary, weathering...) processes. 'Crystals, crystal fragments, glass and rock fragments that have been liberated from any type of pre-existing consolidated rock (volcanic or non-volcanic) by weathering or erosion and transported from the site of origin by gravity, air, water, or ice' [Schmid, 1981]. Distinguished from intraclast in that epiclast is derived from a pre-existing rock from outside the basin of deposition (NADM SLTTs 2004) before introduction into a CompoundMaterial (sense of NADMC1, 2004)." ;
                                                                    
                                                                    skos:prefLabel "Epiclast"@en ;
                                                                    
                                                                    dc:source "Schmid 1981"@en ;
                                                                    
                                                                    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0017> ;
                                                                    
                                                                    skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/granular_particle> ;
                                                                    
                                                                    skos:narrower <http://resource.geosciml.org/classifier/cgi/particletype/lithic_clast> ,
                                                                                  <http://resource.geosciml.org/classifier/cgi/particletype/mineral_clast> ;
                                                                    
                                                                    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/fecal_pellet

<http://resource.geosciml.org/classifier/cgi/particletype/fecal_pellet> rdf:type owl:NamedIndividual ,
                                                                                 owl:Thing ,
                                                                                 skos:Concept ;
                                                                        
                                                                        skos:prefLabel "Fecal pellet"@en ;
                                                                        
                                                                        skos:definition "an organic excrement, mainly of invertebrates, occuring especially in modern marine sediments but also fossilised in some sedimentary rocks, usually with simple ovoid form, less commonly rod-shaped." ;
                                                                        
                                                                        dc:source "Neuendorf et al., 2005" ;
                                                                        
                                                                        skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0018> ;
                                                                        
                                                                        skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/biogenic_particle> ,
                                                                                     <http://resource.geosciml.org/classifier/cgi/particletype/peloid> ;
                                                                        
                                                                        skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/fenestra

<http://resource.geosciml.org/classifier/cgi/particletype/fenestra> rdf:type owl:NamedIndividual ,
                                                                             owl:Thing ,
                                                                             skos:Concept ;
                                                                    
                                                                    skos:prefLabel "Fenestra"@en ;
                                                                    
                                                                    skos:definition "Primary or penecontemporaneous gap or cavity in the framework of a sedimentary rock, larger than grain-supported intersticies. May be open space or have partial to complete fill with secondary cement or introduced sediment." ;
                                                                    
                                                                    dc:source "based on Neuendorf et al 2005" ;
                                                                    
                                                                    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0019> ;
                                                                    
                                                                    skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/cavity> ;
                                                                    
                                                                    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/fiamme

<http://resource.geosciml.org/classifier/cgi/particletype/fiamme> rdf:type owl:NamedIndividual ,
                                                                           owl:Thing ,
                                                                           skos:Concept ;
                                                                  
                                                                  skos:prefLabel "Fiamme"@en ;
                                                                  
                                                                  skos:definition "Lens-shaped bodies, usually mm to cm thick, and centimeters to 1-2 decimeters long, typically seen on surfaces of some pyroclastic rocks. The name fiamme comes from the Italian word for flames, describing their shape. The term is descriptive and non-genetic.Generally interpreted to form by the collapse of pumice fragments during welding in a hot pyroclastic deposit." ;
                                                                  
                                                                  dc:source "Neuendorf et al 2005; http://en.wikipedia.org/wiki/Fiamme" ;
                                                                  
                                                                  skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0020> ;
                                                                  
                                                                  skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/igneous_inclusion> ;
                                                                  
                                                                  skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/fluid_inclusion

<http://resource.geosciml.org/classifier/cgi/particletype/fluid_inclusion> rdf:type owl:NamedIndividual ,
                                                                                    owl:Thing ,
                                                                                    skos:Concept ;
                                                                           
                                                                           skos:definition "A cavity within a crystal containing liquid and/or gas, formed by the entrapment in crystal irregularities of fluid, commonly that from which the mineral crystallised." ;
                                                                           
                                                                           skos:prefLabel "Fluid inclusion"@en ;
                                                                           
