                                                                                                  owl:Thing ,
                                                                                                  skos:Concept ;
                                                                                         
                                                                                         skos:definition "Fragment formed as a direct result of volcanic activity and reworked by sedimentary processes." ;
                                                                                         
                                                                                         dc:source "Gillespie and Styles 1999" ;
                                                                                         
                                                                                         skos:prefLabel "Reworked pyroclastic fragment"@en ;
                                                                                         
                                                                                         skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0051> ;
                                                                                         
                                                                                         skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/granular_particle> ;
                                                                                         
