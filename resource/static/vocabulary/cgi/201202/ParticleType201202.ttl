###  http://resource.geosciml.org/classifier/cgi/particletype/accidental_pyroclastic_fragment

<http://resource.geosciml.org/classifier/cgi/particletype/accidental_pyroclastic_fragment> rdf:type owl:NamedIndividual ,
                                                                                                    owl:Thing ,
                                                                                                    skos:Concept ;
                                                                                           
                                                                                           skos:definition "Fragment of rock generated by disruption as a direct result of volcanic action but not formed by previous activity of the magmatic system; generally derived from subvolcanic basement." ;
                                                                                           
                                                                                           skos:prefLabel "Accidental pyroclastic fragment"@en ;
                                                                                           
                                                                                           dc:source "Gillespie and Styles 1999" ;
                                                                                           
                                                                                           skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0001> ;
                                                                                           
                                                                                           skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/pyroclast> ;
                                                                                           
                                                                                           skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/aggregate_non-clastic_particle

<http://resource.geosciml.org/classifier/cgi/particletype/aggregate_non-clastic_particle> rdf:type owl:NamedIndividual ,
                                                                                                   owl:Thing ,
                                                                                                   skos:Concept ;
                                                                                          
                                                                                          skos:definition "Generic term for a non-clastic particle that is itself composed of an aggregation of particles;" ;
                                                                                          
                                                                                          skos:prefLabel "Aggregate non-clastic particle"@en ;
                                                                                          
                                                                                          dc:source "this vocabulary"@en ;
                                                                                          
                                                                                          skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0002> ;
                                                                                          
                                                                                          skos:narrower <http://resource.geosciml.org/classifier/cgi/particletype/axiolite> ,
                                                                                                        <http://resource.geosciml.org/classifier/cgi/particletype/concretionary_particle> ,
                                                                                                        <http://resource.geosciml.org/classifier/cgi/particletype/igneous_inclusion> ,
                                                                                                        <http://resource.geosciml.org/classifier/cgi/particletype/intrusive_sheet> ,
                                                                                                        <http://resource.geosciml.org/classifier/cgi/particletype/nodule> ,
                                                                                                        <http://resource.geosciml.org/classifier/cgi/particletype/schlieren> ;
                                                                                          
                                                                                          skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/solid_constituent_particle> ;
                                                                                          
                                                                                          skos:narrower <http://resource.geosciml.org/classifier/cgi/particletype/spherule> ;
                                                                                          
                                                                                          skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/amygdule

<http://resource.geosciml.org/classifier/cgi/particletype/amygdule> rdf:type owl:NamedIndividual ,
                                                                             owl:Thing ,
                                                                             skos:Concept ;
                                                                    
                                                                    skos:prefLabel "Amygdule"@en ;
                                                                    
                                                                    skos:definition "Cavity filled with secondary minerals; denotes that cavities are filled vesicles, thus restricted to volcanic rock." ;
                                                                    
                                                                    dc:source "based on Neuendorf et al 2005" ;
                                                                    
                                                                    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0003> ;
                                                                    
                                                                    skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/cavity> ;
                                                                    
                                                                    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/autoclast

<http://resource.geosciml.org/classifier/cgi/particletype/autoclast> rdf:type owl:NamedIndividual ,
                                                                              owl:Thing ,
                                                                              skos:Concept ;
                                                                     
                                                                     skos:definition "Fragments of extrusive igneous rock formed by mechanical friction of moving lava flow, breakage of chilled lava flow rinds, or gravity crumbling of active spines and domes. Gillespie and Styles (1999) include as type of juvenile pyroclastic fragment, inconsistent with definition of pyroclastic by IUGS and BGS." ;
                                                                     
                                                                     skos:prefLabel "Autoclast"@en ;
                                                                     
                                                                     dc:source "Gillespie and Styles,1999" ;
                                                                     
                                                                     skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0004> ;
                                                                     
                                                                     skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/granular_particle> ;
                                                                     
                                                                     skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/axiolite

<http://resource.geosciml.org/classifier/cgi/particletype/axiolite> rdf:type owl:NamedIndividual ,
                                                                             owl:Thing ,
                                                                             skos:Concept ;
                                                                    
                                                                    skos:prefLabel "Axiolite"@en ;
                                                                    
                                                                    dc:source "Williams,Turner,Gilbert 1954" ;
                                                                    
                                                                    skos:definition "Elongate to irregular lense with radial aggregates of acicular and fibrous mineral coalescing along a central axis." ;
                                                                    
                                                                    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0005> ;
                                                                    
                                                                    skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/aggregate_non-clastic_particle> ;
                                                                    
                                                                    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/bioclast

<http://resource.geosciml.org/classifier/cgi/particletype/bioclast> rdf:type owl:NamedIndividual ,
                                                                             owl:Thing ,
                                                                             skos:Concept ;
                                                                    
                                                                    skos:prefLabel "Bioclast"@en ;
                                                                    
                                                                    skos:definition "Granular sedimentary particle that is a \"fragmentary piece of a shell, bone, or other hard skeletal structure of an animal, plant, or protozoan. May be fossilized or non-fossilized\". Use in situations where fossil organism can not be identified, thus 'material fossil' is inappropriate." ;
                                                                    
                                                                    dc:source "NADM SLTTs 2004"@en ;
                                                                    
                                                                    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0006> ;
                                                                    
                                                                    skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/biogenic_particle> ;
                                                                    
                                                                    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/biogenic_particle

<http://resource.geosciml.org/classifier/cgi/particletype/biogenic_particle> rdf:type owl:NamedIndividual ,
                                                                                      owl:Thing ,
                                                                                      skos:Concept ;
                                                                             
                                                                             skos:definition "\"A type of granular particle formed by the physiological activities of organisms (plants, animals, protozoa) that produce body parts which subsequently are incorporated into a sediment aggregate. Contrast with nonbiogenic particle. Includes biogenic objects that are whole or fragmentary (bioclasts); can be fossilized or non-fossilized\"" ;
                                                                             
                                                                             skos:prefLabel "Biogenic particle"@en ;
                                                                             
                                                                             dc:source "NADM SLTTs 2004"@en ;
                                                                             
                                                                             skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0007> ;
                                                                             
                                                                             skos:narrower <http://resource.geosciml.org/classifier/cgi/particletype/bioclast> ,
                                                                                           <http://resource.geosciml.org/classifier/cgi/particletype/coprolite> ,
                                                                                           <http://resource.geosciml.org/classifier/cgi/particletype/fecal_pellet> ;
                                                                             
