                                                                                         skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/lithic_clast

<http://resource.geosciml.org/classifier/cgi/particletype/lithic_clast> rdf:type owl:NamedIndividual ,
                                                                                 owl:Thing ,
                                                                                 skos:Concept ;
                                                                        
                                                                        skos:definition "A clast derived by erosion from older, pre-existing rock materials of any kind, usually beyond the basin of accumulation but occasionally from within the basin of deposition (as from a submarine or subaqueous exposure adjacent to a depositional site). A clast that is identified as a piece of rock." ;
                                                                        
                                                                        skos:prefLabel "Lithic clast"@en ;
                                                                        
                                                                        dc:source "this vocabulary"@en ;
                                                                        
                                                                        skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0033> ;
                                                                        
                                                                        skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/epiclast> ;
                                                                        
                                                                        skos:narrower <http://resource.geosciml.org/classifier/cgi/particletype/lithoclast> ;
                                                                        
                                                                        skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/lithoclast

<http://resource.geosciml.org/classifier/cgi/particletype/lithoclast> rdf:type owl:NamedIndividual ,
                                                                               owl:Thing ,
                                                                               skos:Concept ;
                                                                      
                                                                      dc:source "Neuendorf et al., 2005, p. 375" ;
                                                                      
                                                                      skos:definition "\"A mechanically formed and deposited fragment of a carbonate rock...derived from an older, lithified limestone or dolomite within, adjacent to, or outside the depositional site\"" ;
                                                                      
                                                                      skos:prefLabel "Lithoclast"@en ;
                                                                      
                                                                      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0034> ;
                                                                      
                                                                      skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/lithic_clast> ;
                                                                      
                                                                      skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/lithophysae

<http://resource.geosciml.org/classifier/cgi/particletype/lithophysae> rdf:type owl:NamedIndividual ,
                                                                                owl:Thing ,
                                                                                skos:Concept ;
                                                                       
                                                                       skos:definition "Bubble-like cavity with concentric shells of finely crystalline minerals; open space remains in core of structure. Radiating fibrous structure may be present in secondary mineral fill. Typically in silicic volcanic rock." ;
                                                                       
                                                                       skos:prefLabel "Lithophysae"@en ;
                                                                       
                                                                       dc:source "Neuendorf et al 2005" ;
                                                                       
                                                                       skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0035> ;
                                                                       
                                                                       skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/cavity> ;
                                                                       
                                                                       skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/material_fossil

<http://resource.geosciml.org/classifier/cgi/particletype/material_fossil> rdf:type owl:NamedIndividual ,
                                                                                    owl:Thing ,
                                                                                    skos:Concept ;
                                                                           
                                                                           dc:source "GeoSciML v2 RC1 model element scope note" ;
                                                                           
                                                                           skos:prefLabel "Material fossil"@en ;
                                                                           
                                                                           skos:definition "The preserved remains or replaced remains (casts) of plants and animals. A fossil type may have one or more described associated organisms. If particle type is materialFossil, an additional type property element may provide a reference to a Paleontologic description of the fossil." ;
                                                                           
                                                                           skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0036> ;
                                                                           
                                                                           skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/biogenic_particle> ;
                                                                           
                                                                           skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/miarolitic_cavity

<http://resource.geosciml.org/classifier/cgi/particletype/miarolitic_cavity> rdf:type owl:NamedIndividual ,
                                                                                      owl:Thing ,
                                                                                      skos:Concept ;
                                                                             
                                                                             skos:definition "An irregular cavity in a phaneritic igneous rock into which small crystals of the rock-forming minerals protrude." ;
                                                                             
                                                                             skos:prefLabel "Miarolitic cavity"@en ;
                                                                             
                                                                             dc:source "Neuendorf et al 2005" ;
                                                                             
                                                                             skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0037> ;
                                                                             
                                                                             skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/vug> ;
                                                                             
                                                                             skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/microlite

<http://resource.geosciml.org/classifier/cgi/particletype/microlite> rdf:type owl:NamedIndividual ,
                                                                              owl:Thing ,
                                                                              skos:Concept ;
                                                                     
                                                                     skos:prefLabel "Microlite"@en ;
                                                                     
                                                                     dc:source "Williams,Turner,Gilbert, 1954" ;
                                                                     
                                                                     skos:definition "Constituent occurs as minute incipient crystals that display some birefringence in thin section." ;
                                                                     
                                                                     skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0038> ;
                                                                     
