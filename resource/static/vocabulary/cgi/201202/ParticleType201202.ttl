                                                                           skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/fluidal_pyroclast

<http://resource.geosciml.org/classifier/cgi/particletype/fluidal_pyroclast> rdf:type owl:NamedIndividual ,
                                                                                      owl:Thing ,
                                                                                      skos:Concept ;
                                                                             
                                                                             dc:source "Base on Gillespie and Styles, 1999" ;
                                                                             
                                                                             skos:prefLabel "Fluidal pyroclast"@en ;
                                                                             
                                                                             skos:definition "Pyroclast that has a a rounded, fluidal shape indicating that it was in a wholly or partly molten state during formation and subsequent transport. Concept corresponds to bomb, but with no size denotation." ;
                                                                             
                                                                             skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0022> ;
                                                                             
                                                                             skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/juvenile_pyroclastic_fragment> ;
                                                                             
                                                                             skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/glomerocryst

<http://resource.geosciml.org/classifier/cgi/particletype/glomerocryst> rdf:type owl:NamedIndividual ,
                                                                                 owl:Thing ,
                                                                                 skos:Concept ;
                                                                        
                                                                        skos:definition "An aggregate of crystals of the same mineral" ;
                                                                        
                                                                        skos:prefLabel "Glomerocryst"@en ;
                                                                        
                                                                        dc:source "Neuendorf et al., 2005" ;
                                                                        
                                                                        skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0023> ;
                                                                        
                                                                        skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/igneous_inclusion> ;
                                                                        
                                                                        skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/granular_particle

<http://resource.geosciml.org/classifier/cgi/particletype/granular_particle> rdf:type owl:NamedIndividual ,
                                                                                      owl:Thing ,
                                                                                      skos:Concept ;
                                                                             
                                                                             dc:source "NADM SLTTs 2004"@en ;
                                                                             
                                                                             skos:definition "Solid constituent in a compound earth material (sense of NADMC1) that is a pre-existing object before incorporation into a particulate aggregate. \"...a component of solid material that has the form of grains, clasts, fragments, or whole objects of any size, shape, composition, texture, and structure.\" (NADMSC SLTTs, 2004)" ;
                                                                             
                                                                             skos:prefLabel "Granular particle"@en ;
                                                                             
                                                                             skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0024> ;
                                                                             
                                                                             skos:narrower <http://resource.geosciml.org/classifier/cgi/particletype/autoclast> ,
                                                                                           <http://resource.geosciml.org/classifier/cgi/particletype/biogenic_particle> ,
                                                                                           <http://resource.geosciml.org/classifier/cgi/particletype/coated_particle> ,
                                                                                           <http://resource.geosciml.org/classifier/cgi/particletype/epiclast> ,
                                                                                           <http://resource.geosciml.org/classifier/cgi/particletype/intraclast> ,
                                                                                           <http://resource.geosciml.org/classifier/cgi/particletype/peloid> ,
                                                                                           <http://resource.geosciml.org/classifier/cgi/particletype/pyroclast> ,
                                                                                           <http://resource.geosciml.org/classifier/cgi/particletype/reworked_pyroclastic_fragment> ;
                                                                             
                                                                             skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/solid_constituent_particle> ;
                                                                             
                                                                             skos:narrower <http://resource.geosciml.org/classifier/cgi/particletype/syngenetic_particle> ,
                                                                                           <http://resource.geosciml.org/classifier/cgi/particletype/ventrifact> ;
                                                                             
                                                                             skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/holoblast

<http://resource.geosciml.org/classifier/cgi/particletype/holoblast> rdf:type owl:NamedIndividual ,
                                                                              owl:Thing ,
                                                                              skos:Concept ;
                                                                     
                                                                     dc:source "Neuendorf et al., 2005" ;
                                                                     
                                                                     skos:definition "A crystalline grain in a metamorphic rock that is newly and completely formed during metamorphism" ;
                                                                     
                                                                     skos:prefLabel "Holoblast"@en ;
                                                                     
                                                                     skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0025> ;
                                                                     
                                                                     skos:broader <http://resource.geosciml.org/classifier/cgi/particletype/crystalline_grain> ;
                                                                     
                                                                     skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/particletype> .



###  http://resource.geosciml.org/classifier/cgi/particletype/hydroclast

<http://resource.geosciml.org/classifier/cgi/particletype/hydroclast> rdf:type owl:NamedIndividual ,
                                                                               owl:Thing ,
                                                                               skos:Concept ;
                                                                      
                                                                      skos:prefLabel "Hydroclast"@en ;
                                                                      
                                                                      skos:definition "Juvenile pyroclastic fragment formed by magma-water interaction during subaqueous or subglacial extrusion; typically consist of chilled glass." ;
                                                                      
                                                                      dc:source "Gillespie and Styles 1999" ;
                                                                      
                                                                      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/particletype/0026> ;
                                                                      
