###  http://resource.geosciml.org/classifier/cgi/contacttype/alteration_facies_contact

contacttype2:alteration_facies_contact rdf:type owl:NamedIndividual ,
                                                owl:Thing ,
                                                skos:Concept ;
                                       
                                       skos:definition "A metasomatic facies contact separating rocks that have undergone alteration of a particular facies from those that have undergone metasomatism of another facies. Alteration is a kind of metasomatism that does not introduce economically important minerals." ;
                                       
                                       skos:prefLabel "Alteration facies contact"@en ;
                                       
                                       dc:source "this vocabulary"@en ;
                                       
                                       skos:exactMatch <http://resource.geosciml.org/classifier/cgi/contacttype/0001>  ;
                                       
                                       skos:broader contacttype2:metamorphic_contact ;
                                       
                                       skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/contacttype> .



###  http://resource.geosciml.org/classifier/cgi/contacttype/angular_unconformable_contact

contacttype2:angular_unconformable_contact rdf:type owl:NamedIndividual ,
                                                    owl:Thing ,
                                                    skos:Concept ;
                                           
                                           skos:definition "An unconformable contact between two geological units in which the older, underlying rocks dip at an angle different from the younger, overlying strata, usually in which younger sediments rest upon the eroded surface of tilted or folded older rocks." ;
                                           
                                           skos:prefLabel "Angular unconformable contact"@en ;
                                           
                                           dc:source "Neuendorf, K.K.E, Mehl, J.P. & Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p." ;
                                           
                                           skos:exactMatch <http://resource.geosciml.org/classifier/cgi/contacttype/0002>  ;
                                           
                                           skos:broader contacttype2:unconformable_contact ;
                                           
                                           skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/contacttype> .



###  http://resource.geosciml.org/classifier/cgi/contacttype/buttress_unconformity

contacttype2:buttress_unconformity rdf:type owl:NamedIndividual ,
                                            owl:Thing ,
                                            skos:Concept ;
                                   
                                   skos:definition "An unconformity in which onlapping strata are truncated against a steep topographic scarp." ;
                                   
                                   skos:prefLabel "Buttress unconformity"@en ;
                                   
                                   dc:source "Neuendorf, K.K.E, Mehl, J.P. & Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p." ;
                                   
                                   skos:exactMatch <http://resource.geosciml.org/classifier/cgi/contacttype/0003>  ;
                                   
                                   skos:broader contacttype2:unconformable_contact ;
                                   
                                   skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/contacttype> .



###  http://resource.geosciml.org/classifier/cgi/contacttype/chronostratigraphic_zone_contact

contacttype2:chronostratigraphic_zone_contact rdf:type owl:NamedIndividual ,
                                                       owl:Thing ,
                                                       skos:Concept ;
                                              
                                              skos:definition "A contact between bodies of material having different ages of origin." ;
                                              
                                              skos:prefLabel "Chronostratigraphic-zone contact"@en ;
                                              
                                              dc:source "FGDC"@en ;
                                              
                                              skos:exactMatch <http://resource.geosciml.org/classifier/cgi/contacttype/0004>  ;
                                              
                                              skos:broader contacttype2:contact ;
                                              
                                              skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/contacttype> .



###  http://resource.geosciml.org/classifier/cgi/contacttype/conductivity_contact

contacttype2:conductivity_contact rdf:type owl:NamedIndividual ,
                                           owl:Thing ,
                                           skos:Concept ;
                                  
                                  skos:definition "A geophysical contact between bodies of material distinguished based on electrical conductivity characteristics"@en ;
                                  
                                  skos:prefLabel "Conductivity contact"@en ;
                                  
                                  dc:source "this vocabulary"@en ;
                                  
                                  skos:historyNote "update definition to allow definition of unit based on any characteristics related to electrical conductivity. SMR 2011-02-16"@en ;
                                  
                                  skos:exactMatch <http://resource.geosciml.org/classifier/cgi/contacttype/0005>  ;
                                  
                                  skos:broader contacttype2:geophysical_contact ;
                                  
                                  skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/contacttype> .



###  http://resource.geosciml.org/classifier/cgi/contacttype/conformable_contact

contacttype2:conformable_contact rdf:type owl:NamedIndividual ,
                                          owl:Thing ,
                                          skos:Concept ;
                                 
                                 skos:prefLabel "Conformable contact"@en ;
                                 
                                 dc:source "Neuendorf, K.K.E, Mehl, J.P. & Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p." ;
                                 
                                 skos:definition "A contact separating two geological units in which the layers are formed one above the other in order by regular, uninterrupted deposition under the same general conditions." ;
                                 
                                 skos:exactMatch <http://resource.geosciml.org/classifier/cgi/contacttype/0006>  ;
                                 
                                 skos:broader contacttype2:depositional_contact ;
                                 
                                 skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/contacttype> .



###  http://resource.geosciml.org/classifier/cgi/contacttype/contact

contacttype2:contact rdf:type owl:NamedIndividual ,
                              owl:Thing ,
                              skos:Concept ;
                     
                     skos:definition "A surface that separates geologic units. Very general concept representing any kind of surface separating two geologic units, including primary boundaries such as depositional contacts, all kinds of unconformities, intrusive contacts, and gradational contacts, as well as faults that separate geologic units."@en ;
                     
                     skos:prefLabel "Contact"@en ;
                     
                     rdfs:comment "Note that usage of geologic unit here is per NADM C1 2004, denoting a body of material. Lines on geologic maps may also delineate featured defined geomorphically-- craters, terraces, shorelines, scarps that are not inherently geologic unit boundaries in this sense."@en ;
                     
                     dc:source "adapted from Jackson, 1997, page 137; NADM C1 2004"@en ;
                     
                     skos:exactMatch <http://resource.geosciml.org/classifier/cgi/contacttype/0007>  ;
                     
                     skos:narrower contacttype2:chronostratigraphic_zone_contact ,
                                   contacttype2:faulted_contact ,
                                   contacttype2:geologic_province_contact ,
                                   contacttype2:geophysical_contact ,
                                   contacttype2:glacial_stationary_line ,
                                   contacttype2:lithogenetic_contact ;
                     
                     skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/contacttype> ;
                     
                     skos:topConceptOf <http://resource.geosciml.org/classifierscheme/cgi/201202/contacttype> .



###  http://resource.geosciml.org/classifier/cgi/contacttype/deformation_zone_contact

contacttype2:deformation_zone_contact rdf:type owl:NamedIndividual ,
                                               owl:Thing ,
                                               skos:Concept ;
                                      
                                      skos:definition "A lithogenetic bundary separating rock masses that have different deformation structure, e.g. sheared rock against non sheared rock, brecciated rock against non-brecciated rock."@en ;
                                      
                                      skos:prefLabel "Deformation zone contact"@en ;
                                      
                                      dc:source "this vocabulary"@en ;
                                      
                                      skos:exactMatch <http://resource.geosciml.org/classifier/cgi/contacttype/0008>  ;
                                      
                                      skos:broader contacttype2:lithogenetic_contact ;
                                      
                                      skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/contacttype> .



###  http://resource.geosciml.org/classifier/cgi/contacttype/density_contact

contacttype2:density_contact rdf:type owl:NamedIndividual ,
                                      owl:Thing ,
                                      skos:Concept ;
                             
                             skos:definition "A geophysical contact separating bodies of material with different density characteristics, generally determined through measurement and modelling of gravity variations."@en ;
                             
                             skos:prefLabel "Density contact"@en ;
                             
                             skos:historyNote "modify definitionn to allow unit definition based on any density-based characteristic SMR 2011-02-16"@en ;
                             
                             dc:source "this vocabulary"@en ;
                             
                             skos:exactMatch <http://resource.geosciml.org/classifier/cgi/contacttype/0009>  ;
                             
                             skos:broader contacttype2:geophysical_contact ;
                             
                             skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/contacttype> .



###  http://resource.geosciml.org/classifier/cgi/contacttype/depositional_contact

contacttype2:depositional_contact rdf:type owl:NamedIndividual ,
                                           owl:Thing ,
                                           skos:Concept ;
                                  
                                  skos:prefLabel "Depositional contact"@en ;
                                  
                                  skos:definition "Lithogenetic contact at which a sedimentary or volcanic rock has been deposited on (or against) another rock body. The relationship between the older underlying rocks and younger overlying rocks is unknown or not specfied." ;
                                  
                                  dc:source "this vocabulary"@en ;
                                  
                                  skos:exactMatch <http://resource.geosciml.org/classifier/cgi/contacttype/0010>  ;
                                  
                                  skos:narrower contacttype2:conformable_contact ;
                                  
                                  skos:broader contacttype2:lithogenetic_contact ;
                                  
                                  skos:narrower contacttype2:unconformable_contact ;
                                  
                                  skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/contacttype> .



###  http://resource.geosciml.org/classifier/cgi/contacttype/disconformable_contact

contacttype2:disconformable_contact rdf:type owl:NamedIndividual ,
                                             owl:Thing ,
                                             skos:Concept ;
                                    
                                    skos:definition "An unconformable contact between two geological units in which the bedding of the older, underlying unit is parallel to the bedding of the younger, overlying unit, but in which the contact between the two units is marked by an irregular or uneven surface of appreciable relief." ;
                                    
                                    skos:prefLabel "Disconformable contact"@en ;
                                    
                                    dc:source "Neuendorf, K.K.E, Mehl, J.P. & Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p." ;
                                    
                                    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/contacttype/0011>  ;
                                    
                                    skos:broader contacttype2:unconformable_contact ;
                                    
                                    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/contacttype> .



###  http://resource.geosciml.org/classifier/cgi/contacttype/faulted_contact

contacttype2:faulted_contact rdf:type owl:NamedIndividual ,
                                      owl:Thing ,
                                      skos:Concept ;
                             
                             skos:definition "A contact separating two bodies of material across which one body has slid past the other."@en ;
                             
                             skos:prefLabel "Faulted contact"@en ;
                             
                             dc:source "Neuendorf, K.K.E, Mehl, J.P. & Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p." ;
                             
                             skos:exactMatch <http://resource.geosciml.org/classifier/cgi/contacttype/0012>  ;
                             
                             skos:broader contacttype2:contact ;
                             
                             skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/contacttype> .



###  http://resource.geosciml.org/classifier/cgi/contacttype/geologic_province_contact

contacttype2:geologic_province_contact rdf:type owl:NamedIndividual ,
                                                owl:Thing ,
                                                skos:Concept ;
                                       
                                       dc:source "Neuendorf, K.K.E, Mehl, J.P. & Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p." ;
                                       
                                       skos:definition "A contact between regions characterised by their geological history or by similar structural, petrographic or stratigraphic features"@en ;
                                       
                                       skos:prefLabel "Geologic province contact"@en ;
                                       
                                       skos:exactMatch <http://resource.geosciml.org/classifier/cgi/contacttype/0013>  ;
                                       
                                       skos:broader contacttype2:contact ;
                                       
                                       skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/201202/contacttype> .



###  http://resource.geosciml.org/classifier/cgi/contacttype/geophysical_contact

contacttype2:geophysical_contact rdf:type owl:NamedIndividual ,
                                          owl:Thing ,
                                          skos:Concept ;
                                 
                                 skos:definition "A contact separating bodies of material in the earth that have different geophysical properties. Use for boundaries that are detected by geophysical sensor techniques as opposed to direct lithologic observation." ;
                                 
