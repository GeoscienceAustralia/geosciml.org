    <!-- http://resource.geosciml.org/classifier/cgi/eventprocess/mass_wasting -->

    <skos:Concept rdf:about="&eventprocess;mass_wasting">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
