    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/anastomosing_spaced_cleavage -->

    <skos:Concept rdf:about="&foliationtype;anastomosing_spaced_cleavage">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Term from Geological Survey Victoria, Australia</dc:source>
        <skos:prefLabel xml:lang="en">Anastomosing spaced cleavage</skos:prefLabel>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">spaced cleavage domains branch and rejoin, creating lens-shaped microlithons.</skos:definition>
        <skos:exactMatch rdf:resource="&foliationtype;0001"/>
        <skos:broader rdf:resource="&foliationtype;spaced_cleavage"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/bedding_fabric -->

    <skos:Concept rdf:about="&foliationtype;bedding_fabric">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en">&gt;Fabric defined by textural or mineralogic variations in sediment produced by depositional processes. May be manifested by lamination structure within the sediment, or by layering in which the individual layers (beds) are bounded by discontinuities related to events in the depositional history.</skos:definition>
        <skos:prefLabel xml:lang="en">Bedding fabric</skos:prefLabel>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source>SLTTs 2004; this vocabulary</dc:source>
        <skos:exactMatch rdf:resource="&foliationtype;0002"/>
        <skos:narrower rdf:resource="&foliationtype;bedding_lamination_structure"/>
        <skos:narrower rdf:resource="&foliationtype;crude_or_indistinct_bedding"/>
        <skos:narrower rdf:resource="&foliationtype;parallel_bedding"/>
        <skos:broader rdf:resource="&foliationtype;primary_foliation"/>
        <skos:narrower rdf:resource="&foliationtype;sedimentary_layering"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/bedding_lamination_structure -->

    <skos:Concept rdf:about="&foliationtype;bedding_lamination_structure">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en">A surface internal to the containing sediment mass defined by textural or mineralogical variation, produced by sediment deposition process, and reflecting the orientation of the original depositional surface. Syn Laminated structure (SLTTs 2004).</skos:definition>
        <dc:source>SLTTs 2004</dc:source>
        <skos:prefLabel xml:lang="en">Bedding lamination structure</skos:prefLabel>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:exactMatch rdf:resource="&foliationtype;0003"/>
        <skos:broader rdf:resource="&foliationtype;bedding_fabric"/>
        <skos:narrower rdf:resource="&foliationtype;cryptomicrobial_lamination"/>
        <skos:narrower rdf:resource="&foliationtype;fissile_lamination"/>
        <skos:narrower rdf:resource="&foliationtype;impersistent_lamination"/>
        <skos:narrower rdf:resource="&foliationtype;irregular_lamination"/>
        <skos:narrower rdf:resource="&foliationtype;parallel_bedding_lamination"/>
        <skos:narrower rdf:resource="&foliationtype;planar_lamination"/>
        <skos:narrower rdf:resource="&foliationtype;undulating_lamination"/>
        <skos:narrower rdf:resource="&foliationtype;varve_lamination"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/c_fabric -->

    <skos:Concept rdf:about="&foliationtype;c_fabric">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en">C fabric</skos:prefLabel>
        <dc:source>Geoscience Victoria, Australia</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Ductile shear banding foliation defined by spaced, discrete planar zones of high strain less than 1mm thick. Commonly associated with a particle flattening (&#39;S&#39;) fabric to form a mylonitic foliation.</skos:definition>
        <skos:exactMatch rdf:resource="&foliationtype;0004"/>
        <skos:broader rdf:resource="&foliationtype;ductile_shear_banding_foliation"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/cleavage -->

    <skos:Concept rdf:about="&foliationtype;cleavage">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en">A tectonic foliation in a rock characterized by a tendency for the rock to split along a regular set of parallel or sub-parallel closely spaced surfaces. Cleavage is a more general term than schistosity because schistosity need not be present in order for a rock to display cleavage. Cleavage domains or parting surfaces are not spaced more than 5 cm apart on average (Borradaille et al., 1982)</skos:definition>
        <dc:source xml:lang="en">Brodie, Kate, Fettes, Douglas, and Harte, Ben, 2007, Structural terms including fault rock terms, in Fettes, D., and Desmons, J., Metamorphic rocks-A classification and Glossary of Terms: Cambridge, UK, Cam-bridge University Press, p. 24-31.</dc:source>
        <skos:prefLabel xml:lang="en">Cleavage</skos:prefLabel>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:exactMatch rdf:resource="&foliationtype;0005"/>
        <skos:narrower rdf:resource="&foliationtype;cleavage_parallel_to_bedding"/>
        <skos:narrower rdf:resource="&foliationtype;close_joints"/>
        <skos:narrower rdf:resource="&foliationtype;continuous_cleavage"/>
        <skos:narrower rdf:resource="&foliationtype;crenulation_cleavage"/>
        <skos:narrower rdf:resource="&foliationtype;disjunctive_cleavage"/>
        <skos:narrower rdf:resource="&foliationtype;domainal_cleavage"/>
        <skos:broader rdf:resource="&foliationtype;foliation"/>
        <skos:narrower rdf:resource="&foliationtype;rough_cleavage"/>
        <skos:narrower rdf:resource="&foliationtype;spaced_cleavage"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/cleavage_parallel_to_bedding -->

    <skos:Concept rdf:about="&foliationtype;cleavage_parallel_to_bedding">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>this vocabulary</dc:source>
        <skos:prefLabel xml:lang="en">Cleavage parallel to bedding</skos:prefLabel>
        <skos:definition xml:lang="en">Composite fabric in which a cleavage is developed parallel to bedding.</skos:definition>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:exactMatch rdf:resource="&foliationtype;0006"/>
        <skos:broader rdf:resource="&foliationtype;cleavage"/>
        <skos:broader rdf:resource="&foliationtype;compound_folation"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/close_joints -->

    <skos:Concept rdf:about="&foliationtype;close_joints">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Borradaile et al. 1982; This Vocabulary</dc:source>
        <skos:prefLabel xml:lang="en">Close joints</skos:prefLabel>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Foliation defined by parting surfaces (cleavage domains) that have no apparent thickness, are spaced between 5 cm and 25 cm, and do not crenulate an older foliation. Fabric intermediate between typical cleavage and joints. Parting surfaces are regularly spaced, and penetrative on a 1-10 m scale. 5 cm upper limit on spacing of &#39;cleavage domains&#39; in a &#39;cleavage &#39;fabric is suggested by Borradaile et al. [1982].</skos:definition>
        <skos:exactMatch rdf:resource="&foliationtype;0007"/>
        <skos:broader rdf:resource="&foliationtype;cleavage"/>
        <skos:broader rdf:resource="&foliationtype;joint_fabric"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/comb_layering -->

    <skos:Concept rdf:about="&foliationtype;comb_layering">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Neuendorf et al. 2005; this vocabulary</dc:source>
        <skos:prefLabel xml:lang="en">Comb layering</skos:prefLabel>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Layering fabric defined by layers that have internal comb structure. Layer boundaries are the termination surfaces for the constituent crystals.</skos:definition>
        <skos:exactMatch rdf:resource="&foliationtype;0008"/>
        <skos:broader rdf:resource="&foliationtype;layering"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/compositional_layering -->

    <skos:Concept rdf:about="&foliationtype;compositional_layering">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en">Compositional layering</skos:prefLabel>
        <dc:source>this vocabulary, submitted by British Antarctic Survey</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Layering fabric defined by layers that have different mineralogic composition.</skos:definition>
        <skos:exactMatch rdf:resource="&foliationtype;0009"/>
        <skos:broader rdf:resource="&foliationtype;layering"/>
        <skos:narrower rdf:resource="&foliationtype;metamorphic_differentiation_layering"/>
        <skos:narrower rdf:resource="&foliationtype;metamorphic_layering_inherited_from_protolith"/>
        <skos:narrower rdf:resource="&foliationtype;mineralogical_layering"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/compound_folation -->

    <skos:Concept rdf:about="&foliationtype;compound_folation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>this vocabulary</dc:source>
        <skos:prefLabel xml:lang="en">Compound folation</skos:prefLabel>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Foliation that includes fabric elements indicating classification with foliation types of different genetic origin, e.g. cleavage parallel to bedding is a compound fabric with a sedimentary foliation and a tectonic foliation. No connotation of genetic origin of fabric elements.</skos:definition>
        <skos:exactMatch rdf:resource="&foliationtype;0010"/>
        <skos:narrower rdf:resource="&foliationtype;cleavage_parallel_to_bedding"/>
        <skos:narrower rdf:resource="&foliationtype;compound_metamorphic_foliation"/>
        <skos:broader rdf:resource="&foliationtype;foliation"/>
        <skos:narrower rdf:resource="&foliationtype;layered_mylonitic_foliation"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/compound_metamorphic_foliation -->

    <skos:Concept rdf:about="&foliationtype;compound_metamorphic_foliation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>this vocabulary</dc:source>
        <skos:definition xml:lang="en">A compound foliation in which the constituent fabric elements are of metamorphic origin.</skos:definition>
        <skos:prefLabel xml:lang="en">Compound metamorphic foliation</skos:prefLabel>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:exactMatch rdf:resource="&foliationtype;0011"/>
        <skos:broader rdf:resource="&foliationtype;compound_folation"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/continuous_cleavage -->

    <skos:Concept rdf:about="&foliationtype;continuous_cleavage">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Borradaile et al 1982</dc:source>
        <skos:definition xml:lang="en">A cleavage that is statistically homogeneous down to the scale of individual mineral grains.</skos:definition>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Continuous cleavage</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0012"/>
        <skos:broader rdf:resource="&foliationtype;cleavage"/>
        <skos:narrower rdf:resource="&foliationtype;continuous_crenulation_cleavage"/>
        <skos:narrower rdf:resource="&foliationtype;phyllitic_cleavage"/>
        <skos:narrower rdf:resource="&foliationtype;slaty_cleavage"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/continuous_crenulation_cleavage -->

    <skos:Concept rdf:about="&foliationtype;continuous_crenulation_cleavage">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Borradaile et al 1982</dc:source>
        <skos:definition xml:lang="en">Cleavage defined by long, thin limbs of crenulation folds of pre-existing foliation, in which the boundaries between cleavage domains and lithons are gradational. Both the crenulation foliation and the crenulated (older) foliation are easily visible. Grades to spaced crenulation cleavage as boundaries of cleavage domains become sharp, and to schistosity or compositional layering as the crenulation foliation obscures the crenulated foliation to become the dominant fabric in the rock. In many rocks with continuous crenulation cleavage, the tendency to part along the cleavage may be weak, thus the rock may not technically meet the definition of &#39;cleavage as used in this vocabulary.</skos:definition>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Continuous crenulation cleavage</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0013"/>
        <skos:broader rdf:resource="&foliationtype;continuous_cleavage"/>
        <skos:broader rdf:resource="&foliationtype;crenulation_cleavage"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/crenulation_cleavage -->

    <skos:Concept rdf:about="&foliationtype;crenulation_cleavage">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Brodie, Kate, Fettes, Douglas, and Harte, Ben, 2007, Structural terms including fault rock terms, in Fettes, D., and Desmons, J., Metamorphic rocks-A classification and Glossary of Terms: Cambridge, UK, Cam-bridge University Press, p. 24-31.</dc:source>
        <skos:definition xml:lang="en">Cleavage that is overprinted on an older foliation that is folded to some degree in association with development of the younger cleavage. Cleavage domains commonly associated with long limbs of crenulation folds, and hinge surfaces of folds are roughly parallel. Wavelength of crenulation folds is less than or equal to 1 cm. Cleavage defined by limbs of microfolds coincident with zones of mineral differentiation</skos:definition>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Crenulation cleavage</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0014"/>
        <skos:broader rdf:resource="&foliationtype;cleavage"/>
        <skos:narrower rdf:resource="&foliationtype;continuous_crenulation_cleavage"/>
        <skos:narrower rdf:resource="&foliationtype;discrete_crenulation_cleavage"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/crude_or_indistinct_bedding -->

    <skos:Concept rdf:about="&foliationtype;crude_or_indistinct_bedding">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en">Bedding defined by surfaces that are not distinct, e.g. by gradational grain size variations.</skos:definition>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source>this vocabulary</dc:source>
        <skos:prefLabel xml:lang="en">Crude or indistinct bedding</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0015"/>
        <skos:broader rdf:resource="&foliationtype;bedding_fabric"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/cryptomicrobial_lamination -->

    <skos:Concept rdf:about="&foliationtype;cryptomicrobial_lamination">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>GGIPAC; http://www.brs.gov.au/land&amp;water/groundwater/result.internal_stratification.html</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Cryptomicrobial lamination</skos:prefLabel>
        <skos:definition xml:lang="en">Fabric defined by fine laminae formed by the trapping of grains and lime precipitation in a microbial mat.</skos:definition>
        <skos:exactMatch rdf:resource="&foliationtype;0016"/>
        <skos:broader rdf:resource="&foliationtype;bedding_lamination_structure"/>
        <skos:narrower rdf:resource="&foliationtype;stromatolitic_lamination"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/cumulate_layering -->

    <skos:Concept rdf:about="&foliationtype;cumulate_layering">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Carmichael et al 1974; Best 1982</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Cumulate layering</skos:prefLabel>
        <skos:definition xml:lang="en">Layering in igneous rocks in which layers are characterized by variation in relative proportion of magmatically crystallized minerals. Typically a pattern of mineralogical variation will be repeated many times in a vertical section with layers of relatively consistent thickness. See photos in Best [1982] p. 176-177. Gravity-stratified layers typically have mafic minerals at the base and plagioclase at the top and are interpreted to form by crystal settling in magma.</skos:definition>
        <skos:exactMatch rdf:resource="&foliationtype;0017"/>
        <skos:broader rdf:resource="&foliationtype;igneous_layering"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/discrete_cleavage -->

    <skos:Concept rdf:about="&foliationtype;discrete_cleavage">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Borradaile et al., 1982</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Discrete cleavage</skos:prefLabel>
        <skos:definition xml:lang="en">Spaced cleavage in which cleavage domains have sharply defined edges.</skos:definition>
        <skos:exactMatch rdf:resource="&foliationtype;0018"/>
        <skos:broader rdf:resource="&foliationtype;spaced_cleavage"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/discrete_crenulation_cleavage -->

    <skos:Concept rdf:about="&foliationtype;discrete_crenulation_cleavage">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Borradaile et al 1982</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Crenulation cleavage in which the cleavage domains have discrete boundaries. Amplitude of crenulation folds tends to be small.</skos:definition>
        <skos:prefLabel xml:lang="en">Discrete crenulation cleavage</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0019"/>
        <skos:broader rdf:resource="&foliationtype;crenulation_cleavage"/>
        <skos:broader rdf:resource="&foliationtype;spaced_cleavage"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/discrete_disjunct_spaced_cleavage -->

    <skos:Concept rdf:about="&foliationtype;discrete_disjunct_spaced_cleavage">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source>Neuendorf et al. 2005; Borradaile et al. 1982.</dc:source>
        <skos:prefLabel xml:lang="en">Discrete disjunct spaced cleavage</skos:prefLabel>
        <skos:definition xml:lang="en">Disjunct spaced cleavage defined by discrete cleavage surfaces, typically in low-grade clastic rocks. Syn. Fracture cleavage.</skos:definition>
        <skos:exactMatch rdf:resource="&foliationtype;0020"/>
        <skos:broader rdf:resource="&foliationtype;spaced_cleavage"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/disjunctive_cleavage -->

    <skos:Concept rdf:about="&foliationtype;disjunctive_cleavage">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Borradaile et al. 1982</dc:source>
        <skos:definition xml:lang="en">A spaced cleavage independent of any pre-existing mineral orientation; cleavage in which there is no systematic relationship between a pre-existing preferred orientation and the superimposed cleavage. (Borradaile et al., 1982)</skos:definition>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Disjunctive cleavage</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0021"/>
        <skos:broader rdf:resource="&foliationtype;cleavage"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/domainal_cleavage -->

    <skos:Concept rdf:about="&foliationtype;domainal_cleavage">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>this vocabulary</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Domainal cleavage</skos:prefLabel>
        <skos:definition xml:lang="en">Fabric in which domains of more strongly developed (closely spaced) cleavage are separated by lenses of weakly or non-cleaved rock. Typically observed on outcrop scale and used for geologic unit description</skos:definition>
        <skos:exactMatch rdf:resource="&foliationtype;0022"/>
        <skos:broader rdf:resource="&foliationtype;cleavage"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/ductile_shear_banding_foliation -->

    <skos:Concept rdf:about="&foliationtype;ductile_shear_banding_foliation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source>Geoscience Victoria, Australia</dc:source>
        <skos:prefLabel xml:lang="en">Ductile shear banding foliation</skos:prefLabel>
        <skos:definition xml:lang="en">Foliation defined by regularly spaced ductile shear bands at the scale of descripiton.</skos:definition>
        <skos:exactMatch rdf:resource="&foliationtype;0023"/>
        <skos:narrower rdf:resource="&foliationtype;c_fabric"/>
        <skos:broader rdf:resource="&foliationtype;foliation"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/eutaxitic_foliation -->

    <skos:Concept rdf:about="&foliationtype;eutaxitic_foliation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Neuendorf et al. 2005; this vocabulary</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Eutaxitic foliation</skos:prefLabel>
        <skos:definition xml:lang="en">Foliation defined by flattened pumice clasts or glass shards, aligned elongate lithic fragments, variations in composition, vesicularity, crystallinity, grain size, spherulite and lithophysae abundance, or the degree of devitrification in a welded tuff. This term denotes interpretation that foliation formed during compaction and welding of tuff.</skos:definition>
        <skos:exactMatch rdf:resource="&foliationtype;0024"/>
        <skos:broader rdf:resource="&foliationtype;primary_igneous_foliation"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/fissile_lamination -->

    <skos:Concept rdf:about="&foliationtype;fissile_lamination">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Diffuse to distinct lamination in mudstones and mud, defined by the alignment of clay minerals. Lacks distinct very-fine scale layering and repetition of couplets that characterize varves. Essentially a continuous cleavage parallel to bedding, but considered a sedimentary structure related to sediment compaction during diagenesis.</skos:definition>
        <dc:source>SLTTs 2004; this vocabulary</dc:source>
        <skos:prefLabel xml:lang="en">Fissile lamination</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0025"/>
        <skos:broader rdf:resource="&foliationtype;bedding_lamination_structure"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/flaser_structure -->

    <skos:Concept rdf:about="&foliationtype;flaser_structure">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Flaser structure</skos:prefLabel>
        <dc:source>Neuendorf et al. 2005</dc:source>
        <skos:definition xml:lang="en">Tectonic foliation defined by lenses and layers of original or relatively unaltered granular minerals surrounded by a matrix of highly sheared and crushed material. No connotation of metamorphic grade.</skos:definition>
        <skos:exactMatch rdf:resource="&foliationtype;0026"/>
        <skos:broader rdf:resource="&foliationtype;tectonic_foliation"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/flow_layering -->

    <skos:Concept rdf:about="&foliationtype;flow_layering">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>this vocabulary</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Flow layering</skos:prefLabel>
        <skos:definition xml:lang="en">Layering produced by flow of magma, lava, or diapiric flow of sedimentary rock. Distinction from eutaxitic foliation may be difficult.</skos:definition>
        <skos:exactMatch rdf:resource="&foliationtype;0027"/>
        <skos:narrower rdf:resource="&foliationtype;lava_flow_banding"/>
        <skos:broader rdf:resource="&foliationtype;layering"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/foliation -->

    <skos:Concept rdf:about="&foliationtype;foliation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Jackson 1997</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Fabric defined by the planar arrangement of textural or structural features (fabric elements).</skos:definition>
        <skos:prefLabel xml:lang="en">Foliation</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0028"/>
        <skos:narrower rdf:resource="&foliationtype;cleavage"/>
        <skos:narrower rdf:resource="&foliationtype;compound_folation"/>
        <skos:narrower rdf:resource="&foliationtype;ductile_shear_banding_foliation"/>
        <skos:narrower rdf:resource="&foliationtype;grain_shape_foliation"/>
        <skos:narrower rdf:resource="&foliationtype;joint_fabric"/>
        <skos:narrower rdf:resource="&foliationtype;layering"/>
        <skos:narrower rdf:resource="&foliationtype;low_temperature_flow_foliation"/>
        <skos:narrower rdf:resource="&foliationtype;primary_foliation"/>
        <skos:narrower rdf:resource="&foliationtype;schistosity"/>
        <skos:narrower rdf:resource="&foliationtype;tectonic_foliation"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
        <skos:topConceptOf rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/generic_mylonitic_foliation -->

    <skos:Concept rdf:about="&foliationtype;generic_mylonitic_foliation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source>NADM metamorphic rock vocabulary SLTTm1.0 2004</dc:source>
        <skos:definition xml:lang="en">Foliation defined by the shapes of deformed mineral grains or grain aggregates having aspect ratios greater than 1.5:1, greater than 10 percent of the rock is composed of &#39;matrix&#39; showing evidence of tectonic reduction in grain size, and the foliation and matrix are interpreted to be the product of continuous, crystal-plastic deformation processes. Generic class is used when data are insufficient to determine classification to protomylontic, mylonite, or ultramylonitc foliation, or to specify that any sort of mylontic foliation may be present.</skos:definition>
        <skos:prefLabel xml:lang="en">Generic mylonitic foliation</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0029"/>
        <skos:narrower rdf:resource="&foliationtype;layered_mylonitic_foliation"/>
        <skos:narrower rdf:resource="&foliationtype;mylonite_foliation"/>
        <skos:narrower rdf:resource="&foliationtype;protomylonite_foliation"/>
        <skos:broader rdf:resource="&foliationtype;tectonic_foliation"/>
        <skos:narrower rdf:resource="&foliationtype;ultramylonite_foliation"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/gneissic_layering -->

    <skos:Concept rdf:about="&foliationtype;gneissic_layering">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en">Compositional layering in a metamorphic rock; syn. gneissic banding</skos:definition>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Gneissic layering</skos:prefLabel>
        <dc:source>this vocabulary</dc:source>
        <skos:exactMatch rdf:resource="&foliationtype;0030"/>
        <skos:broader rdf:resource="&foliationtype;metamorphic_layering"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/grain_shape_foliation -->

    <skos:Concept rdf:about="&foliationtype;grain_shape_foliation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Foliation defined by the alignment of oblate particles, which may be detrital grains, crystals, or crystalline grain aggregates.</skos:definition>
        <dc:source>this vocabulary</dc:source>
        <skos:prefLabel xml:lang="en">Grain shape foliation</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0031"/>
        <skos:broader rdf:resource="&foliationtype;foliation"/>
        <skos:narrower rdf:resource="&foliationtype;imbricated_clast_fabric"/>
        <skos:narrower rdf:resource="&foliationtype;s_fabric"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/igneous_flow_foliation -->

    <skos:Concept rdf:about="&foliationtype;igneous_flow_foliation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>this vocabulary</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Foliation interpreted to be due to flow in a body of magma or lava. Includes flow banding in lava, and foliation defined by aligned crystals in a phaneritic igneous rock..</skos:definition>
        <skos:prefLabel xml:lang="en">Igneous flow foliation</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0032"/>
        <skos:broader rdf:resource="&foliationtype;primary_igneous_foliation"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/igneous_lamination -->

    <skos:Concept rdf:about="&foliationtype;igneous_lamination">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>this vocabulary</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Foliation defined by surfaces of textural or mineralogical variation formed by igneous processes. If the lamination surfaces are regularly spaced, they may be used to define layers.</skos:definition>
        <skos:prefLabel xml:lang="en">Igneous lamination</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0033"/>
        <skos:broader rdf:resource="&foliationtype;igneous_layering"/>
        <skos:broader rdf:resource="&foliationtype;primary_igneous_foliation"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/igneous_layering -->

    <skos:Concept rdf:about="&foliationtype;igneous_layering">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Geoscience Victoria, Australia</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Igneous layering</skos:prefLabel>
        <skos:definition xml:lang="en">Layering that is the product of processes operating in magma or lava. syn. magmatic layering</skos:definition>
        <skos:exactMatch rdf:resource="&foliationtype;0034"/>
        <skos:narrower rdf:resource="&foliationtype;cumulate_layering"/>
        <skos:narrower rdf:resource="&foliationtype;igneous_lamination"/>
        <skos:narrower rdf:resource="&foliationtype;igneous_textural_layering"/>
        <skos:narrower rdf:resource="&foliationtype;lava_flow_banding"/>
        <skos:broader rdf:resource="&foliationtype;layering"/>
        <skos:broader rdf:resource="&foliationtype;primary_igneous_foliation"/>
        <skos:narrower rdf:resource="&foliationtype;rhythmic_layering"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/igneous_textural_layering -->

    <skos:Concept rdf:about="&foliationtype;igneous_textural_layering">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>this vocabulary</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Igneous textural layering</skos:prefLabel>
        <skos:definition xml:lang="en">Layering defined by grain size/ grain shape variations resulting from crystallization processes in igneous rock, most commonly observed in pegmatite/aplite dikes or bodies, and in Laramide leucogranites of Wilderness suite in Arizona.</skos:definition>
        <skos:exactMatch rdf:resource="&foliationtype;0035"/>
        <skos:broader rdf:resource="&foliationtype;igneous_layering"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/imbricated_clast_fabric -->

    <skos:Concept rdf:about="&foliationtype;imbricated_clast_fabric">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>based on Neuendorf et al 2005</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Fabric defined by long axis of tabular clasts stacked like shingles to define a surface that makes a small angle with the bedding surface. A depositional fabric formed by disk-shaped or elongate pebbles all tilted in the same direction; flat sides commonly dip upstream</skos:definition>
        <skos:prefLabel xml:lang="en">Imbricated clast fabric</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0036"/>
        <skos:broader rdf:resource="&foliationtype;grain_shape_foliation"/>
        <skos:broader rdf:resource="&foliationtype;primary_foliation"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/impersistent_lamination -->

    <skos:Concept rdf:about="&foliationtype;impersistent_lamination">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>this vocabulary</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Impersistent lamination</skos:prefLabel>
        <skos:definition xml:lang="en">Lamination defined by discontinuous surfaces</skos:definition>
        <skos:exactMatch rdf:resource="&foliationtype;0037"/>
        <skos:broader rdf:resource="&foliationtype;bedding_lamination_structure"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/irregular_lamination -->

    <skos:Concept rdf:about="&foliationtype;irregular_lamination">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source>this vocabulary</dc:source>
        <skos:prefLabel xml:lang="en">Irregular lamination</skos:prefLabel>
        <skos:definition xml:lang="en">Lamination fabric in which individual lamina or groups of lamina are warped in an irregular fashion, like a flattened peice of crumpled paper. A crude planar orientation is discernible.</skos:definition>
        <skos:exactMatch rdf:resource="&foliationtype;0038"/>
        <skos:broader rdf:resource="&foliationtype;bedding_lamination_structure"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/joint_fabric -->

    <skos:Concept rdf:about="&foliationtype;joint_fabric">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Borradaile et al 1982</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Fabric defined by parting surfaces in rock that occur in crudely parallel orientation, spaced greater than 5 cm on average if in a very regular joint set. Spacing is more variable and alignment of surfaces is cruder that cleavage. Typically applies to geologic unit description.</skos:definition>
        <skos:prefLabel xml:lang="en">Joint fabric</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0039"/>
        <skos:narrower rdf:resource="&foliationtype;close_joints"/>
        <skos:broader rdf:resource="&foliationtype;foliation"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/laminated_metamorphic_layering -->

    <skos:Concept rdf:about="&foliationtype;laminated_metamorphic_layering">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Laminated metamorphic layering</skos:prefLabel>
        <skos:definition xml:lang="en">Metamorphic layering in which the layers are less than 1 cm thick. See Weiss, L.E. [1972] plate 36 or Borraidaile et al [1982] plate 137A,B (page 331), plate140A (page 337) for examples.</skos:definition>
        <dc:source>Weiss 1972; Borradaile et al 1982; this vocabulary</dc:source>
        <skos:exactMatch rdf:resource="&foliationtype;0040"/>
        <skos:broader rdf:resource="&foliationtype;metamorphic_layering"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/lava_flow_banding -->

    <skos:Concept rdf:about="&foliationtype;lava_flow_banding">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>GGIPAC, Australia</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Lava flow banding</skos:prefLabel>
        <skos:definition xml:lang="en">Layering formed by laminar lava flow, defined by variations in composition, vesicularity, crystallinity, grainsize, spherulite and lithophysae abundance, degree of devitrification, colour, and/or parting surfaces; syn. flow lamination, flow banding</skos:definition>
        <skos:exactMatch rdf:resource="&foliationtype;0041"/>
        <skos:broader rdf:resource="&foliationtype;flow_layering"/>
        <skos:broader rdf:resource="&foliationtype;igneous_layering"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/layered_mylonitic_foliation -->

    <skos:Concept rdf:about="&foliationtype;layered_mylonitic_foliation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>this vocabulary</dc:source>
        <skos:definition xml:lang="en">Compound foliation in which mylonitic foliation is present in rock that has continuous compositional layering. Continuous means individual layers can be traced more than 1 m.</skos:definition>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Layered mylonitic foliation</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0042"/>
        <skos:broader rdf:resource="&foliationtype;compound_folation"/>
        <skos:broader rdf:resource="&foliationtype;generic_mylonitic_foliation"/>
        <skos:broader rdf:resource="&foliationtype;layering"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/layering -->

    <skos:Concept rdf:about="&foliationtype;layering">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>this vocabulary</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Foliation defined by stacked, differentiable, sheet-like masses of material (layers), with no denotation of the nature (composition, grain size, grain shape...) or origin (igneous, sedimentary, metamorphic) of the layers.</skos:definition>
        <skos:prefLabel xml:lang="en">Layering</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0043"/>
        <skos:narrower rdf:resource="&foliationtype;comb_layering"/>
        <skos:narrower rdf:resource="&foliationtype;compositional_layering"/>
        <skos:narrower rdf:resource="&foliationtype;flow_layering"/>
        <skos:broader rdf:resource="&foliationtype;foliation"/>
        <skos:narrower rdf:resource="&foliationtype;igneous_layering"/>
        <skos:narrower rdf:resource="&foliationtype;layered_mylonitic_foliation"/>
        <skos:narrower rdf:resource="&foliationtype;metamorphic_layering"/>
        <skos:narrower rdf:resource="&foliationtype;sedimentary_layering"/>
        <skos:narrower rdf:resource="&foliationtype;tectonic_layering"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/low_temperature_flow_foliation -->

    <skos:Concept rdf:about="&foliationtype;low_temperature_flow_foliation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en">Foliation interpreted to be due to flow in a body of rock (typically mudrock, salt, or anhydrite) under low-temperature (non-metamorphic) conditions, typically related to diapirism.</skos:definition>
        <skos:prefLabel xml:lang="en">Low-temperature flow foliation</skos:prefLabel>
        <dc:source>this vocabulary</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:exactMatch rdf:resource="&foliationtype;0044"/>
        <skos:broader rdf:resource="&foliationtype;foliation"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/metamorphic_differentiation_layering -->

    <skos:Concept rdf:about="&foliationtype;metamorphic_differentiation_layering">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Layering defined by mineralogical variations in a rock body interpreted to be result of solid-state reactions in rock body (metamorphic segregation).</skos:definition>
        <skos:prefLabel xml:lang="en">Metamorphic differentiation layering</skos:prefLabel>
        <dc:source>this vocabulary</dc:source>
        <skos:exactMatch rdf:resource="&foliationtype;0045"/>
        <skos:broader rdf:resource="&foliationtype;compositional_layering"/>
        <skos:broader rdf:resource="&foliationtype;metamorphic_layering"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/metamorphic_layering -->

    <skos:Concept rdf:about="&foliationtype;metamorphic_layering">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Layering of unspecified origin in a metamorphic rock. Compositional or textural variations may be inherited from protolith, or result from metamorphic processes.</skos:definition>
        <skos:prefLabel xml:lang="en">Metamorphic layering</skos:prefLabel>
        <dc:source>this vocabulary</dc:source>
        <skos:exactMatch rdf:resource="&foliationtype;0046"/>
        <skos:narrower rdf:resource="&foliationtype;gneissic_layering"/>
        <skos:narrower rdf:resource="&foliationtype;laminated_metamorphic_layering"/>
        <skos:broader rdf:resource="&foliationtype;layering"/>
        <skos:narrower rdf:resource="&foliationtype;metamorphic_differentiation_layering"/>
        <skos:narrower rdf:resource="&foliationtype;metamorphic_layering_inherited_from_protolith"/>
        <skos:narrower rdf:resource="&foliationtype;stromatic_layering"/>
        <skos:narrower rdf:resource="&foliationtype;vague_metamorphic_layering"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/metamorphic_layering_inherited_from_protolith -->

    <skos:Concept rdf:about="&foliationtype;metamorphic_layering_inherited_from_protolith">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>this vocabulary</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Metamorphic layering in which layer differntiation is relict of compositional variations in the protolith, typically heterogeneous sedimentary or volcanic rock.</skos:definition>
        <skos:prefLabel xml:lang="en">Metamorphic layering inherited from protolith</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0047"/>
        <skos:broader rdf:resource="&foliationtype;compositional_layering"/>
        <skos:broader rdf:resource="&foliationtype;metamorphic_layering"/>
        <skos:narrower rdf:resource="&foliationtype;transposed_bedding_layering"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/mineralogical_layering -->

    <skos:Concept rdf:about="&foliationtype;mineralogical_layering">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>this vocabulary</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Generic layering defined by variations in mineralogy between individual sheets of material</skos:definition>
        <skos:prefLabel xml:lang="en">Mineralogical layering</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0048"/>
        <skos:broader rdf:resource="&foliationtype;compositional_layering"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/mylonite_foliation -->

    <skos:Concept rdf:about="&foliationtype;mylonite_foliation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>SLTTm 2004; Sibson 1977</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Generic mylonitic foliation in which 50-90 percent of rock is matrix due to tectonic grain size reduction processes. Planar fabric is very apparent due to flattening of mineral grains, development of shear surfaces, and alignment of tabular mineral grains. Lineation is typically present and easily observed.</skos:definition>
        <skos:prefLabel xml:lang="en">Mylonite foliation</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0049"/>
        <skos:broader rdf:resource="&foliationtype;generic_mylonitic_foliation"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/parallel_bedding -->

    <skos:Concept rdf:about="&foliationtype;parallel_bedding">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en">Bedding defined by parallel surfaces. Bedding structure may be lamination or layering.</skos:definition>
        <skos:prefLabel xml:lang="en">Parallel bedding</skos:prefLabel>
        <dc:source>SLTTs 2004</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:exactMatch rdf:resource="&foliationtype;0050"/>
        <skos:broader rdf:resource="&foliationtype;bedding_fabric"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/parallel_bedding_lamination -->

    <skos:Concept rdf:about="&foliationtype;parallel_bedding_lamination">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Parallel bedding lamination</skos:prefLabel>
        <dc:source>this vocabulary</dc:source>
        <skos:definition xml:lang="en">Bedding lamination structure in which all visible bedding surfaces are parallel.</skos:definition>
        <skos:exactMatch rdf:resource="&foliationtype;0051"/>
        <skos:broader rdf:resource="&foliationtype;bedding_lamination_structure"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/phyllitic_cleavage -->

    <skos:Concept rdf:about="&foliationtype;phyllitic_cleavage">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en">Continuous cleavage in phyllitic rock; phyllosilicate mineral grains just barely visible, grain size midway between slaty cleavage and schist; typically has a satiny lustre.</skos:definition>
        <dc:source>SLTTm 2004; Geoscience Victoria, Australia</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Phyllitic cleavage</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0052"/>
        <skos:broader rdf:resource="&foliationtype;continuous_cleavage"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/planar_lamination -->

    <skos:Concept rdf:about="&foliationtype;planar_lamination">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Lamination that is defined by planar bedding surfaces at the scale of description.</skos:definition>
        <dc:source>this vocabulary</dc:source>
        <skos:prefLabel xml:lang="en">Planar lamination</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0053"/>
        <skos:broader rdf:resource="&foliationtype;bedding_lamination_structure"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/primary_foliation -->

    <skos:Concept rdf:about="&foliationtype;primary_foliation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source>this vocabulary</dc:source>
        <skos:definition xml:lang="en">Foliation formed during original process of deposition or crystallization of an igneous or sedimentary material.</skos:definition>
        <skos:prefLabel xml:lang="en">Primary foliation</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0054"/>
        <skos:narrower rdf:resource="&foliationtype;bedding_fabric"/>
        <skos:broader rdf:resource="&foliationtype;foliation"/>
        <skos:narrower rdf:resource="&foliationtype;imbricated_clast_fabric"/>
        <skos:narrower rdf:resource="&foliationtype;primary_igneous_foliation"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/primary_igneous_foliation -->

    <skos:Concept rdf:about="&foliationtype;primary_igneous_foliation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Foliation interpreted to be the product of igneous processes, formed in the rock while melt was still present..</skos:definition>
        <dc:source>this vocabulary</dc:source>
        <skos:prefLabel xml:lang="en">Primary igneous foliation</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0055"/>
        <skos:narrower rdf:resource="&foliationtype;eutaxitic_foliation"/>
        <skos:narrower rdf:resource="&foliationtype;igneous_flow_foliation"/>
        <skos:narrower rdf:resource="&foliationtype;igneous_lamination"/>
        <skos:narrower rdf:resource="&foliationtype;igneous_layering"/>
        <skos:broader rdf:resource="&foliationtype;primary_foliation"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/protomylonite_foliation -->

    <skos:Concept rdf:about="&foliationtype;protomylonite_foliation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>SLTTm 2004; Sibson 1977</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Generic mylonitic foliation in which 10-50 percent of rock is matrix due to tectonic grain size reduction processes. Planar aspect of fabric may be quite subtle in weakly deformed rocks.</skos:definition>
        <skos:prefLabel xml:lang="en">Protomylonite foliation</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0056"/>
        <skos:broader rdf:resource="&foliationtype;generic_mylonitic_foliation"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/rhythmic_layering -->

    <skos:Concept rdf:about="&foliationtype;rhythmic_layering">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source>Jackson 1997</dc:source>
        <skos:definition xml:lang="en">Layering in igneous rock defined by repeated gravity-stratified layers of typically mafic minerals at the base and plagioclase at the top; formed by crystal settling</skos:definition>
        <skos:prefLabel xml:lang="en">Rhythmic layering</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0057"/>
        <skos:broader rdf:resource="&foliationtype;igneous_layering"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/rough_cleavage -->

    <skos:Concept rdf:about="&foliationtype;rough_cleavage">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Disjunct (Borradaile et al, 1990) cleavage characterized by short, discontinuous clavage surfaces, typically with concentrations of phyllosilicate minerals. Common in micaceous sandstone.</skos:definition>
        <dc:source>Neuendorf et al. 2005; Borradaile et al, 1990</dc:source>
        <skos:prefLabel xml:lang="en">Rough cleavage</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0058"/>
        <skos:broader rdf:resource="&foliationtype;cleavage"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/s_fabric -->

    <skos:Concept rdf:about="&foliationtype;s_fabric">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source>Geoscience Victoria, Australia</dc:source>
        <skos:definition xml:lang="en">Foliation in mylonite defined by alignment of tabular crystals or deformed mineral grains; often with sigmoid shape, typically curving into an associated ductile shear banding foliation (C fabric) with a consistent sense.</skos:definition>
        <skos:prefLabel xml:lang="en">S fabric</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0059"/>
        <skos:broader rdf:resource="&foliationtype;grain_shape_foliation"/>
        <skos:broader rdf:resource="&foliationtype;tectonic_foliation"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/scaly_cleavage -->

    <skos:Concept rdf:about="&foliationtype;scaly_cleavage">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Based on Jackson 1997</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Discrete, anastomosing spaced cleavage characterized by smooth and highly polished/slickensided cleavage surfaces that intersect with one another to form lensoid flakes, scales or chips. Common in pelitic rocks in shear zones, melange, or deformed accretionary complexes.</skos:definition>
        <skos:prefLabel xml:lang="en">Scaly cleavage</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0060"/>
        <skos:broader rdf:resource="&foliationtype;spaced_cleavage"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/schistosity -->

    <skos:Concept rdf:about="&foliationtype;schistosity">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>NADM metamorphic rock vocabulary SLTTm1.0 2004</dc:source>
        <skos:definition xml:lang="en">A fabric defined the parallel, planar arrangement of mineral grains having a platy, lamellar, or tabular crystallographic habit that are oriented in a continuous planar or rarely a linear fabric. Schistosity is commonly, but not necessarily, a crystalloblastic foliation. Platy mineral grains typically 1 to 10 mm across (Jackson, 1997), at smaller grain size grades into phyllitic cleavage. Schistosity is typically associated with a continuous cleavage due to the tendency to part parallel to the aligned mineral grains.</skos:definition>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Schistosity</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0061"/>
        <skos:broader rdf:resource="&foliationtype;foliation"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/sedimentary_layering -->

    <skos:Concept rdf:about="&foliationtype;sedimentary_layering">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>this vocabulary</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Layering resulting from primary sedimentary processes, in which the individual layers (beds) are bounded by discontinuities related to events in the depositional history.</skos:definition>
        <skos:prefLabel xml:lang="en">Sedimentary layering</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0062"/>
        <skos:broader rdf:resource="&foliationtype;bedding_fabric"/>
        <skos:broader rdf:resource="&foliationtype;layering"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/slaty_cleavage -->

    <skos:Concept rdf:about="&foliationtype;slaty_cleavage">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Brodie, Kate, Fettes, Douglas, and Harte, Ben, 2007, Structural terms including fault rock terms, in Fettes, D., and Desmons, J., Metamorphic rocks-A classification and Glossary of Terms: Cambridge, UK, Cam-bridge University Press, p. 24-31.; Jackson 1997</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Continuous cleavage defined by aligned fine to very-fine grained phylosilicate mineral grains. The individual grains defining the fabric are too small to be seen by the unaided eye. Grades into phyllitic cleavage as grain size increases. Cleavage domains typically less than 100 micrometres wide; rock easily splits into slabs and thin plates.</skos:definition>
        <skos:prefLabel xml:lang="en">Slaty cleavage</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0063"/>
        <skos:broader rdf:resource="&foliationtype;continuous_cleavage"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/solution_cleavage -->

    <skos:Concept rdf:about="&foliationtype;solution_cleavage">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Geoscience Victoria, Australia</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Solution cleavage</skos:prefLabel>
        <skos:definition xml:lang="en">Spaced cleavage forming an array of parallel to anastomosing, stylolitic to smooth, fracture-like partings formed by rock dissolution. Dissolution is indicated by discontinuity and offset of primary features that intersect cleavage surfaces, and typically by accumulation of insoluble residue along cleavage surface. Syn. stylolitic cleavage, pressure solution cleavage</skos:definition>
        <skos:exactMatch rdf:resource="&foliationtype;0064"/>
        <skos:broader rdf:resource="&foliationtype;spaced_cleavage"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/spaced_cleavage -->

    <skos:Concept rdf:about="&foliationtype;spaced_cleavage">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Borradaile et al. 1982</dc:source>
        <skos:definition xml:lang="en">Cleavage in which cleavage domains are spaced at finite intervals as the scale of description. Microlithons of uncleaved rock separate cleavage domains.</skos:definition>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Spaced cleavage</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0065"/>
        <skos:narrower rdf:resource="&foliationtype;anastomosing_spaced_cleavage"/>
        <skos:broader rdf:resource="&foliationtype;cleavage"/>
        <skos:narrower rdf:resource="&foliationtype;discrete_cleavage"/>
        <skos:narrower rdf:resource="&foliationtype;discrete_crenulation_cleavage"/>
        <skos:narrower rdf:resource="&foliationtype;discrete_disjunct_spaced_cleavage"/>
        <skos:narrower rdf:resource="&foliationtype;scaly_cleavage"/>
        <skos:narrower rdf:resource="&foliationtype;solution_cleavage"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/stromatic_layering -->

    <skos:Concept rdf:about="&foliationtype;stromatic_layering">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Stromatic layering</skos:prefLabel>
        <skos:definition xml:lang="en">Tectonic layering in which variations that distinguish the layers are the result of melt segregation during high-grade metamorphism; some layers have magmatic/igneous mineralogy. Plates 143 and 144, Borradaile et al [1982] (page 344-347) Syn. Migmatitic layering, lit-par-lit layering</skos:definition>
        <dc:source>Weiss 1972; Borradaile et al 1982; Neuendorf et al. 2005</dc:source>
        <skos:exactMatch rdf:resource="&foliationtype;0066"/>
        <skos:broader rdf:resource="&foliationtype;metamorphic_layering"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/stromatolitic_lamination -->

    <skos:Concept rdf:about="&foliationtype;stromatolitic_lamination">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Based on Neuendorf et al 2005</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Lamination produced by sediment trapping, binding and or precipitation by cyanophytes (blue-green algae)</skos:definition>
        <skos:prefLabel xml:lang="en">Stromatolitic lamination</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0067"/>
        <skos:broader rdf:resource="&foliationtype;cryptomicrobial_lamination"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/tectonic_foliation -->

    <skos:Concept rdf:about="&foliationtype;tectonic_foliation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Foliation in a rock body defined by physical components related to deformation subsequent to solidification of the rock.</skos:definition>
        <skos:prefLabel xml:lang="en">Tectonic foliation</skos:prefLabel>
        <dc:source>this vocabulary</dc:source>
        <skos:exactMatch rdf:resource="&foliationtype;0068"/>
        <skos:narrower rdf:resource="&foliationtype;flaser_structure"/>
        <skos:broader rdf:resource="&foliationtype;foliation"/>
        <skos:narrower rdf:resource="&foliationtype;generic_mylonitic_foliation"/>
        <skos:narrower rdf:resource="&foliationtype;s_fabric"/>
        <skos:narrower rdf:resource="&foliationtype;tectonic_layering"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/tectonic_layering -->

    <skos:Concept rdf:about="&foliationtype;tectonic_layering">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Weiss 1972; Borradaile et al 1982; this vocabulary</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Layering defined by mineralogical or textural variations in a rock body that are interpreted to be related to deformation or metamorphism, commonly associated with layers that have sharp boundaries. Also known as gneissic foliation. See plates 142-145 (p. 342-349) in Borradaile et al [1982] and plate 46A to 49A in Weiss [1972].</skos:definition>
        <skos:prefLabel xml:lang="en">Tectonic layering</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0069"/>
        <skos:broader rdf:resource="&foliationtype;layering"/>
        <skos:broader rdf:resource="&foliationtype;tectonic_foliation"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/transposed_bedding_layering -->

    <skos:Concept rdf:about="&foliationtype;transposed_bedding_layering">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>this vocabulary</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Layering interpreted to have originated as sedimentary bedding that has been transposed as a result of metamorphism and deformation.</skos:definition>
        <skos:prefLabel xml:lang="en">Transposed bedding layering</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0070"/>
        <skos:broader rdf:resource="&foliationtype;metamorphic_layering_inherited_from_protolith"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/ultramylonite_foliation -->

    <skos:Concept rdf:about="&foliationtype;ultramylonite_foliation">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Mylonitic foliation in which greater than 90 percent of rock is matrix due to tectonic grain size reduction processes. Typically rock is very-fine grained to aphanitic, with sparse porphyroclasts, and vague to prominent lamination on a mm scale.</skos:definition>
        <dc:source>SLTTm 2004; Sibson 1977</dc:source>
        <skos:prefLabel xml:lang="en">Ultramylonite foliation</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0071"/>
        <skos:broader rdf:resource="&foliationtype;generic_mylonitic_foliation"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/undulating_lamination -->

    <skos:Concept rdf:about="&foliationtype;undulating_lamination">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>this vocabulary</dc:source>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Gently curviplanar or wavy bedding lamination structure</skos:definition>
        <skos:prefLabel xml:lang="en">Undulating lamination</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0072"/>
        <skos:broader rdf:resource="&foliationtype;bedding_lamination_structure"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/vague_metamorphic_layering -->

    <skos:Concept rdf:about="&foliationtype;vague_metamorphic_layering">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en">Metamorphic layering of uncertain origin, defined by subtle mineralogical or textural variations, with gradational layer boundaries. See plate 142 in Borradaile et al [1982].</skos:definition>
        <skos:prefLabel xml:lang="en">Vague metamorphic layering</skos:prefLabel>
        <dc:source>Weiss 1972;Borradaile et al 1982; this vocabulary</dc:source>
        <skos:exactMatch rdf:resource="&foliationtype;0073"/>
        <skos:broader rdf:resource="&foliationtype;metamorphic_layering"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/foliationtype/varve_lamination -->

    <skos:Concept rdf:about="&foliationtype;varve_lamination">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>SLTTs 2004; Jackson 1997</dc:source>
        <skos:definition xml:lang="en">Bedding defined by distinct, parallel laminae (layers less than 1 cm thick), typically occurring in sequences of distinct couplets (e.g. dark and light color). Generally interpreted as deposited from suspension in still water, with each lamina thought to be an annual sedimentation unit. Typically consists of mud-size sediment.</skos:definition>
        <dc:creator xml:lang="en">Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Varve lamination</skos:prefLabel>
        <skos:exactMatch rdf:resource="&foliationtype;0074"/>
        <skos:broader rdf:resource="&foliationtype;bedding_lamination_structure"/>
        <skos:inScheme rdf:resource="&cgi;foliationtype"/>
    </skos:Concept>
    


