    <!-- http://resource.geosciml.org/classifier/cgi/geologicunittype/artificial_ground -->

    <skos:Concept rdf:about="&geologicunittype;artificial_ground">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition>Geologic unit defined by genesis involving direct human action to deposit or modify material.</skos:definition>
        <skos:prefLabel xml:lang="en">Artificial Ground</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:exactMatch rdf:resource="&geologicunittype;0003"/>
