    <!-- http://resource.geosciml.org/classifier/cgi/particleaspectratio/bladed -->

    <skos:Concept rdf:about="&particleaspectratio2;bladed">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en">Bladed</skos:prefLabel>
        <dc:source>Sneed and Folk, 1958; Pettijohn et al., 1972; NADM SLTTs, 2004</dc:source>
        <skos:definition>Bladed group in Sneed&amp;Folk, (1958); S/L is less than 0.7 and (L-I)/(L-S) is between 0.33 and 0.66.</skos:definition>
        <skos:exactMatch rdf:resource="&particleaspectratio2;0001"/>
        <skos:broader rdf:resource="&particleaspectratio2;any_form"/>
        <skos:narrower rdf:resource="&particleaspectratio2;bladed_medium"/>
