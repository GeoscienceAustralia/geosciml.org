        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition>A strike slip fault in which the faut plane is more or less vertical.</skos:definition>
        <dc:source>GGIC vocabularies working group (Australia)</dc:source>
        <skos:prefLabel xml:lang="en">wrench</skos:prefLabel>
        <skos:exactMatch rdf:resource="&faultmovementtype;0015"/>
        <skos:broader rdf:resource="&faultmovementtype;strike_slip"/>
        <skos:inScheme rdf:resource="&cgi;faultmovementtype"/>
    </skos:Concept>
    


