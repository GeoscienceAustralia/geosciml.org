    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/dip_separation_sense -->

    <skos:Concept rdf:about="&faultmovementtype;dip_separation_sense">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>GGIC vocabularies working group (Australia)</dc:source>
        <skos:definition>A fault along which there is some separation parallel to the dip of the fault.</skos:definition>
        <skos:prefLabel xml:lang="en">dip separation sense</skos:prefLabel>
        <skos:exactMatch rdf:resource="&faultmovementtype;0001"/>
        <skos:narrower rdf:resource="&faultmovementtype;dip_slip"/>
        <skos:inScheme rdf:resource="&cgi;faultmovementtype"/>
        <skos:topConceptOf rdf:resource="&cgi;faultmovementtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/dip_slip -->

    <skos:Concept rdf:about="&faultmovementtype;dip_slip">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition>The net slip of the fault lies in the dip direction of the fault.</skos:definition>
        <dc:source>GGIC vocabularies working group (Australia)</dc:source>
        <skos:prefLabel xml:lang="en">dip slip</skos:prefLabel>
        <skos:exactMatch rdf:resource="&faultmovementtype;0002"/>
        <skos:broader rdf:resource="&faultmovementtype;dip_separation_sense"/>
        <skos:inScheme rdf:resource="&cgi;faultmovementtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/extraction -->

    <skos:Concept rdf:about="&faultmovementtype;extraction">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="&cgi;faultmovementtype">extraction</skos:notation>
        <dc:source>Froitzheim, N., Pleuger, J. &amp; Nagel, T.J., 2006. Extraction faults. Journal of Structural Geology v28, pp. 1388-1395.</dc:source>
        <skos:definition>A fault whose two sides have approached each other substantially in the direction perpendicular to the fault.</skos:definition>
        <skos:prefLabel xml:lang="en">extraction</skos:prefLabel>
        <skos:exactMatch rdf:resource="&faultmovementtype;0003"/>
        <skos:narrower rdf:resource="&faultmovementtype;mixed_extraction"/>
        <skos:narrower rdf:resource="&faultmovementtype;pure_extraction"/>
        <skos:inScheme rdf:resource="&cgi;faultmovementtype"/>
        <skos:topConceptOf rdf:resource="&cgi;faultmovementtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/horizontal -->

    <skos:Concept rdf:about="&faultmovementtype;horizontal">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>GGIC vocabularies working group (Australia)</dc:source>
        <skos:definition>The fault is horizontal.</skos:definition>
        <skos:prefLabel xml:lang="en">horizontal</skos:prefLabel>
        <skos:exactMatch rdf:resource="&faultmovementtype;0004"/>
        <skos:topConceptOf rdf:resource="&cgi;faultmovementtype"/>
        <skos:inScheme rdf:resource="&cgi;faultmovementtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/mixed_extraction -->

    <skos:Concept rdf:about="&faultmovementtype;mixed_extraction">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition>An extraction fault with some displacement within the fault plane.</skos:definition>
        <dc:source>Froitzheim, N., Pleuger, J. &amp; Nagel, T.J., 2006. Extraction faults. Journal of Structural Geology v28, pp. 1388-1395.</dc:source>
        <skos:prefLabel xml:lang="en">mixed extraction</skos:prefLabel>
        <skos:exactMatch rdf:resource="&faultmovementtype;0005"/>
        <skos:broader rdf:resource="&faultmovementtype;extraction"/>
        <skos:inScheme rdf:resource="&cgi;faultmovementtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/oblique_slip -->

    <skos:Concept rdf:about="&faultmovementtype;oblique_slip">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition>The net slip of the fault lies between the strike and dip directions of the fault; the slip vector rakes between 10 and 80 degrees in the plane of the fault.</skos:definition>
        <dc:source>GGIC vocabularies working group (Australia); SMR</dc:source>
        <skos:prefLabel xml:lang="en">oblique slip</skos:prefLabel>
        <skos:exactMatch rdf:resource="&faultmovementtype;0006"/>
        <skos:narrower rdf:resource="&faultmovementtype;transpressional"/>
        <skos:narrower rdf:resource="&faultmovementtype;transtensional"/>
        <skos:topConceptOf rdf:resource="&cgi;faultmovementtype"/>
        <skos:inScheme rdf:resource="&cgi;faultmovementtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/pure_extraction -->

    <skos:Concept rdf:about="&faultmovementtype;pure_extraction">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition>An extraction fault with no discernible displacement within the fault plane.</skos:definition>
        <dc:source>Froitzheim, N., Pleuger, J. &amp; Nagel, T.J., 2006. Extraction faults. Journal of Structural Geology v28, pp. 1388-1395.</dc:source>
        <skos:prefLabel xml:lang="en">pure extraction</skos:prefLabel>
        <skos:exactMatch rdf:resource="&faultmovementtype;0007"/>
        <skos:broader rdf:resource="&faultmovementtype;extraction"/>
        <skos:inScheme rdf:resource="&cgi;faultmovementtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/scissor -->

    <skos:Concept rdf:about="&faultmovementtype;scissor">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition>A fault on which there is increasing offset or separation along the strike from an initial point of no offset, with reverse offset in the opposite direction.</skos:definition>
        <dc:source>Neuendorf et al 2005</dc:source>
        <skos:prefLabel xml:lang="en">scissor</skos:prefLabel>
        <skos:exactMatch rdf:resource="&faultmovementtype;0008"/>
        <skos:inScheme rdf:resource="&cgi;faultmovementtype"/>
        <skos:topConceptOf rdf:resource="&cgi;faultmovementtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/strike_separation_sense -->

    <skos:Concept rdf:about="&faultmovementtype;strike_separation_sense">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition>A fault along which there is some separation parallel to the strike of the fault.</skos:definition>
        <dc:source>GGIC vocabularies working group (Australia)</dc:source>
        <skos:prefLabel xml:lang="en">strike separation sense</skos:prefLabel>
        <skos:exactMatch rdf:resource="&faultmovementtype;0009"/>
        <skos:narrower rdf:resource="&faultmovementtype;strike_slip"/>
        <skos:inScheme rdf:resource="&cgi;faultmovementtype"/>
        <skos:topConceptOf rdf:resource="&cgi;faultmovementtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/strike_slip -->

    <skos:Concept rdf:about="&faultmovementtype;strike_slip">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>GGIC vocabularies working group (Australia); SMR</dc:source>
        <skos:definition>The net slip of the fault (slip vector) is parallel to the strike of the fault.</skos:definition>
        <skos:prefLabel xml:lang="en">strike slip</skos:prefLabel>
        <skos:exactMatch rdf:resource="&faultmovementtype;0010"/>
        <skos:broader rdf:resource="&faultmovementtype;strike_separation_sense"/>
        <skos:narrower rdf:resource="&faultmovementtype;transcurrent"/>
        <skos:narrower rdf:resource="&faultmovementtype;transform"/>
        <skos:narrower rdf:resource="&faultmovementtype;wrench"/>
        <skos:inScheme rdf:resource="&cgi;faultmovementtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/transcurrent -->

    <skos:Concept rdf:about="&faultmovementtype;transcurrent">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>GGIC vocabularies working group (Australia)</dc:source>
        <skos:definition>A large scale strike-slip fault in which the fault surface is steeply inclined.</skos:definition>
        <skos:prefLabel xml:lang="en">transcurrent</skos:prefLabel>
        <skos:exactMatch rdf:resource="&faultmovementtype;0011"/>
        <skos:broader rdf:resource="&faultmovementtype;strike_slip"/>
        <skos:inScheme rdf:resource="&cgi;faultmovementtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/transform -->

    <skos:Concept rdf:about="&faultmovementtype;transform">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition>A variety of strike-slip fault along which the displacement suddenly stops or changes form; typically associated with mid-ocean ridges.</skos:definition>
        <dc:source>GGIC vocabularies working group (Australia)</dc:source>
        <skos:prefLabel xml:lang="en">transform</skos:prefLabel>
        <skos:exactMatch rdf:resource="&faultmovementtype;0012"/>
        <skos:broader rdf:resource="&faultmovementtype;strike_slip"/>
        <skos:inScheme rdf:resource="&cgi;faultmovementtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/transpressional -->

    <skos:Concept rdf:about="&faultmovementtype;transpressional">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>GGIC vocabularies working group (Australia)</dc:source>
        <skos:definition>A fault along which strike-slip deformation is accompanied by a component of shortening transverse to the fault.</skos:definition>
        <skos:prefLabel xml:lang="en">transpressional</skos:prefLabel>
        <skos:exactMatch rdf:resource="&faultmovementtype;0013"/>
        <skos:broader rdf:resource="&faultmovementtype;oblique_slip"/>
        <skos:inScheme rdf:resource="&cgi;faultmovementtype"/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/transtensional -->

    <skos:Concept rdf:about="&faultmovementtype;transtensional">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>GGIC vocabularies working group (Australia)</dc:source>
        <skos:definition>A fault along which strike-slip deformation is accompanied by a component of extension transverse to the fault.</skos:definition>
        <skos:prefLabel xml:lang="en">transtensional</skos:prefLabel>
        <skos:exactMatch rdf:resource="&faultmovementtype;0014"/>
        <skos:broader rdf:resource="&faultmovementtype;oblique_slip"/>
        <skos:inScheme rdf:resource="&cgi;faultmovementtype"/>
    </skos:Concept>
    


