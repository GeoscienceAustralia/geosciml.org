   <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particleaspectratio/bladed">
      <skos:inScheme rdf:resource="http://resource.geosciml.org/classifierscheme/cgi/2016.01/particleaspectratio"/>
      <skos:definition xml:lang="en">Bladed group in Sneed&amp;Folk, (1958), S/L is less than 0.7 and (L-I)/(L-S) is between 0.33 and 0.66.</skos:definition>
      <skos:prefLabel xml:lang="en">bladed</skos:prefLabel>
      <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particleaspectratio/any_form"/>
      <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particleaspectratio/bladed_medium"/>
      <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particleaspectratio/compact_bladed"/>
      <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particleaspectratio/very_bladed"/>
      <dcterms:source xml:lang="en">Sneed and Folk, 1958, Pettijohn et al., 1972, NADM SLTTs, 2004</dcterms:source>
   </skos:Concept>
