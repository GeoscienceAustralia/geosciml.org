    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0001 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0001">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >A metasomatic facies contact separating rocks that have undergone alteration of a particular facies from those that have undergone metasomatism of another facies. Alteration is a kind of metasomatism that does not introduce economically important minerals.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Alteration facies contact</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0024"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0002 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0002">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</dc:source>
        <skos:definition
            >An unconformable contact between two geological units in which the older, underlying rocks dip at an angle different from the younger, overlying strata, usually in which younger sediments rest upon the eroded surface of tilted or folded older rocks.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Angular unconformable contact</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0034"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0003 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0003">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >An unconformity in which onlapping strata are truncated against a steep topographic scarp.</skos:definition>
        <dc:source
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</dc:source>
        <skos:prefLabel xml:lang="en"
            >Buttress unconformity</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0034"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0004 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0004">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >A contact between bodies of material having different ages of origin.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Chronostratigraphic-zone contact</skos:prefLabel>
        <dc:source xml:lang="en">FGDC</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0007"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0005 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0005">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en"
            >A geophysical contact between bodies of material distinguished based on electrical conductivity characteristics</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Conductivity contact</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:historyNote xml:lang="en"
            >update definition to allow definition of unit based on any characteristics related to electrical conductivity. SMR 2011-02-16</skos:historyNote>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0014"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0006 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0006">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</dc:source>
        <skos:definition
            >A contact separating two geological units in which the layers are formed one above the other in order by regular, uninterrupted deposition under the same general conditions.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Conformable contact</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0010"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0007 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0007">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition xml:lang="en"
            >A surface that separates geologic units. Very general concept representing any kind of surface separating two geologic units, including primary boundaries such as depositional contacts, all kinds of unconformities, intrusive contacts, and gradational contacts, as well as faults that separate geologic units.</skos:definition>
        <skos:prefLabel xml:lang="en">Contact</skos:prefLabel>
        <rdfs:comment xml:lang="en"
            >Note that usage of geologic unit here is per NADM C1 2004, denoting a body of material. Lines on geologic maps may also delineate featured defined geomorphically-- craters, terraces, shorelines, scarps that are not inherently geologic unit boundaries in this sense.</rdfs:comment>
        <dc:source xml:lang="en"
            >adapted from Jackson, 1997, page 137; NADM C1 2004</dc:source>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0004"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0012"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0013"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0014"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0015"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0019"/>
        <skos:topConceptOf rdf:resource=""/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0008 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0008">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition xml:lang="en"
            >A lithogenetic bundary separating rock masses that have different deformation structure, e.g. sheared rock against non sheared rock, brecciated rock against non-brecciated rock.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Deformation zone contact</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0019"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0009 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0009">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en"
            >A geophysical contact separating bodies of material with different density characteristics, generally determined through measurement and modelling of gravity variations.</skos:definition>
        <skos:prefLabel xml:lang="en">Density contact</skos:prefLabel>
        <skos:historyNote xml:lang="en"
            >modify definitionn to allow unit definition based on any density-based characteristic SMR 2011-02-16</skos:historyNote>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0014"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0010 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0010">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Lithogenetic contact at which a sedimentary or volcanic rock has been deposited on (or against) another rock body. The relationship between the older underlying rocks and younger overlying rocks is unknown or not specfied.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Depositional contact</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0006"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0019"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0034"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0011 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0011">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</dc:source>
        <skos:definition
            >An unconformable contact between two geological units in which the bedding of the older, underlying unit is parallel to the bedding of the younger, overlying unit, but in which the contact between the two units is marked by an irregular or uneven surface of appreciable relief.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Disconformable contact</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0034"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0012 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0012">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en"
            >A contact separating two bodies of material across which one body has slid past the other.</skos:definition>
        <skos:prefLabel xml:lang="en">Faulted contact</skos:prefLabel>
        <dc:source
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0007"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0013 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0013">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</dc:source>
        <skos:definition xml:lang="en"
            >A contact between regions characterised by their geological history or by similar structural, petrographic or stratigraphic features</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Geologic province contact</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0007"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0014 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0014">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >A contact separating bodies of material in the earth that have different geophysical properties. Use for boundaries that are detected by geophysical sensor techniques as opposed to direct lithologic observation.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Geophysical contact</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0005"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0007"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0009"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0020"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0030"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0033"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0015 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0015">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en"
            >A boundary between a subglacial geomorphic unit and a periglacial geomorphic unit, marking the maximum extent of glacial cover. This can be thought of as the outcrop of the contact between a glacier and its substrate at some time at each point along the boundary. This contact type is included as an interim concept, assuming that in the future, there will be extensions to account better for geomorphic units and line types.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Glacial stationary line</skos:prefLabel>
        <rdfs:comment xml:lang="en"
            >Glacial stationary lines are inferred outcrop of the glacial contact representing the boundary of a glacier at its maximum (or some stable) extent, thus they would be instances of this contact type, with metadata indicating observation method by inference.</rdfs:comment>
        <rdfs:comment xml:lang="en">this vocabulary</rdfs:comment>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0007"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0016 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0016">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</dc:source>
        <skos:definition
            >An intrusive contact between a younger igneous rock and an older, pre-existing geological unit into which it has been intruded.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Igneous intrusive contact</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0019"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0017 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0017">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >A lithogenetic contact separating lithologically distinct phases of a single intrusive body. Does not denote nature of contact (intrusive or gradation).</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Igneous phase contact</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0019"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0018 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0018">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en"
            >Impact structure boundary</skos:prefLabel>
        <skos:definition xml:lang="en"
            >surface that bounds a body of rock affected by an extraterrestrial impact event</skos:definition>
        <rdfs:seeAlso xml:lang="en">this vocabulary</rdfs:seeAlso>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0019"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0019 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0019">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition xml:lang="en"
            >A non-faulted contact separating bodies of material in the earth that have different lithologic character or geologic history.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Lithogenetic contact</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
