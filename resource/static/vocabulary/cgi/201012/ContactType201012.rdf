    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0020 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0020">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >A geophysical contact separating bodies of material distinguished based on properties related to magnetic fields.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Magnetic contact</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0014"/>
