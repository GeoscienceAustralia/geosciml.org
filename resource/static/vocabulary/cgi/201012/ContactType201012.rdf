    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0023 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0023">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en"
            >A magnetic contact between bodies of material distinguished based on any aspect of  magnetization of material in the units.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Magnetization contact</skos:prefLabel>
        <skos:historyNote xml:lang="en"
            >reword definition to generalize to allow unit definition based on any aspect of magnetization. SMR 2011-02-16</skos:historyNote>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0020"/>
        <skos:inScheme rdf:resource=""/>
