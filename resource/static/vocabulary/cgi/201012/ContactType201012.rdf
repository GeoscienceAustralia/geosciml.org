    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0021 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0021">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >A magentic contact between bodies of material with different polarity of remnant magnetization, e.g. between sections of ocean floor with different polarity.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Magnetic polarity contact</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0020"/>
