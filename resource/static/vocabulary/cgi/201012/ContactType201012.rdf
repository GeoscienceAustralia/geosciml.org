    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0024 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0024">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >Lithogenetic contact separating rocks that have different lithologic properties related to metamorphism, metasomatism, alteration, or mineralization. Generally separates metamorphic rock bodies, but may separate metamorphosed (broadly speaking) and non-metamorphosed rock.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Metamorphic contact</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0001"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0019"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0025"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0026"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0027"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0025 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0025">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >A metamorphic contact separating rocks that have undergone metamorphism of a particular facies from those that have undergone metamorphism of another facies.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Metamorphic facies contact</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0024"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0026 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0026">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >A metamorphic contact separating rocks that have undergone metasomatism of a particular facies from those that have undergone metasomatism of another facies. Metasomatism is distinguished from metamorphism by significant changes in bulk chemistry of the affected rock.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Metasomatic facies contact</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0024"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0027 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0027">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >A metasomatic facies contact separating rocks which have been mineralised and contain a particular mineral assemblage from those which contain a different assemblage. Mineralization is a kind of metasomatism that introduces ecomomically important minerals.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Mineralisation assemblage contact</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0024"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0028 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0028">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >An unconformable contact between an underlying, older nonstratified geological unit (usually intrusive igneous rocks or metamorphics) and an overlying, younger stratified geological unit.</skos:definition>
        <dc:source
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</dc:source>
        <skos:prefLabel xml:lang="en"
            >Nonconformable contact</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0034"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0029 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0029">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >An unconformable contact between two geological units in which the bedding of the older, underlying unit is parallel to the bedding of the younger, overlying unit, in which the contact between the two units is planar, and may be coincident with a bedding plane.</skos:definition>
        <dc:source
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</dc:source>
        <skos:prefLabel xml:lang="en"
            >Paraconformable contact</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0034"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0030 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0030">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition xml:lang="en"
            >A geophysical contact separating bodies of material distinguished based on the characteristics of emitted of radiant energy related to radioactivity (e.g. gamma rays).</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Radiometric contact</skos:prefLabel>
        <skos:historyNote xml:lang="en"
            >reword definition to generalize to allow unit definition based on any characteristics related to radiant energy emissions related to radioactivity. SMR 2011-02-16</skos:historyNote>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0014"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0031 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0031">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >A lithogenetic contact separating essentially coeval sedimentary material bodies distinguished by characteristics reflecting different physical or chemical processes active at the time of deposition of the sediment.</skos:definition>
        <dc:source
            >base on Nichols, Gary, 1999, Sedimentology and stratigraphy, Blackwell, p. 62-63.</dc:source>
        <skos:prefLabel xml:lang="en"
            >Sedimentary facies contact</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0019"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0032 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0032">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</dc:source>
        <skos:definition
            >An intrusive contact between a sedimentary rock unit and plastic sediment (e.g., clay, chalk, salt, gypsum, etc.), forced upward into it from underlying sediment</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Sedimentary intrusive contact</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0019"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0033 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0033">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en"
            >A geophysical contact separating bodies of material defined based on their seismic character. Seismic character is based on transmission of vibrations (seismic waves) through a rock body, and relates to the velocity of transmission, and the nature of reflection, refraction, or transformation of seismic waves by inhomogeneities in the rock body.</skos:definition>
        <skos:prefLabel xml:lang="en">Seismic contact</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0014"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0034 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0034">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >A contact separating two geological units in which the younger unit succeeds the older after a substantial hiatus in deposition.</skos:definition>
        <dc:source
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</dc:source>
        <skos:prefLabel xml:lang="en"
            >Unconformable contact</skos:prefLabel>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0002"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0003"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0010"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0011"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0028"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0029"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0035 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0035">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en"
            >Volcanic subsidence zone boundary</skos:prefLabel>
        <skos:definition xml:lang="en"
            >boundary around a body of rock that is within a zone of subsidence or cratering produced by volcanic activity.</skos:definition>
        <rdfs:seeAlso xml:lang="en">caldera</rdfs:seeAlso>
        <rdfs:seeAlso xml:lang="en">cauldron</rdfs:seeAlso>
        <rdfs:seeAlso xml:lang="en">crater</rdfs:seeAlso>
        <dc:source xml:lang="en"
            >this vocabulary, concept to encompass boundary of caldron, caldera, or crater.</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0019"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/contacttype/0036 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/contacttype/0036">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >A lithogenetic contact separating bodies of material differentiated based on lithologic properties related to weathering.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Weathering contact</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/contacttype/0019"/>
