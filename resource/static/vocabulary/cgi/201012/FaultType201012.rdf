    <!-- http://resource.geosciml.org/classifier/cgi/faulttype/0001 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/faulttype/0001">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition xml:lang="en"
            >A regional-scale, large displacement, low-angle normal fault.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Detachment fault</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0012"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faulttype/0002 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/faulttype/0002">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Dextral strike slip fault</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Fault with right-lateral strike-parallel displacement component of slip vector more than 10 times the dip-parallel component of the slip vector at at least one location along the fault, and right-lateral displacement over more than half the mapped trace of the fault.</skos:definition>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0022"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faulttype/0003 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/faulttype/0003">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >A fault whose two sides have approached each other substantially in the direction perpendicular to the fault.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Extraction fault</skos:prefLabel>
        <dc:source
            >Froitzheim, N., Pleuger, J. &amp; Nagel, T.J., 2006. Extraction faults. Journal of Structural Geology v28, pp. 1388-1395.</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0004"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0013"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0016"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faulttype/0004 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/faulttype/0004">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition xml:lang="en"
            >A discrete surface, or zone of discrete surfaces, with some thickness, separating two rock masses across which one mass has slid past the other and characterized by brittle deformation.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Fault</skos:prefLabel>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0003"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0005"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0011"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0014"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0015"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0017"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0020"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0022"/>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faulttype/0005 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/faulttype/0005">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Fault that dips at least 45 degrees over more than half of its recognized extent, for which slip or separation is not explicitly specified.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >High-angle fault</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0004"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0006"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0007"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0024"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faulttype/0006 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/faulttype/0006">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Fault that dips at least 45 degrees over more than half of the recognized extent of the fault with the hanging wall displaced from a structurally higher position relative to footwall rocks.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >High-angle normal fault</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0005"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0014"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faulttype/0007 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/faulttype/0007">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >High angle reverse</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Reverse fault that dips at least 45 degrees over more than half of its recognized extent, for which slip or separation is not explicitly specified.</skos:definition>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0005"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0017"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faulttype/0008 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/faulttype/0008">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Fault that dips less than 10 degrees over more than half the recognized extent of the fault.</skos:definition>
        <dc:source
            >GGIC vocabularies working group (Australia)</dc:source>
        <skos:prefLabel xml:lang="en"
            >Horizontal fault</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0011"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faulttype/0009 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/faulttype/0009">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >High angle fault with slip vector that has ratio of strike-parallel to dip-parallel displacement between 10 to 1 and 1 to 10 at at least one location along the mapped trace, with left-lateral strike-parallel component and normal dip-parallel component over at least half the mapped trace of the fault.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Left normal fault</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0015"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faulttype/0010 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/faulttype/0010">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >High angle fault with slip vector that has ratio of strike-parallel to dip-parallel displacement between 10 to 1 and 1 to 10 at at least one location along the mapped trace, with left-lateral strike-parallel component and reverse dip-parallel component over at least half the mapped trace of the fault.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Left reverse fault</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0015"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faulttype/0011 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/faulttype/0011">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Fault that dips less than 45 degrees over more than half of the recognized extent of the fault.</skos:definition>
        <skos:prefLabel xml:lang="en">Low angle fault</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0004"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0008"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0012"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0023"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faulttype/0012 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/faulttype/0012">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Fault that dips less than 45 degrees over more than half of the recognized extent of the fault with the hanging wall displaced from a structurally higher position relative to footwall rocks.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Low-angle normal fault</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0001"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0011"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0014"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faulttype/0013 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/faulttype/0013">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition xml:lang="en"
            >An extraction fault with some displacement within the fault plane.</skos:definition>
        <dc:source
            >Froitzheim, N., Pleuger, J. &amp; Nagel, T.J., 2006. Extraction faults. Journal of Structural Geology v28, pp. 1388-1395.</dc:source>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Mixed extraction fault</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0003"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faulttype/0014 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/faulttype/0014">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Fault with dip-parallel displacement component of slip vector more than 10 times the strike-parallel component of the slip vector over more than half recognized extent of the fault, and for which the fault dips consistently in the same direction, and for which the hanging wall has been displaced down relative to the footwall.</skos:definition>
        <skos:prefLabel xml:lang="en">Normal fault</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0004"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0006"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0012"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faulttype/0015 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/faulttype/0015">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Fault with slip vector that has ratio of strike-parallel to dip-parallel displacement between 10 to 1 and 1 to 10 at at least one location along the mapped trace of the fault.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Oblique slip fault</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0004"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0009"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0010"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0018"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0019"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faulttype/0016 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/faulttype/0016">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >Froitzheim, N., Pleuger, J. &amp; Nagel, T.J., 2006. Extraction faults. Journal of Structural Geology v28, pp. 1388-1395.</dc:source>
        <skos:definition xml:lang="en"
            >An extraction fault with no discernible displacement within the fault plane.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Pure extraction fault</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faulttype/0003"/>
        <skos:inScheme rdf:resource=""/>
