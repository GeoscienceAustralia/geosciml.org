    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0001 -->

    <skos:Concept rdf:about="&CompositionCategory;0001">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en">Argillic</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Greater than accessory amount (greater than 5 percent by volume) of clay is present</skos:definition>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0002 -->

    <owl:Thing rdf:about="&CompositionCategory;0002">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en"
            >Arkosic detrital mode</skos:prefLabel>
        <dc:source xml:lang="en">Folk1968</dc:source>
        <skos:definition xml:lang="en"
            >Sandstone petrographic composition term indicating that detrital mode for clastic rock (Q-F-L) includes less than 75 percent quartz and the feldspar to lithic ratio is greater than 3 to 1. This corresponds to the arkose field of Folk (1968). For clastic rocks with little or no matrix and cement, rocks in this detrital mode composition category will also have quartz-feldspathic mineralogical composition.</skos:definition>
        <skos:broader rdf:resource="&CompositionCategory;0022"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0003 -->

    <owl:Thing rdf:about="&CompositionCategory;0003">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en">Asphaltic</skos:prefLabel>
        <dc:source xml:lang="en">Jackson 1997</dc:source>
        <skos:definition xml:lang="en"
            >Material contains a significant amount of asphalt, which is &#8220;A dark brown to black viscous liquid or low-melting solid bitumen that consists almost entirely of carbon and hydrogen...[and] formed in oil-bearing rocks by the evaporation of volatiles...&#8221; (Jackson, 1997, p. 39).</skos:definition>
        <skos:broader rdf:resource="&CompositionCategory;0011"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0004 -->

    <skos:Concept rdf:about="&CompositionCategory;0004">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en"
            >Barium-bearing carbonate chemistry</skos:prefLabel>
        <skos:definition xml:lang="en"
            >CO3 is the predominant anion, and greater than 5 mole percent of cations are Ba.</skos:definition>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:broader rdf:resource="&CompositionCategory;0015"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0005 -->

    <owl:Thing rdf:about="&CompositionCategory;0005">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en">Bioclastic</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Carbonate sedimentary material contains significant broken skeletal material</skos:definition>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0006 -->

    <owl:Thing rdf:about="&CompositionCategory;0006">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en">Calcareous</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Calcium carbonate minerals are present, carbonate minerals form less than 50 percent of rock, material contains sufficient CaCO3 to react forming bubbles when hydrochloric acid is applied.</skos:definition>
        <dc:source xml:lang="en">NADM SLTTm 2004</dc:source>
        <skos:broader rdf:resource="&CompositionCategory;0014"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0007 -->

    <owl:Thing rdf:about="&CompositionCategory;0007">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en"
            >Calcareous monomeralic carbonate</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Carbonate minerals form greater than 75 percent of rock, and calcium carbonate minerals forms greater than 75 percent of carbonate minerals.</skos:definition>
        <dc:source xml:lang="en">NADM SLTTm 2004</dc:source>
        <skos:broader rdf:resource="&CompositionCategory;0046"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0008 -->

    <skos:Concept rdf:about="&CompositionCategory;0008">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en"
            >CO3 is the predominante anion, and the Ca is the most abundant (mole percent) cation.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Calcium-rich carbonate chemistry</skos:prefLabel>
        <dc:source xml:lang="en"
            >Struik etal 2002; This vocabulary</dc:source>
        <skos:broader rdf:resource="&CompositionCategory;0015"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0009 -->

    <skos:Concept rdf:about="&CompositionCategory;0009">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en">Calcsilicate</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Material consists of greater than or equal to 50 percent calcsilicate or carbonate minerals and carbonate minerals less than or equal to calcsilicate minerals in mineral mode.</skos:definition>
        <dc:source xml:lang="en">NADM SLTTm 2004</dc:source>
        <skos:broader rdf:resource="&CompositionCategory;0029"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0010 -->

    <skos:Concept rdf:about="&CompositionCategory;0010">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en"
            >Calcsilicate minerals of any sort are present, but less than 50 percent of rock</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Calcsilicate-bearing</skos:prefLabel>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:topConceptOf rdf:resource=""/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0011 -->

    <owl:Thing rdf:about="&CompositionCategory;0011">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en">Carbonaceous</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Material containing significant carbon or hydrocarbon mineral phases, including graphite, coal, oil, gas, lignite, peat.</skos:definition>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:narrower rdf:resource="&CompositionCategory;0003"/>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0012 -->

    <owl:Thing rdf:about="&CompositionCategory;0012">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en"
            >Carbonaceous chemistry</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Containing greater than 5 percent reduced carbon in compounds that are typically the diagenetic products of dead organisms (e.g. coal, bitumen and petroleum). 5 percent value is chosen for consistency with SLTTs usage of term carbonaceous.</skos:definition>
        <dc:source xml:lang="en"
            >Struik2002; SLTTs2004; This vocabulary</dc:source>
        <skos:topConceptOf rdf:resource=""/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0013 -->

    <skos:Concept rdf:about="&CompositionCategory;0013">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en">Carbonate</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Material consists of greater than 50 percent calcsilicate or carbonate minerals and carbonate minerals greater than calcsilicate minerals in mineral mode. Metacarbonate of NADMSC SLTTm (2004) terminology, adopted here for non-genetic use.</skos:definition>
        <dc:source xml:lang="en">NADM SLTTm 2004</dc:source>
        <skos:broader rdf:resource="&CompositionCategory;0029"/>
        <skos:narrower rdf:resource="&CompositionCategory;0046"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0014 -->

    <owl:Thing rdf:about="&CompositionCategory;0014">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en"
            >Carbonate bearing</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Carbonate minerals of any sort are present, but less than 50 percent of rock.</skos:definition>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:narrower rdf:resource="&CompositionCategory;0006"/>
        <skos:narrower rdf:resource="&CompositionCategory;0020"/>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0015 -->

    <owl:Thing rdf:about="&CompositionCategory;0015">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en"
            >Carbonate chemistry</skos:prefLabel>
        <dc:source xml:lang="en"
            >Struik etal 2002; This vocabulary</dc:source>
        <skos:definition xml:lang="en"
            >The carbonate (CO3) anion is the predominant anion (by mole percent) in chemical analysis. Sub-categories may be defined by dominant cation. Material in this chemical class overlaps with the monomineralic carbonate class, and probably overlaps with the Carbonate class in the mineralogical composition classification.</skos:definition>
        <skos:narrower rdf:resource="&CompositionCategory;0004"/>
        <skos:narrower rdf:resource="&CompositionCategory;0008"/>
        <skos:narrower rdf:resource="&CompositionCategory;0032"/>
        <skos:narrower rdf:resource="&CompositionCategory;0040"/>
        <skos:topConceptOf rdf:resource=""/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0016 -->

    <owl:Thing rdf:about="&CompositionCategory;0016">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en"
            >Chloride chemistry</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Chloride is the most abundant (mole percent) anion</skos:definition>
        <dc:source xml:lang="en"
            >Struik etal 2002; This vocabulary</dc:source>
        <skos:broader rdf:resource="&CompositionCategory;0027"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0017 -->

    <owl:Thing rdf:about="&CompositionCategory;0017">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en">Chloritic</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Greater than accessory amount (greater than 5 percent by volume) of chlorite group minerals are present</skos:definition>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0018 -->

    <skos:Concept rdf:about="&CompositionCategory;0018">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en"
            >Composition not specified</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Composition not specified. Use in normative descriptions where any composition property is allowed.</skos:definition>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0019 -->

    <skos:Concept rdf:about="&CompositionCategory;0019">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en"
            >Carbonate minerals form greater than 75 percent of rock, and dolomite forms greater than 75 percent of carbonate minerals.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Dolomite monomineralic carbonate</skos:prefLabel>
        <dc:source xml:lang="en">NADM SLTTm 2004</dc:source>
        <skos:broader rdf:resource="&CompositionCategory;0046"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0020 -->

    <skos:Concept rdf:about="&CompositionCategory;0020">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en"
            >Dolomite is present, carbonate minerals form less than 50 percent of rock.</skos:definition>
        <skos:prefLabel xml:lang="en">Dolomitic</skos:prefLabel>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:broader rdf:resource="&CompositionCategory;0014"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0021 -->

    <owl:Thing rdf:about="&CompositionCategory;0021">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en">Feldspathic</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Sedimentary material contains significant feldspar mineral content</skos:definition>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0022 -->

    <skos:Concept rdf:about="&CompositionCategory;0022">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source xml:lang="en"
            >Dott1964; Folk1968</dc:source>
        <skos:prefLabel xml:lang="en"
            >Feldspathic detrital mode</skos:prefLabel>
        <skos:definition xml:lang="en"
            >General sandstone petrography term for a sandstone in which the ratio of feldspar to lithic fragments is greater than 1:1 and less than 75 percent quartz is present in the detrital mode. Subsumes lithic arkosic and arkosic sandstone petrographic composition classes. Usage of modifier feldspathic is consistent with petrographic subdivisions of Dott (1964) and Williams, Turner and Gilbert (1982)</skos:definition>
        <skos:narrower rdf:resource="&CompositionCategory;0002"/>
        <skos:narrower rdf:resource="&CompositionCategory;0035"/>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0023 -->

    <owl:Thing rdf:about="&CompositionCategory;0023">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en"
            >Feldspathic lithic detrital mode</skos:prefLabel>
        <dc:source xml:lang="en">Folk1968</dc:source>
        <skos:definition xml:lang="en"
            >Sandstone petrographic composition term indicating that detrital mode for clastic rock (Q-F-L) includes less than 75 percent quartz and the feldspar to lithic ratio is between 1 to 1 and 1 to 3. This corresponds to the feldspathic litharenite field of Folk (1968). Rocks in this class are likely to be quartzo-feldspathic in terms of mineralogical composition terms in this vocablary, but because the mineralogical composition of the lithic fraction is not constrainged, this correlation is not necessary.</skos:definition>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0024 -->

    <owl:Thing rdf:about="&CompositionCategory;0024">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en">Ferromagnesian</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Material consists of greater than 40 percent dark ferromagnesian silicate minerals. Standard term defined by Bates and Jackson (1987) to mean &#39;containing iron and magnesium&#39;. Subsumes mafic and ultramafic.</skos:definition>
        <dc:source xml:lang="en">NADM SLTTm 2004</dc:source>
        <skos:narrower rdf:resource="&CompositionCategory;0039"/>
        <skos:narrower rdf:resource="&CompositionCategory;0073"/>
        <skos:topConceptOf rdf:resource=""/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0025 -->

    <skos:Concept rdf:about="&CompositionCategory;0025">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en">Ferruginous</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Greater than accessory amount (greater than 5 percent by volume) of non-silicate iron minerals (hematite, magnetite, siderite...) are present. Rocks containing abundant iron-rich silicate minerals would use the &#39;Ferromagnesian&#39; terms.</skos:definition>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0026 -->

    <skos:Concept rdf:about="&CompositionCategory;0026">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en"
            >Fluoride chemistry</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Fluorine is the most abundant (mole percent) anion</skos:definition>
        <dc:source xml:lang="en"
            >Struik etal 2002; This vocabulary</dc:source>
        <skos:broader rdf:resource="&CompositionCategory;0027"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0027 -->

    <owl:Thing rdf:about="&CompositionCategory;0027">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en"
            >Halide chemistry</skos:prefLabel>
        <skos:definition xml:lang="en"
            >One or more of halide group elements (F, Cl, Br, I) are most abundant (mole percent) anion</skos:definition>
        <dc:source xml:lang="en"
            >Struik etal 2002; This vocabulary</dc:source>
        <skos:narrower rdf:resource="&CompositionCategory;0016"/>
        <skos:narrower rdf:resource="&CompositionCategory;0026"/>
        <skos:narrower rdf:resource="&CompositionCategory;0047"/>
        <skos:topConceptOf rdf:resource=""/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0028 -->

    <skos:Concept rdf:about="&CompositionCategory;0028">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en"
            >Containing 65 percent or more SiO2 by weight. Syn acid.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >High silica chemistry</skos:prefLabel>
        <dc:source xml:lang="en"
            >Struik2002; SLTTs2004; This vocabulary</dc:source>
        <skos:broader rdf:resource="&CompositionCategory;0062"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0029 -->

    <skos:Concept rdf:about="&CompositionCategory;0029">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en"
            >Impure calcsilicate or carbonate</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Material consists of greater than 50 percent calcsilicate or carbonate minerals and relative proportion of calcsilicate and carbonate minerals is unknown or not specified.</skos:definition>
        <dc:source xml:lang="en">NADM SLTTm 2004</dc:source>
        <skos:narrower rdf:resource="&CompositionCategory;0009"/>
        <skos:narrower rdf:resource="&CompositionCategory;0013"/>
        <skos:topConceptOf rdf:resource=""/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0030 -->

    <owl:Thing rdf:about="&CompositionCategory;0030">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition xml:lang="en"
            >Containing SiO2 in the range 52-65 percent, sometimes described as &quot;intermediate&quot; rocks.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Intermediate silica chemistry</skos:prefLabel>
        <dc:source xml:lang="en"
            >Struik2002; SLTTs2004; This vocabulary</dc:source>
        <skos:broader rdf:resource="&CompositionCategory;0062"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0031 -->

    <owl:Thing rdf:about="&CompositionCategory;0031">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition xml:lang="en"
            >Carbonate sedimentary material contains signficant intraclast content</skos:definition>
        <skos:prefLabel xml:lang="en">Intraclastic</skos:prefLabel>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:topConceptOf rdf:resource=""/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0032 -->

    <skos:Concept rdf:about="&CompositionCategory;0032">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en"
            >CO3 is the predominant anion, and iron is greater than 5 mole percent of the cations (includes Fe, Mg, Mn, Ba, Sr, Ca...).</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Iron-bearing carbonate chemistry</skos:prefLabel>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:broader rdf:resource="&CompositionCategory;0015"/>
        <skos:narrower rdf:resource="&CompositionCategory;0033"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0033 -->

    <owl:Thing rdf:about="&CompositionCategory;0033">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition xml:lang="en"
            >CO3 is the predominant anion, and iron is the most abundant (mole percent) cation</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Iron-rich carbonate chemistry</skos:prefLabel>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:broader rdf:resource="&CompositionCategory;0032"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0034 -->

    <skos:Concept rdf:about="&CompositionCategory;0034">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en">Lithic</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Sedimentary material contains significant detrital rock fragments derived by erosion from older, pre-existing rock materials</skos:definition>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:topConceptOf rdf:resource=""/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0035 -->

    <skos:Concept rdf:about="&CompositionCategory;0035">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source xml:lang="en">Folk1968</dc:source>
        <skos:prefLabel xml:lang="en"
            >Lithic arkosic detrital mode</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Sandstone petrographic composition term indicating that detrital mode for clastic rock (Q-F-L) includes less than 75 percent quartz and the ratio of lithic fragments to feldspar is between 1:1 and 1:3. This corresponds to the lithic arkose field of Folk (1968); the 75 percent quartz boundary is chosen to be consistent with quartzsose composition as defined in the mineralogic compostion terms vocabulary. Because of the abundance of lithic fragments for which the mineralogic composition is not specified, and the presence of undiscernible matrix and cement that are not incuded in the detrital mode, corresondence with the mineralogical composition terms is very approximate.</skos:definition>
        <skos:broader rdf:resource="&CompositionCategory;0022"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0036 -->

    <skos:Concept rdf:about="&CompositionCategory;0036">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source xml:lang="en">Folk1968</dc:source>
        <skos:prefLabel xml:lang="en"
            >Lithic detrital mode</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Sandstone petrographic composition term indicating that detrital mode for clastic rock (Q-F-L) includes less than 75 percent quartz and the lithic to feldspar ratio is greater than 3 to 1. This corresponds to the litharenite field of Folk (1968). Because the mineralogy of the lithic fragments is not constrained, the mineralogical composition is largely independent of the detrital mode composition for this class.</skos:definition>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0037 -->

    <skos:Concept rdf:about="&CompositionCategory;0037">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source xml:lang="en"
            >Dott1964; Folk1968</dc:source>
        <skos:prefLabel xml:lang="en"
            >Lithic rich detrital mode</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Sandstone petrographic composition term indicating that the ratio of lithic fragments to feldspar is greater than 1:1, and less than 75 percent quartz is present in the detrital mode. Because the mineralogy of the lithic fragments is not constrained, the mineralogical composition is largely independent of the detrital mode composition for this class.</skos:definition>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0038 -->

    <owl:Thing rdf:about="&CompositionCategory;0038">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition xml:lang="en"
            >Containing SiO2 in the range 44-52 percent. Syn basic.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Low silica chemistry</skos:prefLabel>
        <dc:source xml:lang="en"
            >Struik2002; SLTTs2004; This vocabulary</dc:source>
        <skos:broader rdf:resource="&CompositionCategory;0062"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0039 -->

    <skos:Concept rdf:about="&CompositionCategory;0039">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en">Mafic</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Material consists of greater than or equal to 40 percent and less than 90 percent ferromagnesian silicate minerals.</skos:definition>
        <dc:source xml:lang="en">NADM SLTTm 2004</dc:source>
        <skos:broader rdf:resource="&CompositionCategory;0024"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0040 -->

    <skos:Concept rdf:about="&CompositionCategory;0040">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en"
            >CO3 is the predominant anion, and Mg is the most abundant (mole percent) cation.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Magnesium-rich carbonate chemistry</skos:prefLabel>
        <dc:source xml:lang="en"
            >Struik etal 2002; This vocabulary</dc:source>
        <skos:broader rdf:resource="&CompositionCategory;0015"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compositioncategory/0041 -->

