    <!-- http://resource.geosciml.org/classifierscheme/cgi/201012/compositioncategory -->

    <skos:ConceptScheme rdf:about="">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:historyNote xml:lang="en"
