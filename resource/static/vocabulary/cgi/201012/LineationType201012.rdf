    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/0001 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/lineationtype/0001">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source>this vocabulary</dc:source>
        <skos:definition xml:lang="en"
            >A shape lineation defined by elongate augen.</skos:definition>
        <skos:prefLabel xml:lang="en">Augen lineation</skos:prefLabel>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0015"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0022"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/0002 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/lineationtype/0002">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source>this vocabulary</dc:source>
        <skos:prefLabel xml:lang="en"
            >Bedding cleavage intersection lineation</skos:prefLabel>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Lineation defined by the intersection of bedding surfaces and cleavage surfaces.</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0012"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0022"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/0003 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/lineationtype/0003">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en"
            >Biotite mineral lineation</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Mineral lineation defined by aligned elongate biotite aggregates in a foliation surface. Biotite does not have an elongate (prismatic, acicular) mineral habit, so production of lineation must reflect elongate aggregates of biotite crystals.</skos:definition>
        <dc:source>this vocabulary</dc:source>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0014"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/0004 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/lineationtype/0004">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source>this vocabulary</dc:source>
        <skos:prefLabel xml:lang="en"
            >Cleavage intersection lineation</skos:prefLabel>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Lineation defined by the intersection of two cleavages.</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0012"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0022"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/0005 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/lineationtype/0005">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Crenulation lineation</skos:prefLabel>
        <dc:source>this vocabulary</dc:source>
        <skos:definition xml:lang="en"
            >Lineation defined by small-scale wrinkles, typically of phylosillicate minerals, on another surface, typically a cleavage or schistosity. Use instead of intersection lineation if the wrinkles are the measured feature and the intersecting planar feature is not apparent.</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0013"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0022"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/0006 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/lineationtype/0006">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en"
            >A linear alignment of fabric elements, especially elongate or rod-shaped minerals, interpreted to have been aligned by flow of magma, lava, pyroclastic material, or diapiric flow of sedimentary rock.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Flow lineation</skos:prefLabel>
        <dc:source
            >Geoscience Victoria, Australia</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0013"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/0007 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/lineationtype/0007">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Hornblende mineral lineation</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Mineral lineation defined by aligned hornblende crystals.</skos:definition>
        <dc:source>this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0014"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/0008 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/lineationtype/0008">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source>this vocabulary</dc:source>
        <skos:prefLabel xml:lang="en"
            >Igneous lineation</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Lineation defined by alignment of elongate fabric elements formed in lava or magma before complete crystallization. Some typical fabric elements are elongate phenocrysts and vesicles.</skos:definition>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0009"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0010"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0011"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0013"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/0009 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/lineationtype/0009">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Igneous mineral lineation</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Lineation in igneous rock defined by aligned crystals where there is evidence that the crystal alignment is related to process occurring before the rock was solidified.</skos:definition>
        <dc:source>this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0008"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0014"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/0010 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/lineationtype/0010">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source>this vocabulary</dc:source>
        <skos:prefLabel xml:lang="en"
            >Igneous particle shape lineation</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Lineation in an igneous rock, usually welded tuff, defined by elongate objects in the rock (usually pumice pads, also elongate crystals). Descriptive--elogation may be due to deformation or to rotation/alignment of originally elongate (prolate ellipsoid) objects in the rock.</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0008"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0015"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/0011 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/lineationtype/0011">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>this vocabulary</dc:source>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Igneous vesicle lineation</skos:prefLabel>
        <skos:definition xml:lang="en"
            >lineation defined in a volcanic rock by aligned elongation direction of vesicles.</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0008"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/0012 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/lineationtype/0012">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Intersection lineation</skos:prefLabel>
        <dc:source
            >Neuendorf et al 2005; this vocabulary</dc:source>
        <skos:definition xml:lang="en"
            >A lineation produced by the intersection of two planar fabrics.</skos:definition>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0002"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0004"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0013"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/0013 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/lineationtype/0013">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >Neuendorf et al 2005; this vocabulary</dc:source>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Lineation</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Nongenetic term for a penetrative linear structure.</skos:definition>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0005"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0006"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0008"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0012"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0015"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0016"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0018"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0020"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0022"/>
        <skos:topConceptOf rdf:resource=""/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/0014 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/lineationtype/0014">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source
            >Neuendorf et al 2005; this vocabulary</dc:source>
        <skos:definition xml:lang="en"
            >Lineation defined by alignment of elongate crystals that have a prismatic crystal habit or of monomineralic grain aggregates.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Mineral lineation</skos:prefLabel>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0003"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0007"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0009"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0015"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0019"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/0015 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/lineationtype/0015">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source>this vocabulary</dc:source>
        <skos:definition xml:lang="en"
            >Lineation defined by alignment of long axes of prolate-shaped particles such as clasts, fossils, crystals, or phenocrysts. Particles may be polycrystalline aggregates formed by deformation of pre-existing particles. General category does not specify genesis of particles or their alignment.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Particle shape lineation</skos:prefLabel>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0001"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0010"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0013"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0014"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0017"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0021"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/0016 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/lineationtype/0016">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >based on Neuendorf et al 2005</dc:source>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Lineation defined by tendency of rock to break into long, slender pieces resembling pencils. Typically produced by intesection of bedding and cleavage or intersection of two cleavages. Fabric is generally only observed in weakly metamorphosed rocks.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Pencil lineation</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/0013"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lineationtype/0017 -->

