        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0087"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0113"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0139"/>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0046 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0046">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Earth surface setting</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Geologic environments on the surface of the solid Earth. Hierarchy presented here is based on assumption that a particular setting may be specified by a combination of a climatic setting with one or more process or geomorphically defined settings..</skos:definition>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0010"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0011"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0027"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0041"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0062"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0066"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0069"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0070"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0109"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0119"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0123"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0124"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0132"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0146"/>
        <skos:topConceptOf rdf:resource=""/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0047 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0047">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Contained, embedded, or carried within the body of a glacier or ice sheet; said of meltwater streams, till, drift, moraine</skos:definition>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <skos:prefLabel xml:lang="en"
            >Englacial setting</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0062"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0048 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0048">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >North American Geologic-map Data Model Science Language Technical Team, 2004, Sedimentary materials:  science language for their classification, description, and interpretation in digital geologic-map databases, Version 1.0 (12/18/2004): U.S. Geological Survey Open-File Report 2004-1451 appendix C, 595 p., accessed at http://pubs.usgs.gov/of/2004/1451/sltt/appendixC/</dc:source>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Epicontinental marine setting</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Marine setting situated within the interior of the continent, rather than at the edge of a continent.</skos:definition>
        <skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0033"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0090"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0049 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0049">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition xml:lang="en"
            >A delta that has filled, or is in the process of filling, an estuary</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Estuarine delta setting</skos:prefLabel>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0041"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0051"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0050 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0050">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition xml:lang="en"
            >A lagoon produced by the temporary sealing of a river estuary by a storm barrier. Such lagoons are usually seasonal and exist until the river breaches the barrier; they occur in regions of low or spasmodic rainfall</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Estuarine lagoon setting</skos:prefLabel>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0051"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0079"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0051 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0051">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Environments at the seaward end or the widened funnel-shaped tidal mouth of a river valley where fresh water comes into contact with seawater and where tidal effects are evident (adapted from Glossary of Geology, Jackson, 1997, p. 217).</skos:definition>
        <skos:prefLabel xml:lang="en">Estuary setting</skos:prefLabel>
        <dc:source xml:lang="en"
            >North American Geologic-map Data Model Science Language Technical Team, 2004, Sedimentary materials:  science language for their classification, description, and interpretation in digital geologic-map databases, Version 1.0 (12/18/2004): U.S. Geological Survey Open-File Report 2004-1451 appendix C, 595 p., accessed at http://pubs.usgs.gov/of/2004/1451/sltt/appendixC/</dc:source>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0049"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0050"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0119"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0052 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0052">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Extended terrane setting</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Tectonic setting characterized by extension of the upper crust,  manifested by formation of rift valleys or basin and range physiography, with arrays of low to high angle normal faults. Modern examples include the North Sea, East Africa, and the Basin and Range of the North American Cordillera. Typically applied in continental crustal settings.</skos:definition>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0034"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0131"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0053 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0053">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >Neuendorf et al 2005;</dc:source>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Extra-terrestrial setting</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Material originated outside of the Earth or its atmosphere.</skos:definition>
        <skos:topConceptOf rdf:resource=""/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0054 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0054">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Macdonald 1982</dc:source>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Fast spreading center setting</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Spreading center at which the opening rate is greater than 100 mm per year.</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0005"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0055 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0055">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Floodplain setting</skos:prefLabel>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <skos:definition xml:lang="en"
            >The surface or strip of relatively smooth land adjacent to a river channel, constructed by the present river in its existing regimen and covered with water when the river overflows its banks. It is built of alluvium carried by the river during floods and deposited in the sluggish water beyond the influence of the swiftest current. A river has one floodplain and may have one or more terraces representing abandoned floodplains</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0115"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0123"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0056 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0056">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source xml:lang="en"
            >1-GE WP3, Neuendorf et al.  (2005); p. 249</dc:source>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Forearc setting</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Tectonic setting between a subduction-related trench and a volcanic arc</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0106"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0057 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0057">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source xml:lang="en"
            >1-GE WP3, Neuendorf et al.  (2005); p. 250</dc:source>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Foreland setting</skos:prefLabel>
        <skos:definition xml:lang="en"
            >The exterior area of an orogenic belt where deformation occurs without significant metamorphism. Generally the foreland is closer to the continental interior than other portions of the orogenic belt are.</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0033"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0131"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0058 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0058">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <skos:prefLabel xml:lang="en"
            >Forereef setting</skos:prefLabel>
        <skos:definition xml:lang="en"
            >The seaward side of a reef; the slope covered with deposits of coarse reef talus</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0022"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0059 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0059">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <skos:definition xml:lang="en"
            >A desert plain strewn with wind-abraded pebbles, or gibbers; a gravelly desert.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Gibber plain setting</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0011"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0123"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0132"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0060 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0060">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en"
            >Areas adjacent to glacial front dominated by sediment and water supplied by glacial melting.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source
            >North American Geologic-map Data Model Science Language Technical Team, 2004, Sedimentary materials:  science language for their classification, description, and interpretation in digital geologic-map databases, Version 1.0 (12/18/2004): U.S. Geological Survey Open-File Report 2004-1451 appendix C, 595 p., accessed at http://pubs.usgs.gov/of/2004/1451/sltt/appendixC/</dc:source>
        <skos:prefLabel xml:lang="en"
            >Glacial outwash plain setting</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0062"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0132"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0061 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0061">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Glacier lateral setting</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Settings adjacent to edges of confined glacier.</skos:definition>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0062"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0062 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0062">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Earth surface setting with geography defined by spatial relationship to glaciers (e.g. on top of a glacier, next to a glacier, in front of a glacier...). Processes related to moving ice dominate sediment transport and deposition and landform development. Includes subaqueous, shoreline, and terrestrial settings that are impacted by the presence of glaciers. Considered a geographically defined setting in that a glacier is a geographic feature.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Glacier related setting</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0046"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0047"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0060"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0061"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0109"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0111"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0126"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0128"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0063 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0063">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en"
            >Glacier terminus setting</skos:prefLabel>
        <dc:source
            >NADM SLTTs, 2004</dc:source>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Region of sediment deposition due to melting of glacier ice. ablation and flow till setting.</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0111"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0064 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0064">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Hadal setting</skos:prefLabel>
        <skos:definition xml:lang="en"
            >The deepest oceanic environment, i.e., over 6,000 m in depth. Always in deep sea trench.</skos:definition>
        <skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0038"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0090"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0065 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0065">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >High pressure environment characterized by geothermal gradient significantly lower than standard continental geotherm; enviornment in which blueschist facies metamorphic rocks form. Typically associated with subduction zones.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >High pressure low temperature Earth interior setting</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0045"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0066 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0066">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source
            >North American Geologic-map Data Model Science Language Technical Team, 2004, Sedimentary materials:  science language for their classification, description, and interpretation in digital geologic-map databases, Version 1.0 (12/18/2004): U.S. Geological Survey Open-File Report 2004-1451 appendix C, 595 p., accessed at http://pubs.usgs.gov/of/2004/1451/sltt/appendixC/; Hawley, J.W., and Parsons, R.B. 1980. Glossary of selected geomorphic and geologic terms. Mimeo. USDA Soil Conservation Service, West National Technical Center, Portland, OR. 30 p.</dc:source>
        <skos:definition xml:lang="en"
            >Earth surface setting characterized by surface slope angles high enough that gravity alone becomes a significant factor in geomorphic development, as well as base-of-slope areas influenced by hillslope processes. Hillslope activities include creep, sliding, slumping, falling, and other downslope movements caused by slope collapse induced by gravitational influence on earth materials. May be subaerial or subaqueous.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Hillslope setting</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0046"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0067 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0067">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Hinterland tectonic setting</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Tectonic setting in the internal part of an orogenic belt, characterized by plastic deformation of rocks accompanied by significant metamorphism, typically involving crystalline basement rocks. Typically denotes the most structurally thickened part of an orogenic belt, between a magmatic arc or collision zone and a more &#39;external&#39; foreland setting.</skos:definition>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0033"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0131"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0068 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0068">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Hot spot setting</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Setting in a zone of high heat flow from the mantle. Typically identified in intraplate settings, but hot spot may also interact with active plate margins (Iceland...). Includes surface manifestations like volcanic center, but also includes crust and mantle manifestations as well.</skos:definition>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0076"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0131"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0069 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0069">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source xml:lang="en"
            >Cleland, D.T.; Avers, P.E.; McNab, W.H.; Jensen, M.E.; Bailey, R.G., King, T.; Russell, W.E. 1997. National Hierarchical Framework of Ecological Units, in Boyce, M. S.; Haney, A., ed., Ecosystem Management Applications for Sustainable Forest and Wildlife  Resources: Yale University Press, New Haven, CT. pp. 181-200.</dc:source>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Humid temperate climatic setting</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Setting with seasonal climate having hot to cold or humid to arid seasons.</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0046"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0070 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0070">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source xml:lang="en"
            >Cleland, D.T.; Avers, P.E.; McNab, W.H.; Jensen, M.E.; Bailey, R.G., King, T.; Russell, W.E. 1997. National Hierarchical Framework of Ecological Units, in Boyce, M. S.; Haney, A., ed., Ecosystem Management Applications for Sustainable Forest and Wildlife  Resources: Yale University Press, New Haven, CT. pp. 181-200.</dc:source>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Humid tropical climatic setting</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Setting with hot, humid climate influenced by equatorial air masses, no winter season.</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0046"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0071 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0071">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Hypabyssal setting</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Igneous environment close to the Earth&#39;s surface, characterized by more rapid cooling than plutonic setting to produce generally fine-grained intrusive igneous rock that is commonly associated with co-magmatic volcanic rocks.</skos:definition>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0045"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0072 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0072">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Inactive spreading center setting</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Setting on oceanic crust formed at a spreading center that has been abandoned.</skos:definition>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0076"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0107"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0073 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0073">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Inner neritic setting</skos:prefLabel>
        <skos:altLabel xml:lang="en"
            >Subtidal setting</skos:altLabel>
        <skos:definition xml:lang="en"
            >The ocean environment at depths between low tide level and 30 metres</skos:definition>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0098"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0074 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0074">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <skos:definition xml:lang="en"
            >A pronounced indentation of the delta front between advancing stream distributaries, occupied by shallow water, and either open to the sea or partly enclosed by minor distributaries</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Interdistributary bay setting</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0041"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0124"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0075 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0075">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <skos:prefLabel xml:lang="en"
            >Intertidal setting</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Pertaining to the benthic ocean environment or depth zone between high water and low water; also, pertaining to the organisms of that environment</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0124"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0136"/>
        <skos:inScheme rdf:resource=""/>
