        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0114"/>
        <skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0123"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0132"/>
