    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0093 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0093">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Mid ocean ridge setting</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Ocean highland associated with a divergent continental margin (spreading center). Setting is characterized by active volcanism, locally steep relief, hydrothermal activity, and pelagic sedimentation.</skos:definition>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0099"/>
        <skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0107"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


