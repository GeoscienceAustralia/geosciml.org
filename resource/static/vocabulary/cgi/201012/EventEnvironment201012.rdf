    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0116 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0116">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source
            >North American Geologic-map Data Model Science Language Technical Team, 2004, Sedimentary materials:  science language for their classification, description, and interpretation in digital geologic-map databases, Version 1.0 (12/18/2004): U.S. Geological Survey Open-File Report 2004-1451 appendix C, 595 p., accessed at http://pubs.usgs.gov/of/2004/1451/sltt/appendixC/</dc:source>
        <skos:prefLabel xml:lang="en"
            >Rocky coast setting</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Shoreline with significant relief and abundant rock outcrop.</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0119"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0117 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0117">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en"
            >A sand-covered plain dominated by aeolian processes.</skos:definition>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Sand plain setting</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0006"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0011"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0123"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0132"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0118 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0118">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source>Reading 1978</dc:source>
        <skos:prefLabel xml:lang="en"
            >Seamount setting</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Setting that consists of a conical mountain on the ocean floor (guyot). Typically characterized by active volcanism, pelagic sedimentation. If the mountain is high enough to reach the photic zone, carbonate production may result in reef building to produce a carbonate platform or atoll setting.</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0076"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0099"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0119 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0119">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >based on Neuendorf et al, 2005</dc:source>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Geologic settings characterized by location adjacent to the ocean or a lake. A zone of indefinite width (may be many kilometers), bordering a body of water that extends from the water line inland to the first major change in landform features. Includes settings that may be subaerial, intermittently subaqueous, or shallow subaqueous, but are intrinsically associated with the interface between land areas and water bodies.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Shoreline settings</skos:prefLabel>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0015"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0020"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0026"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0029"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0041"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0046"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0051"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0079"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0080"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0116"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0122"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0129"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0136"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0120 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0120">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source
            >based on NADM SLTTs, 2004</dc:source>
        <skos:prefLabel xml:lang="en"
            >Slope-rise setting</skos:prefLabel>
        <skos:definition xml:lang="en"
            >The part of a subaqueous basin that is between a bordering shelf setting, which separate the basin from an adjacent landmass, and a very low-relief basin plain setting.</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0090"/>
        <skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0138"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0121 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0121">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source>Macdonald 1982</dc:source>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Slow spreading center setting</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Spreading center at which the opening rate is less than 50 mm per year.</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0005"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0122 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0122">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >based on Jackson 1997, p. 626</dc:source>
        <skos:definition xml:lang="en"
            >A prograded shore built seaward by waves and currents, and continuous for some distance along the coast. It is characterized by subparallel beach ridges and swales, in places with associated dunes.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Strandplain setting</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0119"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0123"/>
        <skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0132"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0123 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0123">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <skos:definition xml:lang="en"
            >Setting at the interface between the solid earth and the atmosphere, includes some shallow subaqueous settings in river channels and playas. Characterized by conditions and processes, such as erosion, that exist or operate in the open air on or immediately adjacent to the land surface</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Subaerial setting</skos:prefLabel>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0006"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0029"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0046"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0055"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0059"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0105"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0108"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0117"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0122"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0129"/>
        <skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0132"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0124 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0124">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >based on North American Geologic-map Data Model Science Language Technical Team, 2004, Sedimentary materials:  science language for their classification, description, and interpretation in digital geologic-map databases, Version 1.0 (12/18/2004): U.S. Geological Survey Open-File Report 2004-1451 appendix C, 595 p., accessed at http://pubs.usgs.gov/of/2004/1451/sltt/appendixC/;</dc:source>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Setting situated in or under permanent, standing water. Used for marine and lacustrine settings, but not for fluvial settings.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Subaqueous setting</skos:prefLabel>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0016"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0046"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0074"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0075"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0079"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0090"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0110"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0133"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0125 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0125">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Subduction zone setting</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Tectonic setting at which a tectonic plate, usually oceanic, is moving down into the mantle beneath another overriding plate.</skos:definition>
        <rdfs:comment xml:lang="en">This vocabulary</rdfs:comment>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0106"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0126 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0126">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Formed or accumulated in or by the bottom parts of a glacier or ice sheet; said of meltwater streams, till, moraine, etc.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Subglacial setting</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0062"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0127 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0127">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Large fan-shaped cones of sediment on the ocean floor, generally associated with submarine canyons that provide sediment supply to build the fan..</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Submarine fan setting</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0090"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0128 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0128">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition xml:lang="en"
            >&quot;Carried upon, deposited from, or pertaining to the top surface of a glacier or ice sheet; said of meltwater streams, till, drift, etc. &quot; (Jackson, 1997, p. 639). Dreimanis (1988, p. 39) recommendation that &quot;supraglacial&quot; supersede &quot;superglacial&quot; is followed.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source
            >North American Geologic-map Data Model Science Language Technical Team, 2004, Sedimentary materials:  science language for their classification, description, and interpretation in digital geologic-map databases, Version 1.0 (12/18/2004): U.S. Geological Survey Open-File Report 2004-1451 appendix C, 595 p., accessed at http://pubs.usgs.gov/of/2004/1451/sltt/appendixC/</dc:source>
        <skos:prefLabel xml:lang="en"
            >Supraglacial setting</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0062"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0129 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0129">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Pertaining to the shore area marginal to the littoral zone, just above high-tide level</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Supratidal setting</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0119"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0123"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0130 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0130">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >Neuendorf et al 2005; Soil Science Society of America, 1997.</dc:source>
        <skos:definition xml:lang="en"
            >A water-saturated, periodically wet or continually flooded area with the surface not deeply submerged, essentially without the formation of peat. Marshes are characterized by sedges, cattails, rushes, or other aquatic and grasslike vegetation. Swamps are characterized by tree and brush vegetation.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Swamp or marsh setting</skos:prefLabel>
        <skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0124"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0135"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0146"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0131 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0131">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Setting defined by relationships to tectonic plates on or in the Earth.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Tectonically defined setting</skos:prefLabel>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0012"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0030"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0036"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0052"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0057"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0067"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0068"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0076"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0087"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0103"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0106"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0107"/>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/eventenvironment/0132 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/eventenvironment/0132">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Setting characterized by absence of direct marine influence. Most of the subaerial settings are also terrestrial, but lacustrine settings, while terrestrial, are not subaerial, so the subaerial settings are not included as subcategories.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Terrestrial setting</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0024"/>
        <skos:related rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0033"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0046"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0059"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0060"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0078"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/eventenvironment/0108"/>
