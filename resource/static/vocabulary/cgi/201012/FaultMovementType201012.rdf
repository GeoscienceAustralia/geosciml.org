    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/0001 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0001">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >A fault along which there is some separation parallel to the dip of the fault.</skos:definition>
        <dc:source
            >GGIC vocabularies working group (Australia)</dc:source>
        <skos:prefLabel xml:lang="en"
            >dip separation sense</skos:prefLabel>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0002"/>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/0002 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0002">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >GGIC vocabularies working group (Australia)</dc:source>
        <skos:definition
            >The net slip of the fault lies in the dip direction of the fault.</skos:definition>
        <skos:prefLabel xml:lang="en">dip slip</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0001"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/0003 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0003">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="&cgi;faultmovementtype">extraction</skos:notation>
        <dc:source
            >Froitzheim, N., Pleuger, J. &amp; Nagel, T.J., 2006. Extraction faults. Journal of Structural Geology v28, pp. 1388-1395.</dc:source>
        <skos:definition
            >A fault whose two sides have approached each other substantially in the direction perpendicular to the fault.</skos:definition>
        <skos:prefLabel xml:lang="en">extraction</skos:prefLabel>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0005"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0007"/>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/0004 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0004">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >The fault is horizontal.</skos:definition>
        <dc:source
            >GGIC vocabularies working group (Australia)</dc:source>
        <skos:prefLabel xml:lang="en">horizontal</skos:prefLabel>
        <skos:topConceptOf rdf:resource=""/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/0005 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0005">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >Froitzheim, N., Pleuger, J. &amp; Nagel, T.J., 2006. Extraction faults. Journal of Structural Geology v28, pp. 1388-1395.</dc:source>
        <skos:definition
            >An extraction fault with some displacement within the fault plane.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >mixed extraction</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0003"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/0006 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0006">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >The net slip of the fault lies between the strike and dip directions of the fault; the slip vector rakes between 10 and 80 degrees in the plane of the fault.</skos:definition>
        <dc:source
            >GGIC vocabularies working group (Australia); SMR</dc:source>
        <skos:prefLabel xml:lang="en">oblique slip</skos:prefLabel>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0013"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0014"/>
        <skos:topConceptOf rdf:resource=""/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/0007 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0007">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >An extraction fault with no discernible displacement within the fault plane.</skos:definition>
        <dc:source
            >Froitzheim, N., Pleuger, J. &amp; Nagel, T.J., 2006. Extraction faults. Journal of Structural Geology v28, pp. 1388-1395.</dc:source>
        <skos:prefLabel xml:lang="en">pure extraction</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0003"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/0008 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0008">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >Neuendorf et al 2005</dc:source>
        <skos:definition
            >A fault on which there is increasing offset or separation along the strike from an initial point of no offset, with reverse offset in the opposite direction.</skos:definition>
        <skos:prefLabel xml:lang="en">scissor</skos:prefLabel>
        <skos:topConceptOf rdf:resource=""/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/0009 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0009">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >A fault along which there is some separation parallel to the strike of the fault.</skos:definition>
        <dc:source
            >GGIC vocabularies working group (Australia)</dc:source>
        <skos:prefLabel xml:lang="en"
            >strike separation sense</skos:prefLabel>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0010"/>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/0010 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0010">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >GGIC vocabularies working group (Australia); SMR</dc:source>
        <skos:definition
            >The net slip of the fault (slip vector) is parallel to the strike of the fault.</skos:definition>
        <skos:prefLabel xml:lang="en">strike slip</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0009"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0011"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0012"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0015"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/0011 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0011">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >A large scale strike-slip fault in which the fault surface is steeply inclined.</skos:definition>
        <dc:source
            >GGIC vocabularies working group (Australia)</dc:source>
        <skos:prefLabel xml:lang="en">transcurrent</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0010"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/0012 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0012">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >A variety of strike-slip fault along which the displacement suddenly stops or changes form; typically associated with mid-ocean ridges.</skos:definition>
        <dc:source
            >GGIC vocabularies working group (Australia)</dc:source>
        <skos:prefLabel xml:lang="en">transform</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0010"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/0013 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0013">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >A fault along which strike-slip deformation is accompanied by a component of shortening transverse to the fault.</skos:definition>
        <dc:source
            >GGIC vocabularies working group (Australia)</dc:source>
        <skos:prefLabel xml:lang="en">transpressional</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0006"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/0014 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0014">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >GGIC vocabularies working group (Australia)</dc:source>
        <skos:definition
            >A fault along which strike-slip deformation is accompanied by a component of extension transverse to the fault.</skos:definition>
        <skos:prefLabel xml:lang="en">transtensional</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0006"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/faultmovementtype/0015 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0015">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >GGIC vocabularies working group (Australia)</dc:source>
        <skos:definition
            >A strike slip fault in which the faut plane is more or less vertical.</skos:definition>
        <skos:prefLabel xml:lang="en">wrench</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/faultmovementtype/0010"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


