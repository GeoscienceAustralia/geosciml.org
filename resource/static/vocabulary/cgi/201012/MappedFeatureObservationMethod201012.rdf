    <!-- http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0001 -->

    <owl:Thing rdf:about="&mappedfeatureobservationmethod2;0001">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en">Compilation</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Mapped feature geometry derived from one or more published or unpublished data sources, involving some interpretation or generalization by the compiler; not an exact digital reproduction of the sources.</skos:definition>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0002 -->

    <skos:Concept rdf:about="&mappedfeatureobservationmethod2;0002">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en"
            >Direct observation</skos:prefLabel>
        <dc:source
            >Federal Geographic Data Committee, 2006</dc:source>
        <skos:definition xml:lang="en"
            >Indicates that mapped feature was delineated and a specifier assigned by direct visual observation.</skos:definition>
        <skos:narrower rdf:resource="&mappedfeatureobservationmethod2;0017"/>
        <skos:narrower rdf:resource="&mappedfeatureobservationmethod2;0018"/>
        <skos:narrower rdf:resource="&mappedfeatureobservationmethod2;0021"/>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0003 -->

    <skos:Concept rdf:about="&mappedfeatureobservationmethod2;0003">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en">Inertial survey</skos:prefLabel>
        <skos:definition
            >mapped feature is located by inertial survey methods</skos:definition>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="&mappedfeatureobservationmethod2;0025"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0004 -->

    <owl:Thing rdf:about="&mappedfeatureobservationmethod2;0004">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >Indicates that mapped feature is not directly observable, location and specification based on inference from available evidence.</skos:definition>
        <dc:source
            >Federal Geographic Data Committee, 2006</dc:source>
        <skos:prefLabel xml:lang="en">Inferred</skos:prefLabel>
        <skos:narrower rdf:resource="&mappedfeatureobservationmethod2;0007"/>
        <skos:narrower rdf:resource="&mappedfeatureobservationmethod2;0010"/>
        <skos:narrower rdf:resource="&mappedfeatureobservationmethod2;0011"/>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0010 -->

    <owl:Thing rdf:about="&mappedfeatureobservationmethod2;0010">
        <skos:narrower rdf:resource="&mappedfeatureobservationmethod2;0005"/>
        <skos:narrower rdf:resource="&mappedfeatureobservationmethod2;0006"/>
        <skos:narrower rdf:resource="&mappedfeatureobservationmethod2;0008"/>
        <skos:narrower rdf:resource="&mappedfeatureobservationmethod2;0009"/>
        <skos:narrower rdf:resource="&mappedfeatureobservationmethod2;0015"/>
        <skos:narrower rdf:resource="&mappedfeatureobservationmethod2;0016"/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0005 -->

    <skos:Concept rdf:about="&mappedfeatureobservationmethod2;0005">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en"
            >Inferred-aeromagnetic survey</skos:prefLabel>
        <skos:definition
            >Location and specification inferred based on magnetic field data acquired by airborne sensor.</skos:definition>
        <dc:source
            >based on Matti et al 1997; definition this vocabulary</dc:source>
        <skos:broader rdf:resource="&mappedfeatureobservationmethod2;0010"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0006 -->

    <owl:Thing rdf:about="&mappedfeatureobservationmethod2;0006">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >Location and specification inferred based on measurements made by electronic bore hole logging device(s) that measure geophysical properties.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Inferred-borehole geophysical log</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="&mappedfeatureobservationmethod2;0010"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0007 -->

    <owl:Thing rdf:about="&mappedfeatureobservationmethod2;0007">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >Geologic feature in bore hole inferred based on interpretation of drill cuttings.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Inferred-drill cuttings</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="&mappedfeatureobservationmethod2;0004"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0008 -->

    <skos:Concept rdf:about="&mappedfeatureobservationmethod2;0008">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >based on Matti et al 1997; definition this vocabulary</dc:source>
        <skos:definition
            >Location and specification inferred based on measured variations in the acceleration of gravity related to mass of underlying Earth material.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Inferred-gravity data</skos:prefLabel>
        <skos:broader rdf:resource="&mappedfeatureobservationmethod2;0010"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0009 -->

    <skos:Concept rdf:about="&mappedfeatureobservationmethod2;0009">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >based on Matti et al 1997; definition this vocabulary</dc:source>
        <skos:prefLabel xml:lang="en"
            >Inferred-ground magnetic survey</skos:prefLabel>
        <skos:definition
            >Location and specification inferred based on magnetic field data acquired by sensor on the earth surface, either at manually occupied stations, or by a vehicle-based continuously collected sensor data.</skos:definition>
        <skos:broader rdf:resource="&mappedfeatureobservationmethod2;0010"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0010 -->

    <owl:Thing rdf:about="&mappedfeatureobservationmethod2;0010">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en"
            >Inferred using indirect method</skos:prefLabel>
        <skos:definition
            >Location and specification inferred based on properties related to lithology, e.g. geophysical or geochemical signature, vegetation, surface texture, reflectance.</skos:definition>
        <dc:source
            >based on Matti et al 1997; definition this vocabulary</dc:source>
        <skos:broader rdf:resource="&mappedfeatureobservationmethod2;0004"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0011 -->

    <owl:Thing rdf:about="&mappedfeatureobservationmethod2;0011">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en"
            >Inferred projection between observed locations</skos:prefLabel>
        <skos:definition
            >Location and specification determined based on inferred continuity with observed locations, and assumption of continuity of the feature.</skos:definition>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="&mappedfeatureobservationmethod2;0004"/>
        <skos:narrower rdf:resource="&mappedfeatureobservationmethod2;0012"/>
        <skos:narrower rdf:resource="&mappedfeatureobservationmethod2;0013"/>
        <skos:narrower rdf:resource="&mappedfeatureobservationmethod2;0014"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0012 -->

    <owl:Thing rdf:about="&mappedfeatureobservationmethod2;0012">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >Federal Geographic Data Committee, 2006</dc:source>
        <skos:prefLabel xml:lang="en"
            >Inferred projection beneath covering mapped unit</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Phenomenon inferred to exist based on continuity with observed locations, and assumption of continuity of the feature. Mappable feature is covered by mapped younger geologic unit. Concealed feature.</skos:definition>
        <skos:broader rdf:resource="&mappedfeatureobservationmethod2;0011"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0013 -->

    <owl:Thing rdf:about="&mappedfeatureobservationmethod2;0013">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en"
            >Inferred projection beneath unmapped surficial material</skos:prefLabel>
        <skos:definition
            >Location and specification determined based on inferred continuity with observed locations, and assumption of continuity of the feature. Not observable because of covering mantle of soil, talus or colluvium, not mapped as separate geologic units.</skos:definition>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="&mappedfeatureobservationmethod2;0011"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0014 -->

    <skos:Concept rdf:about="&mappedfeatureobservationmethod2;0014">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Location and specification determined based on inferred continuity with observed locations, and assumption of continuity of the feature. Not observable because of obscuring vegetation, but inferred to crop out at surface.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Inferred projection-obscured by vegetation</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="&mappedfeatureobservationmethod2;0011"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0015 -->

    <skos:Concept rdf:about="&mappedfeatureobservationmethod2;0015">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Location and specification inferred based on flux or intensity of radiation related to the decay of radioactive elements in observed Earth material. Use for both airborne and ground-surface based sensor.</skos:definition>
        <dc:source
            >based on Matti et al 1997; definition this vocabulary</dc:source>
        <skos:prefLabel xml:lang="en"
            >Inferred-radiometric survey</skos:prefLabel>
        <skos:broader rdf:resource="&mappedfeatureobservationmethod2;0010"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0016 -->

    <owl:Thing rdf:about="&mappedfeatureobservationmethod2;0016">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >based on Matti et al 1997; definition this vocabulary</dc:source>
        <skos:prefLabel xml:lang="en"
            >Inferred-remote sensing imagery</skos:prefLabel>
        <skos:definition
            >Location and specification inferred based on spectral characteristics of light measured by airborne or satellite based sensor. Phenomenon visible on standard (visible light) air photographs are considered &#39;observable&#39;.</skos:definition>
        <skos:broader rdf:resource="&mappedfeatureobservationmethod2;0010"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0017 -->

    <skos:Concept rdf:about="&mappedfeatureobservationmethod2;0017">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en"
            >Observed in aerial imagery</skos:prefLabel>
        <skos:definition
            >Phenomenon observed and located using aerial imagery (standard visible light)</skos:definition>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="&mappedfeatureobservationmethod2;0002"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0018 -->

    <skos:Concept rdf:about="&mappedfeatureobservationmethod2;0018">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Geologic feature observed in core obtained from a borehole.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Observed in borehole core</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="&mappedfeatureobservationmethod2;0002"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0019 -->

    <owl:Thing rdf:about="&mappedfeatureobservationmethod2;0019">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en"
            >Observed-distant outcrop</skos:prefLabel>
        <dc:source
            >based on Matti et al 1997; definition this vocabulary</dc:source>
        <skos:definition
            >Phenomenon observed and located from a distance.</skos:definition>
        <skos:broader rdf:resource="&mappedfeatureobservationmethod2;0021"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0020 -->

    <owl:Thing rdf:about="&mappedfeatureobservationmethod2;0020">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition xml:lang="en"
            >Geomorphic features located and specified based on surface morphology determined by elevation (or bathemetry) data, e.g.  DEM, LIDAR</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Observed based on elevation data</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0021 -->

    <owl:Thing rdf:about="&mappedfeatureobservationmethod2;0021">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >based on Matti et al 1997; definition this vocabulary</dc:source>
        <skos:prefLabel xml:lang="en"
            >Observed in outcrop</skos:prefLabel>
        <skos:definition
            >Phenomenon observed and located directly in outcrop; geologist occupied location of phenomenon, and traced it explicitly over mapped extent.</skos:definition>
        <skos:broader rdf:resource="&mappedfeatureobservationmethod2;0002"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0022 -->

    <owl:Thing rdf:about="&mappedfeatureobservationmethod2;0022">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en"
            >Observed-outcrop- topographic map location</skos:prefLabel>
        <skos:definition
            >Phenomenon observed directly in outcrop, and located by positioning on a topographic base map relative to physiographic or cultural features</skos:definition>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="&mappedfeatureobservationmethod2;0021"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/mappedfeatureobservationmethod/0023 -->

    <owl:Thing rdf:about="&mappedfeatureobservationmethod2;0023">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en"
            >Observed-outcrop-GPS location</skos:prefLabel>
