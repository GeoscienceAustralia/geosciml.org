        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0006"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0010"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0011"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0012"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0013"/>
        <skos:topConceptOf rdf:resource=""/>
        <skos:inScheme rdf:resource=""/>
