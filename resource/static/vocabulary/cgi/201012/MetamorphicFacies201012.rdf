    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0008 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0008">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >For use in instance descriptions to indicate that no data are available to constrain metamorphic facies.  May be metamorphosed or non-metamorphosed.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Metamorphic facies unknown</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:topConceptOf rdf:resource=""/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0009 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0009">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Corresponds to non-metamorphosed condition; since metamorphic facies classification is based on mineral assemblages, absence of metamorphic minerals means that the classification does not apply.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >No metamorphic minerals</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:topConceptOf rdf:resource=""/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0010 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0010">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Metamorphic facies characterized in rocks of basaltic composition by clinopyroxene-orthopyroxene-plagioclase (olivine stable with plagioclase).</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Pyroxene hornfels metamorphic facies</skos:prefLabel>
        <dc:source xml:lang="en"
            >Smulikowski et al. 2003;Fettes and Desmons 2007</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0007"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0011 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0011">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <dc:source xml:lang="en"
            >Fettes and Desmons 2007</dc:source>
        <skos:prefLabel xml:lang="en"
            >Sanidinite metamorphic facies</skos:prefLabel>
        <skos:definition xml:lang="en"
            >distinguished from the pyroxene hornfels facies by the occurrence of especially high-temperature varieties and polymorphs of minerals (eg. pigeonite, K-rich labradorite); assemblage for rocks of basaltic composition</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0007"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0012 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0012">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Metamorphic facies characterized in rocks of basaltic composition by prehnite-pumpellyite, pumpellyite-actinolite, prehnite-actinolite (prehnite and pumpellyite are the diagnostic Ca-Al silicates rather than minerals of the epidote or zeolite groups).</skos:definition>
        <dc:source xml:lang="en"
            >Smulikowski et al. 2003; Fettes and Desmons 2007</dc:source>
        <skos:prefLabel xml:lang="en"
            >Subgreenschist metamorphic facies</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0007"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0013 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0013">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Metamorphic facies characterized in rocks of basaltic composition by zeolite minerals such as laumontite and heulandites (in place of other Ca-Al silicates such as prehnite, pumpellyite and epidote).</skos:definition>
        <dc:source xml:lang="en"
            >Smulikowski et al. 2003; Fettes and Desmons 2007</dc:source>
        <skos:prefLabel xml:lang="en"
            >Zeolite metamorphic facies</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0007"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


