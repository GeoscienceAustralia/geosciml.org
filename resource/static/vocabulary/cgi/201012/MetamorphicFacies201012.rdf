    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0001 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0001">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >Smulikowski et al. 2003; Fettes and Desmons 2007</dc:source>
        <skos:prefLabel xml:lang="en"
            >Amphibolite metamorphic facies</skos:prefLabel>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Metamorphic facies characterized in rocks of basaltic composition by hornblende-plagioclase (plagioclase more calcic than An17).</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0007"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0002 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0002">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Eclogite metamorphic facies</skos:prefLabel>
        <dc:source xml:lang="en"
            >Fettes and Desmons 2007</dc:source>
        <skos:definition xml:lang="en"
            >omphacite-garnet-quartz (no plagioclase, olivine stable with garnet); assemblage for rocks of basaltic composition</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0007"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0003 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0003">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Epidote amphibolite metamorphic facies</skos:prefLabel>
        <dc:source xml:lang="en"
            >Fettes and Desmons 2007</dc:source>
        <skos:definition xml:lang="en"
            >hornblende-albite-epidote(-chlorite); assemblage for rocks of basaltic composition</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0007"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0004 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0004">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Glaucophane-schist facies</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Metamorphic facies characterized in rocks of basaltic composition by glaucophane-epidote (-garnet), glaucophane-lawsonite, or glaucophane-lawsonite-jadeite.</skos:definition>
        <dc:source xml:lang="en"
            >Smulikowski et al., 2003; Fettes and Desmons 2007</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0007"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0005 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0005">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Granulite facies</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Metamorphic facies characterized in rocks of basaltic composition by the mineral assemblage clinopyroxene-orthopyroxene-plagioclase (olivine not stable with plagioclase or with garnet), or especially high-temperature varieties or polymorphs of minerals (e</skos:definition>
        <dc:source xml:lang="en"
            >Smulikowski et al., 2003; Fettes and Desmons 2007</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0007"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0006 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0006">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Greenschist metamorphic facies</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Metamorphic facies characterized in rocks of basaltic composition by actinolite-albite-epidote-chlorite (an epidote group mineral is the diagnostic Ca-Al silicate rather than prehnite or pumpellyite).</skos:definition>
        <dc:source xml:lang="en"
            >Smulikowski et al., 2003; Fettes and Desmons 2007</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0007"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0007 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0007">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Metamorphic facies not specified</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Metamorphosed, but facies not specified in more detail. For use in normative description to indicate that any metamorphic facies value is valid.</skos:definition>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/metamorphicfacies/0001"/>
