        <skos:prefLabel xml:lang="en"
            >Kalsilitic and melilitic rocks</skos:prefLabel>
        <dc:source
            >based on LeMaitre et al. 2002</dc:source>
        <rdfs:seeAlso xml:lang="en">coppaelite</rdfs:seeAlso>
        <rdfs:seeAlso xml:lang="en">kalsilitite</rdfs:seeAlso>
