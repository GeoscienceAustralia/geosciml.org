        <skos:broader rdf:resource="&SimpleLithology;0039"/>
        <skos:narrower rdf:resource="&SimpleLithology;0051"/>
        <skos:broader rdf:resource="&SimpleLithology;0204"/>
        <skos:narrower rdf:resource="&SimpleLithology;0259"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lithology/0157 -->

    <owl:Thing rdf:about="&SimpleLithology;0157">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >IUGS SCMR 2007 (http://www.bgs.ac.uk/SCMR/), SLTTm1.0 2004</dc:source>
        <skos:prefLabel xml:lang="en">Marble</skos:prefLabel>
        <skos:definition
            >Metamorphic rock consisting of greater than 75 percent fine- to coarse-grained recrystallized calcite and/or dolomite; usually with a granoblastic, saccharoidal texture.</skos:definition>
