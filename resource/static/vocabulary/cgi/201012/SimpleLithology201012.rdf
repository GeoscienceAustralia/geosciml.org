    <!-- http://resource.geosciml.org/classifier/cgi/lithology/0151 -->

    <skos:Concept rdf:about="&SimpleLithology;0151">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:historyNote xml:lang="en"
            >2009-12-14 SMR change ID and pref label from &#39;exotic alkalic igneous rock&#39; to kalsilitic igneous rock to avoid confusion with &#39;exotic alkaline rock&#39; based on OGE recommendation.</skos:historyNote>
        <skos:definition xml:lang="en"
