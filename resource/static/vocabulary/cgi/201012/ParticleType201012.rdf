    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0001 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0001">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Fragment of rock generated by disruption as a direct result of volcanic action but not formed by previous activity of the magmatic system; generally derived from subvolcanic basement.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Accidental pyroclastic fragment</skos:prefLabel>
        <dc:source
            >Gillespie and Styles 1999</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0050"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0002 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0002">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en"
            >Aggregate non-clastic particle</skos:prefLabel>
        <skos:definition
            >Generic term for a non-clastic particle that is itself composed of an aggregation of particles;</skos:definition>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0005"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0012"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0028"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0031"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0040"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0052"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0054"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0055"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0003 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0003">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >based on Neuendorf et al 2005</dc:source>
        <skos:definition
            >Cavity filled with secondary minerals; denotes that cavities are filled vesicles, thus restricted to volcanic rock.</skos:definition>
        <skos:prefLabel xml:lang="en">Amygdule</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0009"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0004 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0004">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >Gillespie and Styles,1999</dc:source>
        <skos:definition
            >Fragments of extrusive igneous rock formed by mechanical friction of moving lava flow, breakage of chilled lava flow rinds, or gravity crumbling of active spines and domes. Gillespie and Styles (1999) include as type of juvenile pyroclastic fragment, inconsistent with definition of pyroclastic by IUGS and BGS.</skos:definition>
        <skos:prefLabel xml:lang="en">Autoclast</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0024"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0005 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0005">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >Williams,Turner,Gilbert 1954</dc:source>
        <skos:definition
            >Elongate to irregular lense with radial aggregates of acicular and fibrous mineral coalescing along a central axis.</skos:definition>
        <skos:prefLabel xml:lang="en">Axiolite</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0002"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0006 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0006">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >Granular sedimentary particle that is a &quot;fragmentary piece of a shell, bone, or other hard skeletal structure of an animal, plant, or protozoan. May be fossilized or non-fossilized&quot;. Use in situations where fossil organism can not be identified, thus &#39;material fossil&#39; is inappropriate.</skos:definition>
        <skos:prefLabel xml:lang="en">Bioclast</skos:prefLabel>
        <dc:source xml:lang="en">NADM SLTTs 2004</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0007"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0007 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0007">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >&quot;A type of granular particle formed by the physiological activities of organisms (plants, animals, protozoa) that produce body parts which subsequently are incorporated into a sediment aggregate. Contrast with nonbiogenic particle. Includes biogenic objects that are whole or fragmentary (bioclasts); can be fossilized or non-fossilized&quot;</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Biogenic particle</skos:prefLabel>
        <dc:source xml:lang="en">NADM SLTTs 2004</dc:source>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0006"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0013"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0018"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0024"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0036"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0008 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0008">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en">Bleb</skos:prefLabel>
        <skos:definition
            >Generic term for a small rounded particle of uncertain origin</skos:definition>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0054"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0009 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0009">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en">Cavity</skos:prefLabel>
        <skos:definition
            >Constituent is empty space between particles in a compound material; generally filled with some sort of fluid. Cavity particle types can be used to describe porosity in a granular material.</skos:definition>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0003"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0019"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0021"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0035"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0048"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0060"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0061"/>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0010 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0010">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >&quot;A general term for a grain that has coats or layers of foreign material, usually fine mud-sized and occasionally recrystallized, that form concentric or overlapping shells around a core of rock, shell, peloidal, or intraclastic material.&quot; (eg: oolith, pisolith)</skos:definition>
        <skos:prefLabel xml:lang="en">Coated particle</skos:prefLabel>
        <dc:source xml:lang="en">NADM SLTTs 2004</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0024"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0041"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0042"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0047"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0011 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0011">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >Base on Gillespie and Styles, 1999</dc:source>
        <skos:prefLabel xml:lang="en"
            >Cognate fragments</skos:prefLabel>
        <skos:definition
            >Pyroclast consisting of rock which formed during earlier (related) volcanic activity that have been ejected with other pyroclastic debris during a later eruption.</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0050"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0012 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0012">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >Neuendorf et al, 2005</dc:source>
        <skos:definition
            >A hard, compact mass or aggregate of mineral matter, normally subsperical but commonly oblate, disc-shaped or irregular. Formed by precipitation of mineral from solution in the pores of a granular rock, localized around a nucleus or center, to define a discrete, sharply separated object. Size ranges from cm to meters. Particle geometry description for concretion describes the concretion size and shape, not the size and shape of particles forming the concretion.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Concretionary particle</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0002"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0013 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0013">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >The fossilised excrement of vertebrate animals</skos:definition>
        <skos:prefLabel xml:lang="en">Coprolite</skos:prefLabel>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0007"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0014 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0014">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >A fragment of a crystal interpreted to have been broken by pyroclastic processes.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Crystal fragment</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0050"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0015 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0015">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >Constituent that is the product of crystallization during formation of a compound Earth material (see NADMC1 2004). Serves to distinguish crystalline from granular rocks (Struik, 2002). Includes constituents crystallized in evaporite environments, during diagenesis or hydrothermal alteration, or in other low-temperature environments.</skos:definition>
        <dc:source
            >Struik, 2002; NADM SLTTs 2004</dc:source>
        <skos:prefLabel xml:lang="en"
            >Crystalline grain</skos:prefLabel>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0016"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0025"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0038"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0044"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0049"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0054"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0062"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0016 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0016">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Constitutent occurs as minute, spherical, rod, or hair-like forms. Typically appear isotropic in thin section.</skos:definition>
        <skos:prefLabel xml:lang="en">Crystallite</skos:prefLabel>
        <dc:source
            >Williams,Turner,Gilbert, 1954</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0015"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0017 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0017">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >An individual constituent, grain or fragment of a sediment or rock, produced by erosion of a larger rock mass. Particle whose origin as a fragment is a result of surface (sedimentary, weathering...) processes. &#39;Crystals, crystal fragments, glass and rock fragments that have been liberated from any type of pre-existing consolidated rock (volcanic or non-volcanic) by weathering or erosion and transported from the site of origin by gravity, air, water, or ice&#39; [Schmid, 1981]. Distinguished from intraclast in that epiclast is derived from a pre-existing rock from outside the basin of deposition (NADM SLTTs 2004) before introduction into a CompoundMaterial (sense of NADMC1, 2004).</skos:definition>
        <skos:prefLabel xml:lang="en">Epiclast</skos:prefLabel>
        <dc:source xml:lang="en">Schmid 1981</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0024"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0033"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0039"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0018 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0018">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en">Fecal pellet</skos:prefLabel>
        <skos:definition
            >an organic excrement, mainly of invertebrates, occuring especially in modern marine sediments but also fossilised in some sedimentary rocks, usually with simple ovoid form, less commonly rod-shaped.</skos:definition>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0007"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0046"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0019 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0019">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >based on Neuendorf et al 2005</dc:source>
        <skos:prefLabel xml:lang="en">Fenestra</skos:prefLabel>
        <skos:definition
            >Primary or penecontemporaneous gap or cavity in the framework of a sedimentary rock, larger than grain-supported intersticies. May be open space or have partial to complete fill with secondary cement or introduced sediment.</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0009"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0020 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0020">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >Lens-shaped bodies, usually mm to cm thick, and centimeters to 1-2 decimeters long, typically seen on surfaces of some pyroclastic rocks. The name fiamme comes from the Italian word for flames, describing their shape. The term is descriptive and non-genetic.Generally interpreted to form by the collapse of pumice fragments during welding in a hot pyroclastic deposit.</skos:definition>
        <skos:prefLabel xml:lang="en">Fiamme</skos:prefLabel>
        <dc:source
            >Neuendorf et al 2005; http://en.wikipedia.org/wiki/Fiamme</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0028"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0021 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0021">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >A cavity within a crystal containing liquid and/or gas, formed by the entrapment in crystal irregularities of fluid, commonly that from which the mineral crystallised.</skos:definition>
        <skos:prefLabel xml:lang="en">Fluid inclusion</skos:prefLabel>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0009"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0022 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0022">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >Base on Gillespie and Styles, 1999</dc:source>
        <skos:prefLabel xml:lang="en"
            >Fluidal pyroclast</skos:prefLabel>
        <skos:definition
            >Pyroclast that has a a rounded, fluidal shape indicating that it was in a wholly or partly molten state during formation and subsequent transport. Concept corresponds to bomb, but with no size denotation.</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0032"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0023 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0023">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >An aggregate of crystals of the same mineral</skos:definition>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <skos:prefLabel xml:lang="en">Glomerocryst</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0028"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0024 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0024">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en"
            >Granular particle</skos:prefLabel>
        <skos:definition
            >Solid constituent in a compound earth material (sense of NADMC1) that is a pre-existing object before incorporation into a particulate aggregate. &quot;...a component of solid material that has the form of grains, clasts, fragments, or whole objects of any size, shape, composition, texture, and structure.&quot; (NADMSC SLTTs, 2004)</skos:definition>
        <dc:source xml:lang="en">NADM SLTTs 2004</dc:source>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0004"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0007"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0010"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0017"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0030"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0046"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0050"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0051"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0054"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0057"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0059"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0025 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0025">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en">Holoblast</skos:prefLabel>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <skos:definition
            >A crystalline grain in a metamorphic rock that is newly and completely formed during metamorphism</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0015"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0026 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0026">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >Gillespie and Styles 1999</dc:source>
        <skos:definition
            >Juvenile pyroclastic fragment formed by magma-water interaction during subaqueous or subglacial extrusion; typically consist of chilled glass.</skos:definition>
        <skos:prefLabel xml:lang="en">Hydroclast</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0032"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0027 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0027">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en"
            >Hydrothermal vein</skos:prefLabel>
        <dc:source
            >Neuendorf et al 2005; this vocabulary</dc:source>
        <skos:definition
            >a tabular or sheet-like part of a compound material formed by hydrothermal (or other metasomatic) mineral filling a fracture; may be associated with replacement of the host rock adjacent to the body</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0031"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0028 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0028">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Aggregate non-clastic particle that are of igneous origin. Particle types for compound material are restricted to inclusions that are typically of a size consistent with hand-sample description to characterize a kind of rock or a rock sample; larger inclusion types are included in the GeologicUnitPart role vocabulary.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Igneous inclusion</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0002"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0020"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0023"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0043"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0029 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0029">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >a tabular or sheet like part of a compound material formed by the intrusion of magma</skos:definition>
        <skos:prefLabel xml:lang="en">Igneous vein</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0031"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0030 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0030">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >NADM SLTTs 2004; Neuendorf et al., 2005; Folk, 1959</dc:source>
        <skos:definition
            >A broad, general term introduced by Folk (1959, p. 4) for a component of limestone, representing a torn-up and re-worked fragment of a penecontemporaneous sediment (usually weakly consolidated) that has been eroded within the basin of deposition...and re-deposited there...The fragment may range in size from fine sand to gravel...</skos:definition>
        <skos:prefLabel xml:lang="en">Intraclast</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0024"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0031 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0031">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en">Intrusive sheet</skos:prefLabel>
        <skos:definition
            >a tabular or sheet-like part of a compound material, genetic origin not specified</skos:definition>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0002"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0027"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0029"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0032 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0032">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >Gillespie and Styles 1999</dc:source>
        <skos:definition
            >Fragment formed directly from cooling magma during transport prior to primary deposition.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Juvenile pyroclastic fragment</skos:prefLabel>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0022"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0026"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0050"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0053"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0033 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0033">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >A clast derived by erosion from older, pre-existing rock materials of any kind, usually beyond the basin of accumulation but occasionally from within the basin of deposition (as from a submarine or subaqueous exposure adjacent to a depositional site). A clast that is identified as a piece of rock.</skos:definition>
        <skos:prefLabel xml:lang="en">Lithic clast</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0017"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0034"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0034 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0034">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >&quot;A mechanically formed and deposited fragment of a carbonate rock...derived from an older, lithified limestone or dolomite within, adjacent to, or outside the depositional site&quot;</skos:definition>
        <dc:source
            >Neuendorf et al., 2005, p. 375</dc:source>
        <skos:prefLabel xml:lang="en">Lithoclast</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0033"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0035 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0035">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >Bubble-like cavity with concentric shells of finely crystalline minerals; open space remains in core of structure. Radiating fibrous structure may be present in secondary mineral fill. Typically in silicic volcanic rock.</skos:definition>
        <skos:prefLabel xml:lang="en">Lithophysae</skos:prefLabel>
        <dc:source
            >Neuendorf et al 2005</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0009"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0036 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0036">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >GeoSciML v2 RC1 model element scope note</dc:source>
        <skos:definition
            >The preserved remains or replaced remains (casts) of plants and animals. A fossil type may have one or more described associated organisms. If particle type is materialFossil, an additional type property element may provide a reference to a Paleontologic description of the fossil.</skos:definition>
        <skos:prefLabel xml:lang="en">Material fossil</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0007"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0037 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0037">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >Neuendorf et al 2005</dc:source>
        <skos:definition
            >An irregular cavity in a phaneritic igneous rock into which small crystals of the rock-forming minerals protrude.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Miarolitic cavity</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0061"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0038 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0038">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >Williams,Turner,Gilbert, 1954</dc:source>
        <skos:definition
            >Constituent occurs as minute incipient crystals that display some birefringence in thin section.</skos:definition>
        <skos:prefLabel xml:lang="en">Microlite</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0015"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0039 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0039">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >An epiclast that is composed of a single mineral.</skos:definition>
        <skos:prefLabel xml:lang="en">Mineral clast</skos:prefLabel>
        <dc:source xml:lang="en">NADM SLTTs 2004</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0017"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0040 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0040">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >An irregularly rounded mass of a mineral or mineral aggregate normally having a warty or knobby surface and no internal sructure, usually with a contrasting composition from the enclosing sediment or rock matrix in which it is embedded, and that can be separated as a discrete mass from the host material. Igneous nodules should be classified as &#39;Igneous inclusions&#39;.</skos:definition>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <skos:prefLabel xml:lang="en">Nodule</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0002"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0056"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0041 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0041">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >Jackson 1997; Hallsworth and Knox 1999</dc:source>
        <skos:definition
            >A &#39;...coated grain with a cortex of irregular, partially overlapping laminae. They are typically irregular in shape and may exhibit biogenic structures. Some forms lack a distinct nucleus. Oncoids are generally larger than 2 mm.&#39; (Hallsworth and Knox, 1999, p. 27). Synonymous with oncolite: A small, variously shaped, concentrically laminated, calcareous sedimentary structure, resembling an oolith, and formed by the accretion of successive layer[s]... It...generally does not exceed 10 cm in dimension. (Jackson, 1997, p. 446).</skos:definition>
        <skos:prefLabel xml:lang="en">Oncoid</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0010"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0042 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0042">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >Jackson1997; Hallsworth and Knox 1999</dc:source>
        <skos:definition
            >A general, nongenetic term for a particle that resembles an oolith in outer appearance and size- (Jackson, 1997, p. 447). Hallsworth and Knox (1999, p. 27) describe ooids as coated grains that typically are spherical or ellipsoidal in shape, with the degree of roundness increasing outward. Concentric to semi-concentric coats are smoothly and evenly laminated. A nucleus usually is present, and may have a composition different from the coatings. Biogenic structures are not obvious. NADMSC SLTTs (2004) considers synomous with oolith.</skos:definition>
        <skos:prefLabel xml:lang="en">Ooid</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0010"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0043 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0043">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >igneous constituent typically mafic, equant rounded spheroid with concentric mineralogic banding</skos:definition>
        <skos:prefLabel xml:lang="en">Orb</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0028"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0044 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0044">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >Neuendorf et al. 2005</dc:source>
        <skos:definition
            >A crystal or remnant of a crystal that is older than other mineral grains in the rock, especially in metamorphic rocks</skos:definition>
        <skos:prefLabel xml:lang="en">Paleoblast</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0015"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0045 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0045">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <skos:prefLabel xml:lang="en">Paramorph</skos:prefLabel>
        <skos:definition
            >a pseudomorph with the same composition as the original crystal (eg: calcite after aragonite)</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0049"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0046 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0046">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >a usually rounded aggregate of clay-sized calcareous (micritic) material; origin and size is not specified</skos:definition>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <skos:prefLabel xml:lang="en">Peloid</skos:prefLabel>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0018"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0024"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0047 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0047">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >NADM SLTTs 2004; Jackson 1997</dc:source>
        <skos:definition
            >&quot;A round or ellipsoidal accretionary body resembling a pea in size and shape.... A pisoid...is larger and less regular in form than an ooid, although it may have the same concentric and/or radial internal structure&quot; (Jackson, 1997, p. 489).</skos:definition>
        <skos:prefLabel xml:lang="en">Pisoid</skos:prefLabel>
