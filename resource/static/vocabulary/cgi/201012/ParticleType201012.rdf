    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0059">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >A granular particle that has been shaped, worn, faceted, cut or polished by the action of windblown sand.</skos:definition>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <skos:prefLabel xml:lang="en">Ventrifact</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0024"/>
