    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0056 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0056">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Particle formed by chemical precipitation at sediment-water interface, lacking layered structure that characterizes coated grains. Includes glauconite grains, manganese nodules, phosphate grains. Manganese nodule--&quot;An irregular, black to brown, friable,</skos:definition>
        <dc:source
            >NADM SLTTs 2004; Jackson 1997</dc:source>
        <skos:prefLabel xml:lang="en"
            >Syngenetic nodule</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0040"/>
