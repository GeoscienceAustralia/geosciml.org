    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0055 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0055">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en">Spherule</skos:prefLabel>
        <skos:definition
            >a rounded or spherical mass of acicular or fibrous mineral, generally in glassy siliceous lava.</skos:definition>
        <dc:source
            >Williams,Turner,Gilbert 1954</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0002"/>
