        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0048 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0048">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Open space between particles in a granular aggregate</skos:definition>
        <dc:source
            >Neuendorf et al 2005, this vocabulary</dc:source>
        <skos:prefLabel xml:lang="en">Pore space</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0009"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0049 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0049">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >Constituent particle that has the outward form of a mineral species or particle type, but the original minerals composing the particle have been altered or replaced by different minerals; a secondary mineral whose outward crystal form has been inherited from the orignal mineral it has replaced</skos:definition>
        <dc:source
            >Neuendorf et al. 2005; Spry 1969; Fettes and Desmons, 2007</dc:source>
        <skos:prefLabel xml:lang="en">Pseudomorph</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0015"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0045"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0050 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0050">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >&quot;An individual particle ejected during a volcanic eruption.&quot; (Jackson, 1997, p. 521). Clast whose origin is a direct result of volcanic process (excludes fragments in lava autobreccia) and has not been reworked by sedimentary processes (Gillespie and Styles 1999).</skos:definition>
        <dc:source
            >NADM SLTTs 2004; Jackson, 1997; Gillespie and Styles 1999</dc:source>
        <skos:prefLabel xml:lang="en">Pyroclast</skos:prefLabel>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0001"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0011"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0014"/>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0024"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0032"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0051 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0051">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >Fragment formed as a direct result of volcanic activity and reworked by sedimentary processes.</skos:definition>
        <dc:source
            >Gillespie and Styles 1999</dc:source>
        <skos:prefLabel xml:lang="en"
            >Reworked pyroclastic fragment</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0024"/>
