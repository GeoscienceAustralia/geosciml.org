        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/particletype/0052 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/particletype/0052">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source xml:lang="en">GGIPAC</dc:source>
        <skos:prefLabel xml:lang="en">Schlieren</skos:prefLabel>
        <skos:definition
            >a tabular body, generally a few cm to a few metres long, within a plutonic rock, having different mineral proportions and colour to the surrounding rock</skos:definition>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/particletype/0002"/>
        <skos:inScheme rdf:resource=""/>
