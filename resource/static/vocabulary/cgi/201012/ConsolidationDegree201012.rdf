    <!-- http://resource.geosciml.org/classifier/cgi/consolidationdegree/001 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/consolidationdegree/001">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en">Consolidated</skos:prefLabel>
        <skos:definition
            >Particulate constituents of a compound material adhere to each other strongly enough that the aggregate can be considered a solid material in its own right.</skos:definition>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/002"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/007"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/012"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/consolidationdegree/002 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/consolidationdegree/002">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >In normative descriptions, indicates that consolidation state is not a determining factor in identification, it may have any value.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Consolidation not specified</skos:prefLabel>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/consolidationdegree/003 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/consolidationdegree/003">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >Consolidation ranges from unconsolidated to indurated on scale of description</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Consolidation variable</skos:prefLabel>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/002"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/consolidationdegree/004 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/consolidationdegree/004">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Particulate constituents of a compound material do not adhere to each other strongly enough that the aggregate can be considered a solid in its own right.</skos:definition>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:prefLabel xml:lang="en">Unconsolidated</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/002"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/005"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/011"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/013"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/consolidationdegree/005 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/consolidationdegree/005">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >NADM SLTTs 2004 after Bowles 1984</dc:source>
        <skos:definition
            >Easily indented with fingers; Relative density 0.0-0.2.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Unconsolidated, very loose</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/004"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/consolidationdegree/006 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/consolidationdegree/006">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Material is lithified, but induration varies at scale of description.</skos:definition>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:prefLabel xml:lang="en"
            >Variable induration</skos:prefLabel>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/consolidationdegree/007"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/consolidationdegree/007 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/consolidationdegree/007">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >NADM SLTTs 2004 after Bowles 1987</dc:source>
        <skos:prefLabel xml:lang="en">Indurated</skos:prefLabel>
        <skos:definition
