        <rdfs:label xml:lang="en">related match</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI"
            >http://www.w3.org/2004/02/skos/core</rdfs:isDefinedBy>
        <rdfs:subPropertyOf rdf:resource="&skos;mappingRelation"/>
        <rdfs:subPropertyOf rdf:resource="&skos;related"/>
        <owl:inverseOf rdf:resource="&skos;relatedMatch"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#semanticRelation -->

    <owl:ObjectProperty rdf:about="&skos;semanticRelation">
        <rdfs:label xml:lang="en"
            >semantic relation</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI"
            >http://www.w3.org/2004/02/skos/core</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en"
