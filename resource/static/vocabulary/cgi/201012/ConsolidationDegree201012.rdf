        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI"
            >http://www.w3.org/2004/02/skos/core</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en"
            >skos:related is disjoint with skos:broaderTransitive</rdfs:comment>
        <owl:inverseOf rdf:resource="&skos;related"/>
        <rdfs:subPropertyOf rdf:resource="&skos;semanticRelation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#relatedMatch -->

    <owl:ObjectProperty rdf:about="&skos;relatedMatch">
