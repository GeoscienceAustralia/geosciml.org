    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0001 -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole;0001">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en"
            >Armoured relict crystal</skos:prefLabel>
        <skos:definition
            >a crystal of an earlier rock that is prevented from further reaction in a later rock by a rim of reaction products</skos:definition>
        <dc:source
            >base on Neuendorf et al 2005</dc:source>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;0025"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0002 -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;0002">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en"
            >Armoured relict inclusion</skos:prefLabel>
        <skos:definition
            >an inclusion of an earlier rock that is prevented from further reaction in a later rock by a rim of reaction products</skos:definition>
        <dc:source
            >base on Neuendorf et al 2005</dc:source>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;0015"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0003 -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;0003">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en">Chadacryst</skos:prefLabel>
        <skos:definition
            >the enclosed crystal in a poikolitic texture</skos:definition>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;0025"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0004 -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;0004">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en"
            >Clast supporting orthomatrix</skos:prefLabel>
        <skos:definition
            >Orthomatrix in matrix supported sedimentary rock.</skos:definition>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;0023"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0005 -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;0005">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >Neuendorf et al, 2005</dc:source>
        <skos:definition
            >A cluster of ferromagnesian minerals in an igneous rock, from several centimeters to decimeters in diameter, that may be a segregation or an altered xenolith.</skos:definition>
        <skos:prefLabel xml:lang="en">Clot</skos:prefLabel>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;0034"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0006 -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole;0006">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >For descriptions in which role of constituent in compound material is not specified.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Constituent role not specified</skos:prefLabel>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:topConceptOf rdf:resource=""/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0007 -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole;0007">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >Constituent occurs as a concentric envelope enclosing another constituent. Corona is a non-genetic term.</skos:definition>
        <skos:prefLabel xml:lang="en">Corona</skos:prefLabel>
        <dc:source
            >Williams, Turner and Gilbert, 1954; Spry, 1969; Fettes and Desmons, 2007</dc:source>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;0008"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;0018"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;0034"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0008 -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole;0008">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >A corona formed by a secondary mineral around an orginal igneous crystal, formed by modification of the crystal by the corrosive action of its parent magma</skos:definition>
        <skos:prefLabel xml:lang="en">Corrosion rim</skos:prefLabel>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;0007"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0009 -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;0009">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Constituent forms an interconnected network of discernible crystals.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Crystalline framework constituent</skos:prefLabel>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;0025"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0010 -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;0010">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >&quot;An oversized stone in laminated sediment that depresses the underlying laminae and may be covered by -draped laminae. Most dropstones originate through ice-rafting; other sources are floating tree roots and kelp holdfasts....&quot;</skos:definition>
        <skos:prefLabel xml:lang="en">Dropstone</skos:prefLabel>
        <dc:source
            >NADM SLTTs 2004; Jackson, 1997, p 193</dc:source>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;0034"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0011 -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole;0011">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >A type of matrix consisting of inhomogeneous interstitial materials grown in originally open interstices during diagenesis, but lacking the homogeneity and clear textural evidence of pore-filling needed to classify as phyllosilicate cement.</skos:definition>
        <dc:source
            >NADM SLTTs 2004; Dickinson, 1970</dc:source>
        <skos:prefLabel xml:lang="en">Epimatrix</skos:prefLabel>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;0020"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0012 -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;0012">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Constituent is a clast that has no visible contacts with other clasts. Interpreted to be largely or completly immersed in matrix or cement.</skos:definition>
        <dc:source
            >Pettijohn et al., 1972, p. 90; SMR</dc:source>
        <skos:prefLabel xml:lang="en">Floating clast</skos:prefLabel>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;0034"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0013 -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;0013">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >&quot;Constituent forms a rigid arrangement of particles that support one another at their points of contact...constituting a mechanically firm structure capable of supporting open pore spaces, although interstices may be occupied by cement or matrix&quot;</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Granular framework constituent</skos:prefLabel>
        <dc:source
            >Jackson, 1997, p. 250</dc:source>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;0025"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0014 -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;0014">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Constituent forms an interconnected network of material enclosing other constituents that are disguished by larger grain size.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Groundmass constituent</skos:prefLabel>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;0016"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0015 -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;0015">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >Neuendorf et al, 2005</dc:source>
        <skos:definition
            >a fragment of older material within an igneous rock to which it may or may not be genetically related</skos:definition>
        <skos:prefLabel xml:lang="en">Inclusion</skos:prefLabel>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;0002"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;0034"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0016 -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole;0016">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >Constituent is distributed through the material between other constituent particles</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Interstitial constituent</skos:prefLabel>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;0014"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;0020"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;0032"/>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0017 -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole;0017">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >A constituent with irregular distribution and geometry, as in pseudobreccia or patch migmatite.</skos:definition>
        <skos:prefLabel xml:lang="en">Irregular part</skos:prefLabel>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;0034"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0018 -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole;0018">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >Williams, Turner and Gilbert, 1954; Fettes and Desmons, 2007</dc:source>
        <skos:definition
            >Corona that consists of concentric bands with radial fibrous texture.</skos:definition>
        <skos:prefLabel xml:lang="en">Kelyphitic rim</skos:prefLabel>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;0007"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0019 -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;0019">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >A thin sheet compositionally distinct from the surrounding material, related to primary genesis of rock, e.g. sedimentary layers, metamorphic segregation</skos:definition>
        <skos:prefLabel xml:lang="en">Layer</skos:prefLabel>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;0034"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0020 -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole;0020">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >Constituent forms finer-grained material interstitial to a framework constituent. &quot;The finer-grained material enclosing, or filling the interstices between, the larger grains or particles of a sediment or sedimentary rock....The term refers to the relative size and disposition of the particles, and no particular particle size is implied&quot; (Jackson, 1997, p. 393). May be classifiable into orthomatrix, protomatrix, epimatrix, pseudomatrix, and unclassified matrix.</skos:definition>
        <skos:prefLabel xml:lang="en">Matrix</skos:prefLabel>
        <dc:source
            >NADM SLTTs 2004; Jackson, 1997</dc:source>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;0011"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;0016"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;0023"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;0030"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;0031"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0021 -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole;0021">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:source
            >Neuendorf et al 2005</dc:source>
        <skos:prefLabel xml:lang="en">Megacryst</skos:prefLabel>
        <skos:definition
            >any crystal in an igneous or metamorphic rock that is sgnificantly larger than the surounding groundmass. May be a phenocryst, xenocryst, porphyroblast or porphyroclast</skos:definition>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;0025"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;0027"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;0028"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;0029"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0022 -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole;0022">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >Constituent occurs as crystals that poikilitically enclose crystals of other phases in an igneous rock.</skos:definition>
        <dc:source
            >Neuendorf et al., 2005</dc:source>
        <skos:prefLabel xml:lang="en">Oikocryst</skos:prefLabel>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;0025"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0023 -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole;0023">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >NADM SLTTs 2004; Dickinson, 1970</dc:source>
        <skos:definition
            >A type of matrix consisting of recrystallized detrital lutum (clay fraction, less than 2 micron) or protomatrix.</skos:definition>
        <skos:prefLabel xml:lang="en">Orthomatrix</skos:prefLabel>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;0004"/>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;0020"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0024 -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole;0024">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:source
            >NADM SLTTs 2004; Neuendorf et al. 2005</dc:source>
        <skos:definition
            >Constituent crystallized in crystallographic continuity with some other mineral constituent, typically quartz or calcite. In a clastic rock, commonly forms cement as well, but this should be represented using two role attribute links, &#39;overgrowth&#39; and cement, because overgrowth does not necessarily imply cement.</skos:definition>
        <skos:prefLabel xml:lang="en">Overgrowth</skos:prefLabel>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole;0034"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/0025 -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole;0025">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en"
            >Particulate constituent</skos:prefLabel>
        <skos:definition
            >constituent occurs as a collection of particles that are characterized by average properties of the individual particles</skos:definition>
        <dc:source xml:lang="en">This vocabulary</dc:source>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;0001"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;0003"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;0009"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;0013"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;0021"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;0022"/>
        <skos:narrower rdf:resource="&CompoundMaterialConstituentPartRole;0026"/>
        <skos:inScheme rdf:resource=""/>
