    <!-- http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/0006 -->

    <owl:Thing rdf:about="http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/0006">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >Orientation determined based on measurements from aerial photography or satellite imagery (in conjunction with an elevation model.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Photogeologic determination</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/0007 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/0007">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Orientation measured using compass or other instrument directly on or at an outcrop of the structure.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Standard on site measure</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:broader rdf:resource="http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/0004"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/0008 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/0008">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Orientation determined by fitting a plane to three or more points located on the geologic surface of interest.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Three point determination</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/0009 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/0009">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Orientation of a surface is measured by visually averaging across one or more outcrops in a small area--e.g. approximating dip by looking down strike of beds, approximating strike by outcrop trace of one or more beds.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Visual surface estimation on outcrop</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
