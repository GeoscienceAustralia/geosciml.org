    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/0001">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en"
            >Calculated average orientation</skos:prefLabel>
        <skos:definition
            >Orientation value is specified using a calculated average of a collection of related orientations (computer generalization).</skos:definition>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/0002 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/0002">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en"
            >Estimate from air photo</skos:prefLabel>
        <skos:definition
            >Orientation of a geologic structure estimated based on inspection or measurements on an air photograph.</skos:definition>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/0003 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/0003">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Orientation of a geologic structure based on observation from a distance great enough to preclude direct inspection of the structure to determine orientation.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Estimate from distance</skos:prefLabel>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/0004 -->

    <skos:Concept rdf:about="http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/0004">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en"
            >Measure on outcrop</skos:prefLabel>
        <skos:definition
            >Orientation of surface or line is measured on an outcrop of that surface or line directly, e.g. by measuring a particular bedding surface, a 3-D exposure of a fold hinge, a particular stretched mineral grain in a foliation surface.</skos:definition>
        <dc:source xml:lang="en">this vocabulary</dc:source>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/0007"/>
        <skos:narrower rdf:resource="http://resource.geosciml.org/classifier/cgi/determinationmethodorientation/0009"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


