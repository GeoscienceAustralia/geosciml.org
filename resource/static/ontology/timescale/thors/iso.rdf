  xml:base="http://resource.geosciml.org/ontology/timescale/thors/iso">
  <owl:Ontology rdf:about="">
    <dc:title>Temporal Hierarchical Ordinal Reference System model aligned to ISO 19108 &amp; ISO 19156</dc:title>
    <dc:creator>Simon Jonathan David COX, CSIRO Australia</dc:creator>
    <dct:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
    <dc:description>Temporal Hierarchical Ordinal Reference System model aligned to OWL implementation of ISO 19108 &amp; ISO 19156</dc:description>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
    >2017-04-28</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
    >2017-07-23</dct:modified>
    <owl:imports rdf:resource="http://def.seegrid.csiro.au/isotc211/iso19108/2002/temporal"/>
    <owl:imports rdf:resource="http://resource.geosciml.org/ontology/timescale/thors"/>
