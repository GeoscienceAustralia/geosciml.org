    <rdfs:subClassOf rdf:resource="http://def.seegrid.csiro.au/isotc211/iso19108/2002/temporal#Edge"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/thors#member">
    <rdfs:subPropertyOf rdf:resource="http://def.seegrid.csiro.au/isotc211/iso19108/2002/temporal#member"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/thors#referencePoint">
    <rdfs:subPropertyOf rdf:resource="http://def.seegrid.csiro.au/isotc211/iso19108/2002/temporal#primitive"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/thors#EraBoundary">
