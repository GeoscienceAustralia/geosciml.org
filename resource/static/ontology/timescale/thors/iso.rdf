  <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/thors#end">
    <rdfs:subPropertyOf rdf:resource="http://def.seegrid.csiro.au/isotc211/iso19108/2002/temporal#end"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/thors#component">
    <rdfs:subPropertyOf rdf:resource="http://def.seegrid.csiro.au/isotc211/iso19108/2002/temporal#primitive"/>
    <rdfs:subPropertyOf rdf:resource="http://def.seegrid.csiro.au/isotc211/iso19108/2002/temporal#component"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/thors#positionalUncertainty">
    <rdfs:subPropertyOf rdf:resource="http://def.seegrid.csiro.au/isotc211/iso19103/2005/basic#measure"/>
    <rdfs:range rdf:resource="http://def.seegrid.csiro.au/isotc211/iso19103/2005/basic#Time"/>
    <rdfs:domain rdf:resource="http://def.seegrid.csiro.au/isotc211/iso19108/2002/temporal#TemporalPosition"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/thors#Era">
