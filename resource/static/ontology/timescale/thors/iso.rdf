  </rdf:Description>
  <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/thors#system">
    <rdfs:subPropertyOf rdf:resource="http://def.seegrid.csiro.au/isotc211/iso19108/2002/temporal#system"/>
    <rdfs:subPropertyOf rdf:resource="http://def.seegrid.csiro.au/isotc211/iso19108/2002/temporal#complex"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/thors#previousEra">
    <rdfs:subPropertyOf rdf:resource="http://def.seegrid.csiro.au/isotc211/iso19108/2002/temporal#previousEdge"/>
